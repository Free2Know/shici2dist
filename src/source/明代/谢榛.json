      "_id": null,
      "Name": "古意",
      "PoetId": "4dd7ae78e5564f1b8d9f84793b1a9c1e",
      "PoetName": "谢榛",
      "Form": "五绝",
      "Tags": [
        "明代",
        "谢榛"
      ],
      "Contents": [
        "青山无大小，",
        "总隔郎行路。",
        "远近生寒云，",
        "愁恨不知数。"
      ]
    },
    {
      "_id": null,
      "Name": "塞下曲",
      "PoetId": "4dd7ae78e5564f1b8d9f84793b1a9c1e",
      "PoetName": "谢榛",
      "Form": "五绝",
      "Tags": [
        "明代",
        "谢榛"
      ],
      "Contents": [
        "暝色满西山，",
        "将军猎骑还。",
        "隔河见烽火，",
        "骄虏夜临关。"
      ]
    },
    {
      "_id": null,
      "Name": "秋闺曲",
      "PoetId": "4dd7ae78e5564f1b8d9f84793b1a9c1e",
      "PoetName": "谢榛",
      "Form": "五绝",
      "Tags": [
        "明代",
        "谢榛"
      ],
      "Contents": [
        "目极江天远，",
        "秋霜下白蘋。",
        "可怜南去雁，",
        "不为倚楼人。"
      ]
    }
  ]
}