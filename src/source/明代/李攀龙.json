    {
      "_id": null,
      "Name": "于郡城送明卿之江西",
      "PoetId": "c03f89c15fbd4fbfb2871b165c6d24b1",
      "PoetName": "李攀龙",
      "Form": "七绝",
      "Tags": [
        "明代",
        "李攀龙"
      ],
      "Contents": [
        "青枫飒飒雨凄凄，",
        "秋色遥看入楚迷。",
        "谁向孤舟怜逐客，",
        "白云相送大江西。"
      ]
    },
    {
      "_id": null,
      "Name": "塞上曲",
      "PoetId": "c03f89c15fbd4fbfb2871b165c6d24b1",
      "PoetName": "李攀龙",
      "Form": "七绝",
      "Tags": [
        "明代",
        "李攀龙"
      ],
      "Contents": [
        "白羽职霜出塞寒，",
        "胡烽不断接长安。",
        "城头一片西山月，",
        "多少征人马上看。"
      ]
    }
  ]
}