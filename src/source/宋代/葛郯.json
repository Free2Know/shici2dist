  "Poems": [
    {
      "_id": null,
      "Name": "兰陵王",
      "PoetId": "504aae071b0946f88d7c6b7e1d9deb4e",
      "PoetName": "葛郯",
      "Form": "词",
      "Tags": [
        "宋代",
        "葛郯"
      ],
      "Contents": [
        "乱烟簇。",
        "帘外青山渐肃。",
        "莲房静。",
        "荷盖半残，",
        "欲放清涟媚溪绿。",
        "凭高送远目。",
        "飞起沧洲雁骛。",
        "寒窗静，",
        "茶碗未深，",
        "一枕胡床画眠足。",
        "闲行问松菊。",
        "今□雨谁家，",
        "空对银烛。",
        "箫声忽下瑶台曲。",
        "看鹤舞风动，",
        "乌啼云起，",
        "何须舟内怨女哭。",
        "抱琴写幽独。",
        "情触。",
        "会相续。",
        "况节近中元，",
        "月浪翻屋。",
        "长鲸愁晓寒蟾促。",
        "要百柁倾□，",
        "万花流玉。",
        "山肴倒尽，",
        "又空腹，",
        "鲙野蔌。"
      ]
    },
    {
      "_id": null,
      "Name": "念奴娇",
      "PoetId": "504aae071b0946f88d7c6b7e1d9deb4e",
      "PoetName": "葛郯",
      "Form": "词",
      "Tags": [
        "宋代",
        "葛郯"
      ],
      "Contents": [
        "冯夷微怒，",
        "被鲛人水府，",
        "织成绡縠。",
        "何处飞来双白鹭，",
        "点破一溪寒玉。",
        "岸柳烟迷，",
        "海棠酒困，",
        "赢得春眠足。",
        "凭栏搔首，",
        "为谁消遣愁目。",
        "遥想居士床头，",
        "竹渠新雨，",
        "溜瓮中春醁。",
        "不惜千锺为客寿，",
        "倒卧南山新绿。",
        "晚月催归，",
        "春风留住，",
        "费尽纱笼烛。",
        "恍疑仙洞，",
        "梦游天柱林屋。"
      ]
    },
    {
      "_id": null,
      "Name": "感皇恩",
      "PoetId": "504aae071b0946f88d7c6b7e1d9deb4e",
      "PoetName": "葛郯",
      "Form": "词",
      "Tags": [
        "宋代",
        "葛郯"
      ],
      "Contents": [
        "风雨半摧残，",
        "一园花老。",
        "绿遍池塘夜来草。",
        "看花何处，",
        "莫被此花相恼。",
        "世间多少事，",
        "邯郸道。",
        "凭远下临，",
        "暗尘飞绕。",
        "数点烟中树、水村小。",
        "斜阳且住，",
        "为我花间留照。",
        "从教红满地，",
        "何须扫。"
      ]
    },
    {
      "_id": null,
      "Name": "朝中措",
      "PoetId": "504aae071b0946f88d7c6b7e1d9deb4e",
      "PoetName": "葛郯",
      "Form": "词",
      "Tags": [
        "宋代",
        "葛郯"
      ],
      "Contents": [
        "双鞬锦领出西山。",
        "戎幕护旌旗。",
        "横槊春风百咏，",
        "临淮夜月千卮。",
        "风流樽俎，",
        "琼花破艳，",
        "红药攒枝。",
        "赏尽竹西歌吹，",
        "珠帘十里香迷。"
      ]
    },
    {
      "_id": null,
      "Name": "柳梢青",
      "PoetId": "504aae071b0946f88d7c6b7e1d9deb4e",
      "PoetName": "葛郯",
      "Form": "词",
      "Tags": [
        "宋代",
        "葛郯"
      ],
      "Contents": [
        "空中雨阁。",
        "一段轻阴，",
        "翠铺林薄。",
        "橘内仙翁，",
        "棋边公子，",
        "共成清乐。",
        "主人有井留车，",
        "看席上、云轻柳弱。",
        "斗转檐头，",
        "鸡鸣窗外，",
        "无人知觉。"
      ]
    },
    {
      "_id": null,
      "Name": "水调歌头",
      "PoetId": "504aae071b0946f88d7c6b7e1d9deb4e",
      "PoetName": "葛郯",
      "Form": "词",
      "Tags": [
        "宋代",
        "葛郯"
      ],
      "Contents": [
        "年来惯行役，",
        "楚尾又吴头。",
        "餐霞吸露，",
        "何事佳处辄迟留。",
        "云听渔舟夜唱，",
        "花落牧童横笛，",
        "占尽五湖秋。",
        "胡床兴不浅，",
        "人在庾公楼。",
        "绣帘卷，",
        "曲阑暝，",
        "翠鬟愁。",
        "森罗万象，",
        "与渠诗里一时收。",
        "天设四桥风月，",
        "地会三州山水，",
        "邀我伴沙鸥。",
        "明朝起归梦，",
        "一枕过苹洲。"
      ]
    },
    {
      "_id": null,
      "Name": "江神子·江城子",
      "PoetId": "504aae071b0946f88d7c6b7e1d9deb4e",
      "PoetName": "葛郯",
      "Form": "词",
      "Tags": [
        "宋代",
        "葛郯"
      ],
      "Contents": [
        "粼粼白水护青田。",
        "想真仙。",
        "弄清莲。",
        "十里香风，",
        "吹下碧云天。",
        "月在草堂人未寝，",
        "松竹暗，",
        "水涓涓。",
        "夜阑何事悄无言。",
        "怨空传。",
        "事难圆。",
        "欲借寒光，",
        "谁与伴清妍。",
        "待得凌波人肯住，",
        "呼玉笛，",
        "劝金船。"
      ]
    },
    {
      "_id": null,
      "Name": "洞仙歌",
      "PoetId": "504aae071b0946f88d7c6b7e1d9deb4e",
      "PoetName": "葛郯",
      "Form": "词",
      "Tags": [
        "宋代",
        "葛郯"
      ],
      "Contents": [
        "藐姑仙子，",
        "天外谁为侣。",
        "八极浮游气为驭。",
        "看朝餐沆瀣，",
        "暮饮醍醐，",
        "瑶台冷，",
        "吹落九天风露。",
        "翠空云幕净，",
        "宝鉴无尘，",
        "碧树秋来暗消暑。",
        "残夜水明楼，",
        "影落寒溪，",
        "行人起、沙头唤渡。",
        "任角声、吹落小梅花，",
        "梦不到渔翁，",
        "一蓑烟雨。"
      ]
    },
    {
      "_id": null,
      "Name": "满庭霜·满庭芳",
      "PoetId": "504aae071b0946f88d7c6b7e1d9deb4e",
      "PoetName": "葛郯",
      "Form": "词",
      "Tags": [
        "宋代",
        "葛郯"
      ],
      "Contents": [
        "红叶飞时，",
        "青山缺处，",
        "云横秋影斜阳。",
        "凤凰旌节，",
        "何事到吾乡。",
        "要见大江东去，",
        "寒光静、水与天长。",
        "人争看，",
        "恩袍焕锦，",
        "新惹御炉香。",
        "满城，",
        "夸盛事，",
        "两邦鼓吹，",
        "几部笙簧。",
        "看万红千翠，",
        "簇拥云裳。",
        "况是重阳近也，",
        "萸露紫、菊吐轻黄。",
        "休辞醉，",
        "明朝一枕，",
        "歌韵尚悠扬。"
      ]
    },
    {
      "_id": null,
      "Name": "满江红",
      "PoetId": "504aae071b0946f88d7c6b7e1d9deb4e",
      "PoetName": "葛郯",
      "Form": "词",
      "Tags": [
        "宋代",
        "葛郯"
      ],
      "Contents": [
        "家住苕西，",
        "小池映、青山一曲。",
        "翠深里、猿呼鹤应，",
        "短墙低屋。",
        "麦影离离翻翠浪，",
        "泉声НН敲寒玉。",
        "怪夜来、有蚁出糟床，",
        "篘新绿。",
        "和月种，",
        "南阳菊。",
        "饶云买，",
        "淇园竹。",
        "任蛮争触战，",
        "世间荣辱。",
        "两目未消凭远恨，",
        "一筇费尽登山足。",
        "便棹舟、炊火荻花中，",
        "鲈鱼熟。"
      ]
    },
    {
      "_id": null,
      "Name": "玉蝴蝶",
      "PoetId": "504aae071b0946f88d7c6b7e1d9deb4e",
      "PoetName": "葛郯",
      "Form": "词",
      "Tags": [
        "宋代",
        "葛郯"
      ],
      "Contents": [
        "忆昨苕溪，",
        "惯弄五亭月笛，",
        "四水烟蓑。",
        "何事毗檀门外，",
        "马驻长坡。",
        "野花中、乱红杳霭，",
        "小桥外、叠翠嵯峨。",
        "且颜酡。",
        "但存长袖，",
        "舞倒婆娑。",
        "云何。",
        "主盟惠政，",
        "春行五马，",
        "月皎千波。",
        "赢得宾僚，",
        "听隔墙、无事高歌。",
        "帐烟寒、瑞麟影坠，",
        "帘雾细、宝鸭香多。",
        "试蹉跎。",
        "一枰落日，",
        "又送焦柯。"
      ]
    },
    {
      "_id": null,
      "Name": "鹧鸪天",
      "PoetId": "504aae071b0946f88d7c6b7e1d9deb4e",
      "PoetName": "葛郯",
      "Form": "七律",
      "Tags": [
        "宋代",
        "葛郯"
      ],
      "Contents": [
        "千树家园锁旧津。",
        "谁移数点在孤村。",
        "海仙探蕊禽留影，",
        "楚客穿花蝶舞魂。",
        "桥断港，",
        "水横门。",
        "残霞零落晚烟昏。",
        "只因留住三更月，",
        "暗里香来别是春。"
      ]
    }
  ]
}