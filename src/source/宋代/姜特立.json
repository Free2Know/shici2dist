  "Poems": [
    {
      "_id": null,
      "Name": "临江仙",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "词",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "桃李飞花春渐老，",
        "海棠次第芬芳。",
        "庭前红叶已成行。",
        "酴醿开未到，",
        "犹更有花王。",
        "从此便须排日醉，",
        "莫将闲事相妨。",
        "老来不是太疏狂。",
        "尊前君看取，",
        "潘宾已成霜。"
      ]
    },
    {
      "_id": null,
      "Name": "卜算子",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "词",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "丹桂一枝芳，",
        "陡觉秋容静。",
        "月里人间总一般，",
        "共此扶疏影。",
        "枕畔忽闻香，",
        "夜半还思省。",
        "争奈姮娥不嫁人，",
        "寂寞孤衾冷。"
      ]
    },
    {
      "_id": null,
      "Name": "声声慢",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "词",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "云迷越岫，",
        "枫冷吴江，",
        "天香忽到人寰。",
        "满额涂黄，",
        "别更一种施丹。",
        "天教素秋独步，",
        "笑同时、霜菊秋兰。",
        "最好处，",
        "向水阶月地，",
        "把酒相看。",
        "应有骚人雅韵，",
        "将胆瓶筠管，",
        "簇向屏山。",
        "野店云房，",
        "争待结屋中间。",
        "无奈猖狂老子，",
        "架巢卧、风露清闲。",
        "待早晚，",
        "约姮娥、同住广寒。"
      ]
    },
    {
      "_id": null,
      "Name": "念奴娇",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "词",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "宦途巇险，",
        "问急流勇退，",
        "几人闻早。",
        "自别修门今正是，",
        "一纪生朝还到。",
        "绿野风光，",
        "平泉草木，",
        "争似梅山好。",
        "园林如画，",
        "芰荷香泛芳沼。",
        "早晚玉节来临，",
        "君恩踵至，",
        "金印应如斗。",
        "好是华堂开宴处，",
        "歌舞管弦声奏。",
        "海上蟠桃，",
        "山中仙杏，",
        "共劝长生酒。",
        "莫辞沈醉，",
        "年年此会依旧。"
      ]
    },
    {
      "_id": null,
      "Name": "感皇恩",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "词",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "儿女沸欢声，",
        "生朝来到。",
        "帘幕中间喷香兽。",
        "京祠新任，",
        "好事日边还又。",
        "清闲无个事，",
        "君恩厚。",
        "赢得乞食，",
        "歌姬一笑。",
        "旧衲云山伴红袖。",
        "蓬莱弱水，",
        "试问神仙何有。",
        "近来饶落托，",
        "贪杯酒。"
      ]
    },
    {
      "_id": null,
      "Name": "朝中措",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "词",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "如山堂上翠横空。",
        "山影浪花中。",
        "夜夜林间明月，",
        "时时柳外清风。",
        "如今到此，",
        "け然万事，",
        "无处情锺。",
        "唯有尊前一笑，",
        "分明好个山翁。"
      ]
    },
    {
      "_id": null,
      "Name": "浣溪沙",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "五律",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "节序回环已献裘。",
        "不堪风叶夜鸣秋。",
        "寒螀吟露月阶幽。",
        "蜗角虚名真误我，",
        "蝇头细字不禁愁。",
        "班超何日定封候。"
      ]
    },
    {
      "_id": null,
      "Name": "浪淘沙",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "七律",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "春事有来期。",
        "且喜春归。",
        "问春何似去年时。",
        "报道今年春意好，",
        "随分开眉。",
        "往事莫伤悲。",
        "光景如飞。",
        "十分潘鬓已成丝。",
        "幸是风流犹未减，",
        "且醉芳菲。"
      ]
    },
    {
      "_id": null,
      "Name": "满庭芳",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "词",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "丹染吴枫，",
        "青环越岫，",
        "镜天霁色凝鲜。",
        "华堂珊珮，",
        "非务拥神仙。",
        "天遣澄清海岳，",
        "亲曾授黄石奇编。",
        "兴王略，",
        "智名俱泯，",
        "功业妙难言。",
        "翩翩。",
        "真迥立，",
        "香飘绣衮，",
        "色映貂蝉。",
        "有清诗千首，",
        "美酒如川。",
        "长与嫦娥共约，",
        "放冰轮，",
        "今夕先圆。",
        "中秋月，",
        "从今屈指，",
        "更借一千年。"
      ]
    },
    {
      "_id": null,
      "Name": "满江红",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "词",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "小小华堂，",
        "朱阑外、乱山如簇。",
        "更云中仙掌，",
        "一峰高矗。",
        "南极老人呈瑞处，",
        "丙丁躔次光相烛。",
        "又谁知、堂上有闲人，",
        "无拘束。",
        "宾朋至，",
        "须歌曲。",
        "风月好，",
        "纷丝竹，",
        "都不管、世间是非荣辱。",
        "屈指如今侪辈少，",
        "几人老後能知足。",
        "问此身、何地寄生涯，",
        "唯松菊。"
      ]
    },
    {
      "_id": null,
      "Name": "画堂春",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "词",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "故园二月正芳菲。",
        "红紫团枝。",
        "一番草绿谢郎池。",
        "人醉如泥。",
        "底事江乡风物，",
        "年年独殿芳时。",
        "无情燕子背人飞。",
        "似愧春迟。"
      ]
    },
    {
      "_id": null,
      "Name": "菩萨蛮",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "词",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "镜天良月皆佳节。",
        "休恨今宵妨皎洁。",
        "玉锁闷蟾宫。",
        "姮娥意自通。",
        "终有开时节。",
        "莫放笙歌歇。",
        "来夕尚婵娟。",
        "何妨把酒看。"
      ]
    },
    {
      "_id": null,
      "Name": "蝶恋花",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "词",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "飘粉吹香三月暮。",
        "病酒情怀，",
        "愁绪浑无数。",
        "有个人人来又去。",
        "归期有恨难留驻。",
        "明日尊前无觅处。",
        "咿轧篮舆，",
        "只向双溪路。",
        "我辈情锺君谩与。",
        "为云为雨应难据。"
      ]
    },
    {
      "_id": null,
      "Name": "西江月",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "词",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "富贵从来自有，",
        "人生最羡长年。",
        "駸駸八秩未华颠。",
        "更喜此身强健。",
        "金印新来如斗，",
        "丝纶御墨犹鲜。",
        "枣如瓜大藕如船。",
        "莫惜尊前满劝。"
      ]
    },
    {
      "_id": null,
      "Name": "阮郎归",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "词",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "绿阴庭院记年时。",
        "家人捧寿卮。",
        "乐声催拍送腰支。",
        "香风匝地衣。",
        "清梦断，",
        "彩云飞。",
        "刘郎今鬓丝。",
        "强将杯酒破愁眉。",
        "如今触事非。"
      ]
    },
    {
      "_id": null,
      "Name": "霜天晓角",
      "PoetId": "bcc04f811a36449d96cc1ced0c296cea",
      "PoetName": "姜特立",
      "Form": "词",
      "Tags": [
        "宋代",
        "姜特立"
      ],
      "Contents": [
        "欢娱电掣。",
        "何况轻离别。",
        "料得两情无奈，",
        "思量尽、总难说。",
        "酒热。",
        "凄兴发。",
        "共寻波底月。",
        "长结西湖心愿，",
        "水有尽、情无歇。"
      ]
    }
  ]
}