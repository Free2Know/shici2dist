  "Poems": [
    {
      "_id": null,
      "Name": "水调歌头",
      "PoetId": "0ca7a3719c01402eb9b7d1028766c3ee",
      "PoetName": "陈造",
      "Form": "词",
      "Tags": [
        "宋代",
        "陈造"
      ],
      "Contents": [
        "胜日探梅去，",
        "邂逅得奇观。",
        "南枝的皪，",
        "陡觉品俗又香悭。",
        "曾是瑶妃清瘦，",
        "帝与金丹换骨，",
        "酒韵上韶颜。",
        "百叠侈罗袂，",
        "小立耐春寒。",
        "凝香地，",
        "古仙伯，",
        "玉尘间。",
        "烦公持并三友，",
        "秀色更堪飡。",
        "定笑芙蓉骚客，",
        "认作东风桃杏，",
        "醉眼自相谩。",
        "相见落诗笔，",
        "字字漱龙阑。"
      ]
    },
    {
      "_id": null,
      "Name": "江神子·江城子",
      "PoetId": "0ca7a3719c01402eb9b7d1028766c3ee",
      "PoetName": "陈造",
      "Form": "词",
      "Tags": [
        "宋代",
        "陈造"
      ],
      "Contents": [
        "歌筵当日小蓬瀛。",
        "晚妆明。",
        "识芳卿。",
        "挽袖新词，",
        "曾博遏云声。",
        "又侍仙翁灯夕饮，",
        "红雾底，",
        "沸箫笙。",
        "催人刻烛待诗成。",
        "捧雕觥。",
        "媚盈盈。",
        "不道醉魂，",
        "入夜已瞢腾。",
        "墨涴香罗回盼处，",
        "和笑道，",
        "太狂生。"
      ]
    },
    {
      "_id": null,
      "Name": "洞仙歌",
      "PoetId": "0ca7a3719c01402eb9b7d1028766c3ee",
      "PoetName": "陈造",
      "Form": "词",
      "Tags": [
        "宋代",
        "陈造"
      ],
      "Contents": [
        "蝶狂风闹，",
        "不到凝香地。",
        "谁见飞琼巧梳洗。",
        "厌孤标冷艳，",
        "不入时宜，",
        "银烛底，",
        "酒沁冰肌未睡。",
        "东君怜索寞，",
        "分寄寒斋，",
        "闹耐残醒嗅芳蕊。",
        "费西湖东阁，",
        "多少诗愁，",
        "援采笔、重与江梅品第。",
        "算肯容、丹杏接仙游，",
        "又却要蕊宫，",
        "侍香扶醉。"
      ]
    },
    {
      "_id": null,
      "Name": "菩萨蛮",
      "PoetId": "0ca7a3719c01402eb9b7d1028766c3ee",
      "PoetName": "陈造",
      "Form": "词",
      "Tags": [
        "宋代",
        "陈造"
      ],
      "Contents": [
        "冰花的皪冰蟾下。",
        "松烟竹雾溪桥夜。",
        "斜倚小峰峦。",
        "依依同岁寒。",
        "生绡明粉墨。",
        "浅笑犹倾国。",
        "恰似野桥看。",
        "飘零只等闲。"
      ]
    },
    {
      "_id": null,
      "Name": "虞美人",
      "PoetId": "0ca7a3719c01402eb9b7d1028766c3ee",
      "PoetName": "陈造",
      "Form": "词",
      "Tags": [
        "宋代",
        "陈造"
      ],
      "Contents": [
        "凝香仙伯莺花主。",
        "雅意怜羁旅。",
        "略分春色便浓欢。",
        "街吏何妨日日、报平安。",
        "诗人一醉龙公妒。",
        "恰限今朝雨。",
        "关门独酌强伸眉。",
        "也胜栖栖腰铺、守风时。"
      ]
    },
    {
      "_id": null,
      "Name": "蝶恋花",
      "PoetId": "0ca7a3719c01402eb9b7d1028766c3ee",
      "PoetName": "陈造",
      "Form": "词",
      "Tags": [
        "宋代",
        "陈造"
      ],
      "Contents": [
        "山立翠屏开几面。",
        "画舸经行，",
        "蒲葺□□岸。",
        "想过溪门帆影转。",
        "湖光忽作浮天远。",
        "诗卷来时春晼晚。",
        "愁把钓游，",
        "佳处寻思遍。",
        "不许冷官人所贱。",
        "拘缠自叹冰蚕茧。"
      ]
    },
    {
      "_id": null,
      "Name": "诉衷情",
      "PoetId": "0ca7a3719c01402eb9b7d1028766c3ee",
      "PoetName": "陈造",
      "Form": "词",
      "Tags": [
        "宋代",
        "陈造"
      ],
      "Contents": [
        "今朝人自藕州来。",
        "花意尚迟迥。",
        "几时画船同载，",
        "云锦照樽罍。",
        "铃斋外，",
        "已全开。",
        "是谁催，",
        "诗仙住处，",
        "和气回春，",
        "羯鼓如雷。"
      ]
    },
    {
      "_id": null,
      "Name": "鹧鸪天",
      "PoetId": "0ca7a3719c01402eb9b7d1028766c3ee",
      "PoetName": "陈造",
      "Form": "七律",
      "Tags": [
        "宋代",
        "陈造"
      ],
      "Contents": [
        "闲去街头赏大花。",
        "翠帷珠幰护豪华。",
        "西真宴罢群仙醉，",
        "千尺黄云错紫霞。",
        "团粉黛，",
        "闹箫笳。",
        "使星躔处住灵槎。",
        "定知今夜游仙梦，",
        "不落西京姚魏家。"
      ]
    }
  ]
}