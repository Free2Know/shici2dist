  "Poems": [
    {
      "_id": null,
      "Name": "一落索",
      "PoetId": "6bbd7fecf1da4d7291599e8d9f50900c",
      "PoetName": "李元膺",
      "Form": "词",
      "Tags": [
        "宋代",
        "李元膺"
      ],
      "Contents": [
        "天上粉云如扫。",
        "放小楼清晓。",
        "古今何处想风流，",
        "最潇洒，",
        "龙山帽。",
        "人似年华易老。",
        "且芳樽频倒。",
        "西风于我更多情，",
        "露金靥，",
        "篱边笑。"
      ]
    },
    {
      "_id": null,
      "Name": "洞仙歌",
      "PoetId": "6bbd7fecf1da4d7291599e8d9f50900c",
      "PoetName": "李元膺",
      "Form": "词",
      "Tags": [
        "宋代",
        "李元膺"
      ],
      "Contents": [
        "雪云散尽，",
        "放晓晴池院。",
        "杨柳于人便青眼。",
        "更风流多处，",
        "一点梅心、相映远。",
        "约略嚬轻笑浅。",
        "一年春好处，",
        "不在浓芳，",
        "小艳疏香最娇软。",
        "到清明时候，",
        "百紫千红花正乱。",
        "已失春风一半。",
        "蚤占取韶光，",
        "共追游，",
        "但莫管春寒，",
        "醉红自暖。"
      ]
    },
    {
      "_id": null,
      "Name": "浣溪沙",
      "PoetId": "6bbd7fecf1da4d7291599e8d9f50900c",
      "PoetName": "李元膺",
      "Form": "五律",
      "Tags": [
        "宋代",
        "李元膺"
      ],
      "Contents": [
        "乞与安仁掠鬓霜。",
        "不须红线小机窗。",
        "剪刀疏下蜀罗长。",
        "纤手捻残针缕细，",
        "金钗翻过齿痕香。",
        "同心小绾寄思量。"
      ]
    },
    {
      "_id": null,
      "Name": "茶瓶儿",
      "PoetId": "6bbd7fecf1da4d7291599e8d9f50900c",
      "PoetName": "李元膺",
      "Form": "词",
      "Tags": [
        "宋代",
        "李元膺"
      ],
      "Contents": [
        "去年相逢深院宇。",
        "海棠下、曾歌金缕。",
        "歌罢花如雨。",
        "翠罗衫上，",
        "点点红无数。",
        "今岁重寻携手处。",
        "空物事、人非春暮。",
        "回首青门路。",
        "乱红飞絮，",
        "相逐东风去。"
      ]
    },
    {
      "_id": null,
      "Name": "菩萨蛮",
      "PoetId": "6bbd7fecf1da4d7291599e8d9f50900c",
      "PoetName": "李元膺",
      "Form": "词",
      "Tags": [
        "宋代",
        "李元膺"
      ],
      "Contents": [
        "彩旗画柱清明後。",
        "花前姊妹争携手。",
        "先紧绣罗裙。",
        "轻衫束领巾。",
        "琐绳金钏响。",
        "渐出花梢上。",
        "笑里问高低。",
        "盘云亸玉螭。"
      ]
    },
    {
      "_id": null,
      "Name": "蓦山溪",
      "PoetId": "6bbd7fecf1da4d7291599e8d9f50900c",
      "PoetName": "李元膺",
      "Form": "词",
      "Tags": [
        "宋代",
        "李元膺"
      ],
      "Contents": [
        "溪堂欢燕。",
        "惯捧玻璃盏。",
        "今日祖西城，",
        "更忍把、一杯重劝。",
        "别离情味，",
        "自古不堪秋，",
        "催泪雨，",
        "湿西风，",
        "肠共危弦断。",
        "夕阳去路，",
        "五马旌旗乱。",
        "便是古都春，",
        "应醉恋，",
        "曲江池馆。",
        "须知别後，",
        "叠翠倚阑情，",
        "青嶂晚，",
        "碧云深，",
        "日近长安远。"
      ]
    },
    {
      "_id": null,
      "Name": "鹧鸪天",
      "PoetId": "6bbd7fecf1da4d7291599e8d9f50900c",
      "PoetName": "李元膺",
      "Form": "七律",
      "Tags": [
        "宋代",
        "李元膺"
      ],
      "Contents": [
        "寂寞秋千两绣旗。",
        "日长花影转阶迟。",
        "燕惊午梦周遮语，",
        "蝶困春游落拓飞。",
        "思往事，",
        "入颦眉。",
        "柳梢阴重又当时。",
        "薄情风絮难拘束，",
        "飞过东墙不肯归。"
      ]
    }
  ]
}