      "_id": null,
      "Name": "临江仙",
      "PoetId": "6e13888b396d4ebf82756c08fbdb8f2f",
      "PoetName": "阳枋",
      "Form": "词",
      "Tags": [
        "宋代",
        "阳枋"
      ],
      "Contents": [
        "乐意相关莺对语，",
        "春风遍满天涯。",
        "生香不断树交花。",
        "个中皆实理，",
        "何处是浮华。",
        "收敛回来还夜气，",
        "一轮明月千家。",
        "看梅休用隔窗纱。",
        "清光辉皎洁，",
        "疏影自横斜。"
      ]
    },
    {
      "_id": null,
      "Name": "念奴娇",
      "PoetId": "6e13888b396d4ebf82756c08fbdb8f2f",
      "PoetName": "阳枋",
      "Form": "词",
      "Tags": [
        "宋代",
        "阳枋"
      ],
      "Contents": [
        "白尽蒹葭，",
        "衰从蒲柳，",
        "我只松筠节。",
        "君民尧舜，",
        "老翁揩眼勋业。"
      ]
    }
  ]
}