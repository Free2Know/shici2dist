  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "八声甘州",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "词",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "正西山、雨过弄晴景，",
        "竹屋贯斜晖。",
        "问谁将千斛，",
        "霏瑛落屑，",
        "吹上花枝。",
        "风外青鞋未熟，",
        "鼻观已先知。",
        "扰损江南客，",
        "诗面难肥。",
        "两句林边倾盖，",
        "笑化工开落，",
        "尤甚儿嬉。",
        "叹额黄人去，",
        "还是隔年期。",
        "渺飞魂、凭谁招取，",
        "赖故人、沈水煮花瓷。",
        "犹堪待，",
        "岭梅开後，",
        "一战雄雌。"
      ]
    },
    {
      "_id": null,
      "Name": "大圣乐令·玉团儿",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "七律",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "豆蔻梢头春正早。",
        "敛修眉、未经重扫。",
        "湖山清远，",
        "几年牢落，",
        "风韵初好。",
        "慢绾垂螺最娇小。",
        "是谁家、舞腰袅袅。",
        "而今莫谓，",
        "春归等闲，",
        "分付芳草。"
      ]
    },
    {
      "_id": null,
      "Name": "好事近",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "词",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "襟韵绝纤尘，",
        "炯炯夜光明月。",
        "合是紫荷持橐，",
        "侍丹墀清切。",
        "暗香疏影想公家，",
        "人与梅超绝。",
        "酒罢帝城早去，",
        "占百花先发。"
      ]
    },
    {
      "_id": null,
      "Name": "忆王孙",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "词",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "庭梧叶密未惊秋。",
        "风雨潇潇特地愁。",
        "愁绪如丝无尽头。",
        "思悠悠。",
        "怅望王孙空倚楼。"
      ]
    },
    {
      "_id": null,
      "Name": "忆秦娥",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "词",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "隈岩侧。",
        "怪生小院香来别。",
        "香来别。",
        "嫩黄细细，",
        "商量齐发。",
        "佳人敛笑贪先折。",
        "重新为翦斜斜叶。",
        "斜斜叶。",
        "钗头常带，",
        "一秋风月。"
      ]
    },
    {
      "_id": null,
      "Name": "念奴娇",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "词",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "练江风静，",
        "卧冰奁百尺，",
        "朱阑飞入。",
        "江远浮天天在水，",
        "水满半天云湿。",
        "白鸟明边，",
        "青山断处，",
        "眼冷江头立。",
        "月明潮上，",
        "苇间渔唱声急。",
        "几度吹老苹花，",
        "野香无数，",
        "欲寄应难及。",
        "天借诗人供醉眼，",
        "尊俎一时收拾。",
        "竹里行厨，",
        "花间步障，",
        "风雨生呼吸。",
        "酒阑歌罢，",
        "钓船先具蓑笠。"
      ]
    },
    {
      "_id": null,
      "Name": "武陵春",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "词",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "门巷乌衣应好在，",
        "风韵尚依然。",
        "知是蓬瀛第几仙。",
        "秀色粲当筵。",
        "索句濡毫云阁里，",
        "清坐袅炉烟。",
        "谁赋回文第二篇。",
        "除是见娟娟。"
      ]
    },
    {
      "_id": null,
      "Name": "水调歌头",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "词",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "华栋一何丽，",
        "移下小壶天。",
        "几多曲房新户，",
        "缥缈似当年。",
        "曾是使君风度，",
        "元有胸中丘壑，",
        "六月竹风寒。",
        "一洗筝笛耳，",
        "歌舞粲筵间。",
        "坐中客，",
        "醒复醉，",
        "听无眠。",
        "已回归梦，",
        "犹复袅袅记清圆。",
        "尚想饮中仙子，",
        "来处馀香飘坐，",
        "胜韵此双全。",
        "为寄月华语，",
        "难与并蝉娟。"
      ]
    },
    {
      "_id": null,
      "Name": "浣溪沙",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "五律",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "说似当年老季伦。",
        "君家虽富客常贫。",
        "何曾一笑任吾真。",
        "酒满罢留天下士，",
        "词新空赋坐中春。",
        "谁能绝笔更书麟。"
      ]
    },
    {
      "_id": null,
      "Name": "浪淘沙",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "七律",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "趁拍舞初筵。",
        "柳袅春烟。",
        "街头桃李莫争妍。",
        "家本凤楼高处住，",
        "锦瑟华年。",
        "不用抹繁弦。",
        "歌韵天然。",
        "天教独立百花前。",
        "但愿人如天上月，",
        "三五团圆。"
      ]
    },
    {
      "_id": null,
      "Name": "点绛唇",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "词",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "秀出群儿，",
        "柳眉濯濯春庭院。",
        "不亲歌扇。",
        "弄笔勤书篆。",
        "翁已无能，",
        "老退惭赬面。",
        "欣同宴。",
        "坐来喜见。",
        "痛饮惊无算。"
      ]
    },
    {
      "_id": null,
      "Name": "瑞鹤仙",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "词",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "试六花院落。",
        "正柳绵飘坠，",
        "因风无著。",
        "吴王旧城郭。",
        "记乌衣门巷，",
        "小桥帘幕。",
        "他州寥索。",
        "漫等闲、桃英杏萼。",
        "认幽香来处，",
        "群芳尽掩，",
        "蕙心先觉。",
        "行乐。",
        "燕雏莺友，",
        "浪语狂歌，",
        "休休莫莫。",
        "兰房绣幄。",
        "添新恨，",
        "念前约。",
        "殢十分芳景，",
        "十分春意，",
        "休惜十分共酌。",
        "任十分、吹老寒梅，",
        "戌楼画角。"
      ]
    },
    {
      "_id": null,
      "Name": "画堂春",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "词",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "春波浅碧涨方池。",
        "池台深锁烟霏。",
        "缓歌争胜早莺啼。",
        "客忍轻归。",
        "合坐香凝宿雾，",
        "垫巾梅插寒枝。",
        "渐西蟾影漾馀辉。",
        "醉倒谁知。"
      ]
    },
    {
      "_id": null,
      "Name": "眼儿媚",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "词",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "铃阁寻盟未肯寒。",
        "鷁首驻江干。",
        "云烟翰墨，",
        "风流尊俎，",
        "不放更残。",
        "金声掷地西清老，",
        "天未许终闲。",
        "知音素赏，",
        "当筵一曲，",
        "流水高山。"
      ]
    },
    {
      "_id": null,
      "Name": "芰荷香",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "词",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "醉凝眸。",
        "正行云遮断，",
        "澄练江头。",
        "皓月今宵何处，",
        "不管中秋。",
        "朱阑倚遍，",
        "又微雨、催下危楼。",
        "秋风空响更筹。",
        "不将好梦，",
        "吹过南州。",
        "浮远轩窗异日到，",
        "山空云净，",
        "江远天浮。",
        "别去客怀，",
        "无赖准拟开愁。",
        "冰轮好在，",
        "解随我、天际归舟。",
        "何须舞袂歌喉。",
        "一觞一咏，",
        "谈笑风流。"
      ]
    },
    {
      "_id": null,
      "Name": "菩萨蛮",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "词",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "宛如姑射人冰雪。",
        "知公不负佳风月。",
        "莫放漏声残。",
        "清风生坐间。",
        "赏春心未足。",
        "翦尽尊前烛。",
        "此乐自难忘。",
        "一觞还一觞。"
      ]
    },
    {
      "_id": null,
      "Name": "蓦山溪",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "词",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "冰清玉映，",
        "自是闺房秀。",
        "十里卷朱帘，",
        "好紫陌、家家未有。",
        "天然情素，",
        "高压一城春，",
        "花艳丽，",
        "月精神，",
        "梅韵腰枝瘦。",
        "曲屏虚幌，",
        "枉著鸳鸯绣。",
        "不是不相逢，",
        "泪空滴、年年别袖。",
        "从他兰菊，",
        "秋露与春风，",
        "终不似，",
        "玉人人，",
        "一片心长久。"
      ]
    },
    {
      "_id": null,
      "Name": "鹧鸪天",
      "PoetId": "6fdf8bbbb64548a592c70d847539f1c2",
      "PoetName": "仲并",
      "Form": "七律",
      "Tags": [
        "宋代",
        "仲并"
      ],
      "Contents": [
        "间世麒麟降自天。",
        "光辉列宿灿初躔。",
        "他年身致夔龙列，",
        "少日心存翰墨边。",
        "簪彩笔，",
        "照金莲。",
        "词华当使思如泉。",
        "未论耸壑昂霄事，",
        "且与衰翁慰暮年。"
      ]
    }
  ]
}