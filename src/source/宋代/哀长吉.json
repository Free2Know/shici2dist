  "Poems": [
    {
      "_id": null,
      "Name": "朝中措",
      "PoetId": "16762b535ae6477da5fea59ede428807",
      "PoetName": "哀长吉",
      "Form": "词",
      "Tags": [
        "宋代",
        "哀长吉"
      ],
      "Contents": [
        "自从佳偶共黄姑。",
        "几见设门弧。",
        "方喜阶庭联玉，",
        "又闻老蚌生珠。",
        "一门三秀，",
        "贾家虎子，",
        "薛氏鸾雏。",
        "从此公侯衮衮，",
        "看看百子成图。"
      ]
    },
    {
      "_id": null,
      "Name": "水调歌头",
      "PoetId": "16762b535ae6477da5fea59ede428807",
      "PoetName": "哀长吉",
      "Form": "词",
      "Tags": [
        "宋代",
        "哀长吉"
      ],
      "Contents": [
        "紫陌风光好，",
        "绣阁绮罗香。",
        "相将人月圆夜，",
        "早庆贺新郎。",
        "先自少年心意，",
        "为惜殢人娇态，",
        "久俟愿成双。",
        "此夕于飞乐，",
        "共学燕归梁。",
        "索酒子，",
        "迎仙客，",
        "醉红妆。",
        "诉衷情处，",
        "些儿好语意难忘。",
        "但愿千秋岁里，",
        "结取万年欢会，",
        "恩爱应天长。",
        "行喜长春宅，",
        "兰玉满庭芳。"
      ]
    },
    {
      "_id": null,
      "Name": "瑞鹤仙",
      "PoetId": "16762b535ae6477da5fea59ede428807",
      "PoetName": "哀长吉",
      "Form": "词",
      "Tags": [
        "宋代",
        "哀长吉"
      ],
      "Contents": [
        "小春天未雪。",
        "见两蕊三花，",
        "放梅时节。",
        "昴宿孕人杰。",
        "对梅花雪片，",
        "平分风月。",
        "冰清玉洁。",
        "天赋与、仙风道骨。",
        "更等闲、来访刘仙，",
        "觅取秘传真诀。",
        "闻说。",
        "辉联台宿，",
        "瑞应文昌，",
        "世承阀阅。",
        "相门事业。",
        "有祖父、旧风烈。",
        "管泥封飞下，",
        "沙堤归来，",
        "光复青毡旧物。",
        "庆家传、八叶联芳，",
        "又添一叶。"
      ]
    },
    {
      "_id": null,
      "Name": "西江月",
      "PoetId": "16762b535ae6477da5fea59ede428807",
      "PoetName": "哀长吉",
      "Form": "词",
      "Tags": [
        "宋代",
        "哀长吉"
      ],
      "Contents": [
        "百和香凝宝络，",
        "长生酒满金尊。",
        "葱葱佳气霭庭萱。",
        "同把椿龄祝愿。",
        "玉树已生谢砌，",
        "孙枝复长于门。",
        "伫看百子共千孙。",
        "此去公侯衮衮。"
      ]
    },
    {
      "_id": null,
      "Name": "齐天乐",
      "PoetId": "16762b535ae6477da5fea59ede428807",
      "PoetName": "哀长吉",
      "Form": "词",
      "Tags": [
        "宋代",
        "哀长吉"
      ],
      "Contents": [
        "青鸾海上传芳信。",
        "蓝田路入仙境。",
        "万卷书传，",
        "六奇计运，",
        "冰玉炯然清润。",
        "帷褰凤锦。",
        "□镜启鸾台，",
        "烟横鸳枕。",
        "一笑相迎，",
        "一双两好恰厮称。",
        "风流人在仙隐。",
        "更一县、陶柳春近。",
        "梦想金桃，",
        "宴分玉果，",
        "指日送尝汤饼。",
        "枌榆接畛。",
        "管此去亲盟，",
        "镇长交聘。",
        "自古朱陈，",
        "一村惟两姓。"
      ]
    }
  ]
}