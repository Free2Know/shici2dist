  "Poems": [
    {
      "_id": null,
      "Name": "丑奴儿·采桑子",
      "PoetId": "f99a3d7b7ce34394be419df37944aa54",
      "PoetName": "王仲甫",
      "Form": "词",
      "Tags": [
        "宋代",
        "王仲甫"
      ],
      "Contents": [
        "牡丹不好长春好，",
        "有个因依。",
        "一两枝儿。",
        "但是风光总属伊。",
        "当初祗为嫦娥种，",
        "月正明时。",
        "教恁芳菲。",
        "伴著团圆十二回。"
      ]
    },
    {
      "_id": null,
      "Name": "永遇乐",
      "PoetId": "f99a3d7b7ce34394be419df37944aa54",
      "PoetName": "王仲甫",
      "Form": "词",
      "Tags": [
        "宋代",
        "王仲甫"
      ],
      "Contents": [
        "风折新英，",
        "雨肥繁实，",
        "又还如豆。",
        "玉核初成，",
        "红腮尚浅，",
        "齿软酸透。",
        "粉墙低亚，",
        "佳人惊见，",
        "不管露沾襟袖。",
        "一枝钗子未插，",
        "应把手挪频嗅。",
        "相思病酒，",
        "只因思此，",
        "免使文君眉皱。",
        "入鼎调羹，",
        "攀林止渴，",
        "功业还依旧。",
        "看看飞燕，",
        "衔将春去，",
        "又将欲、黄昏时候。",
        "争如向、金盘满捧，",
        "共君王对酒。"
      ]
    },
    {
      "_id": null,
      "Name": "浪淘沙",
      "PoetId": "f99a3d7b7ce34394be419df37944aa54",
      "PoetName": "王仲甫",
      "Form": "七律",
      "Tags": [
        "宋代",
        "王仲甫"
      ],
      "Contents": [
        "素手水晶盘。",
        "垒起仙丸。",
        "红绡碾碎却成团。",
        "逗得安排金粟遍，",
        "何似鸡冠。",
        "味胜玉浆寒。",
        "只被宜酸。",
        "莫将荔子一般看。",
        "色淡香消僝僽损，",
        "才到长安。"
      ]
    },
    {
      "_id": null,
      "Name": "满朝欢",
      "PoetId": "f99a3d7b7ce34394be419df37944aa54",
      "PoetName": "王仲甫",
      "Form": "词",
      "Tags": [
        "宋代",
        "王仲甫"
      ],
      "Contents": [
        "忆得延州，",
        "旧曾相见，",
        "东城近东下住。",
        "被若著意引归家，",
        "放十分、以上抬举。",
        "小样罗衫，",
        "淡红拂过，",
        "风流万般做处。",
        "怕伊蓦地忆人时，",
        "梦中来、不要迷路。"
      ]
    },
    {
      "_id": null,
      "Name": "蓦山溪",
      "PoetId": "f99a3d7b7ce34394be419df37944aa54",
      "PoetName": "王仲甫",
      "Form": "词",
      "Tags": [
        "宋代",
        "王仲甫"
      ],
      "Contents": [
        "挂冠神武，",
        "来作烟花主。",
        "千里好江山，",
        "都尽是、君恩赐与。",
        "风勾月引，",
        "催上泛宅时，",
        "酒倾玉，",
        "脍堆雪，",
        "总道神仙侣。",
        "蓑衣箬笠，",
        "更著些儿雨。",
        "横笛两三声，",
        "晚云中、惊鸥来去。",
        "欲烦妙手，",
        "写入散人图，",
        "蜗角名，",
        "蝇头利，",
        "著甚来由顾。"
      ]
    },
    {
      "_id": null,
      "Name": "醉落魄·一斛珠",
      "PoetId": "f99a3d7b7ce34394be419df37944aa54",
      "PoetName": "王仲甫",
      "Form": "词",
      "Tags": [
        "宋代",
        "王仲甫"
      ],
      "Contents": [
        "醉醒醒醉。",
        "凭君会取皆滋味。",
        "浓斟琥珀香浮蚁。",
        "一入愁肠，",
        "便有阳春意。",
        "须将席幕为天地。",
        "歌前起舞花前睡。",
        "从他兀兀陶陶里。",
        "犹胜醒醒，",
        "惹得闲憔悴。"
      ]
    }
  ]
}