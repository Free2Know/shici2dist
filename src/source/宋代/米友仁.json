  "Poems": [
    {
      "_id": null,
      "Name": "临江仙",
      "PoetId": "7381a896398a498796800339030b1d9d",
      "PoetName": "米友仁",
      "Form": "词",
      "Tags": [
        "宋代",
        "米友仁"
      ],
      "Contents": [
        "野外不堪无胜侣，",
        "笑谈安得君同。",
        "四时景物一壶中。",
        "醉馀临望处，",
        "远岫数重重。",
        "溪上新荷初出水，",
        "花房半弄微红。",
        "晓风萧爽韵疏松。",
        "娟娟明月上，",
        "人在广寒宫。"
      ]
    },
    {
      "_id": null,
      "Name": "减字木兰花",
      "PoetId": "7381a896398a498796800339030b1d9d",
      "PoetName": "米友仁",
      "Form": "词",
      "Tags": [
        "宋代",
        "米友仁"
      ],
      "Contents": [
        "柳塘微雨。",
        "两两飞鸥来复去。",
        "倚遍重阑。",
        "人在碧云山外山。",
        "一春离怨。",
        "日照绮窗长几线。",
        "酒病情魔。",
        "两事春来无奈何。"
      ]
    },
    {
      "_id": null,
      "Name": "南歌子",
      "PoetId": "7381a896398a498796800339030b1d9d",
      "PoetName": "米友仁",
      "Form": "词",
      "Tags": [
        "宋代",
        "米友仁"
      ],
      "Contents": [
        "遇酒词先举，",
        "逢山眼暂明。",
        "一川风雨纵留人。",
        "不道此郎归兴、欲兼程。",
        "客久情深□，",
        "寻欢恨不能。",
        "绳床顿睡梦纵横。",
        "赖□□□□□、□□□。"
      ]
    },
    {
      "_id": null,
      "Name": "宴桃源·如梦令",
      "PoetId": "7381a896398a498796800339030b1d9d",
      "PoetName": "米友仁",
      "Form": "词",
      "Tags": [
        "宋代",
        "米友仁"
      ],
      "Contents": [
        "蝶梦初回栩栩。",
        "柳岸几声莺语。",
        "苹末起微风，",
        "山外一川烟雨。",
        "凝顾。",
        "凝顾。",
        "人在玉壶深处。"
      ]
    },
    {
      "_id": null,
      "Name": "小重山",
      "PoetId": "7381a896398a498796800339030b1d9d",
      "PoetName": "米友仁",
      "Form": "词",
      "Tags": [
        "宋代",
        "米友仁"
      ],
      "Contents": [
        "醉倚朱阑一解衣。",
        "碧云迷望眼，",
        "断虹低。",
        "近来休说带宽围。",
        "人千里，",
        "还是燕双飞。",
        "深院日初迟。",
        "绮窗帘幕静，",
        "恨生眉。",
        "不堪虚度是花时。",
        "鸿来速，",
        "争解寄相思。"
      ]
    },
    {
      "_id": null,
      "Name": "念奴娇",
      "PoetId": "7381a896398a498796800339030b1d9d",
      "PoetName": "米友仁",
      "Form": "词",
      "Tags": [
        "宋代",
        "米友仁"
      ],
      "Contents": [
        "阑干倚处。",
        "戏裁成、彭泽当年奇语。",
        "三径荒凉怀旧里，",
        "我欲扁舟归去。",
        "鸟倦知还，",
        "寓形宇内，",
        "今已年如许。",
        "小窗容膝，",
        "要寻情话亲侣。",
        "郭外粗有西畴，",
        "故园松菊，",
        "日涉方成趣。",
        "流水涓涓千涧上，",
        "云绕奇峰无数。",
        "窈窕经丘，",
        "风清月了，",
        "时看烟中雨。",
        "萧然巾岸，",
        "引觞寄傲衡宇。"
      ]
    },
    {
      "_id": null,
      "Name": "渔家傲",
      "PoetId": "7381a896398a498796800339030b1d9d",
      "PoetName": "米友仁",
      "Form": "词",
      "Tags": [
        "宋代",
        "米友仁"
      ],
      "Contents": [
        "郊外春和宜散步。",
        "百花枝上初凝露。",
        "福地神仙多外府。",
        "藏奇趣。",
        "幽寻历遍溪边路。",
        "薄宦浮家无定处。",
        "萍飘梗泛前人语。",
        "与子未须乡国去。",
        "来同住。",
        "且看群岫烟中雨。"
      ]
    },
    {
      "_id": null,
      "Name": "点绛唇",
      "PoetId": "7381a896398a498796800339030b1d9d",
      "PoetName": "米友仁",
      "Form": "词",
      "Tags": [
        "宋代",
        "米友仁"
      ],
      "Contents": [
        "浩渺湖天，",
        "酒浮黄菊携佳侣。",
        "澹烟疏雨。",
        "去鲁方怀土。",
        "倾盖相逢，",
        "引满哦奇语。",
        "山围处。",
        "兕觥频举。",
        "不醉君无去。"
      ]
    },
    {
      "_id": null,
      "Name": "白雪",
      "PoetId": "7381a896398a498796800339030b1d9d",
      "PoetName": "米友仁",
      "Form": "词",
      "Tags": [
        "宋代",
        "米友仁"
      ],
      "Contents": [
        "洞天昼永，",
        "正中和时候，",
        "凉飙初起。",
        "羽扇纶巾，",
        "云流处，",
        "水绕山重云委。",
        "好雨新晴，",
        "绮霞明丽，",
        "全是丹青戏。",
        "豪攘横卷，",
        "楚天应解深秘。",
        "留滞。",
        "字学书林，",
        "折腰缘为米，",
        "无机涉世。",
        "投组归来欣自肆，",
        "目仰云霄醒醉。",
        "论少卑之，",
        "家声接武，",
        "月旦评吾子。",
        "凭高临望，",
        "桂轮徒共千里。"
      ]
    },
    {
      "_id": null,
      "Name": "诉衷情",
      "PoetId": "7381a896398a498796800339030b1d9d",
      "PoetName": "米友仁",
      "Form": "词",
      "Tags": [
        "宋代",
        "米友仁"
      ],
      "Contents": [
        "结庐人境羡陶潜。",
        "车马不来喧。",
        "胜处自多真趣，",
        "飞鸟日相还。",
        "心既远，",
        "地仍偏。",
        "见南山。",
        "手持菊颖，",
        "山气常佳，",
        "欲辨忘言。"
      ]
    },
    {
      "_id": null,
      "Name": "醉春风",
      "PoetId": "7381a896398a498796800339030b1d9d",
      "PoetName": "米友仁",
      "Form": "词",
      "Tags": [
        "宋代",
        "米友仁"
      ],
      "Contents": [
        "一阳来复群阴往。",
        "吾道从今长。",
        "万事莫关情，",
        "月夕风前，",
        "依旧须豪放。",
        "卿云舒卷浮青嶂。",
        "从古书珍赏。",
        "满引唱新词，",
        "春意看看，",
        "又到梅梢上。"
      ]
    },
    {
      "_id": null,
      "Name": "阮郎归",
      "PoetId": "7381a896398a498796800339030b1d9d",
      "PoetName": "米友仁",
      "Form": "词",
      "Tags": [
        "宋代",
        "米友仁"
      ],
      "Contents": [
        "碧溪风动满文漪。",
        "雨馀山更奇。",
        "淡烟横处柳行低。",
        "鸳鸯来去飞。",
        "人似玉，",
        "醉如泥。",
        "一枝随鬓猗。",
        "夷犹双桨月平西。",
        "幽寻归路迷。"
      ]
    }
  ]
}