  "Poems": [
    {
      "_id": null,
      "Name": "念奴娇",
      "PoetId": "11478c255cf849c886e7a38b7c063328",
      "PoetName": "窃杯女子",
      "Form": "词",
      "Tags": [
        "宋代",
        "窃杯女子"
      ],
      "Contents": [
        "桂魄澄辉，",
        "禁城内、万盏花灯罗列。",
        "无限佳人穿绣径，",
        "几多妖艳奇绝。",
        "凤烛交光，",
        "银灯相射，",
        "奏箫韶初歇。",
        "鸣鞘响处，",
        "万民瞻仰宫阙。",
        "妾自闺门给假，",
        "与夫携手，",
        "共赏元宵节。",
        "误到玉皇金殿砌，",
        "赐酒金杯满设。",
        "量窄从来，",
        "红凝粉面，",
        "尊见无凭说。",
        "假王金盏，",
        "免公婆责罚臣妾。"
      ]
    },
    {
      "_id": null,
      "Name": "鹧鸪天",
      "PoetId": "11478c255cf849c886e7a38b7c063328",
      "PoetName": "窃杯女子",
      "Form": "七律",
      "Tags": [
        "宋代",
        "窃杯女子"
      ],
      "Contents": [
        "灯火楼台处处新。",
        "笑携郎手御街行。",
        "回头忽听传呼急，",
        "不觉鸳鸯两处分。",
        "天表近，",
        "帝恩荣。",
        "琼浆饮罢脸生春。",
        "归来恐被儿夫怪，",
        "愿赐金杯作证明。"
      ]
    }
  ]
}