  "Poems": [
    {
      "_id": null,
      "Name": "临江仙",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "问道春来相识否，",
        "岭头昨夜开花。",
        "水村烟坞寄生涯。",
        "月寒疏影淡，",
        "整整复斜斜。",
        "素面玉妃嫌粉污，",
        "晨妆洗尽铅华。",
        "香肌应只饭胡麻。",
        "年年如许瘦，",
        "知是阿谁家。"
      ]
    },
    {
      "_id": null,
      "Name": "凤栖梧·蝶恋花",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "琼海无边银浪卷。",
        "画戟朱楼，",
        "缥缈云间见。",
        "当日使君曾拥传。",
        "海霞光里时开宴。",
        "翠斝红鳞吹酒面。",
        "莫谓今朝，",
        "人在天涯远。",
        "彩凤衔书应不晚。",
        "愿公难老身长健。"
      ]
    },
    {
      "_id": null,
      "Name": "好事近",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "宴罢莫匆匆，",
        "聊驻玉鞍金勒。",
        "闻道建溪新焙，",
        "尽龙蟠苍璧。",
        "黄金碾入碧花瓯，",
        "瓯翻素涛色。",
        "今夜酒醒归去，",
        "觉风生两腋。"
      ]
    },
    {
      "_id": null,
      "Name": "寰海清",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "未知",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "画鼓轰天。",
        "暗尘随宝马，",
        "人似神仙。",
        "天恁不教昼短，",
        "明月长圆。",
        "天应未知道，",
        "天天。",
        "须肯放、三夜如年。",
        "流酥拥上香軿。",
        "为个甚、晚妆特地鲜妍。",
        "花下清阴乍合，",
        "曲水桥边。",
        "高人到此也乘兴，",
        "任横街，",
        "一一须穿。",
        "莫言无国艳，",
        "有朱门、锁婵娟。"
      ]
    },
    {
      "_id": null,
      "Name": "忆秦娥",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "梅花发。",
        "夜寒吹笛千山月。",
        "千山月。",
        "此时愁听，",
        "龙吟幽噎。",
        "数枝飞尽南枝雪。",
        "风光又作年时别。",
        "年时别。",
        "江头心绪，",
        "乱丝千结。"
      ]
    },
    {
      "_id": null,
      "Name": "念奴娇",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "少年时节，",
        "见皇州灯火，",
        "衣冠朝市。",
        "天汉桥边瞻凤辇，",
        "帘幕千家垂地。",
        "人似神仙，",
        "身游佛国，",
        "谪堕红尘里。",
        "如今憔悴，",
        "渐无往岁欢味。",
        "此夜帝里喧传，",
        "太平祥瑞，",
        "有街头人醉。",
        "更值端门新又起，",
        "楼阙千兵严卫。",
        "朝野多欢，",
        "边庭初静，",
        "歌舞方腾沸。",
        "良宵好景，",
        "异时多少遗事。"
      ]
    },
    {
      "_id": null,
      "Name": "感皇恩",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "飞雪满貂裘，",
        "马蹄轻骤。",
        "笔下文章焕星斗。",
        "凌云才调，",
        "尽是夺标高手。",
        "况闻场屋里，",
        "知名久。",
        "金榜篆云，",
        "银鞍披绣。",
        "归去天香满衣袖。",
        "莫辞今夕，",
        "且尽一尊芳酒。",
        "为君歌一曲，",
        "为君寿。"
      ]
    },
    {
      "_id": null,
      "Name": "柳梢青",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "兰亭丝竹。",
        "高会群贤，",
        "其人如玉。",
        "曲水流觞，",
        "灯前细雨，",
        "檐花蔌蔌。",
        "舞雩初试春衣，",
        "听咏歌、童子五六。",
        "泽畔行吟，",
        "沙汀拾翠，",
        "满江新绿。"
      ]
    },
    {
      "_id": null,
      "Name": "桃源忆故人",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "催花一霎清明雨。",
        "留得东风且住。",
        "两岸柳汀烟坞。",
        "未放行人去。",
        "人如双鹄云间举。",
        "明夜扁舟何处。",
        "只向武陵南渡。",
        "便是长安路。"
      ]
    },
    {
      "_id": null,
      "Name": "殢人娇",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "未知",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "小院桃花，",
        "烟锁几重珠箔。",
        "更深後、海棠睡著。",
        "东风吹去，",
        "落谁家墙角。",
        "平白地教人，",
        "为他情恶。",
        "花若有情，",
        "情应不薄。",
        "也须悔从前事错。",
        "而今夜雨，",
        "念玉颜飘泊。",
        "知那里人家，",
        "怎生顿著。"
      ]
    },
    {
      "_id": null,
      "Name": "江城子",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "天回北斗欲中宵。",
        "屡移杓。",
        "客魂消。",
        "记得皇州，",
        "灯火虹成桥。",
        "异俗西南开万里，",
        "冠带尽，",
        "百蛮徭。",
        "卢溪太守未还朝。",
        "起朱楼。",
        "接丛宵。",
        "翠幕红妆，",
        "歌管玉为箫。",
        "民乐丰登无一事，",
        "看下诏，",
        "采风谣。"
      ]
    },
    {
      "_id": null,
      "Name": "浣溪沙",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "五律",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "九里香风动地来。",
        "寻香空绕百千回。",
        "错惊秋色上崔嵬。",
        "谁识芗林三昧手，",
        "能令花落又花开。",
        "返魂元学岭头梅。"
      ]
    },
    {
      "_id": null,
      "Name": "浪淘沙",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "七律",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "翠袖卷轻纱。",
        "玉腕慵遮。",
        "蕊珠宫殿倚彤霞。",
        "不愤江南梅信早，",
        "争下香车。",
        "月露洗凝华。",
        "艳压群葩。",
        "暗传春信到天涯。",
        "试问东风谁第一，",
        "先到人家。"
      ]
    },
    {
      "_id": null,
      "Name": "满庭芳",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "宿雨初收，",
        "晚风微度，",
        "万家帘卷青烟。",
        "暗尘随马，",
        "人物似神仙。",
        "试问天公借月，",
        "天须放、明月教圆。",
        "应移下，",
        "广寒宫殿，",
        "灯火接星躔。",
        "卢川。",
        "元古郡，",
        "当时太守，",
        "宾从俱贤。",
        "到如今井邑，",
        "歌吹喧阗。",
        "花下红妆卖酒，",
        "时相遇、曲水桥边。",
        "谁知道，",
        "山城父老，",
        "重见中兴年。"
      ]
    },
    {
      "_id": null,
      "Name": "点绛唇",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "玉漏春迟，",
        "铁关金锁星桥夜。",
        "暗尘随马。",
        "明月应无价。",
        "天半朱楼，",
        "银汉星光射。",
        "更深也。",
        "翠娥如画。",
        "犹在凉蟾下。"
      ]
    },
    {
      "_id": null,
      "Name": "菩萨蛮",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "武陵西上沅陵渡。",
        "扁舟忘了来时路。",
        "花外有人烟。",
        "相逢疑是仙。",
        "清尊留夜语。",
        "醉倒知何处。",
        "归去客心惊。",
        "金鸡嘲哳鸣。"
      ]
    },
    {
      "_id": null,
      "Name": "虞美人",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "城东楼阁连云起。",
        "冠绝辰州市。",
        "莲灯初发万枝红。",
        "也似江南风景、半天中。",
        "花衢柳陌年时静。",
        "划地今年盛。",
        "棚前箫鼓闹如雷。",
        "添个辰溪女子、舞三台。"
      ]
    },
    {
      "_id": null,
      "Name": "蝶恋花",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "月落灯残人散後。",
        "忽到尊前，",
        "但觉眉儿皱。",
        "数日不来如许瘦。",
        "裙腰减尽君知否。",
        "公子风流应自有。",
        "占断春光，",
        "肯落谁人手。",
        "已是许多时做就。",
        "重教舞彻双罗袖。"
      ]
    },
    {
      "_id": null,
      "Name": "西江月",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "一拂退黄衫子，",
        "几团嗅芯蜂儿。",
        "西风吹下月中枝。",
        "种在寒岩影里。",
        "人道蜡梅相似，",
        "又传菊满东篱。",
        "饶伊颜色入时宜。",
        "安得香传九里。"
      ]
    },
    {
      "_id": null,
      "Name": "解佩令",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "湘江停瑟。",
        "洛川回雪。",
        "是耶非、相逢飘瞥。",
        "云鬓风裳，",
        "照心事、娟娟山月。",
        "翦烟花、带萝同结。",
        "留环盟切。",
        "贻珠情彻。",
        "解携时、玉声愁绝。",
        "罗袜尘生，",
        "早波面、春痕欲灭。",
        "送人行、水声凄咽。"
      ]
    },
    {
      "_id": null,
      "Name": "谒金门",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "溪风紧。",
        "溪上官梅整整。",
        "万木寒痴吹不醒。",
        "一枝先破冷。",
        "梦断香云耿耿。",
        "月淡梨花清影。",
        "长笛倚楼谁共听。",
        "调高成绝品。"
      ]
    },
    {
      "_id": null,
      "Name": "醉桃源·阮郎归",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "朱门映柳画帘垂。",
        "门前闻马嘶。",
        "主人新著绿袍归。",
        "天恩下玉墀。",
        "凭翠袖，",
        "拈花枝。",
        "劝教人醉时。",
        "请君听唱碧云词。",
        "倒倾金屈卮。"
      ]
    },
    {
      "_id": null,
      "Name": "醉花阴",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "玉妃谪堕烟村远。",
        "犹似瑶池见。",
        "缺月挂寒梢，",
        "时有幽香，",
        "飞到朱帘畔。",
        "春风岭上淮南岸。",
        "曾为谁魂断。",
        "依旧瘦稜稜，",
        "天若有情，",
        "天也应须管。"
      ]
    },
    {
      "_id": null,
      "Name": "雨霖铃",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "琼楼玉宇。",
        "满人寰似、海边洲渚。",
        "蓬莱又还水浅，",
        "鲸涛静见，",
        "银宫如许。",
        "紫极鸣筲声断，",
        "望霓舟何处。",
        "待夜深、重倚层霄，",
        "认得瑶池广寒路。",
        "郢中旧曲谁能度。",
        "恨歌声、响入青云去。",
        "西湖近时绝唱，",
        "总不道、月梅盐絮。",
        "暗想当年宾从，",
        "毫端有惊人句。",
        "谩说枚叟邹生，",
        "共作梁园赋。"
      ]
    },
    {
      "_id": null,
      "Name": "鹊桥仙",
      "PoetId": "b43997c07638433f88885a0193805534",
      "PoetName": "王庭珪",
      "Form": "词",
      "Tags": [
        "宋代",
        "王庭珪"
      ],
      "Contents": [
        "云静天高，",
        "山明水阔。",
        "一年待个中秋月。",
        "十分魄少一分圆，",
        "今宵已觉蟾光别。",
        "且尽金尊，",
        "遥瞻玉阙。",
        "更深江水沙如雪。",
        "来宵风雨不相关，",
        "重须买酒追佳节。"
      ]
    }
  ]
}