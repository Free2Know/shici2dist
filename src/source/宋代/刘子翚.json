  "Poems": [
    {
      "_id": null,
      "Name": "蓦山溪",
      "PoetId": "4773d5a876af431c97dca24520ab71d1",
      "PoetName": "刘子翚",
      "Form": "词",
      "Tags": [
        "宋代",
        "刘子翚"
      ],
      "Contents": [
        "浮烟冷雨，",
        "今日还重九。",
        "秋去又秋来，",
        "但黄花、年年如旧。",
        "平台戏马，",
        "无处问英雄；",
        "茅舍底，",
        "竹篱东，",
        "伫立时搔首。",
        "客来何有？",
        "草草三杯酒。",
        "一醉万缘空，",
        "莫贪伊、金印如斗。",
        "病翁老矣，",
        "谁共赋归来？",
        "芟垅麦，",
        "网溪鱼，",
        "未落他人后。"
      ]
    }
  ]
}