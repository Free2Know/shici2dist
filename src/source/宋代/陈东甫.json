  "Dynasty": "宋代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "望江南·忆江南",
      "PoetId": "ec19fd09c2ef48b5ab433dc089b8b7ed",
      "PoetName": "陈东甫",
      "Form": "七绝",
      "Tags": [
        "宋代",
        "陈东甫"
      ],
      "Contents": [
        "芳思远，",
        "南苑惜春时。",
        "翠柳枝柔金笛怨，",
        "碧桃花老玉笙悲。",
        "风日正迟迟。"
      ]
    },
    {
      "_id": null,
      "Name": "谒金门",
      "PoetId": "ec19fd09c2ef48b5ab433dc089b8b7ed",
      "PoetName": "陈东甫",
      "Form": "词",
      "Tags": [
        "宋代",
        "陈东甫"
      ],
      "Contents": [
        "西风竹。",
        "风入翠烟□矗。",
        "红小阑干知几曲。",
        "声声敲碧玉。",
        "窗下凤台银烛。",
        "断梦已惊难续。",
        "曾伴去年庭下菊。",
        "夜阑听雨宿。"
      ]
    },
    {
      "_id": null,
      "Name": "长相思",
      "PoetId": "ec19fd09c2ef48b5ab433dc089b8b7ed",
      "PoetName": "陈东甫",
      "Form": "词",
      "Tags": [
        "宋代",
        "陈东甫"
      ],
      "Contents": [
        "花深深。",
        "柳阴阴。",
        "度柳穿花觅信音。",
        "君心负妾心。",
        "怨鸣琴。",
        "恨孤衾。",
        "钿誓钗盟何处寻。",
        "当初谁料今。"
      ]
    }
  ]
}