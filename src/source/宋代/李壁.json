  "Poems": [
    {
      "_id": null,
      "Name": "南歌子",
      "PoetId": "cc58269de9a443a58a6f5aa570eb890e",
      "PoetName": "李壁",
      "Form": "词",
      "Tags": [
        "宋代",
        "李壁"
      ],
      "Contents": [
        "紫绶新符竹，",
        "叶赬老兄弟。",
        "西风吹棹过湖亭。",
        "杨柳夫渠相伴、也多情。",
        "况是瀛洲侣，",
        "来同酒盏倾。",
        "白沤浑不避双旌。",
        "一种风流人似、玉壶清。"
      ]
    },
    {
      "_id": null,
      "Name": "好事近",
      "PoetId": "cc58269de9a443a58a6f5aa570eb890e",
      "PoetName": "李壁",
      "Form": "词",
      "Tags": [
        "宋代",
        "李壁"
      ],
      "Contents": [
        "莫惜一樽留，",
        "共醉锦屏山色。",
        "多少飞花悠扬，",
        "送征轮南陌。",
        "曲湖归去未多时，",
        "还捧诏黄湿。",
        "生怕别来凄断，",
        "看满园行迹。"
      ]
    },
    {
      "_id": null,
      "Name": "小重山",
      "PoetId": "cc58269de9a443a58a6f5aa570eb890e",
      "PoetName": "李壁",
      "Form": "词",
      "Tags": [
        "宋代",
        "李壁"
      ],
      "Contents": [
        "燕雀风轻二月天。",
        "一枝何处是家园。",
        "有花不惜是谁怜。",
        "生嫌怕，",
        "不为老人妍。",
        "眉黛拥连娟。",
        "高情时载酒、雁湖边。",
        "略无雕饰自天然。",
        "新诗好，",
        "品第入朱弦。"
      ]
    },
    {
      "_id": null,
      "Name": "朝中措",
      "PoetId": "cc58269de9a443a58a6f5aa570eb890e",
      "PoetName": "李壁",
      "Form": "词",
      "Tags": [
        "宋代",
        "李壁"
      ],
      "Contents": [
        "东风歌吹发重闉。",
        "飞旆入山新。",
        "小雨不妨酥润，",
        "江头一并霜晴。",
        "年年心似，",
        "输他钗燕，",
        "蟠带迎春。",
        "怎得尊前避酒，",
        "史君精鉴如神。"
      ]
    },
    {
      "_id": null,
      "Name": "江神子·江城子",
      "PoetId": "cc58269de9a443a58a6f5aa570eb890e",
      "PoetName": "李壁",
      "Form": "词",
      "Tags": [
        "宋代",
        "李壁"
      ],
      "Contents": [
        "露荷香泛小池台。",
        "水云堆。",
        "好风催。",
        "宝扇胡床，",
        "无事且徘徊。",
        "帘外海榴裙一色，",
        "判共酹、两三杯。",
        "此怀能得几番开。",
        "玉山颓。",
        "不须推。",
        "回首慈恩，",
        "前梦老堪咍。",
        "好是上林多少树，",
        "应早晚、待公来。"
      ]
    },
    {
      "_id": null,
      "Name": "浣溪沙",
      "PoetId": "cc58269de9a443a58a6f5aa570eb890e",
      "PoetName": "李壁",
      "Form": "五律",
      "Tags": [
        "宋代",
        "李壁"
      ],
      "Contents": [
        "只记梅花破腊前。",
        "恼人春色又薰然。",
        "山头井似陆公泉。",
        "上客长谣追楚些，",
        "娇娃短舞看胡旋。",
        "崇桃积李自年年。"
      ]
    },
    {
      "_id": null,
      "Name": "西江月",
      "PoetId": "cc58269de9a443a58a6f5aa570eb890e",
      "PoetName": "李壁",
      "Form": "词",
      "Tags": [
        "宋代",
        "李壁"
      ],
      "Contents": [
        "又送鹏程轩翥，",
        "几看驹隙推移。",
        "多端时事只天知。",
        "不饮沈忧如醉。",
        "白首已甘蓬艾，",
        "苍生正倚丞疑。",
        "楚台风转一帆吹。",
        "朝列问君来未。"
      ]
    },
    {
      "_id": null,
      "Name": "阮郎归",
      "PoetId": "cc58269de9a443a58a6f5aa570eb890e",
      "PoetName": "李壁",
      "Form": "词",
      "Tags": [
        "宋代",
        "李壁"
      ],
      "Contents": [
        "苏台一别费三年。",
        "锦书凭雁传。",
        "风姿重见阆江边。",
        "玉壶秋井泉。",
        "翻短舞，",
        "趁么弦。",
        "篆香同夕烟。",
        "多情莫惜为留连。",
        "落花中酒天。"
      ]
    },
    {
      "_id": null,
      "Name": "鹧鸪天",
      "PoetId": "cc58269de9a443a58a6f5aa570eb890e",
      "PoetName": "李壁",
      "Form": "七律",
      "Tags": [
        "宋代",
        "李壁"
      ],
      "Contents": [
        "岸柳阴阴跃锦鳞。",
        "并湖莲子恰尝新。",
        "谁教故岁应官去，",
        "会老堂中少个人。",
        "归未久，",
        "意弥新。",
        "吹香不断酒倾银。",
        "行藏判已天公付，",
        "且斗而今见在身。"
      ]
    }
  ]
}