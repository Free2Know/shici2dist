{
  "_id": "73bca8f74372450eb27414f56a83bcc9",
  "Name": "黄公度",
  "Description": "黄公度（1109-1156），字师宪，莆田（今属福建）人。绍兴八年（1138）进士，著有《知稼翁集》、《知稼翁词》。",
  "Dynasty": "宋代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "一翦梅",
      "PoetId": "73bca8f74372450eb27414f56a83bcc9",
      "PoetName": "黄公度",
      "Form": "词",
      "Tags": [
        "宋代",
        "黄公度"
      ],
      "Contents": [
        "冷艳幽香冰玉姿。",
        "占断孤高，",
        "压尽芳菲。",
        "东君先暖向南枝。",
        "要使天涯，",
        "管领春归。",
        "不受人间莺蝶知。",
        "长是年年，",
        "雪约霜期。",
        "嫣然一笑百花迟。",
        "调鼎行看，",
        "结子黄时。"
      ]
    },
    {
      "_id": null,
      "Name": "千秋岁",
      "PoetId": "73bca8f74372450eb27414f56a83bcc9",
      "PoetName": "黄公度",
      "Form": "词",
      "Tags": [
        "宋代",
        "黄公度"
      ],
      "Contents": [
        "郁葱佳气。",
        "天降麒麟瑞。",
        "回首处，",
        "江城外。",
        "一麾遗爱在，",
        "万口欢声沸。",
        "人乍远，",
        "危楼目断天无际。",
        "五马徘徊地。",
        "春色随归旆。",
        "寿水绿，",
        "壶山翠。",
        "风轻香篆直，",
        "日暖歌喉脆。",
        "椒觞举，",
        "人人尽祝千秋岁。"
      ]
    },
    {
      "_id": null,
      "Name": "卜算子",
      "PoetId": "73bca8f74372450eb27414f56a83bcc9",
      "PoetName": "黄公度",
      "Form": "词",
      "Tags": [
        "宋代",
        "黄公度"
      ],
      "Contents": [
        "寒透小窗纱，",
        "漏断人初醒。",
        "翡翠屏间拾落钗，",
        "背立残釭影。",
        "欲去更踟蹰，",
        "离恨终难整。",
        "陇首流泉不忍闻，",
        "月落双溪冷。"
      ]
    },
    {
      "_id": null,
      "Name": "好事近",
      "PoetId": "73bca8f74372450eb27414f56a83bcc9",
      "PoetName": "黄公度",
      "Form": "词",
      "Tags": [
        "宋代",
        "黄公度"
      ],
      "Contents": [
        "湖上送残春，",
        "已负别时归约。",
        "好在故园桃李，",
        "为谁开谁落。",
        "还家应是荔支天，",
        "浮蚁要人酌。",
        "莫把舞裙歌扇，",
        "便等闲抛却。"
      ]
    },
    {
      "_id": null,
      "Name": "朝中措",
      "PoetId": "73bca8f74372450eb27414f56a83bcc9",
      "PoetName": "黄公度",
      "Form": "词",
      "Tags": [
        "宋代",
        "黄公度"
      ],
      "Contents": [
        "幽香冷艳缀疏枝。",
        "横影卧霜溪。",
        "清楚浑如南郭，",
        "孤高胜似东篱。",
        "岁寒风味，",
        "黄花尽处，",
        "密雪飞时。",
        "不比三春桃李，",
        "芳菲急在人知。"
      ]
    },
    {
      "_id": null,
      "Name": "浣溪沙",
      "PoetId": "73bca8f74372450eb27414f56a83bcc9",
      "PoetName": "黄公度",
      "Form": "五律",
      "Tags": [
        "宋代",
        "黄公度"
      ],
      "Contents": [
        "风送清香过短墙。",
        "烟笼晚色近修篁。",
        "夕阳楼外角声长。",
        "欲去还留无限思，",
        "轻匀淡抹不成妆。",
        "一尊相对月生凉。"
      ]
    },
    {
      "_id": null,
      "Name": "满庭芳",
      "PoetId": "73bca8f74372450eb27414f56a83bcc9",
      "PoetName": "黄公度",
      "Form": "词",
      "Tags": [
        "宋代",
        "黄公度"
      ],
      "Contents": [
        "一径叉分，",
        "三亭鼎峙，",
        "小园别是清幽。",
        "曲阑低槛，",
        "春色四时留。",
        "怪石参差卧虎，",
        "长松偃蹇拿虬。",
        "携筇晚，",
        "风来万里，",
        "冷撼一天秋。",
        "优游。",
        "销永昼，",
        "琴尊左右，",
        "宾主风流。",
        "且偷闲，",
        "不妨身在南州。",
        "故国归帆隐隐，",
        "西崑往事悠悠。",
        "都休问，",
        "金钗十二，",
        "满酌听轻讴。"
      ]
    },
    {
      "_id": null,
      "Name": "点绛唇",
      "PoetId": "73bca8f74372450eb27414f56a83bcc9",
      "PoetName": "黄公度",
      "Form": "词",
      "Tags": [
        "宋代",
        "黄公度"
      ],
      "Contents": [
        "嫩绿娇红，",
        "砌成别恨千千斗。",
        "短亭回首。",
        "不是缘春瘦。",
        "一曲阳关，",
        "杯送纤纤手。",
        "还知否。",
        "凤池归後。",
        "无路陪尊酒。"
      ]
    },
    {
      "_id": null,
      "Name": "眼儿媚",
      "PoetId": "73bca8f74372450eb27414f56a83bcc9",
      "PoetName": "黄公度",
      "Form": "词",
      "Tags": [
        "宋代",
        "黄公度"
      ],
      "Contents": [
        "一枝雪里冷光浮。",
        "空自许清流。",
        "如今憔悴，",
        "蛮烟瘴雨，",
        "谁肯寻搜。",
        "昔年曾共孤芳醉，",
        "争插玉钗头。",
        "天涯幸有，",
        "惜花人在，",
        "杯酒相酬。"
      ]
    },
    {
      "_id": null,
      "Name": "菩萨蛮",
      "PoetId": "73bca8f74372450eb27414f56a83bcc9",
      "PoetName": "黄公度",
      "Form": "词",
      "Tags": [
        "宋代",
        "黄公度"
      ],
      "Contents": [
        "眉尖早识愁滋味。",
        "娇羞未解论心事。",
        "试问忆人不。",
        "无言但点头。",
        "嗔人归不早。",
        "故把金杯恼。",
        "醉看舞时腰。",
        "还如旧日娇。"
      ]
    },
    {
      "_id": null,
      "Name": "青玉案",
      "PoetId": "73bca8f74372450eb27414f56a83bcc9",
      "PoetName": "黄公度",
      "Form": "词",
      "Tags": [
        "宋代",
        "黄公度"
      ],
      "Contents": [
        "邻鸡不管离怀苦，",
        "又还是、催人去。",
        "回首高城音信阻。",
        "霜桥月馆，",
        "水村烟市，",
        "总是思君处。",
        "裛残别袖燕支雨，",
        "谩留得、愁千缕。",
        "欲倩归鸿分付与。",
        "鸿飞不住，",
        "倚阑无语，",
        "独立长天暮。"
      ]
    }
  ]
}