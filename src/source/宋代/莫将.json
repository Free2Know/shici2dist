  "Poems": [
    {
      "_id": null,
      "Name": "木兰花·玉楼春",
      "PoetId": "0997a438cc33430487cfbed1a225f327",
      "PoetName": "莫将",
      "Form": "七律",
      "Tags": [
        "宋代",
        "莫将"
      ],
      "Contents": [
        "暗香浮动黄昏後。",
        "更是月明如白昼。",
        "看来都坐玉壶冰，",
        "折赠徐妃丹桂手。",
        "赏酬风景无过酒。",
        "对影成三谁左右。",
        "劝君携取董妖娆，",
        "拱得醉翁香满袖。"
      ]
    },
    {
      "_id": null,
      "Name": "浣溪沙",
      "PoetId": "0997a438cc33430487cfbed1a225f327",
      "PoetName": "莫将",
      "Form": "五绝",
      "Tags": [
        "宋代",
        "莫将"
      ],
      "Contents": [
        "宝钏香裙上玉梯。",
        "云重应恨翠楼低。",
        "愁同芳草两萋萋。"
      ]
    },
    {
      "_id": null,
      "Name": "独脚令·忆王孙",
      "PoetId": "0997a438cc33430487cfbed1a225f327",
      "PoetName": "莫将",
      "Form": "词",
      "Tags": [
        "宋代",
        "莫将"
      ],
      "Contents": [
        "绛唇初点粉红新。",
        "凤镜临妆已逼真。",
        "苒苒钗头香趁人。",
        "惜芳晨。",
        "玉骨冰姿别是春。"
      ]
    }
  ]
}