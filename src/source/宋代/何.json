    {
      "_id": null,
      "Name": "虞美人",
      "PoetId": "968c0126de474830a0a1b3c725d243e7",
      "PoetName": "何",
      "Form": "词",
      "Tags": [
        "宋代",
        "何"
      ],
      "Contents": [
        "分香帕子揉蓝腻。",
        "欲去殷勤惠。",
        "重来直待牡丹时。",
        "只恐花知、知後故开迟。",
        "别来看尽闲桃李。",
        "日日阑干倚。",
        "催花无计问东风。",
        "梦作一双蝴蝶、绕芳丛。"
      ]
    },
    {
      "_id": null,
      "Name": "采桑子",
      "PoetId": "968c0126de474830a0a1b3c725d243e7",
      "PoetName": "何",
      "Form": "词",
      "Tags": [
        "宋代",
        "何"
      ],
      "Contents": [
        "百花丛里花君子，",
        "取信东君。",
        "取信东君。",
        "名策花中第一勋。",
        "结成宝鼎和羹味，",
        "多谢东君。",
        "多谢东君。",
        "香遍还应号令春。"
      ]
    }
  ]
}