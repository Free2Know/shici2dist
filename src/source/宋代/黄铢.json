  "Poems": [
    {
      "_id": null,
      "Name": "江神子·江城子",
      "PoetId": "289d9378bb424b38a56bbdda00902fde",
      "PoetName": "黄铢",
      "Form": "词",
      "Tags": [
        "宋代",
        "黄铢"
      ],
      "Contents": [
        "秋风袅袅夕阳红。",
        "晚烟浓。",
        "暮云重。",
        "万叠青山，",
        "山外叫孤鸿。",
        "独上高楼三百尺，",
        "凭玉楯，",
        "睇层空。",
        "人间日月去匆匆。",
        "碧梧桐。",
        "又西风。",
        "北去南来，",
        "销尽几英雄。",
        "掷下玉尊天外去，",
        "多少事，",
        "不言中。"
      ]
    },
    {
      "_id": null,
      "Name": "渔家傲",
      "PoetId": "289d9378bb424b38a56bbdda00902fde",
      "PoetName": "黄铢",
      "Form": "词",
      "Tags": [
        "宋代",
        "黄铢"
      ],
      "Contents": [
        "永日离忧千万绪。",
        "雪舟远泛清漳浦。",
        "珍重故人寒夜语。",
        "挥玉麈。",
        "沈沈画阁凝香雾。",
        "风砌落花留不住。",
        "红蜂翠蝶闲飞舞。",
        "明日柳营江上路。",
        "云起处。",
        "苍山万叠人归去。"
      ]
    },
    {
      "_id": null,
      "Name": "菩萨蛮",
      "PoetId": "289d9378bb424b38a56bbdda00902fde",
      "PoetName": "黄铢",
      "Form": "词",
      "Tags": [
        "宋代",
        "黄铢"
      ],
      "Contents": [
        "海山翠叠青螺浅。",
        "暮云散尽天容远。",
        "匹马度江皋。",
        "北风生怒号。",
        "解鞍栖倦翮。",
        "皓月空庭白。",
        "何处小阑干。",
        "玉箫吹夜寒。"
      ]
    }
  ]
}