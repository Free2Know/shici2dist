      "_id": null,
      "Name": "伊州三台令·伊州三台",
      "PoetId": "3a63ec0313fd4987acc20b1d488ae11e",
      "PoetName": "杨韶父",
      "Form": "未知",
      "Tags": [
        "宋代",
        "杨韶父"
      ],
      "Contents": [
        "水村月淡云低。",
        "为爱寒香晚吹。",
        "瘦马立多时。",
        "是谁家、茅舍竹篱。",
        "三三两两芳蕤。",
        "未放琼铺雪堆。",
        "只这一些儿。",
        "胜东风、千枝万枝。"
      ]
    },
    {
      "_id": null,
      "Name": "如梦令",
      "PoetId": "3a63ec0313fd4987acc20b1d488ae11e",
      "PoetName": "杨韶父",
      "Form": "词",
      "Tags": [
        "宋代",
        "杨韶父"
      ],
      "Contents": [
        "凉月薄阴犹殢。",
        "红豆一枝秋思。",
        "门外几重山，",
        "山外行人千里。",
        "归未。",
        "归未。",
        "又是菊花天气。"
      ]
    },
    {
      "_id": null,
      "Name": "长相思",
      "PoetId": "3a63ec0313fd4987acc20b1d488ae11e",
      "PoetName": "杨韶父",
      "Form": "词",
      "Tags": [
        "宋代",
        "杨韶父"
      ],
      "Contents": [
        "溪水清。",
        "溪水浑。",
        "溪上人家数亩园。",
        "垂杨深闭门。",
        "青罗裙。",
        "白罗裙。",
        "采尽青蘩到白苹。",
        "江南三月春。"
      ]
    }
  ]
}