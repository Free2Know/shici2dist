    {
      "_id": null,
      "Name": "水调歌头",
      "PoetId": "d5702231971645d79b784e46c56efc8a",
      "PoetName": "熊以宁",
      "Form": "词",
      "Tags": [
        "宋代",
        "熊以宁"
      ],
      "Contents": [
        "天地有英气，",
        "锺秀在人间。",
        "流虹甫近华旦，",
        "维岳预生贤。",
        "千载明良会遇，",
        "三世勋庸忠烈，",
        "谁得似家传。",
        "高论排风俗，",
        "抗志遏狂澜。",
        "记年时，",
        "胡尘外，",
        "拥朱幡。",
        "洪枢紫府功业，",
        "指日复青毡。",
        "拭目泥封飞下，",
        "趁取梅花消息，",
        "去去簉朝班。",
        "骨相元不老，",
        "何必颂南山。"
      ]
    },
    {
      "_id": null,
      "Name": "醉蓬莱",
      "PoetId": "d5702231971645d79b784e46c56efc8a",
      "PoetName": "熊以宁",
      "Form": "词",
      "Tags": [
        "宋代",
        "熊以宁"
      ],
      "Contents": [
        "渐荷收绿盖，",
        "桂吐金英，",
        "嫩凉天气。",
        "香案仙官，",
        "向人间游戏。",
        "道貌孤高，",
        "灵扃恬淡，",
        "有寿星标致。",
        "梦得文章，",
        "水心谈论，",
        "家声相继。",
        "芙蓉上幕，",
        "龙藩胜地。",
        "剡荐交飞，",
        "宸衷简记。",
        "行趣朝班，",
        "便鸾坡螭陛。",
        "赞画堂中，",
        "垂弧节届，",
        "且举觞一醉。",
        "试问桩椿，",
        "春秋多少，",
        "八千馀岁。"
      ]
    },
    {
      "_id": null,
      "Name": "鹊桥仙",
      "PoetId": "d5702231971645d79b784e46c56efc8a",
      "PoetName": "熊以宁",
      "Form": "词",
      "Tags": [
        "宋代",
        "熊以宁"
      ],
      "Contents": [
        "隐君仙裔，",
        "帅垣佳配。",
        "谁似硕人清贵。",
        "几番鸾诰自天来，",
        "森绿绶、彩衣当砌。",
        "莲开十丈，",
        "蓂留十荚，",
        "迟十日、瑶池秋至。",
        "殷勤祝寿指蟠桃，",
        "更重数、三千馀岁。"
      ]
    }
  ]
}