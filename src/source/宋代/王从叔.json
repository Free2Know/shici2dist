    {
      "_id": null,
      "Name": "南柯子·南歌子",
      "PoetId": "bb75c3736cb64cb6b0bbc6744f85e9f0",
      "PoetName": "王从叔",
      "Form": "词",
      "Tags": [
        "宋代",
        "王从叔"
      ],
      "Contents": [
        "碧树留云湿，",
        "青山似笠低。",
        "鹧鸪啼罢竹鸡啼。",
        "不晓天天何意、要梅肥。",
        "昨日穿新葛，",
        "今朝御夹衣。",
        "思家怀抱政难为。",
        "只恐归来憔悴、却羞归。"
      ]
    },
    {
      "_id": null,
      "Name": "昭君怨",
      "PoetId": "bb75c3736cb64cb6b0bbc6744f85e9f0",
      "PoetName": "王从叔",
      "Form": "五律",
      "Tags": [
        "宋代",
        "王从叔"
      ],
      "Contents": [
        "门外春风几度。",
        "马上行人何处。",
        "休更卷朱帘。",
        "草连天。",
        "立尽海棠花月。",
        "飞到荼シ香雪。",
        "莫怪梦难成。",
        "梦无凭。"
      ]
    },
    {
      "_id": null,
      "Name": "浣溪沙",
      "PoetId": "bb75c3736cb64cb6b0bbc6744f85e9f0",
      "PoetName": "王从叔",
      "Form": "五律",
      "Tags": [
        "宋代",
        "王从叔"
      ],
      "Contents": [
        "水月精神玉雪胎。",
        "乾坤清气化生来。",
        "断桥流水领春回。",
        "昨夜醉眠苔上石，",
        "天香冉冉下瑶台。",
        "起来窗外见花开。"
      ]
    },
    {
      "_id": null,
      "Name": "秋蕊香",
      "PoetId": "bb75c3736cb64cb6b0bbc6744f85e9f0",
      "PoetName": "王从叔",
      "Form": "词",
      "Tags": [
        "宋代",
        "王从叔"
      ],
      "Contents": [
        "薄薄罗衣乍暖，",
        "红入酒痕潮面。",
        "絮花舞倦带娇眼。",
        "昨夜平堤水浅。",
        "故人信断风筝线。",
        "误归燕。",
        "梦魂不怕山路远。",
        "无奈棋声隔院。"
      ]
    },
    {
      "_id": null,
      "Name": "阮郎归",
      "PoetId": "bb75c3736cb64cb6b0bbc6744f85e9f0",
      "PoetName": "王从叔",
      "Form": "词",
      "Tags": [
        "宋代",
        "王从叔"
      ],
      "Contents": [
        "风中柳絮水中萍。",
        "聚散两无情。",
        "斜阳路上短长亭。",
        "今朝第几程。",
        "何限事，",
        "可怜生。",
        "能消几度春。",
        "别时言语总伤心。",
        "何曾一字真。"
      ]
    }
  ]
}