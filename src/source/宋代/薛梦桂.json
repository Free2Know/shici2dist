  "Poems": [
    {
      "_id": null,
      "Name": "三姝媚",
      "PoetId": "4fe4ba4fa898403ea4efbad8c9bdc55e",
      "PoetName": "薛梦桂",
      "Form": "未知",
      "Tags": [
        "宋代",
        "薛梦桂"
      ],
      "Contents": [
        "蔷薇花谢去。",
        "更无情、连夜送春风雨。",
        "燕子呢喃，",
        "似念人憔悴，",
        "往来朱户。",
        "涨绿烟深，",
        "早零落、点池萍絮。",
        "暗忆年华，",
        "罗帐分钗，",
        "又惊春暮。",
        "芳草凄迷征路。",
        "待去也，",
        "还将画轮留住。",
        "纵使重来，",
        "怕粉容销腻，",
        "却羞郎觑。",
        "细数盟言犹在，",
        "怅青楼何处。",
        "绾尽垂杨，",
        "争似相思寸缕。"
      ]
    },
    {
      "_id": null,
      "Name": "浣溪纱·浣溪沙",
      "PoetId": "4fe4ba4fa898403ea4efbad8c9bdc55e",
      "PoetName": "薛梦桂",
      "Form": "五律",
      "Tags": [
        "宋代",
        "薛梦桂"
      ],
      "Contents": [
        "柳映疏帘花映林。",
        "春光一半几销魂。",
        "新诗未了枕先温。",
        "燕子说将千万恨，",
        "海棠开到二三分。",
        "小窗银烛又黄昏。"
      ]
    },
    {
      "_id": null,
      "Name": "眼儿媚",
      "PoetId": "4fe4ba4fa898403ea4efbad8c9bdc55e",
      "PoetName": "薛梦桂",
      "Form": "词",
      "Tags": [
        "宋代",
        "薛梦桂"
      ],
      "Contents": [
        "碧筒新展绿蕉芽。",
        "黄露洒榴花。",
        "蘸烟染就，",
        "和云卷起，",
        "秋水人家。",
        "只因一朵芙蓉月，",
        "生怕黛帘遮。",
        "燕衔不去，",
        "雁飞不到，",
        "愁满天涯。"
      ]
    },
    {
      "_id": null,
      "Name": "醉落魄·一斛珠",
      "PoetId": "4fe4ba4fa898403ea4efbad8c9bdc55e",
      "PoetName": "薛梦桂",
      "Form": "词",
      "Tags": [
        "宋代",
        "薛梦桂"
      ],
      "Contents": [
        "单衣乍著。",
        "滞寒更傍东风作。",
        "珠帘压定银钩索。",
        "雨弄新晴，",
        "轻旋玉尘落。",
        "花唇巧借妆红约。",
        "娇羞才放三分萼。",
        "樽前不用多评泊。",
        "春浅春深，",
        "都向杏梢觉。"
      ]
    }
  ]
}