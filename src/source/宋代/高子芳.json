  "Poems": [
    {
      "_id": null,
      "Name": "念奴娇",
      "PoetId": "6605499fc0c3497ea07082c81571452f",
      "PoetName": "高子芳",
      "Form": "词",
      "Tags": [
        "宋代",
        "高子芳"
      ],
      "Contents": [
        "葱葱佳气，",
        "人都道、今日垂弧令旦。",
        "怪得欢声如鼎沸，",
        "准拟华堂开宴。",
        "三虎容仪，",
        "二乔态度，",
        "争捧金杯劝。",
        "瑶池王母，",
        "何妨引领仙眷。",
        "况有道骨仙风，",
        "灵丹秘决，",
        "钟鼎何心恋。",
        "却笑玉阶轩冕客，",
        "难脱功名羁绊。",
        "萧散陶篱，",
        "徜徉裴野，",
        "风月堪为伴。",
        "拳拳祝颂，",
        "寿龄期等龟算。"
      ]
    },
    {
      "_id": null,
      "Name": "菩萨蛮",
      "PoetId": "6605499fc0c3497ea07082c81571452f",
      "PoetName": "高子芳",
      "Form": "词",
      "Tags": [
        "宋代",
        "高子芳"
      ],
      "Contents": [
        "去年曾祝夫人寿。",
        "今岁也又还依旧。",
        "鬓绿与颜朱。",
        "神仙想不如。",
        "骨相真难老。",
        "疑是居蓬岛。",
        "那更舞霓裳。",
        "笙歌溢画堂。"
      ]
    }
  ]
}