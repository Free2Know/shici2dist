  "Poems": [
    {
      "_id": null,
      "Name": "小重山",
      "PoetId": "0d3bdd251e7d4cd0b46119c6ee38f6fb",
      "PoetName": "章谦亨",
      "Form": "词",
      "Tags": [
        "宋代",
        "章谦亨"
      ],
      "Contents": [
        "久雨初晴天气佳。",
        "远峰犹□被，",
        "乱峰遮。",
        "更无一朵路旁花。",
        "春归也，",
        "光景只桑麻。",
        "山径曲如蛇。",
        "□□□□□，",
        "□□□。",
        "薄醪邀客去程赊。",
        "都输与，",
        "鷖鹭立平沙。"
      ]
    },
    {
      "_id": null,
      "Name": "念奴娇",
      "PoetId": "0d3bdd251e7d4cd0b46119c6ee38f6fb",
      "PoetName": "章谦亨",
      "Form": "词",
      "Tags": [
        "宋代",
        "章谦亨"
      ],
      "Contents": [
        "垂杨得地，",
        "在楼台侧畔，",
        "无人攀折。",
        "不似津亭舟系处，",
        "只伴客愁离别。",
        "丝过摇金，",
        "带铺新翠，",
        "雅称莺调舌。",
        "芳筵相映，",
        "最宜斜挂残月。",
        "却得连日春寒，",
        "未教轻滚，",
        "一片庭前雪。",
        "应恨张郎今老去，",
        "难比风流时节。",
        "醉眼浑醒，",
        "愁眉都展，",
        "舞困腰肢怯。",
        "有时微笑，",
        "把伊绾个双结。"
      ]
    },
    {
      "_id": null,
      "Name": "摸鱼儿",
      "PoetId": "0d3bdd251e7d4cd0b46119c6ee38f6fb",
      "PoetName": "章谦亨",
      "Form": "词",
      "Tags": [
        "宋代",
        "章谦亨"
      ],
      "Contents": [
        "想先生、跨鹤归去。",
        "依然上界官府。",
        "胸中邱壑经营巧，",
        "留下午桥别墅。",
        "堪爱处。",
        "山对起、飞来万马平坡驻。",
        "带湖鸥鹭。",
        "犹不忍寒盟，",
        "时寻门外，",
        "一片芰荷浦。",
        "秋水观，",
        "环绕滔滔瀑布。",
        "参天林木奇古。",
        "云烟只在阑干角，",
        "生出晚来微雨。",
        "东道主。",
        "爱宾客、梅花烂漫开樽俎。",
        "满怀尘土。",
        "扫荡已无馀，",
        "□□时上，",
        "玉峤翠瀛语。"
      ]
    },
    {
      "_id": null,
      "Name": "步蟾宫",
      "PoetId": "0d3bdd251e7d4cd0b46119c6ee38f6fb",
      "PoetName": "章谦亨",
      "Form": "词",
      "Tags": [
        "宋代",
        "章谦亨"
      ],
      "Contents": [
        "团圞小酌醺醺醉。",
        "厮捱著、没人肯睡。",
        "呼卢直到五更头，",
        "便铺了妆台梳洗。",
        "庭前鼓吹喧人耳。",
        "蓦忽地、又添一岁。",
        "休嫌不足少年时，",
        "有多少、老如我底。"
      ]
    },
    {
      "_id": null,
      "Name": "浪淘沙",
      "PoetId": "0d3bdd251e7d4cd0b46119c6ee38f6fb",
      "PoetName": "章谦亨",
      "Form": "七律",
      "Tags": [
        "宋代",
        "章谦亨"
      ],
      "Contents": [
        "台上凭阑干。",
        "犹怯春寒。",
        "被谁偷了最高山。",
        "将谓六丁移取去，",
        "不在人间。",
        "却是晓云闲。",
        "特地遮拦。",
        "与天一样白漫漫。",
        "喜得东风收卷尽，",
        "依旧追还。"
      ]
    },
    {
      "_id": null,
      "Name": "画堂春",
      "PoetId": "0d3bdd251e7d4cd0b46119c6ee38f6fb",
      "PoetName": "章谦亨",
      "Form": "七绝",
      "Tags": [
        "宋代",
        "章谦亨"
      ],
      "Contents": [
        "连朝檐溜几曾乾。",
        "韶华一似衰颜。",
        "牡丹开尽木香残。",
        "忆家山，",
        "愁倚危阑。"
      ]
    },
    {
      "_id": null,
      "Name": "石州引·石州慢",
      "PoetId": "0d3bdd251e7d4cd0b46119c6ee38f6fb",
      "PoetName": "章谦亨",
      "Form": "词",
      "Tags": [
        "宋代",
        "章谦亨"
      ],
      "Contents": [
        "半角庭阴，",
        "弓月映眉，",
        "珠露侵靸。",
        "花棚倒挂风枝，",
        "低罥鬓唇ぢ叶。",
        "凭肩笑问，",
        "甚日罢织流黄，",
        "泥人无语吟虫答。",
        "灯近悄分携，",
        "溜钗钿犀合。",
        "一霎。",
        "莲丝易折。",
        "未稳栖鸳，",
        "陡惊弹鸭。",
        "几度空阶，",
        "宵永笛声孤擫。",
        "玉腰烟瘦，",
        "□□梨梦香消，",
        "醒来凉袖阑干压。",
        "不尽度檐云，",
        "写闲愁千叠。"
      ]
    }
  ]
}