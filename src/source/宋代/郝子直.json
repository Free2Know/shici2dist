  "Poems": [
    {
      "_id": null,
      "Name": "喜迁莺",
      "PoetId": "4039b29309e34e27a3df7d676cddba9a",
      "PoetName": "郝子直",
      "Form": "词",
      "Tags": [
        "宋代",
        "郝子直"
      ],
      "Contents": [
        "风云际会。",
        "自军兴勋业，",
        "唯公屈指。",
        "中镝红心，",
        "骄虏胆丧，",
        "中正已回天意。",
        "采石扶危鏖战，",
        "激励熊罴乘势。",
        "震天地，",
        "沮金狄百万，",
        "元颜诛毙。",
        "英卫。",
        "摅红气，",
        "寰海共欣，",
        "雪恨亡胡岁。",
        "宸眷褒功，",
        "华衮貂蝉，",
        "荣拜济时左揆。",
        "圣君用贤终始，",
        "肯使留侯谦退。",
        "後范蠡，",
        "愿永佐中兴，",
        "庄椿同纪。"
      ]
    },
    {
      "_id": null,
      "Name": "画堂春",
      "PoetId": "4039b29309e34e27a3df7d676cddba9a",
      "PoetName": "郝子直",
      "Form": "词",
      "Tags": [
        "宋代",
        "郝子直"
      ],
      "Contents": [
        "华堂当日诞今辰。",
        "庆门人降西真。",
        "一杯千岁祝遐龄。",
        "莫厌频斟。",
        "不愿头白还黑，",
        "不愿齿落重生。",
        "但容艳只如今。",
        "寿等松椿。"
      ]
    }
  ]
}