{
  "_id": "857e7b8929f54786b846817e06e83590",
  "Name": "晁冲之",
  "Description": "晁冲之（？-？） 字用叔，一字川道，济州巨野（今属山东）人，晁补之从弟。举进士。其词聪俊明媚，与其兄豪健之作相反。赵万里辑其词十六首为一卷，名《晁用叔词》。",
  "Dynasty": "宋代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "上林春慢",
      "PoetId": "857e7b8929f54786b846817e06e83590",
      "PoetName": "晁冲之",
      "Form": "词",
      "Tags": [
        "宋代",
        "晁冲之"
      ],
      "Contents": [
        "帽落宫花，",
        "衣惹御香，",
        "凤辇晚来初过。",
        "鹤降诏飞，",
        "龙擎烛戏，",
        "端门万枝灯火。",
        "满城车马，",
        "对明月、有谁闲坐。",
        "任狂游，",
        "更许傍禁街，",
        "不局金锁。",
        "玉楼人、暗中掷果。",
        "珍帘下、笑著春衫袅娜。",
        "素蛾绕钗，",
        "轻蝉扑鬓，",
        "垂垂柳丝梅朵。",
        "夜阑饮散，",
        "但赢得、翠翘双亸。",
        "醉归来，",
        "又重向、晓窗梳裹。"
      ]
    },
    {
      "_id": null,
      "Name": "临江仙",
      "PoetId": "857e7b8929f54786b846817e06e83590",
      "PoetName": "晁冲之",
      "Form": "词",
      "Tags": [
        "宋代",
        "晁冲之"
      ],
      "Contents": [
        "谩道追欢惟九日，",
        "年年此恨偏浓。",
        "今朝吹帽与谁同。",
        "黄花都未拆，",
        "和泪泣西风。",
        "应恐登临肠更断，",
        "故交烟雨迷空。",
        "为君一曲送飞鸿。",
        "谁能推毂我，",
        "深入醉乡中。"
      ]
    },
    {
      "_id": null,
      "Name": "如梦令",
      "PoetId": "857e7b8929f54786b846817e06e83590",
      "PoetName": "晁冲之",
      "Form": "词",
      "Tags": [
        "宋代",
        "晁冲之"
      ],
      "Contents": [
        "门在垂杨阴里。",
        "楼枕曲江春水。",
        "一阵牡丹风，",
        "香压满园花气。",
        "沈醉。",
        "沈醉。",
        "不记绿窗先睡。"
      ]
    },
    {
      "_id": null,
      "Name": "小重山",
      "PoetId": "857e7b8929f54786b846817e06e83590",
      "PoetName": "晁冲之",
      "Form": "词",
      "Tags": [
        "宋代",
        "晁冲之"
      ],
      "Contents": [
        "碧水浮瓜纹簟前。",
        "只知闲枕手，",
        "不成眠。",
        "晚云如火雨晴天。",
        "轻云远，",
        "亭外一声蝉。",
        "池馆几年年。",
        "倚阑催小艇，",
        "采新莲。",
        "多情还到芰荷边。",
        "应相忆，",
        "折藕看丝牵。"
      ]
    },
    {
      "_id": null,
      "Name": "感皇恩",
      "PoetId": "857e7b8929f54786b846817e06e83590",
      "PoetName": "晁冲之",
      "Form": "词",
      "Tags": [
        "宋代",
        "晁冲之"
      ],
      "Contents": [
        "蝴蝶满西园，",
        "啼莺无数。",
        "水阁桥南路。",
        "凝伫。",
        "两行烟柳，",
        "吹落一池飞絮。",
        "秋千斜挂起，",
        "人何处。",
        "把酒劝君，",
        "闲愁莫诉。",
        "留取笙歌住。",
        "休去。",
        "几多春色，",
        "禁得许多风雨。",
        "海棠花谢也，",
        "君知否。"
      ]
    },
    {
      "_id": null,
      "Name": "汉宫春",
      "PoetId": "857e7b8929f54786b846817e06e83590",
      "PoetName": "晁冲之",
      "Form": "词",
      "Tags": [
        "宋代",
        "晁冲之"
      ],
      "Contents": [
        "黯黯离怀，",
        "向东门系马，",
        "南浦移舟。",
        "薰风乱飞燕子，",
        "时下轻鸥。",
        "无情渭水，",
        "问谁教、日日东流。",
        "常是送、行人去後，",
        "烟波一向离愁。",
        "回首旧游如梦，",
        "记踏青殢饮，",
        "拾翠狂游。",
        "无端彩云易散，",
        "覆水难收。",
        "风流未老，",
        "拚千金，",
        "重入扬州。",
        "应又是、当年载酒，",
        "依前名占青楼。"
      ]
    },
    {
      "_id": null,
      "Name": "渔家傲",
      "PoetId": "857e7b8929f54786b846817e06e83590",
      "PoetName": "晁冲之",
      "Form": "词",
      "Tags": [
        "宋代",
        "晁冲之"
      ],
      "Contents": [
        "浦口潮来沙尾涨。",
        "危樯半落帆游漾，",
        "水调不知何处唱。",
        "风淡荡。",
        "鳜鱼吹起桃花浪。",
        "雪尽小桥梅总放。",
        "层楼一任愁人上。",
        "万里长安回首望。",
        "山四向。",
        "澄江日色如春酿。"
      ]
    },
    {
      "_id": null,
      "Name": "玉蝴蝶",
      "PoetId": "857e7b8929f54786b846817e06e83590",
      "PoetName": "晁冲之",
      "Form": "词",
      "Tags": [
        "宋代",
        "晁冲之"
      ],
      "Contents": [
        "目断江南千里，",
        "灞桥一望，",
        "烟水微茫。",
        "尽锁重门，",
        "人去暗度流光。",
        "雨轻轻，",
        "梨花院落，",
        "风淡淡、杨柳池塘。",
        "恨偏长。",
        "佩沈湘浦，",
        "云散高唐。",
        "清狂。",
        "重来一梦，",
        "手搓梅子，",
        "煮酒初尝。",
        "寂寞经春，",
        "小桥依旧燕飞忙。",
        "玉钩栏、凭多渐暖，",
        "金缕枕、别久犹香。",
        "最难忘。",
        "看花南陌，",
        "待月西厢。"
      ]
    }
  ]
}