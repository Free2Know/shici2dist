  "Poems": [
    {
      "_id": null,
      "Name": "于飞乐",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "薄日烘晴，",
        "轻烟笼晓，",
        "春风绣出林塘。",
        "笑溪桃，",
        "并坞杏，",
        "忒煞寻常。",
        "东君处，",
        "没他後、成甚风光。",
        "翠深深，",
        "谁教入骨，",
        "夜来过雨淋浪。",
        "这些儿颜色，",
        "已恼乱人肠。",
        "如何更道，",
        "可惜处、只是无香。"
      ]
    },
    {
      "_id": null,
      "Name": "千秋岁",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "玉林照坐。",
        "簌簌花微堕。",
        "春院静，",
        "烟扉锁。",
        "黛轻妆未试，",
        "红淡唇微破。",
        "清瘦也，",
        "算应都是风流过。",
        "把盏对横枝，",
        "尚忆年时个。",
        "人不见，",
        "愁无那。",
        "绕林霜掠袂，",
        "嚼蕊香黏唾。",
        "清梦断，",
        "更随月色禁持我。"
      ]
    },
    {
      "_id": null,
      "Name": "卜算子",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "生别有相逢，",
        "死别无消息。",
        "说著从前总是愁，",
        "只是不相忆。",
        "月堕半窗寒，",
        "梦里分明识。",
        "却似瞋人不忆他，",
        "花露盈盈湿。"
      ]
    },
    {
      "_id": null,
      "Name": "如梦令",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "老插黄花不称。",
        "节物撩人且任。",
        "破帽略遮阑，",
        "嫌见星星越甚。",
        "不饮。",
        "不饮。",
        "和取蜂愁蝶恨。"
      ]
    },
    {
      "_id": null,
      "Name": "小重山",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "轻暑单衣四月天。",
        "重来间屈指，",
        "惜流年。",
        "人间何处有神仙。",
        "安排我，",
        "花底与尊前。",
        "争道使君贤。",
        "笔端驱万马，",
        "驻平川。",
        "长安只在日西边。",
        "空回首，",
        "乔木淡疏烟。"
      ]
    },
    {
      "_id": null,
      "Name": "感皇恩",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "万绿压庭柯，",
        "雨晴烟润。",
        "三尺金猊麝微喷。",
        "百花香暖，",
        "酿作九霞仙酝。",
        "祝君如此酒，",
        "年年饮。",
        "插额汉貂，",
        "垂腰苏印。",
        "趁取如今未华鬓。",
        "三茅兄弟，",
        "总有丹台名姓。",
        "蟠桃熟也未，",
        "教人问。"
      ]
    },
    {
      "_id": null,
      "Name": "朝中措",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "相思两地费三年。",
        "明月几回圆。",
        "鸥鸟不知许事，",
        "清江仍绕青山。",
        "尊前歌板，",
        "未终金缕，",
        "已到阳关。",
        "趁取腊前归去，",
        "梅花不奈春寒。"
      ]
    },
    {
      "_id": null,
      "Name": "武陵春",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "晓日帘栊初破睡，",
        "宝鸭宿薰浓。",
        "笑指图中鹤发翁。",
        "仙骨宛然同。",
        "万里郎官遥上寿，",
        "五马茜衫红。",
        "待插华貂酒满锺。",
        "仍是黑头公。"
      ]
    },
    {
      "_id": null,
      "Name": "殢人娇",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "未知",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "痴本无绦，",
        "闷宁有火。",
        "都是你，",
        "自缠自锁。",
        "高来也可。",
        "低来也可。",
        "这宇宙，",
        "何曾碍你一个。",
        "休说荣枯，",
        "强分物我。",
        "惺惺地，",
        "要须识破。",
        "渔樵不小，",
        "公侯不大。",
        "但赢取，",
        "饥飡醉来便卧。"
      ]
    },
    {
      "_id": null,
      "Name": "水调歌头",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "江涨解网雨，",
        "衣润熟梅天。",
        "高人何事，",
        "乘兴来寄五湖船。",
        "才听咚咚叠奏，",
        "呕轧撸声声齐发，",
        "几别故州山。",
        "转盼青楼杪，",
        "已在碧云端。",
        "渡头月，",
        "临晚霁，",
        "泊清湾。",
        "水空天静，",
        "高下相应总团圞。",
        "遥想吾家更好，",
        "尽唤儿曹泛扫，",
        "欣赏共婵娟。",
        "应念思归客，",
        "对此不成眠。"
      ]
    },
    {
      "_id": null,
      "Name": "点绛唇",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "一翦秋光，",
        "阿谁洗得无纤滓。",
        "冰盘彻底。",
        "人也清如此。",
        "万里归来，",
        "著个斑衣戏。",
        "慈颜喜。",
        "问君不醉。",
        "更遣何人醉。"
      ]
    },
    {
      "_id": null,
      "Name": "玉漏迟",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "东南应眷倚。",
        "当年丽绝，",
        "今其馀几。",
        "云锦飘香，",
        "好在藕花十里。",
        "六月长安徂暑，",
        "只一雨、滂沱都洗。",
        "君好为。",
        "携将苏醒，",
        "三吴生齿。",
        "总道秦蜀讴吟，",
        "但消得雍容，",
        "笑谈而致。",
        "稍待秋风，",
        "也拟买舟东逝。",
        "收拾尘编蠹简，",
        "更饱看、江山奇伟。",
        "歌盛美。",
        "远送往趋天陛。"
      ]
    },
    {
      "_id": null,
      "Name": "眼儿媚",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "素娥作意失幽期。",
        "我自不凭伊。",
        "举杯重叹，",
        "帖云微笑，",
        "应道人痴。",
        "如今老去无情绪，",
        "只有睡相宜。",
        "建溪一啜，",
        "木樨数翦，",
        "酒醒归时。"
      ]
    },
    {
      "_id": null,
      "Name": "虞美人",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "吴波亭畔千行柳。",
        "直恁留人久。",
        "晚来船槛再三凭。",
        "又是一钩新月、照江心。",
        "故人有约何时到。",
        "白地令人老。",
        "只愁酒尽更谁赊。",
        "一段闲愁无计、奈何他。"
      ]
    },
    {
      "_id": null,
      "Name": "西江月",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "色似腊梅浑浅，",
        "香如薝卜微清。",
        "更张绿幄蔽轻盈。",
        "巧著工夫斗钉。",
        "露页涓涓月晓，",
        "风英点点秋晴。",
        "江南江北可经行。",
        "梦到吴王香径。"
      ]
    },
    {
      "_id": null,
      "Name": "谒金门",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "行不记。",
        "贪看远峰颦翠。",
        "风约柳花吹又起。",
        "故黏行客袂。",
        "老大浑无欢意。",
        "不为伤春憔悴。",
        "茅屋数间修竹里。",
        "日长春睡美。"
      ]
    },
    {
      "_id": null,
      "Name": "踏莎行",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "菊露晴黄，",
        "枫霜晚翠。",
        "重阳气候偏如此。",
        "异乡牢落怕登临，",
        "吾家落照飞云是。",
        "举扇尘低，",
        "脱巾风细。",
        "灵苗医得人憔悴。",
        "灯前点检欠谁人，",
        "惟有断鸿知此意。"
      ]
    },
    {
      "_id": null,
      "Name": "醉蓬莱",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "正红疏绿密，",
        "浪软波肥，",
        "放舟时节。",
        "载地擎天，",
        "识堂堂人杰。",
        "万里长江，",
        "百年骄虏，",
        "只笑谈烟灭。",
        "葭苇霜秋，",
        "楼船月晓，",
        "渔樵能说。",
        "分陕功成，",
        "沙堤归去，",
        "衮绣光浮，",
        "两眉黄彻。",
        "了却中兴，",
        "看这回勋业。",
        "应有命圭相印，",
        "都用赏、元功重叠。",
        "点检尊前，",
        "太平气象，",
        "今朝浑别。"
      ]
    },
    {
      "_id": null,
      "Name": "青玉案",
      "PoetId": "65479336bc6641e4ba5473917e242409",
      "PoetName": "李流谦",
      "Form": "词",
      "Tags": [
        "宋代",
        "李流谦"
      ],
      "Contents": [
        "相知元早来何暮。",
        "社燕送、秋鸿去。",
        "春草春波愁目注。",
        "酒香花韵，",
        "绮谭妍唱，",
        "怎不思量住。",
        "虚无指点骑鲸路。",
        "个是骚人不凡处。",
        "画栋云飞帘卷雨。",
        "风流千古，",
        "一时人物，",
        "好记尊前语。"
      ]
    }
  ]
}