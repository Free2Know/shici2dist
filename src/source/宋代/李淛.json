  "Poems": [
    {
      "_id": null,
      "Name": "千秋岁",
      "PoetId": "f64054ddc0b54009ad182193785d9156",
      "PoetName": "李淛",
      "Form": "词",
      "Tags": [
        "宋代",
        "李淛"
      ],
      "Contents": [
        "鄮峰凝瑞，",
        "鄞水浮佳气。",
        "晴景转，",
        "花光媚。",
        "碧幢森大纛，",
        "红旆纷千骑。",
        "相遇处，",
        "满城鹤发群仙萃。",
        "绮席张高会。",
        "鼍鼓笙箫沸。",
        "金兽袅，",
        "檀烟翠。",
        "玉山环回座，",
        "休惜今朝醉。",
        "觞再举，",
        "清歌共引千秋岁。"
      ]
    },
    {
      "_id": null,
      "Name": "满庭芳",
      "PoetId": "f64054ddc0b54009ad182193785d9156",
      "PoetName": "李淛",
      "Form": "词",
      "Tags": [
        "宋代",
        "李淛"
      ],
      "Contents": [
        "油幕新开，",
        "騂旄前导，",
        "暂归梓里舂容。",
        "致身槐府，",
        "功在鼎彛中。",
        "慨想东山故侣，",
        "烟霞外，",
        "久阔仙踪。",
        "今何幸，",
        "相逢吐握，",
        "谈笑一尊同。",
        "德星，",
        "占瑞处，",
        "旌髦五马，",
        "衮绣三公。",
        "对渭川遗老，",
        "绛县仙翁。",
        "厚意杯传绀玉，",
        "那堪更，",
        "篚实囊红。",
        "拼沈醉，",
        "今宵盛事，",
        "复见古人风。"
      ]
    },
    {
      "_id": null,
      "Name": "踏莎行",
      "PoetId": "f64054ddc0b54009ad182193785d9156",
      "PoetName": "李淛",
      "Form": "词",
      "Tags": [
        "宋代",
        "李淛"
      ],
      "Contents": [
        "红药香残，",
        "绿筠粉嫩。",
        "春归何处寻春信。",
        "绣鞍初上马啼轻，",
        "举头便觉长安近。",
        "别酒无情，",
        "啼妆有恨。",
        "山城向晚斜阳褪。",
        "清江极目带寒烟，",
        "锦鳞去後凭谁问。"
      ]
    }
  ]
}