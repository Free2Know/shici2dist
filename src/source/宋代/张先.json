{
  "_id": "2043146fa30d4b3088ca0c50154a55cf",
  "Name": "张先",
  "Description": "张先，字子野，乌程人。他与柳永齐名，擅长小令，亦作慢词。其词含蓄工巧，情韵浓郁。长于锻炼字句，因善于用“影”字，世称张三影。有《张子野词》，存词一百八十多首。",
  "Dynasty": "宋代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "一丛花令",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "伤高怀远几时穷？",
        "无物似情浓。",
        "离愁正引千丝乱，",
        "更东陌，",
        "飞絮蒙蒙。",
        "嘶骑渐遥，",
        "征尘不断，",
        "何处认郎踪？",
        "双鸳池沼水溶溶，",
        "南北小桡通。",
        "梯横画阁黄昏后，",
        "又还是，",
        "斜月帘栊。",
        "沉恨细思，",
        "不如桃杏，",
        "犹解嫁东风。"
      ]
    },
    {
      "_id": null,
      "Name": "临江仙",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "自古伤心惟远别，",
        "登山临水迟留。",
        "暮尘衰草一番秋。",
        "寻常景物，",
        "到此尽成愁。",
        "况与佳人分凤侣，",
        "盈盈粉泪难收。",
        "高城深处是青楼。",
        "红尘远道，",
        "明日忍回头。"
      ]
    },
    {
      "_id": null,
      "Name": "于飞乐令·于飞乐",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "宝奁开，",
        "菱鉴静，",
        "一掏清蟾。",
        "新妆脸、旋学花添。",
        "蜀红衫，",
        "双绣蝶，",
        "裙缕鹣鹣。",
        "寻思前事，",
        "小屏风、巧画江南。",
        "怎空教、草解宜男。",
        "柔桑暗、又过春蚕。",
        "正阴晴天气，",
        "更暝色相兼。",
        "幽期消息，",
        "曲房西、碎月筛帘。"
      ]
    },
    {
      "_id": null,
      "Name": "倾杯·倾杯乐",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "横塘水静，",
        "花窥影、孤城转。",
        "浮玉无尘，",
        "五亭争景，",
        "画桥对起，",
        "垂虹不断。",
        "爱溪上琼楼，",
        "凭雕栏、久□飞云远。",
        "人在虚空，",
        "月生溟海，",
        "寒渔夜泛，",
        "游鳞可辨。",
        "正是草长苹老，",
        "江南地暖。",
        "汀洲日晚。",
        "更茶山、已过清明，",
        "风雨暴千岩、啼鸟怨。",
        "芳菲故苑。",
        "深红尽、绿叶阴浓，",
        "青子枝头满。",
        "史君莫放寻春缓。"
      ]
    },
    {
      "_id": null,
      "Name": "偷声木兰花",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "曾居别乘康吴俗。",
        "民到於今歌不足。",
        "骊驭征鞭。",
        "一去东风十二年。",
        "重来却拥诸侯骑。",
        "宝带垂鱼金照地。",
        "和气融人。",
        "清霅千家日日春。"
      ]
    },
    {
      "_id": null,
      "Name": "八宝装·八宝妆",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "七律",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "锦屏罗幌初睡起。",
        "花阴转、重门闭。",
        "正不寒不暖，",
        "和风细雨，",
        "困人天气。",
        "此时无限伤春意。",
        "凭谁诉、厌厌地。",
        "这浅情薄幸，",
        "千山万水，",
        "也须来里。"
      ]
    },
    {
      "_id": null,
      "Name": "减字木兰花",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "垂螺近额。",
        "走上红裀初趁拍。",
        "只恐轻飞。",
        "拟倩游丝惹住伊。",
        "文鸳绣履。",
        "去似杨花尘不起。",
        "舞彻伊州。",
        "头上宫花颤未休。"
      ]
    },
    {
      "_id": null,
      "Name": "凤栖梧·蝶恋花",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "密宴厌厌池馆暮。",
        "天汉沈沈，",
        "借得春光住。",
        "红翠斗为长袖舞。",
        "香檀拍过惊鸿翥。",
        "明日不知花在否。",
        "今夜圆蟾，",
        "後夜忧风雨。",
        "可惜歌云容易去。",
        "东城杨柳东城路。"
      ]
    },
    {
      "_id": null,
      "Name": "劝金船",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "流泉宛转双开窦。",
        "带染轻纱皱。",
        "何人暗得金船酒。",
        "拥罗绮前後。",
        "绿定见花影，",
        "并照与、艳妆争秀。",
        "行尽曲名，",
        "休更再歌杨柳。",
        "光生飞动摇琼甃。",
        "隔障笙箫奏。",
        "须知短景欢无足，",
        "又还过清昼。",
        "翰阁迟归来，",
        "传骑恨、留住难久。",
        "异日凤凰池上，",
        "为谁思旧。"
      ]
    },
    {
      "_id": null,
      "Name": "千秋岁",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "数声鶗鴂。",
        "又报芳菲歇。",
        "惜春更把残红折。",
        "雨轻风色暴，",
        "梅子青时节。",
        "永丰柳，",
        "无人尽日飞花雪。",
        "莫把幺弦拨。",
        "怨极弦能说。",
        "天不老，",
        "情难绝。",
        "心似双丝网，",
        "中有千千结。",
        "夜过也，",
        "东窗未白凝残月。"
      ]
    },
    {
      "_id": null,
      "Name": "南乡子",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "何处可魂消。",
        "京口终朝两信潮。",
        "不管离心千叠恨，",
        "滔滔。",
        "催促行人动去桡。",
        "记得旧江皋。",
        "绿杨轻絮几条条。",
        "春水一篙残照阔，",
        "遥遥。",
        "有个多情立画桥。"
      ]
    },
    {
      "_id": null,
      "Name": "南歌子",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "醉後和衣倒，",
        "愁来殢酒醺。",
        "困人天气近清明。",
        "尽日厌厌□脸、浅含颦。",
        "睡觉□□恨，",
        "依然月映门。",
        "楚天何处觅行云。",
        "唯有暗灯残漏、伴消魂。"
      ]
    },
    {
      "_id": null,
      "Name": "卜算子",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "梦断寒夜长，",
        "坐待清霜晓。",
        "临镜无人为整妆，",
        "但自学、孤鸾照。",
        "楼台红树杪。",
        "风月依前好。",
        "江水东流郎在西，",
        "问尺素、何由到。"
      ]
    },
    {
      "_id": null,
      "Name": "卜算子慢",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "溪山别意，",
        "烟树去程，",
        "日落采苹春晚。",
        "欲上征鞍，",
        "更掩翠帘相眄。",
        "惜弯弯浅黛长长眼。",
        "奈画阁欢游，",
        "也学狂花乱絮轻散。",
        "水影横池馆。",
        "对静夜无人，",
        "月高云远。",
        "一饷凝思，",
        "两袖泪痕还满。",
        "恨私书、又逐东风断。",
        "纵西北层楼万尺，",
        "望重城那见。"
      ]
    },
    {
      "_id": null,
      "Name": "双燕儿",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "榴花帘外飘红。",
        "藕丝罩、小屏风。",
        "东山别後，",
        "高唐梦短，",
        "犹喜相逢。",
        "几时再与眠香翠，",
        "悔旧欢、何事匆匆。",
        "芳心念我，",
        "也应那里，",
        "蹙破眉峰。"
      ]
    },
    {
      "_id": null,
      "Name": "双韵子",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "鸣鞘电过晓闱静。",
        "敛龙旗风定。",
        "凤楼远出霏烟，",
        "闻笑语、中天迥。",
        "清光近。",
        "欢声竟。",
        "鸳鸯集、仙花斗影。",
        "更闻度曲瑶山，",
        "升瑞日、春宫永。"
      ]
    },
    {
      "_id": null,
      "Name": "喜朝天·踏莎行",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "晓云开。",
        "睨仙馆陵虚，",
        "步入蓬莱。",
        "玉宇琼甃，",
        "对青林近，",
        "归鸟徘徊。",
        "风月顿消清暑，",
        "野色对、江山助诗才。",
        "潇鼓宴，",
        "璇题宝字，",
        "浮动持杯。",
        "人多送目天际，",
        "识渡舟帆小，",
        "时见潮回。",
        "故国千里，",
        "共十万室，",
        "日日春台。",
        "睢社朝京非远，",
        "正和羹、民口渴盐梅。",
        "佳景在，",
        "吴侬还望，",
        "分阃重来。"
      ]
    },
    {
      "_id": null,
      "Name": "塞垣春",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "野树秋声满。",
        "对雨壁、风灯乱。",
        "云低翠帐，",
        "烟销素被，",
        "签动重幔。",
        "甚客怀、先自无消遣。",
        "更篱落、秋虫叹。",
        "叹樊川、风流减。",
        "旧欢难得重见。",
        "停酒说扬州，",
        "平山月、应照棋观。",
        "绿绮为谁弹，",
        "空传广陵散。",
        "但光纱短帽，",
        "窄袖轻衫，",
        "犹记竹西庭院。",
        "老鹤何时去，",
        "认琼花一面。"
      ]
    },
    {
      "_id": null,
      "Name": "夜厌厌·夜行船",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "未知",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "昨夜小筵欢纵。",
        "烛房深、舞鸾歌凤。",
        "酒迷花困共厌厌，",
        "倚朱弦、未成归弄。",
        "峡雨忽收寻断梦。",
        "依前是、画楼钟动。",
        "争拂雕鞍葱葱去，",
        "万千恨、不能相送。"
      ]
    },
    {
      "_id": null,
      "Name": "天仙子",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "持节来时初有雁。",
        "十万人家春已满。",
        "龙标名第凤池身，",
        "堂阜远。",
        "江桥晚。",
        "一见湖山看未遍。",
        "障扇欲收歌泪溅。",
        "亭下花空罗绮散。",
        "樯竿渐向望中疏，",
        "旗影转。",
        "鼙声断。",
        "惆怅不如船尾燕。"
      ]
    },
    {
      "_id": null,
      "Name": "天仙子·水调数声持酒听",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "水调数声持酒听。",
        "午醉醒来愁未醒。",
        "送春春去几时回。",
        "临晚镜，",
        "伤流景。",
        "往事后期空记省。",
        "沙上并禽池上暝。",
        "云破月来花弄影。",
        "重重帘幕密遮灯，",
        "风不定。",
        "人初静。",
        "明日落红应满径。"
      ]
    },
    {
      "_id": null,
      "Name": "好事近",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "月色透横枝，",
        "短叶小花无力。",
        "北客一声长笛，",
        "怨江南先得。",
        "谁教强半腊前开，",
        "多情为春忆。",
        "留取大家沈醉，",
        "正雨休风息。"
      ]
    },
    {
      "_id": null,
      "Name": "定西番",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "焊拨紫槽金衬，",
        "双秀萼，",
        "两回鸾。",
        "齐学汉宫妆样，",
        "竞蝉娟。",
        "三十六弦蝉闹，",
        "小弦蜂作团。",
        "听尽昭君幽怨，",
        "莫重弹。"
      ]
    },
    {
      "_id": null,
      "Name": "定风波",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "素藕抽条未放莲。",
        "晚蚕将茧不成眠。",
        "若比相思如乱絮。",
        "何异。",
        "两心俱被暗丝牵。",
        "暂觉欲归还是恨。",
        "莫问。",
        "有情谁信道无缘。",
        "有似中秋云外月。",
        "皎洁。",
        "不团圆待几时圆。"
      ]
    },
    {
      "_id": null,
      "Name": "定风波令·定风波",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "浴殿词臣亦议兵。",
        "禁中颇牧党羌平。",
        "诏卷促归难自缓。",
        "溪馆。",
        "彩花千数酒泉清。",
        "春草未青秋叶暮。",
        "□去。",
        "一家行色万家情。",
        "可恨黄莺相识晚。",
        "望断。",
        "湖边亭上不闻声。"
      ]
    },
    {
      "_id": null,
      "Name": "宴春台慢·宴春台",
      "PoetId": "2043146fa30d4b3088ca0c50154a55cf",
      "PoetName": "张先",
      "Form": "词",
      "Tags": [
        "宋代",
        "张先"
      ],
      "Contents": [
        "丽日千门，",
        "紫烟双阙，",
        "琼林又报春回。",
        "殿阁风微，",
        "当时去燕还来。",
        "五侯池馆频开。",
        "探芳菲、走马天街。",
        "重帘人语，",
        "辚辚绣轩，",
        "远近轻雷。",
        "雕觞霞滟，",
        "翠幕云飞，",
        "楚腰舞柳，",
        "宫面妆梅。",
        "金猊夜暖、罗衣暗裛香煤。",
        "洞府人归，",
        "放笙歌、灯火下楼台。",
        "蓬莱。",
        "犹有花上月，",
        "清影徘徊。"
      ]
    },
