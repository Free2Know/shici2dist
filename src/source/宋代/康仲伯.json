      "_id": null,
      "Name": "忆真妃·相见欢",
      "PoetId": "b618ccf0320646bd8f7b0076c701ba01",
      "PoetName": "康仲伯",
      "Form": "词",
      "Tags": [
        "宋代",
        "康仲伯"
      ],
      "Contents": [
        "匆匆一望关河。",
        "听离歌。",
        "艇子急催双桨、下清波。",
        "淋浪醉。",
        "阑干泪。",
        "奈情何。",
        "明日画桥西畔、暮云多。"
      ]
    }
  ]
}