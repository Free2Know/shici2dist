      "_id": null,
      "Name": "醉蓬莱",
      "PoetId": "ee2d716dba7545be8a40afc3d7dc8c98",
      "PoetName": "段倚",
      "Form": "词",
      "Tags": [
        "宋代",
        "段倚"
      ],
      "Contents": [
        "正星杓首舍，",
        "月律开祥，",
        "嗣兴芳序。",
        "梦协长庚，",
        "诞清朝仪羽。",
        "运继姬周，",
        "庆流嵩岳，",
        "认再生申甫。",
        "职尹京华，",
        "功宣国计，",
        "声猷兼著。",
        "子建文章，",
        "懿侯宗范，",
        "叔度襟怀，",
        "紫芝眉宇。",
        "燕启佳辰，",
        "满东风兰雾，",
        "脆管繁弦，",
        "绣裀檀板，",
        "会饭中仙侣。",
        "世仰真贤，",
        "朝尊大老，",
        "长亲尧主。"
      ]
    }
  ]
}