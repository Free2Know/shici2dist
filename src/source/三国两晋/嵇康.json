  "Poems": [
    {
      "_id": null,
      "Name": "赠秀才从军",
      "PoetId": "410bb2dc4cc44e55a41a2cec2e0d4aae",
      "PoetName": "嵇康",
      "Form": "未知",
      "Tags": [
        "三国两晋",
        "嵇康"
      ],
      "Contents": [
        "闲夜肃清。",
        "朗月照轩。",
        "微风动袿，",
        "组帐高褰。",
        "旨酒盈樽，",
        "莫与交欢。",
        "鸣琴在御，",
        "谁与鼓弹？",
        "仰慕同趣，",
        "其馨若兰。",
        "佳人不在，",
        "能不永叹？"
      ]
    }
  ]
}