      "_id": null,
      "Name": "怨旷思惟歌",
      "PoetId": "6cfc502f07f64f2cbd617baaac922979",
      "PoetName": "王嫱",
      "Form": "未知",
      "Tags": [
        "汉代",
        "王嫱"
      ],
      "Contents": [
        "秋木萋萋，",
        "其叶萎黄。",
        "有鸟处山，",
        "集于苞桑。",
        "养育羽毛，",
        "形容生光。",
        "既得生云，",
        "上游曲房。",
        "离宫绝旷，",
        "身体摧藏。",
        "志念抑沉，",
        "不得颉颃。",
        "虽得委食，",
        "心有彷徨。",
        "我独伊何，",
        "来往变常。",
        "翩翩之燕，",
        "远集西羌。",
        "高山峨峨，",
        "河水泱泱。",
        "父兮母兮，",
        "道里悠长。",
        "呜呼哀哉，",
        "忧心恻伤。",
        "\t"
      ]
    }
  ]
}