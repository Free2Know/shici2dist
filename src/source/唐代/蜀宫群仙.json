  "Name": "蜀宫群仙",
  "Description": "",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "上元夫人",
      "PoetId": "4afd4d4eb1e448e5a78f04d852f3748c",
      "PoetName": "蜀宫群仙",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蜀宫群仙"
      ],
      "Contents": [
        "思量往事一愁容，",
        "阿母曾邀到汉宫。",
        "城阙不存人不见，",
        "茂陵荒草恨无穷。"
      ]
    },
    {
      "_id": null,
      "Name": "后土夫人",
      "PoetId": "4afd4d4eb1e448e5a78f04d852f3748c",
      "PoetName": "蜀宫群仙",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蜀宫群仙"
      ],
      "Contents": [
        "偶引群仙到世间，",
        "熏风殿里醉华筵。",
        "等闲贪赏不归去，",
        "愁杀韦郎一觉眠。"
      ]
    },
    {
      "_id": null,
      "Name": "太真",
      "PoetId": "4afd4d4eb1e448e5a78f04d852f3748c",
      "PoetName": "蜀宫群仙",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蜀宫群仙"
      ],
      "Contents": [
        "春梦悠扬生下界，",
        "一堪成笑一堪悲。",
        "马嵬不是无情地，",
        "自迈蓬莱睡觉时。"
      ]
    },
    {
      "_id": null,
      "Name": "弄玉",
      "PoetId": "4afd4d4eb1e448e5a78f04d852f3748c",
      "PoetName": "蜀宫群仙",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蜀宫群仙"
      ],
      "Contents": [
        "采凤飞来到禁闱，",
        "便随王母驻瑶池。",
        "如今记得秦楼上，",
        "偷见萧郎恼妾时。"
      ]
    },
    {
      "_id": null,
      "Name": "王母",
      "PoetId": "4afd4d4eb1e448e5a78f04d852f3748c",
      "PoetName": "蜀宫群仙",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蜀宫群仙"
      ],
      "Contents": [
        "沧海成尘几万秋，",
        "碧桃花发长春愁。",
        "不来便是数千载，",
        "周穆汉皇何处游。"
      ]
    },
    {
      "_id": null,
      "Name": "麻姑",
      "PoetId": "4afd4d4eb1e448e5a78f04d852f3748c",
      "PoetName": "蜀宫群仙",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蜀宫群仙"
      ],
      "Contents": [
        "世间何事不潸然，",
        "得失人情命不延。",
        "适向蔡家厅上饮，",
        "回头已见一千年。"
      ]
    }
  ]
}