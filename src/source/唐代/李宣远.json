    {
      "_id": null,
      "Name": "并州路",
      "PoetId": "a7fb25501140420a92a17756026b3fc1",
      "PoetName": "李宣远",
      "Form": "五律",
      "Tags": [
        "唐代",
        "李宣远"
      ],
      "Contents": [
        "秋日并州路，",
        "黄榆落故关。",
        "孤城吹角罢，",
        "数骑射雕还。",
        "帐幕遥临水，",
        "牛羊自下山。",
        "征人正垂泪，",
        "烽火起云间。"
      ]
    },
    {
      "_id": null,
      "Name": "近无西耗",
      "PoetId": "a7fb25501140420a92a17756026b3fc1",
      "PoetName": "李宣远",
      "Form": "五律",
      "Tags": [
        "唐代",
        "李宣远"
      ],
      "Contents": [
        "远戎兵压境，",
        "迁客泪横襟。",
        "烽堠惊秦塞，",
        "囚居困越吟。",
        "自怜牛马走，",
        "未识犬羊心。",
        "一月无消息，",
        "西看日又沉。"
      ]
    }
  ]
}