  "Poems": [
    {
      "_id": null,
      "Name": "垄穴遗诗",
      "PoetId": "0133241bc67a4ad3b9c3f8d48e8473bb",
      "PoetName": "沈廷瑞",
      "Form": "七律",
      "Tags": [
        "唐代",
        "沈廷瑞"
      ],
      "Contents": [
        "虚劳营殡玉山前，",
        "殡后那知已脱蝉。",
        "应是元神归洞府，",
        "更与遗魄在黄泉。",
        "灵台已得修真诀，",
        "尘世空留悟道篇。",
        "堪叹浮生今古事，",
        "北邙山下草芊芊。"
      ]
    },
    {
      "_id": null,
      "Name": "寄袁州陈智周",
      "PoetId": "0133241bc67a4ad3b9c3f8d48e8473bb",
      "PoetName": "沈廷瑞",
      "Form": "五律",
      "Tags": [
        "唐代",
        "沈廷瑞"
      ],
      "Contents": [
        "名山相别后，",
        "别后会难期。",
        "金鼎销红日，",
        "丹田老紫芝。",
        "访君虽有路，",
        "怀我岂无诗。",
        "休羡繁华事，",
        "百年能几时。"
      ]
    },
    {
      "_id": null,
      "Name": "答高安宰",
      "PoetId": "0133241bc67a4ad3b9c3f8d48e8473bb",
      "PoetName": "沈廷瑞",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "沈廷瑞"
      ],
      "Contents": [
        "何须问我道成时，",
        "紫府清都自有期。",
        "手握药苗人不识，",
        "体含金骨俗争知。"
      ]
    },
    {
      "_id": null,
      "Name": "赠僧昭莹",
      "PoetId": "0133241bc67a4ad3b9c3f8d48e8473bb",
      "PoetName": "沈廷瑞",
      "Form": "五律",
      "Tags": [
        "唐代",
        "沈廷瑞"
      ],
      "Contents": [
        "南北东西路，",
        "人生会不无。",
        "早曾依阁皂，",
        "又却上玄都。",
        "云片随天阔，",
        "泉声落石孤。",
        "何期早相遇，",
        "乐共煮菖蒲。"
      ]
    }
  ]
}