      "_id": null,
      "Name": "初冬旅舍早怀",
      "PoetId": "d38e4a9a2c94466a97cbb3ae560e8f50",
      "PoetName": "怀浦",
      "Form": "五律",
      "Tags": [
        "唐代",
        "怀浦"
      ],
      "Contents": [
        "枕上角声微，",
        "离情未息机。",
        "梦回三楚寺，",
        "寒入五更衣。",
        "月没栖禽动，",
        "霜晴冻叶飞。",
        "自惭行役早，",
        "深与道相违。"
      ]
    },
    {
      "_id": null,
      "Name": "赠智舟三藏",
      "PoetId": "d38e4a9a2c94466a97cbb3ae560e8f50",
      "PoetName": "怀浦",
      "Form": "五律",
      "Tags": [
        "唐代",
        "怀浦"
      ],
      "Contents": [
        "壮岁心难伏，",
        "师心伏岂难。",
        "寻常独在院，",
        "行坐不离坛。",
        "岳雪当禅暝，",
        "松声入咒寒。",
        "更因文字外，",
        "多把史书看。"
      ]
    }
  ]
}