    {
      "_id": null,
      "Name": "黄台瓜辞",
      "PoetId": "8973e0ac5df6412c94811da11ed2d472",
      "PoetName": "李贤",
      "Form": "未知",
      "Tags": [
        "唐代",
        "李贤"
      ],
      "Contents": [
        "种瓜黄台下，",
        "瓜熟子离离。",
        "一摘使瓜好，",
        "再摘使瓜稀，",
        "三摘尚自可，",
        "摘绝抱蔓归。"
      ]
    }
  ]
