  "Poems": [
    {
      "_id": null,
      "Name": "赋得浊水求珠",
      "PoetId": "ad128b7933fc429798d17e0f6ae01b95",
      "PoetName": "王损之",
      "Form": "未知",
      "Tags": [
        "唐代",
        "王损之"
      ],
      "Contents": [
        "积水非澄彻，",
        "明珠不易求。",
        "依稀沈极浦，",
        "想像在中流。",
        "瞪目思清浅，",
        "褰裳恨暗投。",
        "徒看川色媚，",
        "空爱夜光浮。",
        "月入疑龙吐，",
        "星归似蚌游。",
        "终希识珍者，",
        "采掇冀冥搜。"
      ]
    }
  ]
}