  "Poems": [
    {
      "_id": null,
      "Name": "望九华山",
      "PoetId": "43f9abb38d304bc68aa3e35810928b65",
      "PoetName": "柴夔",
      "Form": "七律",
      "Tags": [
        "唐代",
        "柴夔"
      ],
      "Contents": [
        "九华如剑插云霓，",
        "青霭连空望欲迷。",
        "北截吴门疑地尽，",
        "南连楚界觉天低。",
        "龙池水蘸中秋月，",
        "石路人攀上汉梯。",
        "惆怅旧游无复到，",
        "会须登此出尘泥。"
      ]
    }
  ]
}