      "_id": null,
      "Name": "早春残雪",
      "PoetId": "7eb4602c944b4fcca26cde016a06f7b4",
      "PoetName": "裴虔余",
      "Form": "未知",
      "Tags": [
        "唐代",
        "裴虔余"
      ],
      "Contents": [
        "霁日雕琼彩，",
        "幽庭减夜寒。",
        "梅飘余片积，",
        "日堕晚光残。",
        "零落偏依桂，",
        "霏微不掩兰。",
        "阴林披雾縠，",
        "小沼破冰盘。",
        "曲槛霜凝砌，",
        "疏篁玉碎竿。",
        "已闻三径好，",
        "犹可访袁安。"
      ]
    },
    {
      "_id": null,
      "Name": "柳枝词咏篙水溅妓衣",
      "PoetId": "7eb4602c944b4fcca26cde016a06f7b4",
      "PoetName": "裴虔余",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "裴虔余"
      ],
      "Contents": [
        "半额微黄金缕衣，",
        "玉搔头袅凤双飞。",
        "从教水溅罗裙湿，",
        "还道朝来行雨归。"
      ]
    }
  ]
}