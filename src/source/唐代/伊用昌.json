  "Poems": [
    {
      "_id": null,
      "Name": "望江南词咏鼓",
      "PoetId": "a5ccc076a5784f98a105bd94f26cd4b5",
      "PoetName": "伊用昌",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "伊用昌"
      ],
      "Contents": [
        "江南鼓，",
        "梭肚两头栾。",
        "钉著不知侵骨髓，",
        "打来只是没心肝，",
        "空腹被人谩。"
      ]
    },
    {
      "_id": null,
      "Name": "湖南闯斋吟",
      "PoetId": "a5ccc076a5784f98a105bd94f26cd4b5",
      "PoetName": "伊用昌",
      "Form": "七律",
      "Tags": [
        "唐代",
        "伊用昌"
      ],
      "Contents": [
        "谁人能识白元君，",
        "上士由来尽见闻。",
        "避世早空南火宅，",
        "植田高种北山云。",
        "鸡能抱卵心常听，",
        "蝉到成形壳自分。",
        "学取大罗些子术，",
        "免教松下作孤坟。"
      ]
    },
    {
      "_id": null,
      "Name": "留题阁皂观",
      "PoetId": "a5ccc076a5784f98a105bd94f26cd4b5",
      "PoetName": "伊用昌",
      "Form": "七律",
      "Tags": [
        "唐代",
        "伊用昌"
      ],
      "Contents": [
        "花洞门前吠似雷，",
        "险声流断俗尘埃。",
        "雨喷山脚毒龙起，",
        "月照松梢孤鹤回。",
        "萝幕秋高添碧翠，",
        "画帘时卷到楼台。",
        "两坛诗客何年去，",
        "去后门关更不开。"
      ]
    },
    {
      "_id": null,
      "Name": "题游帷观真君殿后",
      "PoetId": "a5ccc076a5784f98a105bd94f26cd4b5",
      "PoetName": "伊用昌",
      "Form": "七律",
      "Tags": [
        "唐代",
        "伊用昌"
      ],
      "Contents": [
        "日日祥云瑞气连，",
        "侬家应作大神仙。",
        "笔头洒起风雷力，",
        "剑下驱驰造化权。",
        "更无戎夷添礼乐，",
        "永教胡虏绝烽烟。",
        "列仙功业只如此，",
        "直上三清第一天。"
      ]
    },
    {
      "_id": null,
      "Name": "题茶陵县门",
      "PoetId": "a5ccc076a5784f98a105bd94f26cd4b5",
      "PoetName": "伊用昌",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "伊用昌"
      ],
      "Contents": [
        "茶陵一道妇长街，",
        "两畔栽柳不栽槐。",
        "夜后不闻更漏鼓，",
        "只听锤芒织草鞋。"
      ]
    },
    {
      "_id": null,
      "Name": "题酒楼壁",
      "PoetId": "a5ccc076a5784f98a105bd94f26cd4b5",
      "PoetName": "伊用昌",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "伊用昌"
      ],
      "Contents": [
        "此生生在此生先，",
        "何事从玄不复玄。",
        "已在淮南鸡犬后，",
        "而今便到玉皇前。"
      ]
    }
  ]
}