      "_id": null,
      "Name": "小苑春望宫池柳色",
      "PoetId": "5a2d7d255e8b44a690e32c5606d30db6",
      "PoetName": "元友直",
      "Form": "未知",
      "Tags": [
        "唐代",
        "元友直"
      ],
      "Contents": [
        "柳色新池遍，",
        "春光御苑晴。",
        "叶依青阁密，",
        "条向碧流倾。",
        "路暗阴初重，",
        "波摇影转清。",
        "风从垂处度，",
        "烟就望中生。",
        "断续游蜂聚，",
        "飘飖戏蝶轻。",
        "怡然变芳节，",
        "愿及一枝荣。"
      ]
    }
  ]
}