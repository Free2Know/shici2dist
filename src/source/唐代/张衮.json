  "Poems": [
    {
      "_id": null,
      "Name": "梁郊祀乐章庆休",
      "PoetId": "6668ecba262b457e891a31562091ad04",
      "PoetName": "张衮",
      "Form": "五律",
      "Tags": [
        "唐代",
        "张衮"
      ],
      "Contents": [
        "大业来四夷，",
        "仁风和万国。",
        "白日体无私，",
        "皇天辅有德。",
        "七旬罪已服，",
        "六月师方克。",
        "伟哉帝道隆，",
        "终始常作则。"
      ]
    },
    {
      "_id": null,
      "Name": "梁郊祀乐章庆和",
      "PoetId": "6668ecba262b457e891a31562091ad04",
      "PoetName": "张衮",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "张衮"
      ],
      "Contents": [
        "烟燎升，",
        "礼容彻。",
        "诚感达，",
        "人神悦。",
        "灵贶彰，",
        "圣情结。",
        "玉座寂，",
        "金炉歇。"
      ]
    },
    {
      "_id": null,
      "Name": "梁郊祀乐章庆熙",
      "PoetId": "6668ecba262b457e891a31562091ad04",
      "PoetName": "张衮",
      "Form": "未知",
      "Tags": [
        "唐代",
        "张衮"
      ],
      "Contents": [
        "哲后躬享，",
        "旨酒斯陈。",
        "王恭无斁，",
        "严祀惟寅。",
        "皇祖以配，",
        "大孝以振。",
        "宜锡景福，",
        "永休下民。"
      ]
    },
    {
      "_id": null,
      "Name": "梁郊祀乐章庆肃",
      "PoetId": "6668ecba262b457e891a31562091ad04",
      "PoetName": "张衮",
      "Form": "未知",
      "Tags": [
        "唐代",
        "张衮"
      ],
      "Contents": [
        "笾豆簠簋，",
        "黍稷非馨。",
        "懿兹彝器，",
        "厥德惟明。",
        "金石匏革，",
        "以和以平。",
        "由此无疆，",
        "期乎永宁。"
      ]
    },
    {
      "_id": null,
      "Name": "梁郊祀乐章庆融",
      "PoetId": "6668ecba262b457e891a31562091ad04",
      "PoetName": "张衮",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "张衮"
      ],
      "Contents": [
        "道和气兮袭氤氲，",
        "宣皇规兮彰圣神。",
        "服遐裔兮敷质文，",
        "格苗扈兮息烟尘。"
      ]
    },
    {
      "_id": null,
      "Name": "郊庙歌辞·梁郊祀乐章·庆肃",
      "PoetId": "6668ecba262b457e891a31562091ad04",
