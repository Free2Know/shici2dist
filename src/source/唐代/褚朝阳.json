  "_id": "7a8d582e87d64e9dbedd5e96581d2b89",
  "Name": "褚朝阳",
  "Description": "褚朝阳，登天宝进士第。诗三首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "五丝",
      "PoetId": "7a8d582e87d64e9dbedd5e96581d2b89",
      "PoetName": "褚朝阳",
      "Form": "未知",
      "Tags": [
        "唐代",
        "褚朝阳"
      ],
      "Contents": [
        "越人传楚俗，",
        "截竹竞萦丝。",
        "水底深休也，",
        "日中还贺之。",
        "章施文胜质，",
        "列匹美于姬。",
        "锦绣侔新段，",
        "羔羊寝旧诗。",
        "但夸端午节，",
        "谁荐屈原祠。",
        "把酒时伸奠，",
        "汨罗空远而。"
      ]
    },
    {
      "_id": null,
      "Name": "奉上徐中书",
      "PoetId": "7a8d582e87d64e9dbedd5e96581d2b89",
      "PoetName": "褚朝阳",
      "Form": "未知",
      "Tags": [
        "唐代",
        "褚朝阳"
      ],
      "Contents": [
        "中禁仙池越凤凰，",
        "池边词客紫薇郎。",
        "既能作颂雄风起，",
        "何不时吹兰蕙香。",
        "第二百五十五卷"
      ]
    },
    {
      "_id": null,
      "Name": "登圣善寺阁",
      "PoetId": "7a8d582e87d64e9dbedd5e96581d2b89",
      "PoetName": "褚朝阳",
      "Form": "五律",
      "Tags": [
        "唐代",
        "褚朝阳"
      ],
      "Contents": [
        "飞阁青霞里，",
        "先秋独早凉。",
        "天花映窗近，",
        "月桂拂檐香。",
        "华岳三峰小，",
        "黄河一带长。",
        "空间指归路，",
        "烟际有垂杨。"
      ]
    }
  ]
}