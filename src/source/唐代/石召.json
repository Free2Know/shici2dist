      "_id": null,
      "Name": "早行遇雪",
      "PoetId": "863c077cbf6145908233c23d8f42cb06",
      "PoetName": "石召",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "石召"
      ],
      "Contents": [
        "荒郊昨夜雪，",
        "羸马又须行。",
        "四顾无人迹，",
        "鸡鸣第一声。"
      ]
    },
    {
      "_id": null,
      "Name": "送人归山",
      "PoetId": "863c077cbf6145908233c23d8f42cb06",
      "PoetName": "石召",
      "Form": "五律",
      "Tags": [
        "唐代",
        "石召"
      ],
      "Contents": [
        "相逢唯道在，",
        "谁不共知贫。",
        "归路分残雨，",
        "停舟别故人。",
        "霜明松岭晓，",
        "花暗竹房春。",
        "亦有栖闲意，",
        "何年可寄身。"
      ]
    }
  ]
}