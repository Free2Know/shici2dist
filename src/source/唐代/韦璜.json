    {
      "_id": null,
      "Name": "赠夫二首",
      "PoetId": "815daeb7f30e42c28bb37f6b212e6fc8",
      "PoetName": "韦璜",
      "Form": "未知",
      "Tags": [
        "唐代",
        "韦璜"
      ],
      "Contents": [
        "不得长相守，",
        "青春夭蕣华。",
        "旧游今永已，",
        "泉路却为家。",
        "早知离别切人心，",
        "悔作从来恩爱深。",
        "黄泉冥寞虽长逝，",
        "白日屏帷还重寻。"
      ]
    },
    {
      "_id": null,
      "Name": "赠姊",
      "PoetId": "815daeb7f30e42c28bb37f6b212e6fc8",
      "PoetName": "韦璜",
      "Form": "未知",
      "Tags": [
        "唐代",
        "韦璜"
      ],
      "Contents": [
        "修短各有分，",
        "浮华亦非真。",
        "断肠泉壤下，",
        "幽忧难具陈。",
        "凄凄白杨风，",
        "日暮堪愁人。"
      ]
    },
    {
      "_id": null,
      "Name": "赠嫂",
      "PoetId": "815daeb7f30e42c28bb37f6b212e6fc8",
      "PoetName": "韦璜",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "韦璜"
      ],
      "Contents": [
        "赤心用尽为相知，",
        "虑后防前只定疑。",
        "案牍可申生节目，",
        "桃符虽圣欲何为。"
      ]
    }
  ]
}