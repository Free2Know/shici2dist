  "Poems": [
    {
      "_id": null,
      "Name": "宝剑篇",
      "PoetId": "6526071f235147738508bf002afeafeb",
      "PoetName": "刘长川",
      "Form": "五律",
      "Tags": [
        "唐代",
        "刘长川"
      ],
      "Contents": [
        "宝剑不可得，",
        "相逢几许难。",
        "今朝一度见，",
        "赤色照人寒。",
        "匣里星文动，",
        "环边月影残。",
        "自然神鬼伏，",
        "无事莫空弹。"
      ]
    },
    {
      "_id": null,
      "Name": "将赴东都上李相公",
      "PoetId": "6526071f235147738508bf002afeafeb",
      "PoetName": "刘长川",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "刘长川"
      ],
      "Contents": [
        "四海兵初偃，",
        "平津阁正开。",
        "谁知大炉下，",
        "还有不然灰。"
      ]
    }
  ]
}