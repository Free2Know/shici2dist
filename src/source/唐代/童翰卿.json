  "Poems": [
    {
      "_id": null,
      "Name": "昆明池织女石",
      "PoetId": "154c28b51b1443e5935db5cf22e1f711",
      "PoetName": "童翰卿",
      "Form": "词",
      "Tags": [
        "唐代",
        "童翰卿"
      ],
      "Contents": [
        "一片昆明石，",
        "千秋织女名。",
        "见人虚脉脉，",
        "临水更盈盈。",
        "苔作轻衣色，",
        "波为促杼声。",
        "岸云连鬓湿，",
        "沙月对眉生。",
        "有脸莲同笑，",
        "无心鸟不惊。",
        "还如朝镜里，",
        "形影两分明。"
      ]
    },
    {
      "_id": null,
      "Name": "绝句",
      "PoetId": "154c28b51b1443e5935db5cf22e1f711",
      "PoetName": "童翰卿",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "童翰卿"
      ],
      "Contents": [
        "大朴逐物尽，",
        "哀我天地功。",
        "争得荣辱心，",
        "洒然归西风。"
      ]
    }
  ]
}