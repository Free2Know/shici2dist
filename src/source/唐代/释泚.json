  "Poems": [
    {
      "_id": null,
      "Name": "北原别业",
      "PoetId": "5f14414d107541b48405b77fe2de86fa",
      "PoetName": "释泚",
      "Form": "五律",
      "Tags": [
        "唐代",
        "释泚"
      ],
      "Contents": [
        "野外车骑绝，",
        "古村桑柘阴。",
        "流莺出谷静，",
        "春草闭门深。",
        "学稼农为业，",
        "忘情道作心。",
        "因知上皇日，",
        "凿井在灵林。"
      ]
    },
    {
      "_id": null,
      "Name": "游元象泊",
      "PoetId": "5f14414d107541b48405b77fe2de86fa",
      "PoetName": "释泚",
      "Form": "五律",
      "Tags": [
        "唐代",
        "释泚"
      ],
      "Contents": [
        "空水潮色净，",
        "澹然湖上心。",
        "舳舻轻且进，",
        "汀洲如可寻。",
        "秋风洄溯险，",
        "落日波涛深。",
        "寂寞武陵去，",
        "中流方至今。"
      ]
    }
  ]
}