      "_id": null,
      "Name": "棋",
      "PoetId": "aadb15c63b5d432d88741a965631e59e",
      "PoetName": "高辇",
      "Form": "五律",
      "Tags": [
        "唐代",
        "高辇"
      ],
      "Contents": [
        "野客围棋坐，",
        "耆颐向暮秋。",
        "不言如守默，",
        "设计似平雠。",
        "决胜虽关勇，",
        "防危亦合忧。",
        "看他终一局，",
        "白却少年头。"
      ]
    }
  ]
}