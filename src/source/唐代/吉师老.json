  "Poems": [
    {
      "_id": null,
      "Name": "放猿",
      "PoetId": "cec5b4ddd8f74181bd467d422d3936aa",
      "PoetName": "吉师老",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "吉师老"
      ],
      "Contents": [
        "放尔千山万水身，",
        "野泉晴树好为邻。",
        "啼时莫近潇湘岸，",
        "明月孤舟有旅人。"
      ]
    },
    {
      "_id": null,
      "Name": "看蜀女转昭君变",
      "PoetId": "cec5b4ddd8f74181bd467d422d3936aa",
      "PoetName": "吉师老",
      "Form": "七律",
      "Tags": [
        "唐代",
        "吉师老"
      ],
      "Contents": [
        "妖姬未著石榴裙，",
        "自道家连锦水濆。",
        "檀口解知千载事，",
        "清词堪叹九秋文。",
        "翠眉颦处楚边月，",
        "画卷开时塞外云。",
        "说尽绮罗当日恨，",
        "昭君传意向文君。"
      ]
    },
    {
      "_id": null,
      "Name": "题春梦秋归故里",
      "PoetId": "cec5b4ddd8f74181bd467d422d3936aa",
      "PoetName": "吉师老",
      "Form": "五律",
      "Tags": [
        "唐代",
        "吉师老"
      ],
      "Contents": [
        "故国归路赊，",
        "春晚在天涯。",
        "明月夜来梦，",
        "碧山秋到家。",
        "开窗闻落叶，",
        "远墅见晴鸦。",
        "惊起晓庭际，",
        "莺啼桃杏花。"
      ]
    },
    {
      "_id": null,
      "Name": "鸳鸯",
      "PoetId": "cec5b4ddd8f74181bd467d422d3936aa",
      "PoetName": "吉师老",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "吉师老"
      ],
      "Contents": [
        "江岛濛濛烟霭微，",
        "绿芜深处刷毛衣。",
        "渡头惊起一双去，",
        "飞上文君旧锦机。"
      ]
    }
  ]
}