  "Poems": [
    {
      "_id": null,
      "Name": "北邙山",
      "PoetId": "b00c1fa781ec4af19939b650ae5abf0f",
      "PoetName": "吴商浩",
      "Form": "七律",
      "Tags": [
        "唐代",
        "吴商浩"
      ],
      "Contents": [
        "北邙山草又青青，",
        "今日销魂事可明。",
        "绿酒醉来春未歇，",
        "白杨风起柳初晴。",
        "冈原旋葬松新长，",
        "年代无人阙半平。",
        "堪取金炉九还药，",
        "不能随梦向浮生。"
      ]
    },
    {
      "_id": null,
      "Name": "塞上即事",
      "PoetId": "b00c1fa781ec4af19939b650ae5abf0f",
      "PoetName": "吴商浩",
      "Form": "七律",
      "Tags": [
        "唐代",
        "吴商浩"
      ],
      "Contents": [
        "身似星流迹似蓬，",
        "玉关孤望杳溟濛。",
        "寒沙万里平铺月，",
        "晓角一声高卷风。",
        "战士殁边魂尚哭，",
        "单于猎处火犹红。",
        "分明更想残宵梦，",
        "故国依然在甬东。"
      ]
    },
    {
      "_id": null,
      "Name": "宿山驿",
      "PoetId": "b00c1fa781ec4af19939b650ae5abf0f",
      "PoetName": "吴商浩",
      "Form": "七律",
      "Tags": [
        "唐代",
        "吴商浩"
      ],
      "Contents": [
        "文战何堪功未图，",
        "又驱羸马指天衢。",
        "露华凝夜渚莲尽，",
        "月彩满轮山驿孤。",
        "岐路辛勤终日有，",
        "乡关音信隔年无。",
        "好同范蠡扁舟兴，",
        "高挂一帆归五湖。"
      ]
    },
    {
      "_id": null,
      "Name": "巫峡听猿",
      "PoetId": "b00c1fa781ec4af19939b650ae5abf0f",
      "PoetName": "吴商浩",
      "Form": "五律",
      "Tags": [
        "唐代",
        "吴商浩"
      ],
      "Contents": [
        "巴江猿啸苦，",
        "响入客舟中。",
        "孤枕破残梦，",
        "三声随晓风。",
        "连云波澹澹，",
        "和雾雨濛濛。",
        "巫峡去家远，",
        "不堪魂断空。"
      ]
    },
    {
      "_id": null,
      "Name": "水楼感事",
      "PoetId": "b00c1fa781ec4af19939b650ae5abf0f",
      "PoetName": "吴商浩",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "吴商浩"
      ],
      "Contents": [
        "高柳螀啼雨后秋，",
        "年光空感泪如流。",
        "满湖菱荇东归晚，",
        "闲倚南轩尽日愁。"
      ]
    },
    {
      "_id": null,
      "Name": "泊舟",
      "PoetId": "b00c1fa781ec4af19939b650ae5abf0f",
      "PoetName": "吴商浩",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "吴商浩"
      ],
      "Contents": [
        "身逐烟波魂自惊，",
        "木兰舟上一帆轻。",
        "云中有寺在何处，",
        "山底宿时闻磬声。"
      ]
    },
    {
      "_id": null,
      "Name": "湘云",
      "PoetId": "b00c1fa781ec4af19939b650ae5abf0f",
      "PoetName": "吴商浩",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "吴商浩"
      ],
      "Contents": [
        "□满湘江云莹空，",
        "纷纷长对水溶溶。",
        "日西遥望自归处，",
        "尽挂九疑千万峰。"
      ]
    },
    {
      "_id": null,
      "Name": "秋塘晓望",
      "PoetId": "b00c1fa781ec4af19939b650ae5abf0f",
      "PoetName": "吴商浩",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "吴商浩"
      ],
      "Contents": [
        "钟尽疏桐散曙鸦，",
        "故山烟树隔天涯。",
        "西风一夜秋塘晓，",
        "零落几多红藕花。"
      ]
    },
    {
      "_id": null,
      "Name": "长安春赠友人",
      "PoetId": "b00c1fa781ec4af19939b650ae5abf0f",
      "PoetName": "吴商浩",
      "Form": "七律",
      "Tags": [
        "唐代",
        "吴商浩"
      ],
      "Contents": [
        "繁华堪泣帝城春，",
        "粉堞青楼势碍云。",
        "花对玉钩帘外发，",
        "歌飘尘土路边闻。",
        "几多远客魂空断，",
        "何处王孙酒自醺。",
        "各有归程千万里，",
        "东风时节恨离群。"
      ]
    }
  ]
}