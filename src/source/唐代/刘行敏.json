    {
      "_id": null,
      "Name": "又嘲杨文瓘",
      "PoetId": "a84cf731c2b14b738cf2054158e69e6d",
      "PoetName": "刘行敏",
      "Form": "五律",
      "Tags": [
        "唐代",
        "刘行敏"
      ],
      "Contents": [
        "武陵敬爱客，",
        "终宴不知疲。",
        "遣共浑王饮，",
        "错宴延陀儿。",
        "始被鸿胪识，",
        "终蒙御史知。",
        "精神既如此，",
        "长叹复何为。"
      ]
    },
    {
      "_id": null,
      "Name": "嘲崔生",
      "PoetId": "a84cf731c2b14b738cf2054158e69e6d",
      "PoetName": "刘行敏",
      "Form": "五律",
      "Tags": [
        "唐代",
        "刘行敏"
      ],
      "Contents": [
        "崔生犯夜行，",
        "武候正严更。",
        "幞头拳下落，",
        "高髻掌中擎。",
        "杖迹胸前出，",
        "绳文腕后生。",
        "愁人不惜夜，",
        "随意晓参横。"
      ]
    },
    {
      "_id": null,
      "Name": "嘲李叔慎、贺兰僧伽、杜善贤",
      "PoetId": "a84cf731c2b14b738cf2054158e69e6d",
      "PoetName": "刘行敏",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "刘行敏"
      ],
      "Contents": [
        "叔慎骑乌马，",
        "僧伽把漆弓。",
        "唤取长安令，",
        "共猎北山熊。"
      ]
    }
  ]
}