  "Poems": [
    {
      "_id": null,
      "Name": "贺邓璠使君正拜袁州",
      "PoetId": "473d5dfb055a456eb57aa09a53f1f109",
      "PoetName": "彭蟾",
      "Form": "七律",
      "Tags": [
        "唐代",
        "彭蟾"
      ],
      "Contents": [
        "六年惠爱及黎甿，",
        "大府论功俟陟明。",
        "尺一诏书天上降，",
        "二千石禄世间荣。",
        "新添画戟门增峻，",
        "旧蹑青云路转平。",
        "更待皇恩醒善政，",
        "碧油幢到郡斋迎。"
      ]
    }
  ]
}