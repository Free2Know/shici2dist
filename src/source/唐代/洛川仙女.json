  "Poems": [
    {
      "_id": null,
      "Name": "张郁洛川沿步吟",
      "PoetId": "682c49de29804dee8e33be9bcb40ffaf",
      "PoetName": "洛川仙女",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "洛川仙女"
      ],
      "Contents": [
        "浮生如梦能几何，",
        "浮生复更忧患多。",
        "无人与我长生术，",
        "洛川春日且长歌。"
      ]
    },
    {
      "_id": null,
      "Name": "答张郁歌",
      "PoetId": "682c49de29804dee8e33be9bcb40ffaf",
      "PoetName": "洛川仙女",
      "Form": "五律",
      "Tags": [
        "唐代",
        "洛川仙女"
      ],
      "Contents": [
        "彩云入帝乡，",
        "白鹤又回翔。",
        "久留深不可，",
        "蓬岛路遐长。",
        "空爱长生术，",
        "不是长生人。",
        "今日洛川别，",
        "可惜洞中春。"
      ]
    }
  ]
}