  "Poems": [
    {
      "_id": null,
      "Name": "赋得花发上林",
      "PoetId": "3884361fe6fa4fe49cb4f54e8c53c4b4",
      "PoetName": "周渭",
      "Form": "未知",
      "Tags": [
        "唐代",
        "周渭"
      ],
      "Contents": [
        "灼灼花凝雪，",
        "春来发上林。",
        "向风初散蕊，",
        "垂叶欲成阴。",
        "人过香随远，",
        "烟晴色自深。",
        "净时空结雾，",
        "疏处未藏禽。",
        "菶茸何年值，",
        "间关几日吟。",
        "一枝如可冀，",
        "不负折芳心。"
      ]
    },
    {
      "_id": null,
      "Name": "赠龙兴观主吴崇岳",
      "PoetId": "3884361fe6fa4fe49cb4f54e8c53c4b4",
      "PoetName": "周渭",
      "Form": "未知",
      "Tags": [
        "唐代",
        "周渭"
      ],
      "Contents": [
        "楮为冠子布为裳，",
        "吞得丹霞寿最长。",
        "混俗性灵常乐道，",
        "出尘风格早休粮。",
        "枕中经妙谁传与，",
        "肘后方新自写将。",
        "百尺松梢几飞步，",
        "鹤栖板上礼虚皇。",
        "第二百八十二卷"
      ]
    }
  ]
}