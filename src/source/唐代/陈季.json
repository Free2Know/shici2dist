  "Poems": [
    {
      "_id": null,
      "Name": "湘灵鼓瑟",
      "PoetId": "ff5d18d0cfa043a0b4942eaed53286e0",
      "PoetName": "陈季",
      "Form": "未知",
      "Tags": [
        "唐代",
        "陈季"
      ],
      "Contents": [
        "神女泛瑶瑟，",
        "古祠严野亭。",
        "楚云来泱漭，",
        "湘水助清泠。",
        "妙指微幽契，",
        "繁声入杳冥。",
        "一弹新月白，",
        "数曲暮山青。",
        "调苦荆人怨，",
        "时遥帝子灵。",
        "遗音如可赏，",
        "试奏为君听。"
      ]
    },
    {
      "_id": null,
      "Name": "鹤警露",
      "PoetId": "ff5d18d0cfa043a0b4942eaed53286e0",
      "PoetName": "陈季",
      "Form": "未知",
      "Tags": [
        "唐代",
        "陈季"
      ],
      "Contents": [
        "南国商飙动，",
        "东皋野鹤鸣。",
        "溪松寒暂宿，",
        "露草滴还惊。",
        "欲有高飞意，",
        "空闻召侣情。",
        "风间传藻质，",
        "月下引清声。",
        "未假抟扶势，",
        "焉知羽翼轻。",
        "吾君开太液，",
        "愿得应皇明。"
      ]
    }
  ]
}