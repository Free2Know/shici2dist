  "Poems": [
    {
      "_id": null,
      "Name": "奉和扈从温泉宫承恩赐浴",
      "PoetId": "d63c0fd014784833ba0c31e737246317",
      "PoetName": "蔡希周",
      "Form": "七律",
      "Tags": [
        "唐代",
        "蔡希周"
      ],
      "Contents": [
        "天行云从指骊宫，",
        "浴日馀波锡诏同。",
        "彩殿氤氲拥香溜，",
        "纱窗宛转闭和风。",
        "来将兰气冲皇泽，",
        "去引星文捧碧空。",
        "自怜遇坎便能止，",
        "愿托仙槎路未通。"
      ]
    }
  ]
}