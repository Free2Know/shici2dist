    {
      "_id": null,
      "Name": "又赠",
      "PoetId": "c4ddbbed0639415bba38a7dac49d3ea3",
      "PoetName": "洛中举子",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "洛中举子"
      ],
      "Contents": [
        "少插花枝少下筹，",
        "须防女伴妒风流。",
        "坐中若打占相令，",
        "除却尚书莫点头。"
      ]
    },
    {
      "_id": null,
      "Name": "赠妓茂英",
      "PoetId": "c4ddbbed0639415bba38a7dac49d3ea3",
      "PoetName": "洛中举子",
      "Form": "七律",
      "Tags": [
        "唐代",
        "洛中举子"
      ],
      "Contents": [
        "忆昔当初过柳楼，",
        "茂英年小尚娇羞。",
        "隔窗未省闻高语，",
        "对镜曾窥学上头。",
        "一别中原俱老大，",
        "再来南国见风流。",
        "弹弦酌酒话前事，",
        "零落碧云生暮愁。"
      ]
    }
  ]
}