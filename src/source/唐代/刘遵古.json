    {
      "_id": null,
      "Name": "御沟新柳",
      "PoetId": "a9f6903fbc2a49b9bc65e66d344c27d9",
      "PoetName": "刘遵古",
      "Form": "未知",
      "Tags": [
        "唐代",
        "刘遵古"
      ],
      "Contents": [
        "韶光先禁柳，",
        "几处覆沟新。",
        "映水疑分翠，",
        "含烟欲占春。",
        "悠悠迟日晚，",
        "袅袅好风频。",
        "吐节茸犹嫩，",
        "通条泽稍均。",
        "远和瑶草色，",
        "暗拂玉楼尘。",
        "愿假骞飞便，",
        "归栖及此辰。"
      ]
    }
  ]
}