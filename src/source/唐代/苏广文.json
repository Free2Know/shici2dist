  "Poems": [
    {
      "_id": null,
      "Name": "夜归华川因寄幕府",
      "PoetId": "abf288d989ab4a0badc1866d59f70bf7",
      "PoetName": "苏广文",
      "Form": "七律",
      "Tags": [
        "唐代",
        "苏广文"
      ],
      "Contents": [
        "山村寥落野人稀，",
        "竹里衡门掩翠微。",
        "溪路夜随明月入，",
        "亭皋春伴白云归。",
        "嵇康懒慢仍耽酒，",
        "范蠡逋逃又拂衣。",
        "汀畔数鸥闲不起，",
        "只应知我已忘机。"
      ]
    },
    {
      "_id": null,
      "Name": "春日过田明府遇焦山人",
      "PoetId": "abf288d989ab4a0badc1866d59f70bf7",
      "PoetName": "苏广文",
      "Form": "七律",
      "Tags": [
        "唐代",
        "苏广文"
      ],
      "Contents": [
        "陶公归隐白云溪，",
        "买得春泉溉药畦。",
        "夜静林间风虎啸，",
        "月明竹上露禽栖。",
        "陈仓邑吏惊烽火，",
        "太白山人讶鼓鼙。",
        "相见只言秦汉事，",
        "武陵溪里草萋萋。"
      ]
    },
    {
      "_id": null,
      "Name": "自商山宿隐居",
      "PoetId": "abf288d989ab4a0badc1866d59f70bf7",
      "PoetName": "苏广文",
      "Form": "七律",
      "Tags": [
        "唐代",
        "苏广文"
      ],
      "Contents": [
        "闻道桃源堪避秦，",
        "寻幽数日不逢人。",
        "烟霞洞里无鸡犬，",
        "风雨林中有鬼神。",
        "黄公石上三芝秀，",
        "陶令门前五柳春。",
        "醉卧白云闲入梦，",
        "不知何物是吾身。"
      ]
    }
  ]
}