    {
      "_id": null,
      "Name": "敕赐三相马",
      "PoetId": "826eb3aa0b3b4064a5b1d41ac8ae952b",
      "PoetName": "张随",
      "Form": "未知",
      "Tags": [
        "唐代",
        "张随"
      ],
      "Contents": [
        "上苑骅骝出，",
        "中宫诏命传。",
        "九天班锡礼，",
        "三相代劳年。",
        "顾主声犹发，",
        "追风力正全。",
        "鸣珂龙阙下，",
        "喷玉凤池前。",
        "四足疑云灭，",
        "双瞳比镜悬。",
        "为因能致远，",
        "今日表求贤。"
      ]
    },
    {
      "_id": null,
      "Name": "河中献捷",
      "PoetId": "826eb3aa0b3b4064a5b1d41ac8ae952b",
      "PoetName": "张随",
      "Form": "未知",
      "Tags": [
        "唐代",
        "张随"
      ],
      "Contents": [
        "叛将忘恩久，",
        "王师不战通。",
        "凯歌千里内，",
        "喜气二仪中。",
        "寇尽条山下，",
        "兵回汉苑东。",
        "将军初执讯，",
        "明主欲论功。",
        "落日烟尘静，",
        "寒郊壁垒空。",
        "苍生幸无事，",
        "自此乐尧风。"
      ]
    }
  ]
}