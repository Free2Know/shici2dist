  "Description": "唐末人。诗四首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "东西行",
      "PoetId": "d527bb1df0604177b376dc833c5cb1d7",
      "PoetName": "卢频",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "卢频"
      ],
      "Contents": [
        "种荷玉盆里，",
        "不及沟中水。",
        "养雉黄金笼，",
        "见草心先喜。"
      ]
    },
    {
      "_id": null,
      "Name": "失题",
      "PoetId": "d527bb1df0604177b376dc833c5cb1d7",
      "PoetName": "卢频",
      "Form": "五律",
      "Tags": [
        "唐代",
        "卢频"
      ],
      "Contents": [
        "春泪烂罗绮，",
        "泣声抽恨多。",
        "莫滴芙蓉池，",
        "愁伤连蒂荷。",
        "一朵花叶飞，",
        "一枝花光彩。",
        "美人惜花心，",
        "但愿春长在。"
      ]
    },
    {
      "_id": null,
      "Name": "蛱蝶行",
      "PoetId": "d527bb1df0604177b376dc833c5cb1d7",
      "PoetName": "卢频",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "卢频"
      ],
      "Contents": [
        "东园宫草绿，",
        "上下飞相逐。",
        "君恩不禁春，",
        "昨夜花中宿。"
      ]
    }
  ]
}