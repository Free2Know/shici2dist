  "Poems": [
    {
      "_id": null,
      "Name": "咏蛙",
      "PoetId": "efa4be69a1d84b2d9d6a2e0892f6f7d3",
      "PoetName": "杨收",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "杨收"
      ],
      "Contents": [
        "兔边分玉树，",
        "龙底耀铜仪。",
        "会当同鼓吹，",
        "不复问官私。"
      ]
    },
    {
      "_id": null,
      "Name": "嘲吴人观者",
      "PoetId": "efa4be69a1d84b2d9d6a2e0892f6f7d3",
      "PoetName": "杨收",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "杨收"
      ],
      "Contents": [
        "尔幸无羸角，",
        "何用触吾藩。",
        "若是升堂者，",
        "还应自得门。"
      ]
    },
    {
      "_id": null,
      "Name": "笔",
      "PoetId": "efa4be69a1d84b2d9d6a2e0892f6f7d3",
      "PoetName": "杨收",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "杨收"
      ],
      "Contents": [
        "虽匪囊中物，",
        "何坚不可钻。",
        "一朝操政柄，",
        "定使冠三端。"
      ]
    }
  ]
}