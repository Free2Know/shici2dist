  "Poems": [
    {
      "_id": null,
      "Name": "奉和九月九日登慈恩寺浮图应制",
      "PoetId": "c4b1876076c44081913913d2cceb0ef2",
      "PoetName": "麹瞻",
      "Form": "五律",
      "Tags": [
        "唐代",
        "麹瞻"
      ],
      "Contents": [
        "扈跸游玄地，",
        "陪仙瞰紫微。",
        "似迈铢衣劫，",
        "将同羽化飞。",
        "雕戈秋日丽，",
        "宝剑晓霜霏。",
        "献觞乘菊序，",
        "长愿奉天晖。"
      ]
    }
  ]
}