  "Poems": [
    {
      "_id": null,
      "Name": "上盐城令述德诗",
      "PoetId": "09fa6dc1c3aa463ea4027ca9bdac10cc",
      "PoetName": "张辞",
      "Form": "七律",
      "Tags": [
        "唐代",
        "张辞"
      ],
      "Contents": [
        "门风常有蕙兰馨，",
        "鼎族家传霸国名。",
        "容貌静悬秋月彩，",
        "文章高振海涛声。",
        "讼堂无事调琴轸，",
        "郡阁何妨醉玉觥。",
        "今日东渐桥下水，",
        "一条从此镇常清。"
      ]
    },
    {
      "_id": null,
      "Name": "别令诗",
      "PoetId": "09fa6dc1c3aa463ea4027ca9bdac10cc",
      "PoetName": "张辞",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "张辞"
      ],
      "Contents": [
        "张辞张辞自不会，",
        "天下经书在腹内。",
        "身即腾腾处世间，",
        "心即逍遥出天外。"
      ]
    },
    {
      "_id": null,
      "Name": "谢令学道诗",
      "PoetId": "09fa6dc1c3aa463ea4027ca9bdac10cc",
      "PoetName": "张辞",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "张辞"
      ],
      "Contents": [
        "何用梯媒向外求，",
        "长生只合内中修。",
        "莫言大道人难得，",
        "自是行心不到头。"
      ]
    },
    {
      "_id": null,
      "Name": "题壁",
      "PoetId": "09fa6dc1c3aa463ea4027ca9bdac10cc",
      "PoetName": "张辞",
      "Form": "五律",
      "Tags": [
        "唐代",
        "张辞"
      ],
      "Contents": [
        "争那金乌何，",
        "头上飞不住。",
        "红炉漫烧药，",
        "玉颜安可驻。",
        "今年花发枝，",
        "明年叶落树。",
        "不如且饮酒，",
        "莫管流年度。"
      ]
    }
  ]
}