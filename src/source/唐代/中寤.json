  "Poems": [
    {
      "_id": null,
      "Name": "赠王仙柯",
      "PoetId": "8f848724abfe4ff7984f94c112933c09",
      "PoetName": "中寤",
      "Form": "七律",
      "Tags": [
        "唐代",
        "中寤"
      ],
      "Contents": [
        "瞻思不及望仙兄，",
        "早晚升霞入太清。",
        "手种一株松未老，",
        "炉烧九转药新成。",
        "心中已得黄庭术，",
        "头上应无白发生。",
        "异日却归华表语，",
        "待教凡俗普闻名。"
      ]
    }
  ]
}