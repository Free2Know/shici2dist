{
  "_id": "94930da630ce48f28241ac87a0576de5",
  "Name": "薛涛",
  "Description": "薛涛字洪度，生于大历五年，卒于大和六年（即公元770-832年），享年63岁。原籍长安，幼随父居成都，八九岁能诗，十六岁入乐籍，脱乐籍后终身未嫁。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "上川主武元衡相国二首",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七律",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "落日重城夕雾收，",
        "玳筵雕俎荐诸侯。",
        "因令朗月当庭燎，",
        "不使珠帘下玉钩。",
        "东阁移尊绮席陈，",
        "貂簪龙节更宜春。",
        "军城画角三声歇，",
        "云幕初垂红烛新。"
      ]
    },
    {
      "_id": null,
      "Name": "上王尚书",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "碧玉双幢白玉郎，",
        "初辞天帝下扶桑。",
        "手持云篆题新榜，",
        "十万人家春日长。"
      ]
    },
    {
      "_id": null,
      "Name": "九日遇雨二首",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七律",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "万里惊飙朔气深，",
        "江城萧索昼阴阴。",
        "谁怜不得登山去，",
        "可惜寒芳色似金。",
        "茱萸秋节佳期阻，",
        "金菊寒花满院香。",
        "神女欲来知有意，",
        "先令云雨暗池塘。"
      ]
    },
    {
      "_id": null,
      "Name": "乡思",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "峨嵋山下水如油，",
        "怜我心同不系舟。",
        "何日片帆离锦浦，",
        "棹声齐唱发中流。"
      ]
    },
    {
      "_id": null,
      "Name": "井梧吟",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "未知",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "庭除一古桐，",
        "耸干入云中。",
        "枝迎南北鸟，",
        "叶送往来风。"
      ]
    },
    {
      "_id": null,
      "Name": "别李郎中",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "花落梧桐凤别凰，",
        "想登秦岭更凄凉。",
        "安仁纵有诗将赋，",
        "一半音词杂悼亡。"
      ]
    },
    {
      "_id": null,
      "Name": "十离诗·犬离主",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "驯扰朱门四五年，",
        "毛香足净主人怜。",
        "无端咬著亲情客，",
        "不得红丝毯上眠。"
      ]
    },
    {
      "_id": null,
      "Name": "听僧吹芦管",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "晓蝉呜咽暮莺愁，",
        "言语殷勤十指头。",
        "罢阅梵书聊一弄，",
        "散随金磬泥清秋。"
      ]
    },
    {
      "_id": null,
      "Name": "和刘宾客玉蕣",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "琼枝的皪露珊珊，",
        "欲折如披玉彩寒。",
        "闲拂朱房何所似，",
        "缘山偏映月轮残。"
      ]
    },
    {
      "_id": null,
      "Name": "和李书记席上见赠",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "翩翩射策东堂秀，",
        "岂复相逢豁寸心。",
        "借问风光为谁丽，",
        "万条丝柳翠烟深。"
      ]
    },
    {
      "_id": null,
      "Name": "和郭员外题万里桥",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "万里桥头独越吟，",
        "知凭文字写愁心。",
        "细侯风韵兼前事，",
        "不止为舟也作霖。"
      ]
    },
    {
      "_id": null,
      "Name": "咏八十一颗",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "未知",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "色比丹霞朝日，",
        "形如合浦圆珰。",
        "开时九九知数，",
        "见处双双颉颃。"
      ]
    },
    {
      "_id": null,
      "Name": "四友赞",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "未知",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "磨润色先生之腹，",
        "濡藏锋都尉之头。",
        "引书媒而黯黯，",
        "入文亩以休休。"
      ]
    },
    {
      "_id": null,
      "Name": "宣上人见示与诸公",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "许厕高斋唱，",
        "涓泉定不如。",
        "可怜谯记室，",
        "流水满禅居。"
      ]
    },
    {
      "_id": null,
      "Name": "寄张元夫",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "前溪独立后溪行，",
        "鹭识朱衣自不惊。",
        "借问人间愁寂意，",
        "伯牙弦绝已无声。"
      ]
    },
    {
      "_id": null,
      "Name": "寄旧诗与元微之",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七律",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "诗篇调态人皆有，",
        "细腻风光我独知。",
        "月下咏花怜暗淡，",
        "雨朝题柳为欹垂。",
        "长教碧玉藏深处，",
        "总向红笺写自随。",
        "老大不能收拾得，",
        "与君开似好男儿。"
      ]
    },
    {
      "_id": null,
      "Name": "寄词",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "菌阁芝楼杳霭中，",
        "霞开深见玉皇宫。",
        "紫阳天上神仙客，",
        "称在人间立世功。"
      ]
    },
    {
      "_id": null,
      "Name": "忆荔枝",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "传闻象郡隔南荒，",
        "绛实丰肌不可忘。",
        "近有青衣连楚水，",
        "素浆还得类琼浆。"
      ]
    },
    {
      "_id": null,
      "Name": "摩诃池赠萧中丞",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "昔以多能佐碧油，",
        "今朝同泛旧仙舟。",
        "凄凉逝水颓波远，",
        "惟有碑泉咽不流。"
      ]
    },
    {
      "_id": null,
      "Name": "斛石山书事",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "王家山水画图中，",
        "意思都卢粉墨容。",
        "今日忽登虚境望，",
        "步摇冠翠一千峰。"
      ]
    },
    {
      "_id": null,
      "Name": "斛石山晓望寄吕侍御",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "未知",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "曦轮初转照仙扃，",
        "旋擘烟岚上(穴目)冥。",
        "不得玄晖同指点，",
        "天涯苍翠漫青青。"
      ]
    },
    {
      "_id": null,
      "Name": "春望词四首",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "未知",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "花开不同赏，",
        "花落不同悲。",
        "欲问相思处，",
        "花开花落时。",
        "揽草结同心，",
        "将以遗知音。",
        "春愁正断绝，",
        "春鸟复哀吟。",
        "风花日将老，",
        "佳期犹渺渺。",
        "不结同心人，",
        "空结同心草。",
        "那堪花满枝，",
        "翻作两相思。",
        "玉箸垂朝镜，",
        "春风知不知。"
      ]
    },
    {
      "_id": null,
      "Name": "春郊游眺寄孙处士二首",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七律",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "低头久立向蔷薇，",
        "爱似零陵香惹衣。",
        "何事碧鸡孙处士，",
        "伯劳东去燕西飞。",
        "今朝纵目玩芳菲，",
        "夹缬笼裙绣地衣。",
        "满袖满头兼手把，",
        "教人识是看花归。"
      ]
    },
    {
      "_id": null,
      "Name": "月",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "魄依钩样小，",
        "扇逐汉机团。",
        "细影将圆质，",
        "人间几处看。"
      ]
    },
    {
      "_id": null,
      "Name": "朱槿花",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "红开露脸误文君，",
        "司蒡芙蓉草绿云。",
        "造化大都排比巧，",
        "衣裳色泽总薰薰。"
      ]
    },
    {
      "_id": null,
      "Name": "柳絮",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "二月杨花轻复微，",
        "春风摇荡惹人衣。",
        "他家本是无情物，",
        "一向南飞又北飞。"
      ]
    },
    {
      "_id": null,
      "Name": "棠梨花和李太尉",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "吴均蕙圃移嘉木，",
        "正及东溪春雨时。",
        "日晚莺啼何所为，",
        "浅深红腻压繁枝。"
      ]
    },
    {
      "_id": null,
      "Name": "段相国游武担寺病不能从题寄",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "消瘦翻堪见令公，",
        "落花无那恨东风。",
        "侬心犹道青春在，",
        "羞看飞蓬石镜中。"
      ]
    },
    {
      "_id": null,
      "Name": "江亭饯别",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "未知",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "绿沼红泥物象幽，",
        "范汪兼(亻卒)李并州。",
        "离亭急管四更后，",
        "不见车公心独愁。"
      ]
    },
    {
      "_id": null,
      "Name": "江月楼",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "五律",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "秋风仿佛吴江冷，",
        "鸥鹭参差夕阳影。",
        "垂虹纳纳卧谯门，",
        "雉堞耽耽俯渔艇。",
        "阳安小儿拍手笑，",
        "使君幻出江南景。"
      ]
    },
    {
      "_id": null,
      "Name": "江边",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "西风忽报雁双双，",
        "人世心形两自降。",
        "不为鱼肠有真诀，",
        "谁能夜夜立清江。"
      ]
    },
    {
      "_id": null,
      "Name": "池上双凫",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "双栖绿池上，",
        "朝去暮飞还。",
        "更忆将雏日，",
        "同心莲叶间。"
      ]
    },
    {
      "_id": null,
      "Name": "浣花亭陪川主王播相公暨僚同",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "五律",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "西陆行终令，",
        "东篱始再阳。",
        "绿英初濯露，",
        "金蕊半含霜。",
        "自有兼材用，",
        "那同众草芳。",
        "献酬樽俎外，",
        "宁有惧豺狼。"
      ]
    },
    {
      "_id": null,
      "Name": "海棠溪",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "春教风景驻仙霞，",
        "水面鱼身总带花。",
        "人世不思灵卉异，",
        "竞将红缬染轻沙。"
      ]
    },
    {
      "_id": null,
      "Name": "燕离巢",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "出入朱门未忍抛，",
        "主人常爱语交交。",
        "衔泥秽污珊瑚枕，",
        "不得梁间更垒巢。"
      ]
    },
    {
      "_id": null,
      "Name": "牡丹",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七律",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "去春零落暮春时，",
        "泪湿红笺怨别离。",
        "常恐便同巫峡散，",
        "因何重有武陵期？",
        "传情每向馨香得，",
        "不语还应彼此知。",
        "只欲栏边安枕席，",
        "夜深闲共说相思。"
      ]
    },
    {
      "_id": null,
      "Name": "犬离主",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "出入朱门四五年，",
        "为知人意得人怜。",
        "近缘咬着亲知客，",
        "不得红丝毯上眠。"
      ]
    },
    {
      "_id": null,
      "Name": "珠离掌",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "皎洁圆明内外通，",
        "清光似照水晶宫。",
        "只缘一点玷相秽，",
        "不得终宵在掌中。"
      ]
    },
    {
      "_id": null,
      "Name": "秋泉",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "冷色初澄一带烟，",
        "幽声遥泻十丝弦。",
        "长来枕上牵情思，",
        "不使愁人半夜眠。"
      ]
    },
    {
      "_id": null,
      "Name": "竹离亭",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "蓊郁新栽四五行，",
        "常将劲节负秋霜。",
        "为缘春笋钻墙破，",
        "不得垂阴覆玉堂。"
      ]
    },
    {
      "_id": null,
      "Name": "笔离手",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "越管宣毫始称情，",
        "红笺纸上撒花琼。",
        "都缘用久锋头尽，",
        "不得羲之手里擎。"
      ]
    },
    {
      "_id": null,
      "Name": "筹边楼",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "平临云鸟八窗秋，",
        "壮压西川四十州。",
        "诸将莫贪羌族马，",
        "最高层处见边头。"
      ]
    },
    {
      "_id": null,
      "Name": "续嘉陵驿诗献武相国",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "蜀门西更上青天，",
        "强为公歌蜀国弦。",
        "卓氏长卿称士女，",
        "锦江玉垒献山川。"
      ]
    },
    {
      "_id": null,
      "Name": "罚赴边上韦相公二首",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七律",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "萤在荒芜月在天，",
        "萤飞岂到月轮边。",
        "重光万里应相照，",
        "目断云霄信不传。",
        "按辔岭头寒复寒，",
        "微风细雨彻心肝。",
        "但得放儿归舍去，",
        "山水屏风永不看。"
      ]
    },
    {
      "_id": null,
      "Name": "罚赴边有怀上韦令公二首",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "五律",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "闻道边城苦，",
        "今来到始知。",
        "羞将门下曲，",
        "唱与陇头儿。",
        "黠虏犹违命，",
        "烽烟直北愁。",
        "却教严谴妾，",
        "不敢向松州。"
      ]
    },
    {
      "_id": null,
      "Name": "菱荇沼",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "水荇斜牵绿藻浮，",
        "柳丝和叶卧清流。",
        "何时得向溪头赏，",
        "旋摘菱花旋泛舟。"
      ]
    },
    {
      "_id": null,
      "Name": "蝉",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "露涤清音远，",
        "风吹数叶齐。",
        "声声似相接，",
        "各在一枝栖。"
      ]
    },
    {
      "_id": null,
      "Name": "西岩",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "凭阑却忆骑鲸客，",
        "把酒临风手自招。",
        "细雨声中停去马，",
        "夕阳影里乱鸣蜩。"
      ]
    },
    {
      "_id": null,
      "Name": "试新服裁制初成三首",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "未知",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "紫阳宫里赐红绡，",
        "仙雾朦胧隔海遥。",
        "霜兔毳寒冰茧净，",
        "嫦娥笑指织星桥。",
        "九气分为九色霞，",
        "五灵仙驭五云车。",
        "春风因过东君舍，",
        "偷样人间染百花。",
        "长裾本是上清仪，",
        "曾逐群仙把玉芝。",
        "每到宫中歌舞会，",
        "折腰齐唱步虚词。"
      ]
    },
    {
      "_id": null,
      "Name": "谒巫山庙",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七律",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "乱猿啼处访高唐，",
        "路入烟霞草木香。",
        "山色未能忘宋玉，",
        "水声犹是哭襄王。",
        "朝朝夜夜阳台下，",
        "为雨为云楚国亡。",
        "惆怅庙前多少柳，",
        "春来空斗画眉长。"
      ]
    },
    {
      "_id": null,
      "Name": "贼平后上高相公",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "惊看天地白荒荒，",
        "瞥见青山旧夕阳。",
        "始信大威能照映，",
        "由来日月借生光。"
      ]
    },
    {
      "_id": null,
      "Name": "赋凌云寺二首",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七律",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "闻说凌云寺里苔，",
        "风高日近绝纤埃。",
        "横云点染芙蓉壁，",
        "似待诗人宝月来。",
        "闻说凌云寺里花，",
        "飞空绕磴逐江斜。",
        "有时锁得嫦娥镜，",
        "镂出瑶台五色霞。"
      ]
    },
    {
      "_id": null,
      "Name": "赠杨蕴中",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "玉漏声长灯耿耿，",
        "东墙西墙时见影。",
        "月明窗外子规啼，",
        "忍使孤魂愁夜永。"
      ]
    },
    {
      "_id": null,
      "Name": "赠段校书",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "公子翩翩说校书，",
        "玉弓金勒紫绡裾。",
        "玄成莫便骄名誉，",
        "文采风流定不知。"
      ]
    },
    {
      "_id": null,
      "Name": "赠苏十三中丞",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "洛阳陌上埋轮气，",
        "欲逐秋空击隼飞。",
        "今日芝泥检征诏，",
        "别须台外振霜威。"
      ]
    },
    {
      "_id": null,
      "Name": "赠远二首",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七律",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "芙蓉新落蜀山秋，",
        "锦字开缄到是愁。",
        "闺阁不知戎马事，",
        "月高还上望夫楼。",
        "扰弱新蒲叶又齐，",
        "春深花落塞前溪。",
        "知君未转秦关骑，",
        "月照千门掩袖啼。"
      ]
    },
    {
      "_id": null,
      "Name": "赠韦校书",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "芸香误比荆山玉，",
        "那似登科甲乙年。",
        "淡沲鲜风将绮思，",
        "飘花散蕊媚青天。"
      ]
    },
    {
      "_id": null,
      "Name": "送卢员外",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "玉垒山前风雪夜，",
        "锦官城外别离魂。",
        "信陵公子如相问，",
        "长向夷门感旧恩。"
      ]
    },
    {
      "_id": null,
      "Name": "送友人",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "水国蒹葭夜有霜，",
        "月寒山色共苍苍。",
        "谁言千里自今夕，",
        "离梦杳如关塞长。"
      ]
    },
    {
      "_id": null,
      "Name": "送姚员外",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "万条江柳早秋枝，",
        "袅地翻风色未衰。",
        "欲折尔来将赠别，",
        "莫教烟月两乡悲。"
      ]
    },
    {
      "_id": null,
      "Name": "送扶炼师",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "锦浦归舟巫峡云，",
        "绿波迢递雨纷纷。",
        "山阴妙术人传久，",
        "也说将鹅与右军。"
      ]
    },
    {
      "_id": null,
      "Name": "送郑资州",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "雨暗眉山江水流，",
        "离人掩袂立高楼。",
        "双旌千骑骈东陌，",
        "独有罗敷望上头。"
      ]
    },
    {
      "_id": null,
      "Name": "酬人雨后玩竹",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "五律",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "南天春雨时，",
        "那鉴雪霜姿。",
        "众类亦云茂，",
        "虚心宁自持。",
        "多留晋贤醉，",
        "早伴舜妃悲。",
        "晚岁君能赏，",
        "苍苍劲节奇。"
      ]
    },
    {
      "_id": null,
      "Name": "酬吴使君",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "支公别墅接花扃，",
        "买得前山总未经。",
        "入户剡溪云水满，",
        "高斋咫尺蹑青冥。"
      ]
    },
    {
      "_id": null,
      "Name": "酬文使君",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "延英晓拜汉恩新，",
        "五马腾骧九陌尘。",
        "今日谢庭飞白雪，",
        "巴歌不复旧阳春。"
      ]
    },
    {
      "_id": null,
      "Name": "酬李校书",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "才游象外身虽远，",
        "学茂区中事易闻。",
        "自顾漳滨多病后，",
        "空瞻逸翮舞青云。"
      ]
    },
    {
      "_id": null,
      "Name": "酬杜舍人",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "双鱼底事到侬家，",
        "扑手新诗片片霞。",
        "唱到白苹洲畔曲，",
        "芙蓉空老蜀江花。"
      ]
    },
    {
      "_id": null,
      "Name": "酬杨供奉法师见招",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "远水长流洁复清，",
        "雪窗高卧与云平。",
        "不嫌袁室无烟火，",
        "惟笑商山有姓名。"
      ]
    },
    {
      "_id": null,
      "Name": "酬祝十三秀才",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "浩思蓝山玉彩寒，",
        "冰囊敲碎楚金盘。",
        "诗家利器驰声久，",
        "何用春闱榜下看。"
      ]
    },
    {
      "_id": null,
      "Name": "酬辛员外折花见遗",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "青鸟东飞正落梅，",
        "衔花满口下瑶台。",
        "一枝为授殷勤意，",
        "把向风前旋旋开。"
      ]
    },
    {
      "_id": null,
      "Name": "酬郭简州寄柑子",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "霜规不让黄金色，",
        "圆质仍含御史香。",
        "何处同声情最异，",
        "临川太守谢家郎。"
      ]
    },
    {
      "_id": null,
      "Name": "酬雍秀才贻巴峡图",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "千叠云峰万顷湖，",
        "白波分去绕荆吴。",
        "感君识我枕流意，",
        "重示瞿塘峡口图。"
      ]
    },
    {
      "_id": null,
      "Name": "采莲舟",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "风前一叶压荷蕖，",
        "解报新秋又得鱼。",
        "兔走乌驰人语静，",
        "满溪红袂棹歌初。"
      ]
    },
    {
      "_id": null,
      "Name": "金灯花",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "阑边不见蘘蘘叶，",
        "砌下惟翻艳艳丛。",
        "细视欲将何物比，",
        "晓霞初叠赤城宫。"
      ]
    },
    {
      "_id": null,
      "Name": "镜离台",
      "PoetId": "94930da630ce48f28241ac87a0576de5",
      "PoetName": "薛涛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "薛涛"
      ],
      "Contents": [
        "铸泻黄金镜始开，",
        "初生三五月徘徊。",
        "为遭无限尘蒙蔽，",
        "不得华堂上玉台。"
      ]
    },
