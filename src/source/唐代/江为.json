{
  "_id": "96166da5666340e7aa3e362a3cfe309f",
  "Name": "江为",
  "Description": "宋州人，避乱家建阳。游庐山，师陈贶为诗。集一卷，今存诗八首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "临刑诗",
      "PoetId": "96166da5666340e7aa3e362a3cfe309f",
      "PoetName": "江为",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "江为"
      ],
      "Contents": [
        "街鼓侵人急，",
        "西倾日欲斜。",
        "黄泉无旅店，",
        "今夜宿谁家。"
      ]
    },
    {
      "_id": null,
      "Name": "句",
      "PoetId": "96166da5666340e7aa3e362a3cfe309f",
      "PoetName": "江为",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "江为"
      ],
      "Contents": [
        "吟登萧寺旃檀阁，",
        "醉倚王家玳瑁筵。",
        "远远朝宗出白云，",
        "方圆随处性长存。"
      ]
    },
    {
      "_id": null,
      "Name": "塞下曲",
      "PoetId": "96166da5666340e7aa3e362a3cfe309f",
      "PoetName": "江为",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "江为"
      ],
      "Contents": [
        "万里黄云冻不飞，",
        "碛烟烽火夜深微。",
        "胡儿移帐寒笳绝，",
        "雪路时闻探马归。"
      ]
    },
    {
      "_id": null,
      "Name": "岳阳楼",
      "PoetId": "96166da5666340e7aa3e362a3cfe309f",
      "PoetName": "江为",
      "Form": "五律",
      "Tags": [
        "唐代",
        "江为"
      ],
      "Contents": [
        "倚楼高望极，",
        "展转念前途。",
        "晚叶红残楚，",
        "秋江碧入吴。",
        "云中来雁急，",
        "天末去帆孤。",
        "明月谁同我，",
        "悠悠上帝都。"
      ]
    },
    {
      "_id": null,
      "Name": "旅怀",
      "PoetId": "96166da5666340e7aa3e362a3cfe309f",
      "PoetName": "江为",
      "Form": "五律",
      "Tags": [
        "唐代",
        "江为"
      ],
      "Contents": [
        "迢迢江汉路，",
        "秋色又堪惊。",
        "半夜闻鸿雁，",
        "多年别弟兄。",
        "高风云影断，",
        "微雨菊花明。",
        "欲寄东归信，",
        "裴回无限情。"
      ]
    },
    {
      "_id": null,
      "Name": "江行",
      "PoetId": "96166da5666340e7aa3e362a3cfe309f",
      "PoetName": "江为",
      "Form": "五律",
      "Tags": [
        "唐代",
        "江为"
      ],
      "Contents": [
        "越信隔年稀，",
        "孤舟几梦归。",
        "月寒花露重，",
        "江晚水烟微。",
        "峰直帆相望，",
        "沙空鸟自飞。",
        "何时洞庭上，",
        "春雨满蓑衣。"
      ]
    },
    {
      "_id": null,
      "Name": "送客",
      "PoetId": "96166da5666340e7aa3e362a3cfe309f",
      "PoetName": "江为",
      "Form": "五律",
      "Tags": [
        "唐代",
        "江为"
      ],
      "Contents": [
        "明月孤舟远，",
        "吟髭镊更华。",
        "天形围泽国，",
        "秋色露人家。",
        "水馆萤交影，",
        "霜洲橘委花。",
        "何当寻旧隐，",
        "泉石好生涯。"
      ]
    },
    {
      "_id": null,
      "Name": "隋堤柳",
      "PoetId": "96166da5666340e7aa3e362a3cfe309f",
      "PoetName": "江为",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "江为"
      ],
      "Contents": [
        "锦缆龙舟万里来，",
        "醉乡繁盛忽尘埃。",
        "空余两岸千株柳，",
        "雨叶风花作恨媒。"
      ]
    }
  ]
}