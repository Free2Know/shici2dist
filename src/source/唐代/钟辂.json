      "_id": null,
      "Name": "缑山月夜闻王子晋吹笙",
      "PoetId": "4bc7a56fb9874afa88bad646121c27f8",
      "PoetName": "钟辂",
      "Form": "未知",
      "Tags": [
        "唐代",
        "钟辂"
      ],
      "Contents": [
        "月满缑山夜，",
        "风传子晋笙。",
        "初闻盈谷远，",
        "渐听入云清。",
        "杳异人间曲，",
        "遥分鹤上情。",
        "孤鸾惊欲舞，",
        "万籁寂无声。",
        "此夕留烟驾，",
        "何时返玉京。",
        "唯愁音响绝，",
        "晓色出都城。"
      ]
    }
  ]
}