  "Poems": [
    {
      "_id": null,
      "Name": "赋得春风扇微和",
      "PoetId": "c6f6abab585c423eb54440699298888e",
      "PoetName": "柳道伦",
      "Form": "未知",
      "Tags": [
        "唐代",
        "柳道伦"
      ],
      "Contents": [
        "青阳初入律，",
        "淑气应春风。",
        "始辨梅花里，",
        "俄分柳色中。",
        "依微开夕照，",
        "澹荡媚晴空。",
        "拂水生蘋末，",
        "经岩触桂丛。",
        "稍抽兰叶紫，",
        "微吐杏花红。",
        "愿逐仁风布，",
        "将俾生植功。"
      ]
    }
  ]
}