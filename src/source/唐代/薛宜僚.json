  "Poems": [
    {
      "_id": null,
      "Name": "别青州妓段东美",
      "PoetId": "667c050dd5704b61a0242ac3b0515c9e",
      "PoetName": "薛宜僚",
      "Form": "七律",
      "Tags": [
        "唐代",
        "薛宜僚"
      ],
      "Contents": [
        "经年邮驿许安栖，",
        "一会他乡别恨迷。",
        "今日海帆飘万里，",
        "不堪肠断对含啼。",
        "阿母桃花方似锦，",
        "王孙草色正如烟。",
        "不须更向沧溟望，",
        "惆怅欢情恰一年。"
      ]
    }
  ]
}