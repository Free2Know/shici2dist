  "Poems": [
    {
      "_id": null,
      "Name": "白云溪",
      "PoetId": "c52684c73a294fa1adedf193334ab636",
      "PoetName": "吴巩",
      "Form": "五律",
      "Tags": [
        "唐代",
        "吴巩"
      ],
      "Contents": [
        "山径入修篁，",
        "深林蔽日光。",
        "夏云生嶂远，",
        "瀑水引溪长。",
        "秀迹逢皆胜，",
        "清芬坐转凉。",
        "回看玉樽夕，",
        "归路赏前忘。"
      ]
    }
  ]
}