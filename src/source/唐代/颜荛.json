  "Poems": [
    {
      "_id": null,
      "Name": "戏张道人不饮酒",
      "PoetId": "a5bfe8f3a9c54fefb42bb0edc20fceee",
      "PoetName": "颜荛",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "颜荛"
      ],
      "Contents": [
        "言自云山访我来，",
        "每闻奇秘觉叨陪。",
        "吾师不饮人间酒，",
        "应待流霞即举杯。"
      ]
    }
  ]
}