  "Poems": [
    {
      "_id": null,
      "Name": "东峰亭各赋一物得幽径石",
      "PoetId": "23cc4a158d954bae904ac1fff754b3e1",
      "PoetName": "王纬",
      "Form": "未知",
      "Tags": [
        "唐代",
        "王纬"
      ],
      "Contents": [
        "片石东溪上，",
        "阴崖剩阻修。",
        "雨馀青石霭，",
        "岁晚绿苔幽。",
        "从来不可转，",
        "今日为人留。"
      ]
    },
    {
      "_id": null,
      "Name": "喜陆侍御破石埭草寇东峰亭赋诗",
      "PoetId": "23cc4a158d954bae904ac1fff754b3e1",
      "PoetName": "王纬",
      "Form": "未知",
      "Tags": [
        "唐代",
        "王纬"
      ],
      "Contents": [
        "蜂虿聚吴州，",
        "推贤奉圣忧。",
        "忠诚资上策，",
        "仁勇佐前筹。",
        "草木成鹅鹳，",
        "戈鋋复斗牛。",
        "戎车一战后，",
        "残垒五兵收。",
        "野静山戎险，",
        "江平水面流。",
        "更怜羁旅客，",
        "从此罢葵丘。"
      ]
    }
  ]
}