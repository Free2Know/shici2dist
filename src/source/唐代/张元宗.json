  "Poems": [
    {
      "_id": null,
      "Name": "望终南山",
      "PoetId": "35226a1875d341cea04549fef23192c4",
      "PoetName": "张元宗",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "张元宗"
      ],
      "Contents": [
        "红尘白日长安路，",
        "马足车轮不暂闲。",
        "唯有茂陵多病客，",
        "每来高处望南山。"
      ]
    },
    {
      "_id": null,
      "Name": "登景云寺阁",
      "PoetId": "35226a1875d341cea04549fef23192c4",
      "PoetName": "张元宗",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "张元宗"
      ],
      "Contents": [
        "胡马饮河洛，",
        "我家从此迁。",
        "今来独垂泪，",
        "三十六峰前。"
      ]
    }
  ]
}