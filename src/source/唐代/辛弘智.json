      "_id": null,
      "Name": "杂曲歌辞·自君之出矣",
      "PoetId": "8a383f70667447cdb59c2ae335348a9e",
      "PoetName": "辛弘智",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "辛弘智"
      ],
      "Contents": [
        "自君之出矣，",
        "梁尘静不飞。",
        "思君如满月，",
        "夜夜减容晖。"
      ]
    },
    {
      "_id": null,
      "Name": "赋诗",
      "PoetId": "8a383f70667447cdb59c2ae335348a9e",
      "PoetName": "辛弘智",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "辛弘智"
      ],
      "Contents": [
        "君为河边草，",
        "逢春心剩生。",
        "妾如台上镜，",
        "得照始分明。"
      ]
    }
  ]
}