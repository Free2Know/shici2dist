  "Poems": [
    {
      "_id": null,
      "Name": "答同年李昉见赠次韵",
      "PoetId": "ca2b48608b894046b1337e16cfd3f753",
      "PoetName": "邓洵美",
      "Form": "七律",
      "Tags": [
        "唐代",
        "邓洵美"
      ],
      "Contents": [
        "词场几度让长鞭，",
        "又向清朝贺九迁。",
        "品秩虽然殊此日，",
        "岁寒终不改当年。",
        "驰名早已超三院，",
        "侍直仍忻步八砖。",
        "今日相逢翻自愧，",
        "闲吟对酒倍潸然。"
      ]
    }
  ]
}