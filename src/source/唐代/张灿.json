    {
      "_id": null,
      "Name": "寒食遣怀",
      "PoetId": "4193a53d0e834448a8daa13031ce80ec",
      "PoetName": "张灿",
      "Form": "五律",
      "Tags": [
        "唐代",
        "张灿"
      ],
      "Contents": [
        "繁华泣清露，",
        "悄悄落衣巾。",
        "明日逢寒食，",
        "春风见故人。",
        "病来羞滞楚，",
        "西去欲迷秦。",
        "憔悴此时久，",
        "青山归四邻。"
      ]
    }
  ]
}