{
  "_id": "1e74bec467124e7698c8f190402413bb",
  "Name": "王鲁复",
  "Description": "王鲁复，字梦周，连江人，从事邕府。诗四首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "吊灵均",
      "PoetId": "1e74bec467124e7698c8f190402413bb",
      "PoetName": "王鲁复",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "王鲁复"
      ],
      "Contents": [
        "万古汨罗深，",
        "骚人道不沉。",
        "明明唐日月，",
        "应见楚臣心。"
      ]
    },
    {
      "_id": null,
      "Name": "吊韩侍郎",
      "PoetId": "1e74bec467124e7698c8f190402413bb",
      "PoetName": "王鲁复",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "王鲁复"
      ],
      "Contents": [
        "星落少微宫，",
        "高人入古风。",
        "几年才子泪，",
        "并写五言中。"
      ]
    },
    {
      "_id": null,
      "Name": "故白岩禅师院",
      "PoetId": "1e74bec467124e7698c8f190402413bb",
      "PoetName": "王鲁复",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "王鲁复"
      ],
      "Contents": [
        "能师还世名还在，",
        "空闭禅堂满院苔。",
        "花树不随人寂寞，",
        "数枝犹自出墙来。"
      ]
    },
    {
      "_id": null,
      "Name": "诣李侍郎",
      "PoetId": "1e74bec467124e7698c8f190402413bb",
      "PoetName": "王鲁复",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "王鲁复"
      ],
      "Contents": [
        "文字元无底，",
        "功夫转到难。",
        "苦心三百首，",
        "暂请侍郎看。"
      ]
    }
  ]
}