  "Poems": [
    {
      "_id": null,
      "Name": "又嘲武懿宗",
      "PoetId": "4157cd1edd5841f5a436a958b04589a8",
      "PoetName": "张元一",
      "Form": "未知",
      "Tags": [
        "唐代",
        "张元一"
      ],
      "Contents": [
        "裹头极草草，",
        "掠鬓不菶菶。",
        "未见桃花面皮，",
        "漫作杏子眼孔。"
      ]
    },
    {
      "_id": null,
      "Name": "叙可笑事",
      "PoetId": "4157cd1edd5841f5a436a958b04589a8",
      "PoetName": "张元一",
      "Form": "未知",
      "Tags": [
        "唐代",
        "张元一"
      ],
      "Contents": [
        "朱前疑著绿，",
        "逯仁杰著朱。",
        "阎知微骑马，",
        "马吉甫骑驴。",
        "将名作姓李千里，",
        "将姓作名吴栖梧。",
        "左台胡御史，",
        "右台御史胡。"
      ]
    },
    {
      "_id": null,
      "Name": "咏静乐县主",
      "PoetId": "4157cd1edd5841f5a436a958b04589a8",
      "PoetName": "张元一",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "张元一"
      ],
      "Contents": [
        "马带桃花锦，",
        "裙衔绿草罗。",
        "定知帏帽底，",
        "仪容似大哥。"
      ]
    },
    {
      "_id": null,
      "Name": "嘲武懿宗",
      "PoetId": "4157cd1edd5841f5a436a958b04589a8",
      "PoetName": "张元一",
      "Form": "未知",
      "Tags": [
        "唐代",
        "张元一"
      ],
      "Contents": [
        "长弓短度箭，",
        "蜀马临阶骗。",
        "去贼七百里，",
        "隈墙独自战。",
        "忽然逢著贼，",
        "骑猪向南趣。"
      ]
    }
  ]
}