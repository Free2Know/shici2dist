{
  "_id": "e9910ccbb067466abde89cda8b557b1d",
  "Name": "包何",
  "Description": "包何，字幼嗣，润州延陵人。隔之子。与弟佶齐名，世称二包。登天宝进士第。大历中，为起居舍人。诗一卷。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "同李郎中净律院梡子树",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "五律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "本梡稀难识，",
        "沙门种则生。",
        "叶殊经写字，",
        "子为佛称名。",
        "滤水浇新长，",
        "燃灯暖更荣。",
        "亭亭无别意，",
        "只是劝修行。"
      ]
    },
    {
      "_id": null,
      "Name": "同舍弟佶、班、韦二员外秋苔对之成咏",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "五律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "每看苔藓色，",
        "如向簿书闲。",
        "幽思缠芳树，",
        "高情寄远山。",
        "雨痕连地绿，",
        "日色出林斑。",
        "却笑兴公赋，",
        "临危滑石间。"
      ]
    },
    {
      "_id": null,
      "Name": "同诸公寻李方直不遇",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "闻说到扬州，",
        "吹箫忆旧游。",
        "人来多不见，",
        "莫是上迷楼。"
      ]
    },
    {
      "_id": null,
      "Name": "同阎伯均宿道士观有述",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "七律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "南国佳人去不回，",
        "洛阳才子更须媒。",
        "绮琴白雪无心弄，",
        "罗幌清风到晓开。",
        "冉冉修篁依户牖，",
        "迢迢列宿映楼台。",
        "纵令奔月成仙去，",
        "且作行云入梦来。"
      ]
    },
    {
      "_id": null,
      "Name": "和孟虔州闲斋即事",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "五律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "古郡邻江岭，",
        "公庭半薜萝。",
        "府僚闲不入，",
        "山鸟静偏过。",
        "睥睨临花柳，",
        "栏干枕芰荷。",
        "麦秋今欲至，",
        "君听两岐歌。"
      ]
    },
    {
      "_id": null,
      "Name": "和程员外春日东郊即事",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "七律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "郎官休浣怜迟日，",
        "野老欢娱为有年。",
        "几处折花惊蝶梦，",
        "数家留叶待蚕眠。",
        "藤垂宛地萦珠履，",
        "泉迸侵阶浸绿钱。",
        "直到闭关朝谒去，",
        "莺声不散柳含烟。"
      ]
    },
    {
      "_id": null,
      "Name": "和苗员外寓直中书",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "五律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "朝列称多士，",
        "君家有二难。",
        "贞为台里柏，",
        "芳作省中兰。",
        "夜宿分曹阔，",
        "晨趋接武欢。",
        "每怜双阙下，",
        "雁序入鸳鸾。"
      ]
    },
    {
      "_id": null,
      "Name": "婺州留别邓使君",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "五律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "西掖驰名久，",
        "东阳出守时。",
        "江山婺女分，",
        "风月隐侯诗。",
        "别恨双溪急，",
        "留欢五马迟。",
        "回舟映沙屿，",
        "未远剩相思。"
      ]
    },
    {
      "_id": null,
      "Name": "寄杨侍御",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "一官何幸得同时，",
        "十载无媒独见遗。",
        "今日不论腰下组，",
        "请君看取鬓边丝。"
      ]
    },
    {
      "_id": null,
      "Name": "江上田家",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "五律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "近海川原薄，",
        "人家本自稀。",
        "黍苗期腊酒，",
        "霜叶是寒衣。",
        "市井谁相识，",
        "渔樵夜始归。",
        "不须骑马问，",
        "恐畏狎鸥飞。"
      ]
    },
    {
      "_id": null,
      "Name": "相里使君第七男生日",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "七律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "娶妻生子复生男，",
        "独有君家众所谈。",
        "荀氏八龙唯欠一，",
        "桓山四凤已过三。",
        "他时干蛊声名著，",
        "今日悬弧宴乐酣。",
        "谁道众贤能继体，",
        "须知个个出于蓝。"
      ]
    },
    {
      "_id": null,
      "Name": "裴端公使院，赋得隔帘见春雨",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "五律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "细雨未成霖，",
        "垂帘但觉阴。",
        "唯看上砌湿，",
        "不遣入檐深。",
        "度隙沾霜简，",
        "因风润绮琴。",
        "须移户外屦，",
        "檐溜夜相侵。"
      ]
    },
    {
      "_id": null,
      "Name": "赋得秤，送孟孺卿",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "五律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "愿以金秤锤，",
        "因君赠别离。",
        "钩悬新月吐，",
        "衡举众星随。",
        "掌握须平执，",
        "锱铢必尽知。",
        "由来投分审，",
        "莫放弄权移。"
      ]
    },
    {
      "_id": null,
      "Name": "送乌程王明府贬巴江",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "一片孤帆无四邻，",
        "北风吹过五湖滨。",
        "相看尽是江南客，",
        "独有君为岭外人。"
      ]
    },
    {
      "_id": null,
      "Name": "送泉州李使君之任李使君赴泉州",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "五律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "傍海皆荒服，",
        "分符重汉臣。",
        "云山百越路，",
        "市井十洲人。",
        "执玉来朝远，",
        "还珠入贡频。",
        "连年不见雪，",
        "到处即行春。"
      ]
    },
    {
      "_id": null,
      "Name": "送王汶宰江阴",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "五律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "郡北乘流去，",
        "花间竟日行。",
        "海鱼朝满市，",
        "江鸟夜喧城。",
        "止酒非关病，",
        "援琴不在声。",
        "应缘五斗米，",
        "数日滞渊明。"
      ]
    },
    {
      "_id": null,
      "Name": "送韦侍御奉使江岭诸道催青苗钱",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "五律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "近远从王事，",
        "南行处处经。",
        "手持霜简白，",
        "心在夏苗青。",
        "回雁书应报，",
        "愁猿夜屡听。",
        "因君使绝域，",
        "方物尽来庭。"
      ]
    },
    {
      "_id": null,
      "Name": "长安晓望，寄崔补阙",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "未知",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "迢递山河拥帝京，",
        "参差宫殿接云平。",
        "风吹晓漏经长乐，",
        "柳带晴烟出禁城。",
        "天净笙歌临路发，",
        "日高车马隔尘行。",
        "自怜久滞诸生列，",
        "未得金闺籍姓名。",
        "第二百零九卷"
      ]
    },
    {
      "_id": null,
      "Name": "阙下芙蓉",
      "PoetId": "e9910ccbb067466abde89cda8b557b1d",
      "PoetName": "包何",
      "Form": "七律",
      "Tags": [
        "唐代",
        "包何"
      ],
      "Contents": [
        "一人理国致升平，",
        "万物呈祥助圣明。",
        "天上河从阙下过，",
        "江南花向殿前生。",
        "广云垂荫开难落，",
        "湛露为珠满不倾。",
        "更对乐悬张宴处，",
        "歌工欲奏采莲声。"
      ]
    }
  ]
}