{
  "_id": "cb9fa4727e1f43b59c0f652691f5e351",
  "Name": "何希尧",
  "Description": "何希尧，字唐臣，分水人。诗四首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "一枝花",
      "PoetId": "cb9fa4727e1f43b59c0f652691f5e351",
      "PoetName": "何希尧",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "何希尧"
      ],
      "Contents": [
        "几树晴葩映水开，",
        "乱红狼藉点苍苔。",
        "东风留得残枝在，",
        "为惜余芳独看来。"
      ]
    },
    {
      "_id": null,
      "Name": "操莲曲",
      "PoetId": "cb9fa4727e1f43b59c0f652691f5e351",
      "PoetName": "何希尧",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "何希尧"
      ],
      "Contents": [
        "锦莲浮处水粼粼，",
        "风外香生袜底尘。",
        "荷叶荷裙相映色，",
        "闻歌不见采莲人。"
      ]
    },
    {
      "_id": null,
      "Name": "柳枝词",
      "PoetId": "cb9fa4727e1f43b59c0f652691f5e351",
      "PoetName": "何希尧",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "何希尧"
      ],
      "Contents": [
        "大堤杨柳雨沈沈，",
        "万缕千条惹恨深。",
        "飞絮满天人去远，",
        "东风无力系春心。"
      ]
    },
    {
      "_id": null,
      "Name": "海棠",
      "PoetId": "cb9fa4727e1f43b59c0f652691f5e351",
      "PoetName": "何希尧",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "何希尧"
      ],
      "Contents": [
        "著雨胭脂点点消，",
        "半开时节最妖娆。",
        "谁家更有黄金屋，",
        "深锁东风贮阿娇。"
      ]
    }
  ]
}