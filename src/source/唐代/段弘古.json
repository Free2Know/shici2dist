      "_id": null,
      "Name": "奉陪吕使君楼上夜看花",
      "PoetId": "9108724526664d16b1d64bba759c6aa8",
      "PoetName": "段弘古",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "段弘古"
      ],
      "Contents": [
        "城上芳园花满枝，",
        "城头太守夜看时。",
        "为报林中高举烛，",
        "感人情思欲题诗。"
      ]
    }
  ]
