  "Poems": [
    {
      "_id": null,
      "Name": "悼罗隐",
      "PoetId": "7ccc6843f6954a72999dc4ab6f1346a5",
      "PoetName": "归仁",
      "Form": "七律",
      "Tags": [
        "唐代",
        "归仁"
      ],
      "Contents": [
        "一著谗书未快心，",
        "几抽胸臆纵狂吟。",
        "管中窥豹我犹在，",
        "海上钓鳌君也沈。",
        "岁月尽能消愤懑，",
        "寰区那更有知音。",
        "长安冠盖皆涂地，",
        "仍喜先生葬碧岑。"
      ]
    },
    {
      "_id": null,
      "Name": "牡丹",
      "PoetId": "7ccc6843f6954a72999dc4ab6f1346a5",
      "PoetName": "归仁",
      "Form": "七律",
      "Tags": [
        "唐代",
        "归仁"
      ],
      "Contents": [
        "三春堪惜牡丹奇，",
        "半倚朱栏欲绽时。",
        "天下更无花胜此，",
        "人间偏得贵相宜。",
        "偷香黑蚁斜穿叶，",
        "觑蕊黄蜂倒挂枝。",
        "除却解禅心不动，",
        "算应狂杀五陵儿。"
      ]
    },
    {
      "_id": null,
      "Name": "自遣",
      "PoetId": "7ccc6843f6954a72999dc4ab6f1346a5",
      "PoetName": "归仁",
      "Form": "五律",
      "Tags": [
        "唐代",
        "归仁"
      ],
      "Contents": [
        "日日为诗苦，",
        "谁论春与秋。",
        "一联如得意，",
        "万事总忘忧。",
        "雨堕花临砌，",
        "风吹竹近楼。",
        "不吟头也白，",
        "任白此生头。"
      ]
    },
    {
      "_id": null,
      "Name": "酬沈先辈卷",
      "PoetId": "7ccc6843f6954a72999dc4ab6f1346a5",
      "PoetName": "归仁",
      "Form": "五律",
      "Tags": [
        "唐代",
        "归仁"
      ],
      "Contents": [
        "一百八十首，",
        "清泠韵可敲。",
        "任从人不爱，",
        "终是我难抛。",
        "桂魄吟来满，",
        "蒲团坐得凹。",
        "先生声价在，",
        "寰宇几人抄。"
      ]
    },
    {
      "_id": null,
      "Name": "题楚庙",
      "PoetId": "7ccc6843f6954a72999dc4ab6f1346a5",
      "PoetName": "归仁",
      "Form": "七律",
      "Tags": [
        "唐代",
        "归仁"
      ],
      "Contents": [
        "羞容难更返江东，",
        "谁问从来百战功。",
        "天地有心归道德，",
        "山河无力为英雄。",
        "芦花尚认霜戈白，",
        "海日犹思火阵红。",
        "也是男儿成败事，",
        "不须惆怅对西风。"
      ]
    },
    {
      "_id": null,
      "Name": "题贾岛吟诗台",
      "PoetId": "7ccc6843f6954a72999dc4ab6f1346a5",
      "PoetName": "归仁",
      "Form": "五律",
      "Tags": [
        "唐代",
        "归仁"
      ],
      "Contents": [
        "此台如可废，",
        "此恨有谁平。",
        "纵使迷青草，",
        "终难没旧名。",
        "天悲朝雨色，",
        "岳哭夜猿声。",
        "不是心偏苦，",
        "应关自古情。"
      ]
    }
  ]
}