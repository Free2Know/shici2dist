  "_id": "bdc2e6e198f4484e84a88b820cbba41f",
  "Name": "黄巢",
  "Description": "唐末农民起义领袖。冤句人。举进士不第，广明作乱，破京都。后灭于泰山狼虎谷。诗三首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "不第后赋菊",
      "PoetId": "bdc2e6e198f4484e84a88b820cbba41f",
      "PoetName": "黄巢",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "黄巢"
      ],
      "Contents": [
        "待到秋来九月八，",
        "我花开后百花杀。",
        "冲天香阵透长安，",
        "满城尽带黄金甲。"
      ]
    },
    {
      "_id": null,
      "Name": "自题像",
      "PoetId": "bdc2e6e198f4484e84a88b820cbba41f",
      "PoetName": "黄巢",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "黄巢"
      ],
      "Contents": [
        "记得当年草上飞，",
        "铁衣著尽著僧衣。",
        "天津桥上无人识，",
        "独倚栏干看落晖。"
      ]
    },
    {
      "_id": null,
      "Name": "题菊花",
      "PoetId": "bdc2e6e198f4484e84a88b820cbba41f",
      "PoetName": "黄巢",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "黄巢"
      ],
      "Contents": [
        "飒飒西风满院栽，",
        "蕊寒香冷蝶难来。",
        "他年我若为青帝，",
        "报与桃花一处开。"
      ]
    }
  ]
}