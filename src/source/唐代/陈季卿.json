  "Poems": [
    {
      "_id": null,
      "Name": "别兄弟",
      "PoetId": "9f328ed1130a4076b10beedd2ab19bbc",
      "PoetName": "陈季卿",
      "Form": "五律",
      "Tags": [
        "唐代",
        "陈季卿"
      ],
      "Contents": [
        "谋身非不早，",
        "其奈命来迟。",
        "旧友皆霄汉，",
        "此身犹路岐。",
        "北风微雪后，",
        "晚景有云时。",
        "惆怅清江上，",
        "区区趁试期。"
      ]
    },
    {
      "_id": null,
      "Name": "别妻",
      "PoetId": "9f328ed1130a4076b10beedd2ab19bbc",
      "PoetName": "陈季卿",
      "Form": "五律",
      "Tags": [
        "唐代",
        "陈季卿"
      ],
      "Contents": [
        "月斜寒露白，",
        "此夕去留心。",
        "酒至添愁饮，",
        "诗成和泪吟。",
        "离歌凄凤管，",
        "别鹤怨瑶琴。",
        "明夜相思处，",
        "秋风吹半衾。"
      ]
    },
    {
      "_id": null,
      "Name": "江亭晚望题书斋",
      "PoetId": "9f328ed1130a4076b10beedd2ab19bbc",
      "PoetName": "陈季卿",
      "Form": "七律",
      "Tags": [
        "唐代",
        "陈季卿"
      ],
      "Contents": [
        "立向江亭满目愁，",
        "十年前事信悠悠。",
        "田园已逐浮云散，",
        "乡里半随逝水流。",
        "川上莫逢诸钓叟，",
        "浦边难得旧沙鸥。",
        "不缘齿发未迟暮，",
        "吟对远山堪白头。"
      ]
    },
    {
      "_id": null,
      "Name": "题潼关普通院门",
      "PoetId": "9f328ed1130a4076b10beedd2ab19bbc",
      "PoetName": "陈季卿",
      "Form": "五律",
      "Tags": [
        "唐代",
        "陈季卿"
      ],
      "Contents": [
        "度关悲失志，",
        "万绪乱心机。",
        "下坂马无力，",
        "扫门尘满衣。",
        "计谋多不就，",
        "心口自相违。",
        "已作羞归计，",
        "还胜羞不归。"
      ]
    },
    {
      "_id": null,
      "Name": "题禅窟兰若",
      "PoetId": "9f328ed1130a4076b10beedd2ab19bbc",
      "PoetName": "陈季卿",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "陈季卿"
      ],
      "Contents": [
        "霜钟鸣时夕风急，",
        "乱鸦又望寒林集。",
        "此时辍棹悲且吟，",
        "独向莲华一峰立。"
      ]
    }
  ]
}