  "Name": "高正臣",
  "Description": "高正臣，广平人，襄州刺史、卫尉卿。习右军书法，睿宗最爱其笔。诗二首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "晦日置酒林亭",
      "PoetId": "d634708215044c9da26662182e963129",
      "PoetName": "高正臣",
      "Form": "五律",
      "Tags": [
        "唐代",
        "高正臣"
      ],
      "Contents": [
        "正月符嘉节，",
        "三春玩物华。",
        "忘怀寄尊酒，",
        "陶性狎山家。",
        "柳翠含烟叶，",
        "梅芳带雪花。",
        "光阴不相借，",
        "迟迟落景斜。"
      ]
    },
    {
      "_id": null,
      "Name": "晦日重宴",
      "PoetId": "d634708215044c9da26662182e963129",
      "PoetName": "高正臣",
      "Form": "五律",
      "Tags": [
        "唐代",
        "高正臣"
      ],
      "Contents": [
        "芳辰重游衍，",
        "乘景共追随。",
        "班荆陪旧识，",
        "倾盖得新知。",
        "水叶分莲沼，",
        "风花落柳枝。",
        "自符河朔趣，",
        "宁羡高阳池。"
      ]
    }
  ]
}