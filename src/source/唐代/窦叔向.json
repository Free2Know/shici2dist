{
  "_id": "cb7383d3fd0e40d188b4e5f9b37e31d0",
  "Name": "窦叔向",
  "Description": "窦叔向，字遗直，京兆人。代宗时，常衮为相，引为左拾遗、内供奉。衮贬，出为溧水令。五子群、常、牟、庠、巩，皆工词章，有《联珠集》行于时。叔向工五言，名冠时辈。集七卷，今存诗九首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "句",
      "PoetId": "cb7383d3fd0e40d188b4e5f9b37e31d0",
      "PoetName": "窦叔向",
      "Form": "未知",
      "Tags": [
        "唐代",
        "窦叔向"
      ],
      "Contents": [
        "禁兵环素帟，",
        "宫女哭寒云。",
        "（《哀挽》第三首，",
        "止存二句。",
        "见《联珠集叙》）。"
      ]
    },
    {
      "_id": null,
      "Name": "夏夜宿表兄话旧",
      "PoetId": "cb7383d3fd0e40d188b4e5f9b37e31d0",
      "PoetName": "窦叔向",
      "Form": "七律",
      "Tags": [
        "唐代",
        "窦叔向"
      ],
      "Contents": [
        "夜合花开香满庭，",
        "夜深微雨醉初醒。",
        "远书珍重何曾达，",
        "旧事凄凉不可听。",
        "去日儿童皆长大，",
        "昔年亲友半凋零。",
        "明朝又是孤舟别，",
        "愁见河桥酒幔青。"
      ]
    },
    {
      "_id": null,
      "Name": "寒食日恩赐火",
      "PoetId": "cb7383d3fd0e40d188b4e5f9b37e31d0",
      "PoetName": "窦叔向",
      "Form": "未知",
      "Tags": [
        "唐代",
        "窦叔向"
      ],
      "Contents": [
        "恩光及小臣，",
        "华烛忽惊春。",
        "电影随中使，",
        "星辉拂路人。",
        "幸因榆柳暖，",
        "一照草茅贫。"
      ]
    },
    {
      "_id": null,
      "Name": "春日早朝应制",
      "PoetId": "cb7383d3fd0e40d188b4e5f9b37e31d0",
      "PoetName": "窦叔向",
      "Form": "五律",
      "Tags": [
        "唐代",
        "窦叔向"
      ],
      "Contents": [
        "紫殿俯千官，",
        "春松应合欢。",
        "御炉香焰暖，",
        "驰道玉声寒。",
        "乳燕翻珠缀，",
        "祥乌集露盘。",
        "宫花一万树，",
        "不敢举头看。"
      ]
    },
    {
      "_id": null,
      "Name": "秋砧送邑大夫",
      "PoetId": "cb7383d3fd0e40d188b4e5f9b37e31d0",
      "PoetName": "窦叔向",
      "Form": "五律",
      "Tags": [
        "唐代",
        "窦叔向"
      ],
      "Contents": [
        "断续长门下，",
        "清泠逆旅秋。",
        "征夫应待信，",
        "寒女不胜愁。",
        "带月飞城上，",
        "因风散陌头。",
        "离居偏入听，",
        "况复送归舟。"
      ]
    },
    {
      "_id": null,
      "Name": "端午日恩赐百索",
      "PoetId": "cb7383d3fd0e40d188b4e5f9b37e31d0",
      "PoetName": "窦叔向",
      "Form": "未知",
      "Tags": [
        "唐代",
        "窦叔向"
      ],
      "Contents": [
        "仙宫长命缕，",
        "端午降殊私。",
        "事盛蛟龙见，",
        "恩深犬马知。",
        "馀生倘可续，",
        "终冀答明时。"
      ]
    },
    {
      "_id": null,
      "Name": "贞懿皇后挽歌三首",
      "PoetId": "cb7383d3fd0e40d188b4e5f9b37e31d0",
      "PoetName": "窦叔向",
      "Form": "未知",
      "Tags": [
        "唐代",
        "窦叔向"
      ],
      "Contents": [
        "二陵恭妇道，",
        "六寝盛皇情。",
        "礼逊生前贵，",
        "恩追殁后荣。",
        "幼王亲捧土，",
        "爱女复边茔。",
        "东望长如在，",
        "谁云向玉京。",
        "后庭攀画柳，",
        "上陌咽清笳。",
        "命妇羞蘋叶，",
        "都人插柰花。",
        "寿宫星月异，",
        "仙路往来赊。",
        "纵有迎仙术，",
        "终悲隔绛纱。"
      ]
    },
    {
      "_id": null,
      "Name": "过担石湖",
      "PoetId": "cb7383d3fd0e40d188b4e5f9b37e31d0",
      "PoetName": "窦叔向",
      "Form": "五律",
      "Tags": [
        "唐代",
        "窦叔向"
      ],
      "Contents": [
        "晓发渔门戍，",
        "晴看担石湖。",
        "日衔高浪出，",
        "天入四空无。",
        "尺寸分洲岛，",
        "纤毫指舳舻。",
        "渺然从此去，",
        "谁念客帆孤。"
      ]
    },
    {
      "_id": null,
      "Name": "酬李袁州嘉祐",
      "PoetId": "cb7383d3fd0e40d188b4e5f9b37e31d0",
      "PoetName": "窦叔向",
      "Form": "七律",
      "Tags": [
        "唐代",
        "窦叔向"
      ],
      "Contents": [
        "少年轻会复轻离，",
        "老大关心总是悲。",
        "强说前程聊自慰，",
        "未知携手定何时。",
        "公才屈指登黄阁，",
        "匪服胡颜上赤墀。",
        "想到长安诵佳句，",
        "满朝谁不念琼枝。"
      ]
    },
    {
      "_id": null,
      "Name": "青阳馆望九子山",
      "PoetId": "cb7383d3fd0e40d188b4e5f9b37e31d0",
      "PoetName": "窦叔向",
      "Form": "七律",
      "Tags": [
        "唐代",
        "窦叔向"
      ],
      "Contents": [
        "苍翠岧峣上碧天，",
        "九峰遥落县门前。",
        "毫芒映日千重树，",
        "涓滴垂空万丈泉。",
        "武帝南游曾驻跸，",
        "始皇东幸亦祈年。",
        "云祠绝迹终难访，",
        "唯有猿声到客边。"
      ]
    }
  ]
}