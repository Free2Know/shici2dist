{
  "_id": "63621069271e4fdc85f426a8e0f9c92d",
  "Name": "韦安石",
  "Description": "韦安石，京兆万年人。举明经，久视中，以鸾台侍郎同凤阁鸾台平章事，数折辱二张、三思，复相中宗，不附太平公主。睿宗时，为姜皎所构，贬卒。诗三首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "侍宴旋师喜捷应制",
      "PoetId": "63621069271e4fdc85f426a8e0f9c92d",
      "PoetName": "韦安石",
      "Form": "五律",
      "Tags": [
        "唐代",
        "韦安石"
      ],
      "Contents": [
        "蜂蚁屯夷落，",
        "熊罴逐汉飞。",
        "忘躯百战后，",
        "屈指一年归。",
        "厚眷纾天藻，",
        "深慈解御衣。",
        "兴酣歌舞出，",
        "朝野叹光辉。"
      ]
    },
    {
      "_id": null,
      "Name": "奉和九日幸临渭亭登高应制得枝字",
      "PoetId": "63621069271e4fdc85f426a8e0f9c92d",
      "PoetName": "韦安石",
      "Form": "五律",
      "Tags": [
        "唐代",
        "韦安石"
      ],
      "Contents": [
        "重九开秋节，",
        "得一动宸仪。",
        "金风飘菊蕊，",
        "玉露泫萸枝。",
        "睿览八纮外，",
        "天文七曜披。",
        "临深应在即，",
        "居高岂忘危。"
      ]
    },
    {
      "_id": null,
      "Name": "梁王宅侍宴应制同用风字",
      "PoetId": "63621069271e4fdc85f426a8e0f9c92d",
      "PoetName": "韦安石",
      "Form": "五律",
      "Tags": [
        "唐代",
        "韦安石"
      ],
      "Contents": [
        "梁园开胜景，",
        "轩驾动宸衷。",
        "早荷承湛露，",
        "修竹引薰风。",
        "九酝倾钟石，",
        "百兽协丝桐。",
        "小臣陪宴镐，",
        "献寿奉维嵩。"
      ]
    }
  ]
}