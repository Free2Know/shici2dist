  "Poems": [
    {
      "_id": null,
      "Name": "和武相公中秋夜西蜀锦楼望月得清字",
      "PoetId": "1c5294cec55b414284b43d39d1ba9f7e",
      "PoetName": "王良会",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王良会"
      ],
      "Contents": [
        "德星摇此夜，",
        "珥耳满重城。",
        "杳霭烟氛色，",
        "飘飖砧杵声。",
        "令行秋气爽，",
        "乐感素风轻。",
        "共赏千年圣，",
        "长歌四海清。"
      ]
    }
  ]
}