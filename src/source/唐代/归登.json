      "_id": null,
      "Name": "郊庙歌辞·享惠昭太子庙乐章·请神",
      "PoetId": "ffc772a7b117477eb63f6c6a00c0b670",
      "PoetName": "归登",
      "Form": "未知",
      "Tags": [
        "唐代",
        "归登"
      ],
      "Contents": [
        "嘉荐既陈，",
        "祀事孔明。",
        "闲歌在堂，",
        "万舞在庭。",
        "外则尽物，",
        "内则尽诚。",
        "凤笙如闻，",
        "歌其洁精。"
      ]
    }
