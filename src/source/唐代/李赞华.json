      "_id": null,
      "Name": "立木海上刻诗",
      "PoetId": "e2d7bf7cf52c4ea08228d176ff013005",
      "PoetName": "李赞华",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "李赞华"
      ],
      "Contents": [
        "小山压大山，",
        "大山全无力。",
        "羞见故乡人，",
        "从此投外国。"
      ]
    }
  ]
