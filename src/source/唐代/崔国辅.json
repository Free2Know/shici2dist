{
  "_id": "78514e6eb3c9498a8598b617714c2caf",
  "Name": "崔国辅",
  "Description": "崔国辅（公元678－755年），吴郡人（因曾任山阴县尉，被",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "七夕",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "太守仙潢族，",
        "含情七夕多。",
        "扇风生玉漏，",
        "置水写银河。",
        "阁下陈书籍，",
        "闺中曝绮罗。",
        "遥思汉武帝，",
        "青鸟几时过？"
      ]
    },
    {
      "_id": null,
      "Name": "中流曲",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "归时日尚早，",
        "更欲向芳洲。",
        "渡口水流急，",
        "回船不自由。"
      ]
    },
    {
      "_id": null,
      "Name": "丽人曲",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "红颜称绝代，",
        "欲并真无侣。",
        "独有镜中人，",
        "由来自相许。"
      ]
    },
    {
      "_id": null,
      "Name": "九日侍宴应制",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "词",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "运偶千年圣，",
        "时传九日神。",
        "尧樽列钟鼓，",
        "汉阙辟钩陈。",
        "金箓三清降，",
        "琼筵五老巡。",
        "始惊兰佩出，",
        "复咏柏梁新。",
        "云雁楼前晚，",
        "霜花酒里春。",
        "欢娱无限极，",
        "书剑太平人。"
      ]
    },
    {
      "_id": null,
      "Name": "九月九日",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "江边枫落菊花黄，",
        "少长登高一望乡。",
        "九日陶家虽载酒，",
        "三年楚客已沾裳。"
      ]
    },
    {
      "_id": null,
      "Name": "今别离",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "送别未能旋，",
        "相望连水口。",
        "船行欲映洲，",
        "几度急摇手。"
      ]
    },
    {
      "_id": null,
      "Name": "从军行",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "塞北胡霜下，",
        "营州索兵救。",
        "夜里偷道行，",
        "将军马亦瘦。",
        "刀光照塞月，",
        "阵色明如昼。",
        "传闻贼满山，",
        "已共前锋斗。"
      ]
    },
    {
      "_id": null,
      "Name": "卫艳词",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "淇上桑叶青，",
        "青楼含白日。",
        "比时遥望君，",
        "车马城中出。"
      ]
    },
    {
      "_id": null,
      "Name": "古意",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "净扫黄金阶，",
        "飞霜皎如雪。",
        "下帘弹箜篌，",
        "不忍见秋月。"
      ]
    },
    {
      "_id": null,
      "Name": "古意二首",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "玉笼薰绣裳，",
        "着罢眠洞房。",
        "不能春风里，",
        "吹却兰麝香。",
        "种棘遮蘼芜，",
        "畏人来采杀。",
        "比至狂夫还，",
        "看看几花发。"
      ]
    },
    {
      "_id": null,
      "Name": "奉和华清宫观行香应制",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "天子蕊珠宫，",
        "楼台碧落通。",
        "豫游皆汗漫，",
        "斋宫即崆峒。",
        "云物三光里，",
        "君臣一气中。",
        "道言何所说，",
        "宝历自无穷。"
      ]
    },
    {
      "_id": null,
      "Name": "奉和圣制上巳祓禊应制",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "未知",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "元巳秦中节，",
        "吾君灞上游。",
        "鸣銮通禁苑，",
        "别馆绕芳洲。",
        "鹓鹭千官列，",
        "鱼龙百戏浮。",
        "桃花春欲尽，",
        "谷雨夜来收。",
        "庆向尧樽祝，",
        "欢从楚棹讴。",
        "逸诗何足对，",
        "窅作掩东周。"
      ]
    },
    {
      "_id": null,
      "Name": "妾薄命",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "虽入秦帝宫，",
        "不上秦帝床。",
        "夜夜玉窗里，",
        "与他卷罗裳。"
      ]
    },
    {
      "_id": null,
      "Name": "子夜冬歌",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "寂寥抱冬心，",
        "裁罗又褧褧。",
        "夜久频挑灯，",
        "霜寒剪刀冷。"
      ]
    },
    {
      "_id": null,
      "Name": "宿法华寺",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "松雨时复滴，",
        "寺门清且凉。",
        "此心竟谁证，",
        "回憩支公床。",
        "壁画感灵迹，",
        "龛经传异香。",
        "独游寄象外，",
        "忽忽归南昌。"
      ]
    },
    {
      "_id": null,
      "Name": "宿范浦",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "月暗潮又落，",
        "西陵渡暂停。",
        "村烟和海雾，",
        "舟火乱江星。",
        "路转定山绕，",
        "塘连范浦横。",
        "鸱夷近何去，",
        "空山临沧溟。"
      ]
    },
    {
      "_id": null,
      "Name": "对酒吟",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "未知",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "行行日将夕，",
        "荒村古冢无人迹。",
        "朦胧荆棘一鸟吟，",
        "屡唱提壶沽酒吃。",
        "古人不达酒不足，",
        "遗恨精灵传此曲。",
        "寄言世上诸少年，",
        "平生且尽杯中(酉录)。"
      ]
    },
    {
      "_id": null,
      "Name": "小长干曲",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "月暗送湖风，",
        "相寻路不通。",
        "菱歌唱不辍，",
        "知在此塘中。"
      ]
    },
    {
      "_id": null,
      "Name": "少年行",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "遗却珊瑚鞭，",
        "白马骄不行。",
        "章台折杨柳，",
        "春日路傍情。"
      ]
    },
    {
      "_id": null,
      "Name": "怨词二首",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "妾有罗衣裳，",
        "秦王在时作。",
        "为舞春风多，",
        "秋来不堪着。",
        "楼头桃李疏，",
        "池上芙蓉落。",
        "织锦犹未成，",
        "跫声入罗幕。"
      ]
    },
    {
      "_id": null,
      "Name": "杂诗",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "逢著平乐儿，",
        "论交鞍马前。",
        "与酤一斗酒，",
        "恰用十千钱。",
        "后余在关内，",
        "作事多迍邅。",
        "何肯相救援，",
        "徒闻宝剑篇。"
      ]
    },
    {
      "_id": null,
      "Name": "杭州北郭戴氏荷池送侯愉",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "未知",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "秋近万物肃，",
        "况当临水时。",
        "折花赠归客，",
        "离绪断荷丝。",
        "谁谓江国永，",
        "故人感在兹。",
        "道存过北郭，",
        "情极望东菑。",
        "乔木故园意，",
        "鸣蝉穷巷悲。",
        "扁舟竟何待，",
        "中路每迟迟。"
      ]
    },
    {
      "_id": null,
      "Name": "渭水西别李仑",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "陇右长亭堠，",
        "山阴古塞秋。",
        "不知呜咽水，",
        "何事向西流。"
      ]
    },
    {
      "_id": null,
      "Name": "湖南曲",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "湖南送君去，",
        "湖北送君归。",
        "湖里鸳鸯鸟，",
        "双双他自飞。"
      ]
    },
    {
      "_id": null,
      "Name": "漂母岸",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "未知",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "泗水入淮处，",
        "南边古岸存。",
        "秦时有漂母，",
        "于此饭王孙。",
        "王孙初未遇，",
        "寄食何足论。",
        "后为楚王来，",
        "黄金答母恩。",
        "事迹遗在此，",
        "空伤千载魂。",
        "茫茫水中渚，",
        "上有一孤墩。",
        "遥望不可到，",
        "苍苍烟树昏。",
        "几年崩冢色，",
        "每日落潮痕。",
        "古地多堙圮，",
        "时哉不敢言。",
        "向夕泪沾裳，",
        "只宿芦洲村。"
      ]
    },
    {
      "_id": null,
      "Name": "王孙游",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "自与王孙别，",
        "频看黄鸟飞。",
        "应由春草误，",
        "着处不成归。"
      ]
    },
    {
      "_id": null,
      "Name": "王昭君",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "一回望月一回悲，",
        "望月月移人不移。",
        "何时得见汉朝使，",
        "为妾传书斩画师。"
      ]
    },
    {
      "_id": null,
      "Name": "白纻辞二首",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "七律",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "洛阳梨花落如霰，",
        "河阳桃叶生复齐。",
        "坐惜玉楼春欲尽，",
        "红绵粉絮裛妆啼。",
        "董贤女弟在椒风，",
        "窈窕繁华贵后宫。",
        "璧带金釭皆翡翠，",
        "一朝零落变成空。"
      ]
    },
    {
      "_id": null,
      "Name": "相和歌辞·子夜冬歌",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "寂寥抱冬心，",
        "裁罗又絅絅。",
        "夜久频挑灯，",
        "霜寒剪刀冷。"
      ]
    },
    {
      "_id": null,
      "Name": "相和歌辞·怨诗二首",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "楼前桃李疏，",
        "池上芙蓉落。",
        "织锦犹未成，",
        "虫声入罗幕。",
        "妾有罗衣裳，",
        "秦王在时作。",
        "为舞春风多，",
        "秋来不堪著。"
      ]
    },
    {
      "_id": null,
      "Name": "相和歌辞·采莲曲",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "玉溆花红发，",
        "金塘水碧流。",
        "相逢畏相失，",
        "并着采莲舟。"
      ]
    },
    {
      "_id": null,
      "Name": "石头滩作",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "未知",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "怅矣秋风时，",
        "余临石头濑。",
        "因高见远境，",
        "尽此数州内。",
        "羽山数点青，",
        "海岸杂光碎。",
        "离离树木少，",
        "漭漭波潮大。",
        "日暮千里帆，",
        "南飞落天外。",
        "须臾遂入夜，",
        "楚色有微霭。",
        "寻源迹已穷，",
        "遗荣事多昧。",
        "一身犹未理，",
        "安得济时代。",
        "且泛朝夕潮，",
        "荷衣蕙为带。"
      ]
    },
    {
      "_id": null,
      "Name": "襄阳曲二首",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔国辅"
      ],
      "Contents": [
        "蕙草娇红萼，",
        "时光舞碧鸡。",
        "城中美年少，",
        "相见白铜鞮。",
        "少年襄阳地，",
        "来往襄阳城。",
        "城中轻薄子，",
        "知妾解秦筝。"
      ]
    },
    {
      "_id": null,
      "Name": "送韩十四被鲁王推递往济南府",
      "PoetId": "78514e6eb3c9498a8598b617714c2caf",
      "PoetName": "崔国辅",
      "Form": "五绝",
