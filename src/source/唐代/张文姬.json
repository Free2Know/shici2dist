{
  "_id": "20929c84259e47bca40691ecdd79687e",
  "Name": "张文姬",
  "Description": "张文姬，鲍参军妻也。诗四首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "双槿树",
      "PoetId": "20929c84259e47bca40691ecdd79687e",
      "PoetName": "张文姬",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "张文姬"
      ],
      "Contents": [
        "绿影竞扶疏，",
        "红姿相照灼。",
        "不学桃李花，",
        "乱向春风落。"
      ]
    },
    {
      "_id": null,
      "Name": "池上竹",
      "PoetId": "20929c84259e47bca40691ecdd79687e",
      "PoetName": "张文姬",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "张文姬"
      ],
      "Contents": [
        "此君临此池，",
        "枝低水相近。",
        "碧色绿波中，",
        "日日流不尽。"
      ]
    },
    {
      "_id": null,
      "Name": "沙上鹭",
      "PoetId": "20929c84259e47bca40691ecdd79687e",
      "PoetName": "张文姬",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "张文姬"
      ],
      "Contents": [
        "沙头一水禽，",
        "鼓翼扬清音。",
        "只待高风便，",
        "非无云汉心。"
      ]
    },
    {
      "_id": null,
      "Name": "溪口云",
      "PoetId": "20929c84259e47bca40691ecdd79687e",
      "PoetName": "张文姬",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "张文姬"
      ],
      "Contents": [
        "溶溶溪口云，",
        "才向溪中吐。",
        "不复归溪中，",
        "还作溪中雨。"
      ]
    }
  ]
}