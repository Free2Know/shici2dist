    {
      "_id": null,
      "Name": "哭李远",
      "PoetId": "5f9df9e625714566820077c3bd4042ad",
      "PoetName": "卢尚书",
      "Form": "七律",
      "Tags": [
        "唐代",
        "卢尚书"
      ],
      "Contents": [
        "昨日舟还浙水湄，",
        "今朝丹旐欲何为。",
        "才收北浦一竿钓，",
        "未了西斋半局棋。",
        "洛下已传平子赋，",
        "临川争写谢公诗。",
        "不堪旧里经行处，",
        "风木萧萧邻笛悲。"
      ]
    },
    {
      "_id": null,
      "Name": "题安国观",
      "PoetId": "5f9df9e625714566820077c3bd4042ad",
      "PoetName": "卢尚书",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "卢尚书"
      ],
      "Contents": [
        "夕照纱窗起暗尘，",
        "青松绕殿不知春。",
        "君看白首诵经者，",
        "半是宫中歌舞人。"
      ]
    }
  ]
}