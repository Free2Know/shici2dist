    {
      "_id": null,
      "Name": "鬻丹砂醉吟",
      "PoetId": "baaa4d4779694a7faa9169cb3947d679",
      "PoetName": "刘道昌",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "刘道昌"
      ],
      "Contents": [
        "心田但使灵芝长，",
        "气海常教法水朝。",
        "功满自然留不住，",
        "更将何物驭丹霄。"
      ]
    },
    {
      "_id": null,
      "Name": "龟市告别",
      "PoetId": "baaa4d4779694a7faa9169cb3947d679",
      "PoetName": "刘道昌",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "刘道昌"
      ],
      "Contents": [
        "还丹功满气成胎，",
        "九百年来混俗埃。",
        "自此三山一归去，",
        "无因重到世间来。"
      ]
    }
  ]
}