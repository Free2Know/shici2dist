  "Poems": [
    {
      "_id": null,
      "Name": "入塞曲",
      "PoetId": "da1944a5588e413d8b95e3335fed313b",
      "PoetName": "郑鏦",
      "Form": "五律",
      "Tags": [
        "唐代",
        "郑鏦"
      ],
      "Contents": [
        "留滞边庭久，",
        "归思岁月赊。",
        "黄云同入塞，",
        "白首独还家。",
        "宛马随秦草，",
        "胡人问汉花。",
        "还伤李都尉，",
        "独自没黄沙。"
      ]
    },
    {
      "_id": null,
      "Name": "婕妤怨",
      "PoetId": "da1944a5588e413d8b95e3335fed313b",
      "PoetName": "郑鏦",
      "Form": "五律",
      "Tags": [
        "唐代",
        "郑鏦"
      ],
      "Contents": [
        "南国承欢日，",
        "东方候晓时。",
        "那能妒褒姒，",
        "只爱笑唐儿。",
        "宝叶随云髻，",
        "珠丝锻履綦。",
        "不知飞燕意，",
        "何事苦相疑。"
      ]
    },
    {
      "_id": null,
      "Name": "玉阶怨",
      "PoetId": "da1944a5588e413d8b95e3335fed313b",
      "PoetName": "郑鏦",
      "Form": "五律",
      "Tags": [
        "唐代",
        "郑鏦"
      ],
      "Contents": [
        "昔日同飞燕，",
        "今朝似伯劳。",
        "情深争掷果，",
        "宠罢怨残桃。",
        "别殿春心断，",
        "长门夜树高。",
        "虽能不自悔，",
        "谁见旧衣褒。"
      ]
    },
    {
      "_id": null,
      "Name": "邯郸侠少年",
      "PoetId": "da1944a5588e413d8b95e3335fed313b",
      "PoetName": "郑鏦",
      "Form": "五律",
      "Tags": [
        "唐代",
        "郑鏦"
      ],
      "Contents": [
        "夜渡浊河津，",
        "衣中剑满身。",
        "兵符劫晋鄙，",
        "匕首刺秦人。",
        "执事非无胆，",
        "高堂念有亲。",
        "昨缘秦苦赵，",
        "来往大梁频。"
      ]
    }
  ]
}