    {
      "_id": null,
      "Name": "早行",
      "PoetId": "ec89a0cdf1d746f2b864e1a0a37427a7",
      "PoetName": "郭良",
      "Form": "五律",
      "Tags": [
        "唐代",
        "郭良"
      ],
      "Contents": [
        "早行星尚在，",
        "数里未天明。",
        "不辨云林色，",
        "空闻风水声。",
        "月从山上落，",
        "河入斗间横。",
        "渐至重门外，",
        "依稀见洛城。"
      ]
    },
    {
      "_id": null,
      "Name": "题李将军山亭",
      "PoetId": "ec89a0cdf1d746f2b864e1a0a37427a7",
      "PoetName": "郭良",
      "Form": "五律",
      "Tags": [
        "唐代",
        "郭良"
      ],
      "Contents": [
        "凤辖将军位，",
        "龙门司隶家。",
        "衣冠为隐逸，",
        "山水作繁华。",
        "径出重林草，",
        "池摇两岸花。",
        "谁知贵公第，",
        "亭院有烟霞。"
      ]
    }
  ]
}