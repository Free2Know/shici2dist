  "Name": "李敬伯",
  "Description": "李敬伯，西川观察巡官，试大理评事。诗二首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "和段相公登武担寺西台",
      "PoetId": "877aa21b8fbd4db6ab1162c00b6edf4c",
      "PoetName": "李敬伯",
      "Form": "五律",
      "Tags": [
        "唐代",
        "李敬伯"
      ],
      "Contents": [
        "台上起凉风，",
        "乘闲览岁功。",
        "自随台席贵，",
        "尽许羽觞同。",
        "楼殿斜晖照，",
        "江山极望通。",
        "赋诗思共乐，",
        "俱得咏诗丰。"
      ]
    },
    {
      "_id": null,
      "Name": "奉陪段相公晚夏登张仪楼",
      "PoetId": "877aa21b8fbd4db6ab1162c00b6edf4c",
      "PoetName": "李敬伯",
      "Form": "五律",
      "Tags": [
        "唐代",
        "李敬伯"
      ],
      "Contents": [
        "层屋架城隈，",
        "宾筵此日开。",
        "文锋摧八阵，",
        "星分应三台。",
        "望雪烦襟释，",
        "当欢远思来。",
        "披云霄汉近，",
        "暂觉出尘埃。"
      ]
    }
  ]
}