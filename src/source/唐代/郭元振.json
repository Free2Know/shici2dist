  "Poems": [
    {
      "_id": null,
      "Name": "杂曲歌辞·春江曲",
      "PoetId": "de6c726aa47a417dbaa1723c5ec184df",
      "PoetName": "郭元振",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "郭元振"
      ],
      "Contents": [
        "江水春沉沉，",
        "上有双竹林。",
        "竹叶坏水色，",
        "郎亦坏人心。"
      ]
    },
    {
      "_id": null,
      "Name": "相和歌辞·子夜四时歌六首·冬歌二首",
      "PoetId": "de6c726aa47a417dbaa1723c5ec184df",
      "PoetName": "郭元振",
      "Form": "五律",
      "Tags": [
        "唐代",
        "郭元振"
      ],
      "Contents": [
        "北极严气升，",
        "南至温风谢。",
        "调丝竞短歌，",
        "拂枕怜长夜。",
        "帷横双翡翠，",
        "被卷两鸳鸯。",
        "婉态不自得，",
        "宛转君王床。"
      ]
    },
    {
      "_id": null,
      "Name": "相和歌辞·子夜四时歌六首·春歌二首",
      "PoetId": "de6c726aa47a417dbaa1723c5ec184df",
      "PoetName": "郭元振",
      "Form": "五律",
      "Tags": [
        "唐代",
        "郭元振"
      ],
      "Contents": [
        "青楼含日光，",
        "绿池起风色。",
        "赠子同心花，",
        "殷勤此何极。",
        "陌头杨柳枝，",
        "已被春风吹。",
        "妾心正断绝，",
        "君怀那得知。"
      ]
    },
    {
      "_id": null,
      "Name": "相和歌辞·子夜四时歌六首·秋歌二首",
      "PoetId": "de6c726aa47a417dbaa1723c5ec184df",
      "PoetName": "郭元振",
      "Form": "五律",
      "Tags": [
        "唐代",
        "郭元振"
      ],
      "Contents": [
        "邀欢空伫立，",
        "望美频回顾。",
        "何时复采菱，",
        "江中密相遇。",
        "辟恶茱萸囊，",
        "延年菊花酒。",
        "与子结绸缪，",
        "丹心此何有。"
      ]
    }
  ]
}