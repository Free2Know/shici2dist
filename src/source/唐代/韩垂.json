      "_id": null,
      "Name": "题金山",
      "PoetId": "8171ac2f3897486785c3e617f137cf04",
      "PoetName": "韩垂",
      "Form": "五律",
      "Tags": [
        "唐代",
        "韩垂"
      ],
      "Contents": [
        "灵山一峰秀，",
        "岌然殊众山。",
        "盘根大江底，",
        "插影浮云间。",
        "雷霆常间作，",
        "风雨时往还。",
        "象外悬清影，",
        "千载长跻攀。"
      ]
    }
  ]
}