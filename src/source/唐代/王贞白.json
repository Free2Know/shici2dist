{
  "_id": "79c5c60df76e4ff8b08c56462176b26a",
  "Name": "王贞白",
  "Description": "字有道，永丰人，乾宁二年张贻宪榜进士。后七年，始授校书郎，尝与罗隐、方干、贯休同倡和。有《灵溪集》七卷，今编诗一卷。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "九日长安作",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "无酒泛金菊，",
        "登高但忆秋。",
        "归心随旅雁，",
        "万里在沧洲。",
        "残照明天阙，",
        "孤砧隔御沟。",
        "谁能思落帽，",
        "两鬓已添愁。"
      ]
    },
    {
      "_id": null,
      "Name": "书陶潜醉石",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "片石陶真性，",
        "非为曲蘖昏。",
        "争如累月醉，",
        "不笑独醒人。",
        "积叠莓苔色，",
        "交加薜荔根。",
        "至今重九日，",
        "犹待白衣魂。"
      ]
    },
    {
      "_id": null,
      "Name": "云居长老",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "巘路蹑云上，",
        "来参出世僧。",
        "松高半岩雪，",
        "竹覆一溪冰。",
        "不说有为法，",
        "非传无尽灯。",
        "了然方寸内，",
        "应只见南能。"
      ]
    },
    {
      "_id": null,
      "Name": "从军行",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "从军朔方久，",
        "未省用干戈。",
        "只以恩信及，",
        "自然戎虏和。",
        "边声动白草，",
        "烧色入枯河。",
        "每度因看猎，",
        "令人勇气多。"
      ]
    },
    {
      "_id": null,
      "Name": "仙岩二首",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "未知",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "白烟昼起丹灶，",
        "红叶秋书篆文。",
        "二十四岩天上，",
        "一鸡啼破晴云。",
        "风呼山鬼服役，",
        "月照衡薇结花。",
        "江暖客寻瑶草，",
        "洞深人咽丹霞。"
      ]
    },
    {
      "_id": null,
      "Name": "依韵和幹公题庭中太湖石二首",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "未知",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "山立只盈寻，",
        "高奇药圃阴。",
        "风涛打欲碎，",
        "岩穴蛰方深。",
        "藓点晴偏绿，",
        "蛩藏晓竞吟。",
        "岁寒终不变，",
        "堪比古人心。",
        "徒劳水府寻，",
        "宛在玉堂阴。",
        "兰圃安虽窄，",
        "盆池映转深。",
        "山僧来尽爱，",
        "诗客见先吟。",
        "若是买花者，",
        "年年不计心。"
      ]
    },
    {
      "_id": null,
      "Name": "入一作出塞",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "玉殿论兵事，",
        "君王诏出征。",
        "新除羽林将，",
        "曾破月支兵。",
        "惯历塞垣险，",
        "能分部落情。",
        "从今一战胜，",
        "不使虏尘生。"
      ]
    },
    {
      "_id": null,
      "Name": "冯氏书斋小松二首",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "未知",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "孤根生远岳，",
        "移植翠枝添。",
        "自秉雪霜操，",
        "任他蜂蝶嫌。",
        "微阴连迥竹，",
        "清韵入疏帘。",
        "耸势即空碧，",
        "时人看莫厌。",
        "得地已经岁，",
        "清音昼夜闻。",
        "根涵旧山土，",
        "叶间近溪云。",
        "野鹤望长远，",
        "庭花笑不群。",
        "须知摇落后，",
        "众木始能分。"
      ]
    },
    {
      "_id": null,
      "Name": "出自蓟北门行",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "蓟北连极塞，",
        "塞色昼冥冥。",
        "战地骸骨满，",
        "长时风雨腥。",
        "沙河留不定，",
        "春草冻难青。",
        "万户封侯者，",
        "何谋静虏庭。"
      ]
    },
    {
      "_id": null,
      "Name": "古悔从军行",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "忆昔仗孤剑，",
        "十年从武威。",
        "论兵亲玉帐，",
        "逐虏过金微。",
        "陇水秋先冻，",
        "关云寒不飞。",
        "辛勤功业在，",
        "麟阁志犹违。"
      ]
    },
    {
      "_id": null,
      "Name": "句",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "未知",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "太阳虽不照，",
        "梁栋每重阴。",
        "白发未逢媒，",
        "对景且裴回。",
        "别酒莫辞今夜醉，",
        "故人知我几时来。",
        "改贯永留乡党额，",
        "减租重感郡侯恩。"
      ]
    },
    {
      "_id": null,
      "Name": "商山",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "商山名利路，",
        "夜亦有人行。",
        "四皓卧云处，",
        "千秋叠藓生。",
        "昼烧笼涧黑，",
        "残雪隔林明。",
        "我待酬恩了，",
        "来听水石声。"
      ]
    },
    {
      "_id": null,
      "Name": "塞上曲",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "岁岁但防虏，",
        "西征早晚休。",
        "匈奴不系颈，",
        "汉将但封侯。",
        "夕照低烽火，",
        "寒笳咽戍楼。",
        "燕然山上字，",
        "男子见须羞。"
      ]
    },
    {
      "_id": null,
      "Name": "太湖石",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "谁怜孤峭质，",
        "移在太湖心。",
        "出得风波外，",
        "任他池馆深。",
        "不同花逞艳，",
        "多愧竹垂阴。",
        "一片至坚操，",
        "那忧岁月侵。"
      ]
    },
    {
      "_id": null,
      "Name": "妾薄命",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "薄命头欲白，",
        "频年嫁不成。",
        "秦娥未十五，",
        "昨夜事公卿。",
        "岂有机杼力，",
        "空传歌舞名。",
        "妾专修妇德，",
        "媒氏却相轻。"
      ]
    },
    {
      "_id": null,
      "Name": "宫池产瑞莲帖经日试",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "雨露及万物，",
        "嘉祥有瑞莲。",
        "香飘鸡树近，",
        "荣占凤池先。",
        "驿日临双丽，",
        "恩波照并妍。",
        "愿同指佞草，",
        "生向帝尧前。"
      ]
    },
    {
      "_id": null,
      "Name": "宿新安村步",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "淅淅寒流涨浅沙，",
        "月明空渚遍芦花。",
        "离人偶宿孤村下，",
        "永夜闻砧一两家。"
      ]
    },
    {
      "_id": null,
      "Name": "寄天台叶尊师",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "师住天台久，",
        "长闻过石桥。",
        "晴峰见沧海，",
        "深洞彻丹霄。",
        "采药霞衣湿，",
        "煎芝古鼎焦。",
        "念予无俗骨，",
        "频与鹤书招。"
      ]
    },
    {
      "_id": null,
      "Name": "寄郑谷",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "五百首新诗，",
        "缄封寄去时。",
        "只凭夫子鉴，",
        "不要俗人知。",
        "火鼠重收布，",
        "冰蚕乍吐丝。",
        "直须天上手，",
        "裁作领巾披。"
      ]
    },
    {
      "_id": null,
      "Name": "少年行二首",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "未知",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "游宴不知厌，",
        "杜陵狂少年。",
        "花时轻暖酒，",
        "春服薄装绵。",
        "戏马上林苑，",
        "斗鸡寒食天。",
        "鲁儒甘被笑，",
        "对策鬓皤然。",
        "弱冠投边急，",
        "驱兵夜渡河。",
        "追奔铁马走，",
        "杀虏宝刀讹。",
        "威静黑山路，",
        "气含清海波。",
        "常闻为突骑，",
        "天子赐长戈。"
      ]
    },
    {
      "_id": null,
      "Name": "庐山",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "岳立镇南楚，",
        "雄名天下闻。",
        "五峰高阂日，",
        "九叠翠连云。",
        "夏谷雪犹在，",
        "阴岩昼不分。",
        "唯应嵩与华，",
        "清峻得为群。"
      ]
    },
    {
      "_id": null,
      "Name": "度关山",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "只领千余骑，",
        "长驱碛邑间。",
        "云州多警急，",
        "雪夜度关山。",
        "石响铃声远，",
        "天寒弓力悭。",
        "秦楼休怅望，",
        "不日凯歌还。"
      ]
    },
    {
      "_id": null,
      "Name": "庾楼晓望",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "七律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "独凭朱槛亦凌晨，",
        "山色初明水色新。",
        "竹雾晓笼衔岭月，",
        "蘋风暖送过江春。",
        "子城阴处犹残雪，",
        "衙鼓声前未有尘。",
        "三百年来庾楼上，",
        "曾经多少望乡人。"
      ]
    },
    {
      "_id": null,
      "Name": "御沟水",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "一带御沟水，",
        "绿槐相荫清。",
        "此中涵帝泽，",
        "无处濯尘缨。",
        "鸟道来虽险，",
        "龙池到自平。",
        "朝宗本心切，",
        "愿向急流倾。"
      ]
    },
    {
      "_id": null,
      "Name": "御试后进诗",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "三时赐食天厨近，",
        "再宿偷吟禁漏清。",
        "二十五家齐拔宅，",
        "人间已写上升名。"
      ]
    },
    {
      "_id": null,
      "Name": "忆张处士",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "天台张处士，",
        "诗句造玄微。",
        "古乐知音少，",
        "名言与俗违。",
        "山风入松径，",
        "海月上岩扉。",
        "毕世唯高卧，",
        "无人说是非。"
      ]
    },
    {
      "_id": null,
      "Name": "折杨柳三首一作段成式诗",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "词",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "枝枝交影锁长门，",
        "嫩色曾沾雨露恩。",
        "凤辇不来春欲尽，",
        "空留莺语到黄昏。",
        "水殿年年占早芳，",
        "柔条风里御炉香。",
        "如今万乘多巡狩，",
        "辇路无阴绿草长。",
        "嫩叶初齐不耐寒，",
        "风和时拂玉栏干。",
        "征人去日曾攀折，",
        "泣雨伤春翠黛残。"
      ]
    },
    {
      "_id": null,
      "Name": "拟塞外征行",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "未知",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "寇骑满鸡田，",
        "都护欲临边。",
        "青泥方绝漠，",
        "怀剑始辞燕。",
        "旌旗挂龙虎，",
        "壮士募鹰  。",
        "长城威十万，",
        "高岭奋三千。",
        "行行向马邑，",
        "去去指祁连。",
        "鼓声遥赤塞，",
        "兵气远冲天。",
        "对阵云初上，",
        "临城月始悬。",
        "风惊烽易灭，",
        "沙暗马难前。",
        "恩重恒思报，",
        "劳心屡损年。",
        "微功一可立，",
        "身轻不自怜。"
      ]
    },
    {
      "_id": null,
      "Name": "春日咏梅花见绝句辩体",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "靓妆才罢粉痕新，",
        "迨晓风回散玉尘。",
        "若遣有情应怅望，",
        "已兼残雪又兼春。"
      ]
    },
    {
      "_id": null,
      "Name": "晓发萧关",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "早发长风里，",
        "边城曙色间。",
        "数鸿寒背碛，",
        "片月落临关。",
        "陇上明星没，",
        "沙中夜探还。",
        "归程不可问，",
        "几日到家山。"
      ]
    },
    {
      "_id": null,
      "Name": "晓泊汉阳渡",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "落月临古渡，",
        "武昌城未开。",
        "残灯明市井，",
        "晓色辨楼台。",
        "云自苍梧去，",
        "水从嶓冢来。",
        "芳洲号鹦鹉，",
        "用记祢生才。"
      ]
    },
    {
      "_id": null,
      "Name": "有所思一作长相思",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "芙蓉出水时，",
        "偶尔便分离。",
        "自此无因见，",
        "长教挂所思。",
        "残春不入梦，",
        "芳信欲传谁。",
        "寂寞秋堂下，",
        "空吟小谢诗。"
      ]
    },
    {
      "_id": null,
      "Name": "杂曲歌辞·妾薄命",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "薄命头欲白，",
        "频年嫁不成。",
        "秦娥未十五，",
        "昨夜事公卿。",
        "岂有机杼力，",
        "空传歌舞名。",
        "妾专修妇德，",
        "媒氏却相轻。"
      ]
    },
    {
      "_id": null,
      "Name": "歌一作凉州行",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "谁唱关西曲，",
        "寂寞夜景深。",
        "一声长在耳，",
        "万恨重经心。",
        "调古清风起，",
        "曲终凉月沉。",
        "却应筵上客，",
        "未必是知音。"
      ]
    },
    {
      "_id": null,
      "Name": "江上吟晓",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "一叶野人舟，",
        "长将载酒游。",
        "夜来吟思苦，",
        "江上月华秋。",
        "晓露满红蓼，",
        "轻波飏白鸥。",
        "渔翁似有约，",
        "相伴钓中流。"
      ]
    },
    {
      "_id": null,
      "Name": "泛镜湖某某",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "我泛镜湖日，",
        "未生千里莼。",
        "时无贺宾客，",
        "谁识谪仙人。",
        "吟对四时雪，",
        "忆游三岛春。",
        "恶闻亡越事，",
        "洗耳大江滨。"
      ]
    },
    {
      "_id": null,
      "Name": "洗竹",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "未知",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "道院竹繁教略洗，",
        "鸣琴酌酒看扶疏。",
        "不图结实来双凤，",
        "且要长竿钓巨鱼。",
        "锦箨裁冠添散逸，",
        "玉芽修馔称清虚。",
        "有时记得三天事，",
        "自向琅  节下书。"
      ]
    },
    {
      "_id": null,
      "Name": "洛阳道",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "喧喧洛阳路，",
        "奔走争先步。",
        "唯恐著鞭迟，",
        "谁能更回顾。",
        "覆车虽在前，",
        "润屋何曾惧。",
        "贤哉只二疏，",
        "东门挂冠去。"
      ]
    },
    {
      "_id": null,
      "Name": "游仙",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "我家三岛上，",
        "洞户眺波涛。",
        "醉背云屏卧，",
        "谁知海日高。",
        "露香红玉树，",
        "风绽碧蟠桃。",
        "悔与仙子别，",
        "思归梦钓鳌。"
      ]
    },
    {
      "_id": null,
      "Name": "湘妃怨",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "舜欲省蛮陬，",
        "南巡非逸游。",
        "九江沉白日，",
        "二女泣沧洲。",
        "目极楚云断，",
        "恨深湘水流。",
        "至今闻鼓瑟，",
        "咽绝不胜愁。"
      ]
    },
    {
      "_id": null,
      "Name": "独芙蓉",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "方塘清晓镜，",
        "独照玉容秋。",
        "蠹芰不相采，",
        "敛蘋空自愁。",
        "日斜还顾影，",
        "风起强垂头。",
        "芳意羡何物，",
        "双双鸂鶒游。"
      ]
    },
    {
      "_id": null,
      "Name": "田舍曲",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "未知",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "古今利名路，",
        "只在侬门前。",
        "至老不离家，",
        "一生常晏眠。",
        "牛羊晚自归，",
        "儿童戏野田。",
        "岂思封侯贵，",
        "唯只待丰年。",
        "征赋岂辞苦，",
        "但愿时苟贤。",
        "时官苟贪浊，",
        "田舍生忧煎。"
      ]
    },
    {
      "_id": null,
      "Name": "白牡丹",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "谷雨洗纤素，",
        "裁为白牡丹。",
        "异香开玉合，",
        "轻粉泥银盘。",
        "晓贮露华湿，",
        "宵倾月魄寒。",
        "家人淡妆罢，",
        "无语倚朱栏。"
      ]
    },
    {
      "_id": null,
      "Name": "短歌",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "物候来相续，",
        "新蝉送晚莺。",
        "百年休倚赖，",
        "一梦甚分明。",
        "金鼎神仙隐，",
        "铜壶昼夜倾。",
        "不如早立德，",
        "万古有其名。"
      ]
    },
    {
      "_id": null,
      "Name": "秋日旅怀寄右省郑拾遗",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "永夕愁不寐，",
        "草虫喧客庭。",
        "半窗分晓月，",
        "当枕落残星。",
        "鬓发游梁白，",
        "家山近越青。",
        "知音在谏省，",
        "苦调有谁听。"
      ]
    },
    {
      "_id": null,
      "Name": "终南山",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "终朝异五岳，",
        "列翠满长安。",
        "地去搜扬近，",
        "人谋隐遁难。",
        "水穿诸苑过，",
        "雪照一城寒。",
        "为问红尘里，",
        "谁同驻马看。"
      ]
    },
    {
      "_id": null,
      "Name": "经故洛城",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "卜世何久远，",
        "由来仰圣明。",
        "山河徒自壮，",
        "周召不长生。",
        "几主任奸谄，",
        "诸侯各战争。",
        "但余崩垒在，",
        "今古共伤情。"
      ]
    },
    {
      "_id": null,
      "Name": "胡笳曲",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "陇底悲笳引，",
        "陇头鸣北风。",
        "一轮霜月落，",
        "万里塞天空。",
        "戍卒泪应尽，",
        "胡儿哭未终。",
        "争教班定远，",
        "不念玉关中。"
      ]
    },
    {
      "_id": null,
      "Name": "芍药",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "芍药承春宠，",
        "何曾羡牡丹。",
        "麦秋能几日，",
        "谷雨只微寒。",
        "妒态风频起，",
        "娇妆露欲残。",
        "芙蓉浣纱伴，",
        "长恨隔波澜。"
      ]
    },
    {
      "_id": null,
      "Name": "芦苇",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "未知",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "高士想江湖，",
        "湖闲庭植芦。",
        "清风时有至，",
        "绿竹兴何殊。",
        "嫩喜日光薄，",
        "疏忧雨点粗。",
        "惊蛙跳得过，",
        "斗雀袅如无。",
        "未织巴篱护，",
        "几抬邛竹扶。",
        "惹烟轻弱柳，",
        "蘸水漱清蒲。",
        "溉灌情偏重，",
        "琴樽赏不孤。",
        "穿花思钓叟，",
        "吹叶少羌雏。",
        "寒色暮天映，",
        "秋声远籁俱。",
        "朗吟应有趣，",
        "潇洒十余株。"
      ]
    },
    {
      "_id": null,
      "Name": "赠刘凝评事",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "棘寺官初罢，",
        "梁园静掩扉。",
        "春深颜子巷，",
        "花映老莱衣。",
        "谈史曾无滞，",
        "攻书已造微。",
        "即膺新宠命，",
        "称庆向庭闱。"
      ]
    },
    {
      "_id": null,
      "Name": "过商山",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "一宿白云根，",
        "时经采麝村。",
        "数峰虽似蜀，",
        "当昼不闻猿。",
        "马立溪沙浅，",
        "人争阁道喧。",
        "明朝弃襦罢，",
        "步步入金门。"
      ]
    },
    {
      "_id": null,
      "Name": "远闻本郡行春到旧山二首",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "七律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "一身从宦留京邑，",
        "五马遥闻到旧山。",
        "已领烟霞光野径，",
        "深惭老幼候柴关。",
        "清风借响松筠外，",
        "画隼停晖水石间。",
        "定掩溪名在图传，",
        "共知轩盖此登攀。"
      ]
    },
    {
      "_id": null,
      "Name": "述松",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "远谷呈材干，",
        "何由入栋梁。",
        "岁寒虚胜竹，",
        "功绩不如桑。",
        "秋露落松子，",
        "春深裛嫩黄。",
        "虽蒙匠者顾，",
        "樵采日难防。"
      ]
    },
    {
      "_id": null,
      "Name": "送友人南归",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "南国菖蒲老，",
        "知君忆钓船。",
        "离京近残暑，",
        "归路有新蝉。",
        "岘首白云起，",
        "洞庭秋月悬。",
        "若教吟兴足，",
        "西笑是何年。"
      ]
    },
    {
      "_id": null,
      "Name": "送芮尊师",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "七律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "石上菖蒲节节灵，",
        "先生服食得长生。",
        "早知避世忧身老，",
        "近日登山觉步轻。",
        "黄鹤待传蓬岛信，",
        "丹书应换蕊宫名。",
        "他年控鲤升天去，",
        "庐岳逋民愿从行。"
      ]
    },
    {
      "_id": null,
      "Name": "送韩从事归本道",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "献捷灵州倅，",
        "归时宠拜新。",
        "论边多称旨，",
        "许国誓亡身。",
        "马渴黄河冻，",
        "雁回青冢春。",
        "到蕃唯促战，",
        "应不肯和亲。"
      ]
    },
    {
      "_id": null,
      "Name": "送马明府归山",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "辞秩入匡庐，",
        "重修靖节居。",
        "免遭黑绶束，",
        "不与白云疏。",
        "送吏各献酒，",
        "群儿自担书。",
        "到时看瀑布，",
        "为我谢清虚。"
      ]
    },
    {
      "_id": null,
      "Name": "金陵一本同下题作二首",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "六代江山在，",
        "繁华古帝都。",
        "乱来城不守，",
        "战后地多芜。",
        "寒日随潮落，",
        "归帆与鸟孤。",
        "兴亡多少事，",
        "回首一长吁。"
      ]
    },
    {
      "_id": null,
      "Name": "金陵怀古",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "恃险不种德，",
        "兴亡叹数穷。",
        "石城几换主，",
        "天堑谩连空。",
        "御路叠民冢，",
        "台基聚牧童。",
        "折碑犹有字，",
        "多记晋英雄。"
      ]
    },
    {
      "_id": null,
      "Name": "钓台",
      "PoetId": "79c5c60df76e4ff8b08c56462176b26a",
      "PoetName": "王贞白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王贞白"
      ],
      "Contents": [
        "异代有巢许，",
        "方知严子情。",
        "旧交虽建国，",
        "高卧不求荣。",
        "溪鸟寒来浴，",
        "汀兰暖重生。",
        "何颜吟过此，",
        "辛苦得浮名。"
      ]
