  "Poems": [
    {
      "_id": null,
      "Name": "长至日上公献寿",
      "PoetId": "56f186d38284468e9912a86908d6a657",
      "PoetName": "李竦",
      "Form": "未知",
      "Tags": [
        "唐代",
        "李竦"
      ],
      "Contents": [
        "候晓金门辟，",
        "乘时玉历长。",
        "羽仪瞻上宰，",
        "云物丽初阳。",
        "汉礼方传珮，",
        "尧年正捧觞。",
        "日行临观阙，",
        "帝锡洽珪璋。",
        "盛美超三代，",
        "洪休降百祥。",
        "自怜朝末坐，",
        "空此咏无疆。"
      ]
    }
  ]
}