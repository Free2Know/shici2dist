{
  "_id": "cb681d27fa30468a890f3ead5b192954",
  "Name": "雍裕之",
  "Description": "雍裕之，贞元后诗人也。诗一卷。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "不了语",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "浮名世利知多少，",
        "朝市喧喧尘扰扰。",
        "车马交驰往复来，",
        "钟鼓相催天又晓。"
      ]
    },
    {
      "_id": null,
      "Name": "两头纤纤",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "两头纤纤八字眉，",
        "半白半黑灯影帷。",
        "腷腷膊膊晓禽飞，",
        "磊磊落落秋果垂。"
      ]
    },
    {
      "_id": null,
      "Name": "了语",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "扫却烟尘寇初剿，",
        "深水高林放鱼鸟。",
        "鸡人唱绝残漏晓，",
        "仙乐拍终天悄悄。"
      ]
    },
    {
      "_id": null,
      "Name": "农家望晴",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "尝闻秦地西风雨，",
        "为问西风早晚回。",
        "白发老农如鹤立，",
        "麦场高处望云开。"
      ]
    },
    {
      "_id": null,
      "Name": "剪彩花",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "敢竞桃李色，",
        "自呈刀尺功。",
        "蝶犹迷剪翠，",
        "人岂辨裁红。"
      ]
    },
    {
      "_id": null,
      "Name": "听弹沈湘",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "贾谊投文吊屈平，",
        "瑶琴能写此时情。",
        "秋风一奏沈湘曲，",
        "流水千年作恨声。"
      ]
    },
    {
      "_id": null,
      "Name": "四气",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "春禽犹竞啭，",
        "夏木忽交阴。",
        "稍觉秋山远，",
        "俄惊冬霰深。"
      ]
    },
    {
      "_id": null,
      "Name": "四色",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "未知",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "壶中冰始结，",
        "盘上露初圆。",
        "何意瑶池雪，",
        "欲夺鹤毛鲜。",
        "道士牛已至，",
        "仙家鸟亦来。",
        "骨为神不朽，",
        "眼向故人开。",
        "劳鲂莲渚内，",
        "汗马火旂间。",
        "平生血诚尽，",
        "不独左轮殷。",
        "已见池尽墨，",
        "谁言突不黔。",
        "漆身恩未报，",
        "貂裘弊岂嫌。"
      ]
    },
    {
      "_id": null,
      "Name": "大言",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "四溟杯渌醑，",
        "五岳髻青螺。",
        "挥汗曾成雨，",
        "画地亦成河。"
      ]
    },
    {
      "_id": null,
      "Name": "宫人斜",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "几多红粉委黄泥，",
        "野鸟如歌又似啼。",
        "应有春魂化为燕，",
        "年来飞入未央栖。"
      ]
    },
    {
      "_id": null,
      "Name": "宿棣华馆闻雁",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "不堪旅宿棣花馆，",
        "况有离群鸿雁声。",
        "一点秋灯残影下，",
        "不知寒梦几回惊。"
      ]
    },
    {
      "_id": null,
      "Name": "山中桂",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "八树拂丹霄，",
        "四时青不凋。",
        "秋风何处起，",
        "先袅最长条。"
      ]
    },
    {
      "_id": null,
      "Name": "折柳赠行人",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "那言柳乱垂，",
        "尽日任风吹。",
        "欲识千条恨，",
        "和烟折一枝。"
      ]
    },
    {
      "_id": null,
      "Name": "早蝉",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "一声清溽暑，",
        "几处促流年。",
        "志士心偏苦，",
        "初闻独泫然。"
      ]
    },
    {
      "_id": null,
      "Name": "春晦送客",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "野酌乱无巡，",
        "送君兼送春。",
        "明年春色至，",
        "莫作未归人。"
      ]
    },
    {
      "_id": null,
      "Name": "曲江池上",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "殷勤春在曲江头，",
        "全藉群仙占胜游。",
        "何必三山待鸾鹤，",
        "年年此地是瀛洲。"
      ]
    },
    {
      "_id": null,
      "Name": "杂曲歌辞·自君之出矣",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "自君之出矣，",
        "宝镜为谁明。",
        "思君如陇水，",
        "长闻呜咽声。"
      ]
    },
    {
      "_id": null,
      "Name": "柳絮",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "无风才到地，",
        "有风还满空。",
        "缘渠偏似雪，",
        "莫近鬓毛生。"
      ]
    },
    {
      "_id": null,
      "Name": "残莺",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "花阑莺亦懒，",
        "不语似含情。",
        "何言百啭舌，",
        "唯余一两声。"
      ]
    },
    {
      "_id": null,
      "Name": "江上山",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "绮霞明赤岸，",
        "锦缆绕丹枝。",
        "楚客正愁绝，",
        "西风且莫吹。"
      ]
    },
    {
      "_id": null,
      "Name": "江上闻猿",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "枫岸月斜明，",
        "猿啼旅梦惊。",
        "愁多肠易断，",
        "不待第三声。"
      ]
    },
    {
      "_id": null,
      "Name": "江边柳",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "袅袅古堤边，",
        "青青一树烟。",
        "若为丝不断，",
        "留取系郎船。"
      ]
    },
    {
      "_id": null,
      "Name": "游丝",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "游丝何所似，",
        "应最似春心。",
        "一向风前乱，",
        "千条不可寻。"
      ]
    },
    {
      "_id": null,
      "Name": "秋蛩",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "雨绝苍苔地，",
        "月斜青草阶。",
        "蛩鸣谁不怨，",
        "况是正离怀。"
      ]
    },
    {
      "_id": null,
      "Name": "细言",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "蚊眉自可托，",
        "蜗角岂劳争。",
        "欲效丝毫力，",
        "谁知蝼蚁诚。"
      ]
    },
    {
      "_id": null,
      "Name": "芦花",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "夹岸复连沙，",
        "枝枝摇浪花。",
        "月明浑似雪，",
        "无处认渔家。"
      ]
    },
    {
      "_id": null,
      "Name": "豪家夏冰咏",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "金错银盘贮赐冰，",
        "清光如耸玉山棱。",
        "无论尘客闲停扇，",
        "直到消时不见蝇。"
      ]
    },
    {
      "_id": null,
      "Name": "赠苦行僧",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "幽深红叶寺，",
        "清净白毫僧。",
        "古殿长鸣磬，",
        "低头礼昼灯。"
      ]
    },
    {
      "_id": null,
      "Name": "题蒲葵扇",
      "PoetId": "cb681d27fa30468a890f3ead5b192954",
      "PoetName": "雍裕之",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "雍裕之"
      ],
      "Contents": [
        "倾心曾向日，",
        "在手幸摇风。",
        "羡尔逢提握，",
        "知名自谢公。"
      ]
    }
  ]
}