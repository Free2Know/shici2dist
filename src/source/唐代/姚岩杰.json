  "Poems": [
    {
      "_id": null,
      "Name": "报颜标",
      "PoetId": "3f37a9218afd40ae90430bce16a14a6c",
      "PoetName": "姚岩杰",
      "Form": "七律",
      "Tags": [
        "唐代",
        "姚岩杰"
      ],
      "Contents": [
        "为报颜公识我么，",
        "我心唯只与天和。",
        "眼前俗物关情少，",
        "醉后青山入意多。",
        "田子莫嫌弹铗恨，",
        "宁生休唱饭牛歌。",
        "圣朝若为苍生计，",
        "也合公车到薜萝。"
      ]
    }
  ]
}