{
  "_id": "f9af3f77efbb40c88578fd69be824416",
  "Name": "崔峒",
  "Description": "崔峒，博陵人。登进士第，为拾遗、集贤学士，终于州刺史。《艺文传》云终右补阙，大历十才子之一也。诗一卷。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "书怀寄杨郭李王判官",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "惯作云林客，",
        "因成懒漫人。",
        "吏欺从政拙，",
        "妻笑理家贫。",
        "李郭应时望，",
        "王杨入幕频。",
        "从容丞相阁，",
        "知忆故园春。"
      ]
    },
    {
      "_id": null,
      "Name": "书情寄上苏州韦使君兼呈吴县李明府",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "七律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "数年湖上谢浮名，",
        "竹杖纱巾遂性情。",
        "云外有时逢寺宿，",
        "日西无事傍江行。",
        "陶潜县里看花发，",
        "庾亮楼中对月明。",
        "谁念献书来万里，",
        "君王深在九重城。"
      ]
    },
    {
      "_id": null,
      "Name": "刘展下判官相招以诗答之",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "国有非常宠，",
        "家承异姓勋。",
        "背恩惭皎日，",
        "不义若浮云。",
        "但使忠贞在，",
        "甘从玉石焚。",
        "窜身如有地，",
        "梦寐见明君。"
      ]
    },
    {
      "_id": null,
      "Name": "初入集贤院赠李献仁",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "燕代官初罢，",
        "江湖路便分。",
        "九迁从命薄，",
        "四十幸人闻。",
        "迹愧趋丹禁，",
        "身曾系白云。",
        "何由返沧海，",
        "昨日谒明君。"
      ]
    },
    {
      "_id": null,
      "Name": "初除拾遗酬丘二十二见寄",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "江海久垂纶，",
        "朝衣忽挂身。",
        "丹墀初谒帝，",
        "白发免羞人。",
        "才愧文章士，",
        "名当谏诤臣。",
        "空馀荐贤分，",
        "不敢负交亲。"
      ]
    },
    {
      "_id": null,
      "Name": "咏门下画小松上元王杜三相公",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "昔闻生涧底，",
        "今见起毫端。",
        "众草此时没，",
        "何人知岁寒。",
        "岂能裨栋宇，",
        "且贵出门阑。",
        "只在丹青意，",
        "凌云也不难。"
      ]
    },
    {
      "_id": null,
      "Name": "喜逢妻弟郑损因送入京",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "乱后自江城，",
        "相逢喜复惊。",
        "为经多载别，",
        "欲问小时名。",
        "对酒悲前事，",
        "论文畏后生。",
        "遥知盈卷轴，",
        "纸贵在江城。"
      ]
    },
    {
      "_id": null,
      "Name": "奉和给事寓直",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "桂枝家共折，",
        "鸡树代相传。",
        "忝向鸾台下，",
        "仍看雁影连。",
        "夜闲方步月，",
        "漏尽欲朝天。",
        "知去丹墀近，",
        "明王许荐贤。"
      ]
    },
    {
      "_id": null,
      "Name": "客舍书情寄赵中丞",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "东楚复西秦，",
        "浮云类此身。",
        "关山劳策蹇，",
        "僮仆惯投人。",
        "孤客来千里，",
        "全家托四邻。",
        "生涯难自料，",
        "中夜问情亲。"
      ]
    },
    {
      "_id": null,
      "Name": "客舍有怀因呈诸在事",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "读书常苦节，",
        "待诏岂辞贫。",
        "暮雪犹驱马，",
        "晡餐又寄人。",
        "愁来占吉梦，",
        "老去惜良辰。",
        "延首平津阁，",
        "家山日已春。"
      ]
    },
    {
      "_id": null,
      "Name": "宿江西窦主簿厅",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "广庭方缓步，",
        "星汉话中移。",
        "月满关山道，",
        "乌啼霜树枝。",
        "时艰难会合，",
        "年长重亲知。",
        "前事成金石，",
        "凄然泪欲垂。"
      ]
    },
    {
      "_id": null,
      "Name": "宿禅智寺上方演大师院",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "石林高几许，",
        "金刹在中峰。",
        "白日空山梵，",
        "清霜后夜钟。",
        "竹窗回翠壁，",
        "苔径入寒松。",
        "幸接无生法，",
        "疑心怯所从。"
      ]
    },
    {
      "_id": null,
      "Name": "寄上礼部李侍郎",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "未知",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "吴楚相逢处，",
        "江湖共泛时。",
        "任风舟去远，",
        "待月酒行迟。",
        "白发常同叹，",
        "青云本要期。",
        "贵来君却少，",
        "秋至老偏悲。",
        "玉佩明朝盛，",
        "苍苔陋巷滋。",
        "追寻恨无路，",
        "唯有梦相思。"
      ]
    },
    {
      "_id": null,
      "Name": "扬州选蒙相公赏判雪后呈上",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "自得山公许，",
        "休耕海上田。",
        "惭看长史传，",
        "欲弃钓鱼船。",
        "穷巷殷忧日，",
        "芜城雨雪天。",
        "此时瞻相府，",
        "心事比旌悬。"
      ]
    },
    {
      "_id": null,
      "Name": "春日忆姚氏外甥",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "离乱人相失，",
        "春秋雁自飞。",
        "只缘行路远，",
        "未必寄书稀。",
        "二月花无数，",
        "频年意有违。",
        "落晖看过后，",
        "独坐泪沾衣。"
      ]
    },
    {
      "_id": null,
      "Name": "武康郭外望许纬先生山居",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "未知",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "湖上千峰带落晖，",
        "白云开处见柴扉。",
        "松门一径仍生草，",
        "应是仙人向郭稀。",
        "第二百九十五卷"
      ]
    },
    {
      "_id": null,
      "Name": "江上书怀",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "未知",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "骨肉天涯别，",
        "江山日落时。",
        "泪流襟上血，",
        "发变镜中丝。",
        "胡越书难到，",
        "存亡梦岂知。",
        "登高回首罢，",
        "形影自相随。",
        "·250 ·"
      ]
    },
    {
      "_id": null,
      "Name": "江南回逢赵曜，因送任十一赴交城主簿",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "江上长相忆，",
        "因高北望看。",
        "不知携老幼，",
        "何处度艰难。",
        "屈指同人尽，",
        "伤心故里残。",
        "遥怜驱匹马，",
        "白首到微官。"
      ]
    },
    {
      "_id": null,
      "Name": "润州送友人",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "见君还此地，",
        "洒泪向江边。",
        "国士劳相问，",
        "家书无处传。",
        "荒城胡马迹，",
        "塞木戍人烟。",
        "一路堪愁思，",
        "孤舟何渺然。"
      ]
    },
    {
      "_id": null,
      "Name": "润州送师弟自江夏往台州",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "远客乘流去，",
        "孤帆向夜开。",
        "春风江上使，",
        "前日汉阳来。",
        "别路犹千里，",
        "离心重一杯。",
        "剡溪木未落，",
        "羡尔过天台。"
      ]
    },
    {
      "_id": null,
      "Name": "清江曲内一绝",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "八月长江去浪平，",
        "片帆一道带风轻。",
        "极目不分天水色，",
        "南山南是岳阳城。"
      ]
    },
    {
      "_id": null,
      "Name": "登润州芙蓉楼",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "上古人何在，",
        "东流水不归。",
        "往来潮有信，",
        "朝暮事成非。",
        "烟树临沙静，",
        "云帆入海稀。",
        "郡楼多逸兴，",
        "良牧谢玄晖。"
      ]
    },
    {
      "_id": null,
      "Name": "登蒋山开善寺",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "山殿秋云里，",
        "香烟出翠微。",
        "客寻朝磬至，",
        "僧背夕阳归。",
        "下界千门见，",
        "前朝万事非。",
        "看心兼送目，",
        "葭菼暮依依。"
      ]
    },
    {
      "_id": null,
      "Name": "秋晚送丹徒许明府赴上国，因寄江南故人",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "秋暮之彭泽，",
        "篱花远近逢。",
        "君书前日至，",
        "别后此时重。",
        "寒夜江边月，",
        "晴天海上峰。",
        "还知南地客，",
        "招引住新丰。"
      ]
    },
    {
      "_id": null,
      "Name": "虔州见郑表新诗因以寄赠",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "七律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "梅花岭里见新诗，",
        "感激情深过楚词。",
        "平子四愁今莫比，",
        "休文八咏自同时。",
        "萍乡露冕真堪惜，",
        "凤沼鸣珂已讶迟。",
        "才子风流定难见，",
        "湖南春草但相思。"
      ]
    },
    {
      "_id": null,
      "Name": "赠元秘书",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "七律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "旧书稍稍出风尘，",
        "孤客逢秋感此身。",
        "秦地谬为门下客，",
        "淮阴徒笑市中人。",
        "也闻阮籍寻常醉，",
        "见说陈平不久贫。",
        "幸有故人茅屋在，",
        "更将心事问情亲。"
      ]
    },
    {
      "_id": null,
      "Name": "赠窦十九",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "七律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "灵台暮宿意多违，",
        "木落花开羡客归。",
        "江海几时传锦字，",
        "风尘不觉化缁衣。",
        "山阳会里同人少，",
        "灞曲农时故老稀。",
        "幸得汉皇容直谏，",
        "怜君未遇觉人非。"
      ]
    },
    {
      "_id": null,
      "Name": "越中送王使君赴江华",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "七律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "皂盖春风自越溪，",
        "独寻芳树桂阳西。",
        "远水浮云随马去，",
        "空山弱筱向云低。",
        "遥知异政荆门北，",
        "旧许新诗康乐齐。",
        "万里相思在何处，",
        "九疑残雪白猿啼。"
      ]
    },
    {
      "_id": null,
      "Name": "送丘二十二之苏州",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "积水与寒烟，",
        "嘉禾路几千。",
        "孤猿啼海岛，",
        "群雁起湖田。",
        "曾见长洲苑，",
        "尝闻大雅篇。",
        "却将封事去，",
        "知尔爱闲眠。"
      ]
    },
    {
      "_id": null,
      "Name": "送侯山人赴会稽",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "仙客辞萝月，",
        "东来就一官。",
        "且归沧海住，",
        "犹向白云看。",
        "猿叫江天暮，",
        "虫声野浦寒。",
        "时游镜湖里，",
        "为我把鱼竿。"
      ]
    },
    {
      "_id": null,
      "Name": "送冯八将军奏事毕归滑台幕府",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "七律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "王门别后到沧洲，",
        "帝里相逢俱白头。",
        "自叹马卿常带疾，",
        "还嗟李广不封侯。",
        "棠梨宫里瞻龙衮，",
        "细柳营中著虎裘。",
        "想到滑台桑叶落，",
        "黄河东注杏园秋。"
      ]
    },
    {
      "_id": null,
      "Name": "送张芬东归",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "喧喧五衢上，",
        "鞍马自驱驰。",
        "落日临阡陌，",
        "贫交欲别离。",
        "早知时事异，",
        "堪与世人随。",
        "握手将何赠，",
        "君心我独知。"
      ]
    },
    {
      "_id": null,
      "Name": "送李道士归山",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "秋城临古路，",
        "城上望君还。",
        "旷野入寒草，",
        "独行随远山。",
        "授人鸿宝内，",
        "将犬白云间。",
        "早晚烧丹罢，",
        "遥知冰雪寒。"
      ]
    },
    {
      "_id": null,
      "Name": "送王侍御佐婺州",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "七律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "闻君作尉向江潭。",
        "吴越风烟到自谙。",
        "客路寻常经竹径，",
        "人家大底傍山岚。",
        "缘溪花木偏宜远，",
        "避地衣冠尽向南。",
        "惟有夜猿啼海树，",
        "思乡望北意难堪。"
      ]
    },
    {
      "_id": null,
      "Name": "送皇甫冉往白田",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "七律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "江边尽日雉鸣飞，",
        "君向白田何日归。",
        "楚地蒹葭连海迥，",
        "隋朝杨柳映堤稀。",
        "津楼故市无行客，",
        "山馆空庭闭落晖。",
        "试问疲人与征战，",
        "使君双泪定沾衣。"
      ]
    },
    {
      "_id": null,
      "Name": "送真上人还兰若",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "得道云林久，",
        "年深暂一归。",
        "出山逢世乱，",
        "乞食觉人稀。",
        "半偈初传法，",
        "中峰又掩扉。",
        "爱憎应不染，",
        "尘俗自依依。"
      ]
    },
    {
      "_id": null,
      "Name": "送苏修游上饶",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "爱尔无羁束，",
        "云山恣意过。",
        "一身随远岫，",
        "孤棹任轻波。",
        "世事关情少，",
        "渔家寄宿多。",
        "芦花浅淡处，",
        "江月奈人何。"
      ]
    },
    {
      "_id": null,
      "Name": "送薛仲方归扬州",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "佳句应无敌，",
        "贞心不有猜。",
        "惭为丈人行，",
        "怯见后生才。",
        "泛舸贪斜月，",
        "浮桡值早梅。",
        "绿杨新过雨，",
        "芳草待君来。"
      ]
    },
    {
      "_id": null,
      "Name": "送薛良史往越州谒从叔",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "辞家年已久，",
        "与子分偏深。",
        "易得相思泪，",
        "难为欲别心。",
        "孤云随浦口，",
        "几日到山阴。",
        "遥想兰亭下，",
        "清风满竹林。"
      ]
    },
    {
      "_id": null,
      "Name": "送贺兰广赴选",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "而今用武尔攻文，",
        "流辈干时独卧云。",
        "白发青袍趋会府，",
        "定应衡镜却惭君。"
      ]
    },
    {
      "_id": null,
      "Name": "送陆明府之盱眙",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "陶令之官去，",
        "穷愁惨别魂。",
        "白烟横海戍，",
        "红叶下淮村。",
        "澹浪摇山郭，",
        "平芜到县门。",
        "政成堪吏隐，",
        "免负府公恩。"
      ]
    },
    {
      "_id": null,
      "Name": "送韦八少府判官归东京",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "七律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "玄成世业紫真官，",
        "文似相如貌胜潘。",
        "鸿雁南飞人独去，",
        "云山一别岁将阑。",
        "清淮水急桑林晚，",
        "古驿霜多柿叶寒。",
        "琼树相思何日见，",
        "银钩数字莫为难。"
      ]
    },
    {
      "_id": null,
      "Name": "送韦员外还京",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "十年离乱后，",
        "此去若为情。",
        "春晚香山绿，",
        "人稀豫水清。",
        "野陂看独树，",
        "关路逐残莺。",
        "前殿朝明主，",
        "应怜白发生。"
      ]
    },
    {
      "_id": null,
      "Name": "酬李补阙雨中寄赠",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "十年随马宿，",
        "几度受人恩。",
        "白发还乡井，",
        "微官有子孙。",
        "竹窗寒雨滴，",
        "苦砌夜虫喧。",
        "独愧东垣友，",
        "新诗慰旅魂。"
      ]
    },
    {
      "_id": null,
      "Name": "题兰若",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "绝顶茅庵老此生，",
        "寒云孤木独经行。",
        "世人那得知幽径，",
        "遥向青峰礼磬声。"
      ]
    },
    {
      "_id": null,
      "Name": "题崇福寺禅院",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "僧家竟何事，",
        "扫地与焚香。",
        "清磬度山翠，",
        "闲云来竹房。",
        "身心尘外远，",
        "岁月坐中长。",
        "向晚禅堂掩，",
        "无人空夕阳。"
      ]
    },
    {
      "_id": null,
      "Name": "题桐庐李明府官舍",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "七律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "讼堂寂寂对烟霞，",
        "五柳门前聚晓鸦。",
        "流水声中视公事，",
        "寒山影里见人家。",
        "观风竞美新为政，",
        "计日还知旧触邪。",
        "可惜陶潜无限酒，",
        "不逢篱菊正开花。"
      ]
    },
    {
      "_id": null,
      "Name": "题空山人石室",
      "PoetId": "f9af3f77efbb40c88578fd69be824416",
      "PoetName": "崔峒",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔峒"
      ],
      "Contents": [
        "早晚悟无生，",
        "头陀不到城。",
        "云山知夏腊，",
        "猿鸟见修行。",
        "地僻无溪路，",
        "人寻逐水声。",
        "年年深谷里，",
        "谁识远公名。"
      ]
    }
  ]
}