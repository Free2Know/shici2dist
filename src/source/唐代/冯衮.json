  "Poems": [
    {
      "_id": null,
      "Name": "戏酒妓",
      "PoetId": "6389471468b847f3b385e46bf3195fa6",
      "PoetName": "冯衮",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "冯衮"
      ],
      "Contents": [
        "醉眼从伊百度斜，",
        "是他家属是他家。",
        "低声向道人知也，",
        "隔坐刚抛豆蔻花。"
      ]
    },
    {
      "_id": null,
      "Name": "掷卢作",
      "PoetId": "6389471468b847f3b385e46bf3195fa6",
      "PoetName": "冯衮",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "冯衮"
      ],
      "Contents": [
        "八尺台盘照面新，",
        "千金一掷斗精神。",
        "合是赌时须赌取，",
        "不妨回首乞闲人。"
      ]
    }
  ]
}