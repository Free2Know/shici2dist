      "_id": null,
      "Name": "琴曲歌辞·湘夫人",
      "PoetId": "1b204f5b05c84c8398c816e061f8958c",
      "PoetName": "邹绍先",
      "Form": "未知",
      "Tags": [
        "唐代",
        "邹绍先"
      ],
      "Contents": [
        "枫叶下秋渚，",
        "二妃愁渡湘。",
        "疑山空杳蔼，",
        "何处望君王。",
        "日落水云里，",
        "悠悠心自伤。"
      ]
    }
  ]
