      "_id": null,
      "Name": "仲月赏花",
      "PoetId": "33446cfdf9324c7b8b2ee2c018e87938",
      "PoetName": "韦同则",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "韦同则"
      ],
      "Contents": [
        "梅花似雪柳含烟，",
        "南地风光腊月前。",
        "把酒且须拚却醉，",
        "风流何必待歌筵。"
      ]
    }
  ]
