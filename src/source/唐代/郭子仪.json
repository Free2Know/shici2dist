      "_id": null,
      "Name": "郊庙歌辞·享太庙乐章·保大舞",
      "PoetId": "a9711754905342009ae5c7a83d1fa0d3",
      "PoetName": "郭子仪",
      "Form": "未知",
      "Tags": [
        "唐代",
        "郭子仪"
      ],
      "Contents": [
        "於穆文考，",
        "圣神昭章。",
        "肃勺群慝，",
        "含光远方。",
        "万物茂遂，",
        "九夷宾王。",
        "愔愔云韶，",
        "德音不忘。"
      ]
    }
  ]
