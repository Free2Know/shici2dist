  "Name": "祝元膺",
  "Description": "祝元膺，句曲人，与段成式同时。诗三首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "寄道友",
      "PoetId": "8c4fb389ec324960b5a0e87c2d0f4264",
      "PoetName": "祝元膺",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "祝元膺"
      ],
      "Contents": [
        "两颔凝清霜，",
        "玉炉焚天香。",
        "为我延岁华，",
        "得入不死乡。"
      ]
    },
    {
      "_id": null,
      "Name": "梦仙谣",
      "PoetId": "8c4fb389ec324960b5a0e87c2d0f4264",
      "PoetName": "祝元膺",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "祝元膺"
      ],
      "Contents": [
        "蟾蜍夜作青冥烛，",
        "螮蝀晴为碧落梯。",
        "好个分明天上路，",
        "谁教深入武陵溪。"
      ]
    },
    {
      "_id": null,
      "Name": "送高遂赴举",
      "PoetId": "8c4fb389ec324960b5a0e87c2d0f4264",
      "PoetName": "祝元膺",
      "Form": "未知",
      "Tags": [
        "唐代",
        "祝元膺"
      ],
      "Contents": [
        "句曲旧宅真，",
        "自产日月英。",
        "既涵岳渎气，",
        "安无神仙名。",
        "松桂逦迤色，",
        "与君相送情。"
      ]
    }
  ]
}