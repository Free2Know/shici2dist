  "Poems": [
    {
      "_id": null,
      "Name": "赠人",
      "PoetId": "c9abdf1ea28044fbac76890f3cd4ab04",
      "PoetName": "滕传胤",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "滕传胤"
      ],
      "Contents": [
        "平生才不足，",
        "立身信有余。",
        "自叹无大故，",
        "君子莫相疏。"
      ]
    },
    {
      "_id": null,
      "Name": "赠僧",
      "PoetId": "c9abdf1ea28044fbac76890f3cd4ab04",
      "PoetName": "滕传胤",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "滕传胤"
      ],
      "Contents": [
        "卓立不求名出家，",
        "长怀片志在青霞。",
        "今日英雄气冲盖，",
        "谁能久坐宝莲花。"
      ]
    },
    {
      "_id": null,
      "Name": "郑锋宅神诗",
      "PoetId": "c9abdf1ea28044fbac76890f3cd4ab04",
      "PoetName": "滕传胤",
      "Form": "七律",
      "Tags": [
        "唐代",
        "滕传胤"
      ],
      "Contents": [
        "浦口潮来初渺漫，",
        "莲舟摇飏采花难。",
        "春心不惬空归去，",
        "会待潮回更折看。",
        "忽然湖上片云飞，",
        "不觉舟中雨湿衣。",
        "折得莲花浑忘却，",
        "空将荷叶盖头归。"
      ]
    }
  ]
}