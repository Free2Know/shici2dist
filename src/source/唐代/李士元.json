  "Poems": [
    {
      "_id": null,
      "Name": "登单于台",
      "PoetId": "d55ac77134c84906914d147d6d3ce31e",
      "PoetName": "李士元",
      "Form": "五律",
      "Tags": [
        "唐代",
        "李士元"
      ],
      "Contents": [
        "悔上层楼望，",
        "翻成极目愁。",
        "路沿葱岭去，",
        "河背玉关流。",
        "马散眠沙碛，",
        "兵闲倚戍楼。",
        "残阳三会角，",
        "吹白旅人头。"
      ]
    },
    {
      "_id": null,
      "Name": "登阁",
      "PoetId": "d55ac77134c84906914d147d6d3ce31e",
      "PoetName": "李士元",
      "Form": "七律",
      "Tags": [
        "唐代",
        "李士元"
      ],
      "Contents": [
        "乱后独来登大阁，",
        "凭阑举目尽伤心。",
        "长堤过雨人行少，",
        "废苑经秋草自深。",
        "破落侯家通永巷，",
        "萧条宫树接疏林。",
        "总输释氏青莲馆，",
        "依旧重重布地金。"
      ]
    }
  ]
}