{
  "_id": "a2265d8ec0734bbb94a98facaab7a82f",
  "Name": "裴迪",
  "Description": "河东人，与王维隐辋川。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "与卢员外象过崔处士兴宗林亭",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "乔柯门里自成阴，",
        "散发窗中曾不簪。",
        "逍遥且喜从吾事，",
        "荣宠从来非我心。"
      ]
    },
    {
      "_id": null,
      "Name": "华子冈",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "日落松风起，",
        "还家草露晞。",
        "云光侵履迹，",
        "山翠拂人衣。"
      ]
    },
    {
      "_id": null,
      "Name": "夏日过青龙寺谒操禅师",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "五律",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "安禅一室内，",
        "左右竹亭幽。",
        "有法知不染，",
        "无言谁敢酬。",
        "鸟飞争向夕，",
        "蝉噪已先秋。",
        "烦暑自兹适，",
        "清凉何所求。"
      ]
    },
    {
      "_id": null,
      "Name": "文杏馆",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "迢迢文杏馆，",
        "跻攀日已屡。",
        "南岭与北湖，",
        "前后复回顾。"
      ]
    },
    {
      "_id": null,
      "Name": "春日与王右丞过新昌里访吕逸人不遇",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "七律",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "恨不逢君出荷蓑，",
        "青松白屋更无他。",
        "陶令五男曾不有，",
        "蒋生三径枉相过。",
        "芙蓉曲沼春流满，",
        "薜荔成帷晚霭多。",
        "闻说桃源好迷客，",
        "不如高卧眄庭柯。"
      ]
    },
    {
      "_id": null,
      "Name": "木兰柴",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "苍苍落日诗，",
        "鸟声乱溪水。",
        "缘溪路转深，",
        "幽兴何时已。"
      ]
    },
    {
      "_id": null,
      "Name": "栾家獭",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "濑声喧极浦，",
        "沿步向南津。",
        "泛泛凫鸥渡，",
        "时时欲近人。"
      ]
    },
    {
      "_id": null,
      "Name": "游感化寺昙兴上人山院",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "五律",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "不远灞陵边，",
        "安居向十年。",
        "入门穿竹径，",
        "留客听山泉。",
        "鸟啭深林里，",
        "心闲落照前。",
        "浮名竟何益，",
        "从此愿栖禅。"
      ]
    },
    {
      "_id": null,
      "Name": "漆园",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "好闲早成性，",
        "果此谐宿诺。",
        "今日漆园游，",
        "还同庄叟乐。"
      ]
    },
    {
      "_id": null,
      "Name": "西塔寺陆羽茶泉",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "五律",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "竟陵西塔寺，",
        "踪迹尚空虚。",
        "不独支公住，",
        "曾经陆羽居。",
        "草堂荒产蛤，",
        "茶井冷生鱼。",
        "一汲清泠水，",
        "高风味有馀。"
      ]
    },
    {
      "_id": null,
      "Name": "辋口遇雨忆终南山因献王维",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "积雨晦空曲，",
        "平沙灭浮彩。",
        "辋水去悠悠，",
        "南山复何在。"
      ]
    },
    {
      "_id": null,
      "Name": "辋川集二十首",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "未知",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "跂石复临水，",
        "弄波情未极。",
        "日下川上寒，",
        "浮云澹无色。",
        "南山北垞下，",
        "结宇临欹湖。",
        "每欲采樵去，",
        "扁舟出菰蒲。",
        "门前宫槐陌，",
        "是向欹湖道。",
        "秋来山雨多，",
        "落叶无人扫。",
        "落日松风起，",
        "还家草露晞。",
        "云光侵履迹，",
        "山翠拂人衣。",
        "丹刺罥人衣，",
        "芳香留过客。",
        "幸堪调鼎用，",
        "愿君垂采摘。",
        "明流纡且直，",
        "绿筱密复深。",
        "一径通山路，",
        "行歌望旧岑。",
        "萦渟澹不流，",
        "金碧如可拾。",
        "迎晨含素华，",
        "独往事朝汲。",
        "当轩弥滉漾，",
        "孤月正裴回。",
        "谷口猿声发，",
        "风传入户来。",
        "映池同一色，",
        "逐吹散如丝。",
        "结阴既得地，",
        "何谢陶家时。",
        "结庐古城下，",
        "时登古城上。",
        "古城非畴昔，",
        "今人自来往。",
        "苍苍落日时，",
        "鸟声乱溪水。",
        "缘溪路转深，",
        "幽兴何时已。",
        "孤舟信一泊，",
        "南垞湖水岸。",
        "落日下崦嵫，",
        "清波殊淼漫。",
        "空阔湖水广，",
        "青荧天色同。",
        "舣舟一长啸，",
        "四面来清风。",
        "飘香乱椒桂，",
        "布叶间檀栾。",
        "云日虽回照，",
        "森沉犹自寒。",
        "来过竹里馆，",
        "日与道相亲。",
        "出入唯山鸟，",
        "幽深无世人。",
        "以上裴迪作品《辋川集二十首》共15首        "
      ]
    },
    {
      "_id": null,
      "Name": "辛夷坞",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "绿堤春草合，",
        "王孙自留玩。",
        "况有辛夷花，",
        "色与芙蓉乱。"
      ]
    },
    {
      "_id": null,
      "Name": "送崔九",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "归山深浅去，",
        "须尽丘壑美。",
        "莫学武陵人，",
        "暂游桃源里。"
      ]
    },
    {
      "_id": null,
      "Name": "青雀歌",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "未知",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "动息自适性，",
        "不曾妄与燕雀群。",
        "幸忝鹓鸾早相识，",
        "何时提携致青云。"
      ]
    },
    {
      "_id": null,
      "Name": "青龙寺昙壁上人院集",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "未知",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "灵境信为绝，",
        "法堂出尘氛。",
        "自然成高致，",
        "向下看浮云。",
        "迤逦峰岫列，",
        "参差闾井分。",
        "林端远堞见，",
        "风末疏钟闻。",
        "吾师久禅寂，",
        "在世超人群。"
      ]
    },
    {
      "_id": null,
      "Name": "鹿柴",
      "PoetId": "a2265d8ec0734bbb94a98facaab7a82f",
      "PoetName": "裴迪",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "裴迪"
      ],
      "Contents": [
        "日夕见寒山，",
        "便为独往客。",
        "不知深林事，",
        "但有麏麚迹。"
      ]
    }
  ]
}