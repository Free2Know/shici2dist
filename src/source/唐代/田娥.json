    {
      "_id": null,
      "Name": "寄远",
      "PoetId": "d96af09bcd5743fd961864cfca50ebb7",
      "PoetName": "田娥",
      "Form": "五律",
      "Tags": [
        "唐代",
        "田娥"
      ],
      "Contents": [
        "忆昨会诗酒，",
        "终日相逢迎。",
        "今来成故事，",
        "岁月令人惊。",
        "泪流红粉薄，",
        "风度罗衣轻。",
        "难为子猷志，",
        "虚负文君名。"
      ]
    },
    {
      "_id": null,
      "Name": "杂曲歌辞·夜夜曲",
      "PoetId": "d96af09bcd5743fd961864cfca50ebb7",
      "PoetName": "田娥",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "田娥"
      ],
      "Contents": [
        "愁人夜独伤，",
        "灭烛卧兰房。",
        "只恐多情月，",
        "旋来照妾床。"
      ]
    },
    {
      "_id": null,
      "Name": "杂曲歌辞·携手曲",
      "PoetId": "d96af09bcd5743fd961864cfca50ebb7",
      "PoetName": "田娥",
      "Form": "五律",
      "Tags": [
        "唐代",
        "田娥"
      ],
      "Contents": [
        "携手共惜芳菲节，",
        "莺啼锦花满城阙。",
        "行乐逶迤念容色，",
        "色衰只恐君恩歇。",
        "凤笙龙管白日阴，",
        "盈亏自感青天月。"
      ]
    },
    {
      "_id": null,
      "Name": "长信宫",
      "PoetId": "d96af09bcd5743fd961864cfca50ebb7",
      "PoetName": "田娥",
      "Form": "未知",
      "Tags": [
        "唐代",
        "田娥"
      ],
      "Contents": [
        "团圆手中扇，",
        "昔为君所持。",
        "今日君弃捐，",
        "复值秋风时。",
        "悲将入箧笥，",
        "自叹知何为。"
      ]
    }
  ]
}