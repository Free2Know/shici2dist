  "_id": "e958a0321dab47b7bbcffd8d4581b5d2",
  "Name": "于季子",
  "Description": "于季子，咸亨中登进士第，则天时司封员外。诗七首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "咏云",
      "PoetId": "e958a0321dab47b7bbcffd8d4581b5d2",
      "PoetName": "于季子",
      "Form": "五律",
      "Tags": [
        "唐代",
        "于季子"
      ],
      "Contents": [
        "瑞云千里映，",
        "祥辉四望新。",
        "随风乱鸟翅，",
        "泛水结鱼鳞。",
        "布叶疑临夏，",
        "开花讵待春。",
        "愿得承嘉景，",
        "无令掩桂轮。"
      ]
    },
    {
      "_id": null,
      "Name": "咏汉高祖",
      "PoetId": "e958a0321dab47b7bbcffd8d4581b5d2",
      "PoetName": "于季子",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "于季子"
      ],
      "Contents": [
        "百战方夷项，",
        "三章且代秦。",
        "功归萧相国，",
        "气尽戚夫人。"
      ]
    },
    {
      "_id": null,
      "Name": "咏萤",
      "PoetId": "e958a0321dab47b7bbcffd8d4581b5d2",
      "PoetName": "于季子",
      "Form": "五律",
      "Tags": [
        "唐代",
        "于季子"
      ],
      "Contents": [
        "卉草诚幽贱，",
        "枯朽绝因依。",
        "忽逢借羽翼，",
        "不觉生光辉。",
        "直念恩华重，",
        "长嗟报效微。",
        "方思助日月，",
        "为许愿曾飞。"
      ]
    },
    {
      "_id": null,
      "Name": "咏项羽",
      "PoetId": "e958a0321dab47b7bbcffd8d4581b5d2",
      "PoetName": "于季子",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "于季子"
      ],
      "Contents": [
        "北伐虽全赵，",
        "东归不王秦。",
        "空歌拔山力，",
        "羞作渡江人。"
      ]
    },
    {
      "_id": null,
      "Name": "奉和圣制夏日游石淙山",
      "PoetId": "e958a0321dab47b7bbcffd8d4581b5d2",
      "PoetName": "于季子",
      "Form": "七律",
      "Tags": [
        "唐代",
        "于季子"
      ],
      "Contents": [
        "九旗云布临嵩室，",
        "万骑星陈集颍川。",
        "瑞液含滋登禹膳，",
        "飞流荐响入虞弦。",
        "山扉野径朝花积，",
        "帐殿帷宫夏叶连。",
        "微臣献寿迎千寿，",
        "愿奉尧年倚万年。"
      ]
    },
    {
      "_id": null,
      "Name": "早春洛阳答杜审言",
      "PoetId": "e958a0321dab47b7bbcffd8d4581b5d2",
      "PoetName": "于季子",
      "Form": "七律",
      "Tags": [
        "唐代",
        "于季子"
      ],
      "Contents": [
        "梓泽年光往复来，",
        "杜霸游人去不回。",
        "若非载笔登麟阁，",
        "定是吹箫伴凤台。",
        "路傍桃李花犹嫩，",
        "波上芙蕖叶未开。",
        "分明寄语长安道，",
        "莫教留滞洛阳才。"
      ]
    }
  ]
}