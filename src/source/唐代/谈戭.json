  "Poems": [
    {
      "_id": null,
      "Name": "清谿馆作",
      "PoetId": "8611e8a4614f45d4a8c0d250b0b9ce04",
      "PoetName": "谈戭",
      "Form": "五律",
      "Tags": [
        "唐代",
        "谈戭"
      ],
      "Contents": [
        "指途清谿里，",
        "左右唯深林。",
        "云蔽望乡处，",
        "雨愁为客心。",
        "遇人多物役，",
        "听鸟时幽音。",
        "何必沧浪水，",
        "庶兹浣尘襟。"
      ]
    }
  ]
}