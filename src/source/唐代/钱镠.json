  "Name": "钱镠",
  "Description": "吳越王錢鏐 ",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "句",
      "PoetId": "e94e2f018256461d9c6523369e2893c7",
      "PoetName": "钱镠",
      "Form": "未知",
      "Tags": [
        "唐代",
        "钱镠"
      ],
      "Contents": [
        "须将一片地，",
        "付与有心人。",
        "黄河信有澄清日，",
        "后代应难继此才。",
        "传语龙王并水府，",
        "钱塘借与筑钱城。"
      ]
    },
    {
      "_id": null,
      "Name": "巡衣锦军制还乡歌",
      "PoetId": "e94e2f018256461d9c6523369e2893c7",
      "PoetName": "钱镠",
      "Form": "未知",
      "Tags": [
        "唐代",
        "钱镠"
      ],
      "Contents": [
        "三节还乡兮挂锦衣，",
        "碧天朗朗兮爱日晖。",
        "功成道上兮列旌旗，",
        "父老远来兮相追随。",
        "家山乡眷兮会时稀，",
        "今朝设宴兮觥散飞。",
        "斗牛无孛兮民无欺，",
        "吴越一王兮驷马归。"
      ]
    },
    {
      "_id": null,
      "Name": "没了期歌",
      "PoetId": "e94e2f018256461d9c6523369e2893c7",
      "PoetName": "钱镠",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "钱镠"
      ],
      "Contents": [
        "没了期，",
        "没了期，",
        "营基才了又仓基。",
        "没了期，",
        "没了期，",
        "春衣才了又冬衣。"
      ]
    }
  ]
}