  "Poems": [
    {
      "_id": null,
      "Name": "题汉州西湖",
      "PoetId": "a42d52832a35464ca5335c4a12382b19",
      "PoetName": "严公贶",
      "Form": "五律",
      "Tags": [
        "唐代",
        "严公贶"
      ],
      "Contents": [
        "凤沼才难尽，",
        "余思凿西湖。",
        "珍木罗修岸，",
        "冰光映坐隅。",
        "琴台今寂寞，",
        "竹岛尚萦纡。",
        "犹蕴济川志，",
        "芳名终不渝。"
      ]
    }
  ]
}