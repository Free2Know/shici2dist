  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "东峰亭同刘太真各赋一物得垂涧藤",
      "PoetId": "e3c27d62e4f24a68855f56e48dc75f8c",
      "PoetName": "袁傪",
      "Form": "未知",
      "Tags": [
        "唐代",
        "袁傪"
      ],
      "Contents": [
        "寒涧流不息，",
        "古藤终日垂。",
        "迎风仍未定，",
        "拂水更相宜。",
        "新花与旧叶，",
        "惟有幽人知。"
      ]
    },
    {
      "_id": null,
      "Name": "喜陆侍御破石埭草寇东峰亭赋诗",
      "PoetId": "e3c27d62e4f24a68855f56e48dc75f8c",
      "PoetName": "袁傪",
      "Form": "未知",
      "Tags": [
        "唐代",
        "袁傪"
      ],
      "Contents": [
        "古寺东峰上，",
        "登临兴有馀。",
        "同观白简使，",
        "新报赤囊书。",
        "几处闲烽堠，",
        "千方庆里闾。",
        "欣欣夏木长，",
        "寂寂晚烟徐。",
        "战罢言归马，",
        "还师赋出车。",
        "因知越范蠡，",
        "湖海意何如。"
      ]
    }
  ]
}