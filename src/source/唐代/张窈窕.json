{
  "_id": "d591fd402f794951b1976e7105865619",
  "Name": "张窈窕",
  "Description": "张窈窕，寓居于蜀，当时诗人雅相推重。诗六首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "上成都在事",
      "PoetId": "d591fd402f794951b1976e7105865619",
      "PoetName": "张窈窕",
      "Form": "五律",
      "Tags": [
        "唐代",
        "张窈窕"
      ],
      "Contents": [
        "昨日卖衣裳，",
        "今日卖衣裳。",
        "衣裳浑卖尽，",
        "羞见嫁时箱。",
        "有卖愁仍缓，",
        "无时心转伤。",
        "故园有虏隔，",
        "何处事蚕桑。"
      ]
    },
    {
      "_id": null,
      "Name": "寄故人",
      "PoetId": "d591fd402f794951b1976e7105865619",
      "PoetName": "张窈窕",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "张窈窕"
      ],
      "Contents": [
        "淡淡春风花落时，",
        "不堪愁望更相思。",
        "无金可买长门赋，",
        "有恨空吟团扇诗。"
      ]
    },
    {
      "_id": null,
      "Name": "春思二首",
      "PoetId": "d591fd402f794951b1976e7105865619",
      "PoetName": "张窈窕",
      "Form": "七律",
      "Tags": [
        "唐代",
        "张窈窕"
      ],
      "Contents": [
        "门前梅柳烂春辉，",
        "闭妾深闺绣舞衣。",
        "双燕不知肠欲断，",
        "衔泥故故傍人飞。",
        "井上梧桐是妾移，",
        "夜来花发最高枝。",
        "若教不向深闺种，",
        "春过门前争得知。"
      ]
    },
    {
      "_id": null,
      "Name": "赠所思",
      "PoetId": "d591fd402f794951b1976e7105865619",
      "PoetName": "张窈窕",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "张窈窕"
      ],
      "Contents": [
        "与君咫尺长离别，",
        "遣妾容华为谁说。",
        "夕望层城眼欲穿，",
        "晓临明镜肠堪绝。"
      ]
    }
  ]
}