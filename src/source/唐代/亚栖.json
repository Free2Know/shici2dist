    {
      "_id": null,
      "Name": "对御书后一绝",
      "PoetId": "c1de5ed9f1cb41e5bbaf316f2e1527c7",
      "PoetName": "亚栖",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "亚栖"
      ],
      "Contents": [
        "通神笔法得玄门，",
        "亲入长安谒至尊。",
        "莫怪出来多意气，",
        "草书曾悦圣明君。"
      ]
    },
    {
      "_id": null,
      "Name": "题英禅师",
      "PoetId": "c1de5ed9f1cb41e5bbaf316f2e1527c7",
      "PoetName": "亚栖",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "亚栖"
      ],
      "Contents": [
        "将知德行异寻常，",
        "每见持经在道场。",
        "欲识用心精洁处，",
        "一瓶秋水一炉香。"
      ]
    }
  ]
}