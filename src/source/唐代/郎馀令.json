      "_id": null,
      "Name": "晦日宴高氏林亭",
      "PoetId": "48ee75b6262c4945b2221d39f8fbf60d",
      "PoetName": "郎馀令",
      "Form": "五律",
      "Tags": [
        "唐代",
        "郎馀令"
      ],
      "Contents": [
        "三春休晦节，",
        "九谷泛年华。",
        "半晴馀细雨，",
        "全晚澹残霞。",
        "尊开疏竹叶，",
        "管应落梅花。",
        "兴阑相顾起，",
        "流水送香车。"
      ]
    }
  ]
