{
  "_id": "920cc2cf2a104959bd9de26b2601005d",
  "Name": "张旭",
  "Description": "张旭，字伯高，吴郡人。工书法。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "山行留客",
      "PoetId": "920cc2cf2a104959bd9de26b2601005d",
      "PoetName": "张旭",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "张旭"
      ],
      "Contents": [
        "山光物态弄春辉，",
        "莫为轻阴便拟归。",
        "纵使晴明无雨色，",
        "入云深处亦沾衣。"
      ]
    },
    {
      "_id": null,
      "Name": "春游值雨",
      "PoetId": "920cc2cf2a104959bd9de26b2601005d",
      "PoetName": "张旭",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "张旭"
      ],
      "Contents": [
        "欲寻轩槛列清尊，",
        "江上烟云向晚昏。",
        "须倩东风吹散雨，",
        "明朝却待入华园。"
      ]
    },
    {
      "_id": null,
      "Name": "春草",
      "PoetId": "920cc2cf2a104959bd9de26b2601005d",
      "PoetName": "张旭",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "张旭"
      ],
      "Contents": [
        "春草青青万里馀，",
        "边城落日见离居。",
        "情知海上三年别，",
        "不寄云间一纸书。"
      ]
    },
    {
      "_id": null,
      "Name": "柳",
      "PoetId": "920cc2cf2a104959bd9de26b2601005d",
      "PoetName": "张旭",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "张旭"
      ],
      "Contents": [
        "濯濯烟条拂地垂，",
        "城边楼畔结春思。",
        "请君细看风流意，",
        "未减灵和殿里时。"
      ]
    },
    {
      "_id": null,
      "Name": "桃花溪",
      "PoetId": "920cc2cf2a104959bd9de26b2601005d",
      "PoetName": "张旭",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "张旭"
      ],
      "Contents": [
        "隐隐飞桥隔野烟，",
        "石矶西畔问渔船。",
        "桃花尽日随流水，",
        "洞在清溪何处边。"
      ]
    },
    {
      "_id": null,
      "Name": "清溪泛舟",
      "PoetId": "920cc2cf2a104959bd9de26b2601005d",
      "PoetName": "张旭",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "张旭"
      ],
      "Contents": [
        "旅人倚征棹，",
        "薄暮起劳歌。",
        "笑揽清溪月，",
        "清辉不厌多。"
      ]
    }
  ]
}