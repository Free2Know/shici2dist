    {
      "_id": null,
      "Name": "孙玄照琴中歌赠王仙仙",
      "PoetId": "e568fe038bd5418c9260453da12468c4",
      "PoetName": "王仙仙",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "王仙仙"
      ],
      "Contents": [
        "相如曾作凤兮吟，",
        "昔被文君会此音。",
        "今日孤鸾还独语，",
        "痛哉仙子不弹琴。"
      ]
    },
    {
      "_id": null,
      "Name": "答孙玄照",
      "PoetId": "e568fe038bd5418c9260453da12468c4",
      "PoetName": "王仙仙",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "王仙仙"
      ],
      "Contents": [
        "鸳鸯相见不相随，",
        "笼里笼前整羽衣。",
        "但得他时人放去，",
        "水中长作一双飞。"
      ]
    }
  ]
}