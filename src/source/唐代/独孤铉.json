      "_id": null,
      "Name": "日南长至",
      "PoetId": "ba61f2d9eded416aa14580370c8d4a29",
      "PoetName": "独孤铉",
      "Form": "未知",
      "Tags": [
        "唐代",
        "独孤铉"
      ],
      "Contents": [
        "玉历颁新律，",
        "凝阴发一阳。",
        "轮辉犹惜短，",
        "圭影此偏长。",
        "晷度经南斗，",
        "流晶尽北堂。",
        "乍疑周户耀，",
        "可爱逗林光。",
        "积雪销微照，",
        "初萌动早芒。",
        "更升台上望，",
        "云物已昭彰。"
      ]
    }
  ]
}