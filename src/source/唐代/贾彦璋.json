    {
      "_id": null,
      "Name": "宿香山阁",
      "PoetId": "c252112172cf46588271ff7c7be702b1",
      "PoetName": "贾彦璋",
      "Form": "五律",
      "Tags": [
        "唐代",
        "贾彦璋"
      ],
      "Contents": [
        "暝望香山阁，",
        "梯云宿半空。",
        "轩窗闭潮海，",
        "枕席拂烟虹。",
        "朱网防栖鸽，",
        "纱灯护夕虫。",
        "一闻鸡唱晓，",
        "已见日曈曈。"
      ]
    },
    {
      "_id": null,
      "Name": "晚霁登汝南大云阁",
      "PoetId": "c252112172cf46588271ff7c7be702b1",
      "PoetName": "贾彦璋",
      "Form": "五律",
      "Tags": [
        "唐代",
        "贾彦璋"
      ],
      "Contents": [
        "禅宫新歇雨，",
        "香阁晚登临。",
        "邑树晴光起，",
        "川苗佳气深。",
        "水包城下岸，",
        "云细郢中岑。",
        "自叹牵卑日，",
        "聊开望远心。"
      ]
    },
    {
      "_id": null,
      "Name": "王龙骧墓",
      "PoetId": "c252112172cf46588271ff7c7be702b1",
      "PoetName": "贾彦璋",
      "Form": "五律",
      "Tags": [
        "唐代",
        "贾彦璋"
      ],
      "Contents": [
        "昔擅登坛宠，",
        "爰光典午朝。",
        "刀悬临益梦，",
        "龙启渡江谣。",
        "茂绩当年举，",
        "英魂此地销。",
        "唯余孤垄上，",
        "日夕起松飙。"
      ]
    },
    {
      "_id": null,
      "Name": "苏著作山池",
      "PoetId": "c252112172cf46588271ff7c7be702b1",
      "PoetName": "贾彦璋",
      "Form": "五律",
      "Tags": [
        "唐代",
        "贾彦璋"
      ],
      "Contents": [
        "水树子云家，",
        "峰瀛宛不赊。",
        "芥浮舟是叶，",
        "莲发岫为花。",
        "酌蚁开春瓮，",
        "观鱼凭海查。",
        "游苏多石友，",
        "题赠满瑶华。"
      ]
    }
  ]
}