  "Poems": [
    {
      "_id": null,
      "Name": "游东观山",
      "PoetId": "93270ed016c84da9830005eea2da71fb",
      "PoetName": "张丛",
      "Form": "五律",
      "Tags": [
        "唐代",
        "张丛"
      ],
      "Contents": [
        "岩岫碧孱颜，",
        "灵踪若可攀。",
        "楼台烟霭外，",
        "松竹翠微间。",
        "玉液寒深洞，",
        "秋光秀远山。",
        "凭君指归路，",
        "何处是人寰。"
      ]
    }
  ]
}