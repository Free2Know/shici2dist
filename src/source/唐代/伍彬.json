  "Poems": [
    {
      "_id": null,
      "Name": "分水岭",
      "PoetId": "1925fd9aa1b647de9f6a3b36c7dd9598",
      "PoetName": "伍彬",
      "Form": "五律",
      "Tags": [
        "唐代",
        "伍彬"
      ],
      "Contents": [
        "前贤功及物，",
        "禹后杳难俦。",
        "不改古今色，",
        "平分南北流。",
        "寒冲山影岸，",
        "清绕荻花洲。",
        "尽是朝宗去，",
        "潺潺早晚休。"
      ]
    },
    {
      "_id": null,
      "Name": "句",
      "PoetId": "1925fd9aa1b647de9f6a3b36c7dd9598",
      "PoetName": "伍彬",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "伍彬"
      ],
      "Contents": [
        "稚子出看莎径没，",
        "渔翁来报竹桥流。",
        "踪迹未辞鸳鹭客，",
        "梦魂先到鹧鸪村。"
      ]
    }
  ]
}