  "Poems": [
    {
      "_id": null,
      "Name": "释奠日国学观礼闻雅颂",
      "PoetId": "f8ecfe1897994844b9f110d9ea38beed",
      "PoetName": "滕珦",
      "Form": "未知",
      "Tags": [
        "唐代",
        "滕珦"
      ],
      "Contents": [
        "太学时观礼，",
        "东方晓色分。",
        "威仪何棣棣，",
        "环珮又纷纷。",
        "古乐从空尽，",
        "清歌几处闻。",
        "六和成远吹，",
        "九奏动行云。",
        "圣上尊儒学，",
        "春秋奠茂勋。",
        "幸因陪齿列，",
        "聊以颂斯文。",
        "第二百五十四卷"
      ]
    }
  ]
}