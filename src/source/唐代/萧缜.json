      "_id": null,
      "Name": "前望江麹令颂德",
      "PoetId": "dd8860938a60414387a38e10e1f119a8",
      "PoetName": "萧缜",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "萧缜"
      ],
      "Contents": [
        "政绩虽殊道且同，",
        "无辞买石纪前功。",
        "谁论重德光青史，",
        "过里犹歌卧辙风。"
      ]
    }
  ]
