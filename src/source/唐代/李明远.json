  "Poems": [
    {
      "_id": null,
      "Name": "送韦觐谪潘州",
      "PoetId": "6c4297227206423e861fe03afd068fd7",
      "PoetName": "李明远",
      "Form": "五律",
      "Tags": [
        "唐代",
        "李明远"
      ],
      "Contents": [
        "北鸟飞不到，",
        "南人谁去游。",
        "天涯浮瘴水，",
        "岭外向潘州。",
        "草木春秋暮，",
        "猿猱日夜愁。",
        "定知迁客泪，",
        "应只对君流。"
      ]
    }
  ]
}