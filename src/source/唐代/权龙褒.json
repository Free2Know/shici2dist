  "Poems": [
    {
      "_id": null,
      "Name": "初到沧州呈州官",
      "PoetId": "202ea31baacc40ccad01ab86957a6a75",
      "PoetName": "权龙褒",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "权龙褒"
      ],
      "Contents": [
        "遥看沧海城，",
        "杨柳郁青青。",
        "中央一群汉，",
        "聚坐打杯觥。"
      ]
    },
    {
      "_id": null,
      "Name": "喜雨",
      "PoetId": "202ea31baacc40ccad01ab86957a6a75",
      "PoetName": "权龙褒",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "权龙褒"
      ],
      "Contents": [
        "暗去也没雨，",
        "明来也没云。",
        "日头赫赤出，",
        "地上绿氤氲。"
      ]
    },
    {
      "_id": null,
      "Name": "岭南归后献诗",
      "PoetId": "202ea31baacc40ccad01ab86957a6a75",
      "PoetName": "权龙褒",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "权龙褒"
      ],
      "Contents": [
        "龙褒有何罪，",
        "天恩放岭南。",
        "敕知无罪过，",
        "追来与将军。"
      ]
    },
    {
      "_id": null,
      "Name": "秋日述怀",
      "PoetId": "202ea31baacc40ccad01ab86957a6a75",
      "PoetName": "权龙褒",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "权龙褒"
      ],
      "Contents": [
        "檐前飞七百，",
        "雪白后园强。",
        "饱食房里侧，",
        "家粪集野螂。"
      ]
    }
  ]
}