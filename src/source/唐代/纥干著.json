  "Poems": [
    {
      "_id": null,
      "Name": "古仙词",
      "PoetId": "c8c030011d5d4e25b83c0aed8926be5d",
      "PoetName": "纥干著",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "纥干著"
      ],
      "Contents": [
        "珠幡绛节晓霞中，",
        "汉武清斋待少翁。",
        "不向人间恋春色，",
        "桃花自满紫阳宫。"
      ]
    },
    {
      "_id": null,
      "Name": "感春词",
      "PoetId": "c8c030011d5d4e25b83c0aed8926be5d",
      "PoetName": "纥干著",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "纥干著"
      ],
      "Contents": [
        "未得鸣珂谒汉宫，",
        "江头寂寞向春风。",
        "悲歌一曲心应醉，",
        "万叶千花泪眼中。"
      ]
    },
    {
      "_id": null,
      "Name": "灞上",
      "PoetId": "c8c030011d5d4e25b83c0aed8926be5d",
      "PoetName": "纥干著",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "纥干著"
      ],
      "Contents": [
        "鸣鞭晚日禁城东，",
        "渭水晴烟灞岸风。",
        "都傍柳阴回首望，",
        "春天楼阁五云中。"
      ]
    },
    {
      "_id": null,
      "Name": "赏残花",
      "PoetId": "c8c030011d5d4e25b83c0aed8926be5d",
      "PoetName": "纥干著",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "纥干著"
      ],
      "Contents": [
        "零落多依草，",
        "芳香散著人。",
        "低檐一枝在，",
        "犹占满堂春。"
      ]
    }
  ]
}