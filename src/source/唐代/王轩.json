  "Poems": [
    {
      "_id": null,
      "Name": "佳人去千载",
      "PoetId": "03605141e1fa46779590f7b14aca49da",
      "PoetName": "王轩",
      "Form": "五律",
      "Tags": [
        "唐代",
        "王轩"
      ],
      "Contents": [
        "佳人去千载，",
        "溪山久寂寞。",
        "野水浮白烟，",
        "岩花自开落。",
        "猿鸟旧清音，",
        "风月闲楼阁。",
        "无语立斜阳，",
        "幽情入天幕。"
      ]
    },
    {
      "_id": null,
      "Name": "当时计拙笑将军",
      "PoetId": "03605141e1fa46779590f7b14aca49da",
      "PoetName": "王轩",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "王轩"
      ],
      "Contents": [
        "当时计拙笑将军，",
        "何事安邦赖美人。",
        "一自仙葩入吴国，",
        "从兹越国更无春。"
      ]
    },
    {
      "_id": null,
      "Name": "题西施石诗",
      "PoetId": "03605141e1fa46779590f7b14aca49da",
      "PoetName": "王轩",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "王轩"
      ],
      "Contents": [
        "岭上千峰秀，",
        "江边细草春。",
        "今逢浣纱石，",
        "不见浣纱人。"
      ]
    }
  ]
}