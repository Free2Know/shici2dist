  "Poems": [
    {
      "_id": null,
      "Name": "奉和九月九日登慈恩寺浮图应制",
      "PoetId": "2242e246174649e3b0753f7ec03af711",
      "PoetName": "李从远",
      "Form": "五律",
      "Tags": [
        "唐代",
        "李从远"
      ],
      "Contents": [
        "九月从时豫，",
        "三乘为法开。",
        "中霄日天子，",
        "半座宝如来。",
        "摘果珠盘献，",
        "攀萸玉辇回。",
        "愿将尘露点，",
        "遥奉光明台。"
      ]
    }
  ]
}