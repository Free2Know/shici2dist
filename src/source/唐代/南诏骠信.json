  "Poems": [
    {
      "_id": null,
      "Name": "星回节游避风台与清平官赋",
      "PoetId": "2877ce1a41a947b4900c4c6a08596aab",
      "PoetName": "南诏骠信",
      "Form": "未知",
      "Tags": [
        "唐代",
        "南诏骠信"
      ],
      "Contents": [
        "避风善阐台，",
        "极目见藤越。",
        "悲哉古与今，",
        "依然烟与月。",
        "自我居震旦，",
        "翊卫类夔契。",
        "伊昔颈皇运，",
        "艰难仰忠烈。",
        "不觉岁云暮，",
        "感极星回节。",
        "元昶同一心，",
        "子孙堪贻厥。"
      ]
    }
  ]
}