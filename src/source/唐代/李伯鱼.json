  "Poems": [
    {
      "_id": null,
      "Name": "桐竹赠张燕公",
      "PoetId": "2d0f2a595cd947e19c2b2e67d660760a",
      "PoetName": "李伯鱼",
      "Form": "五律",
      "Tags": [
        "唐代",
        "李伯鱼"
      ],
      "Contents": [
        "北竹青桐北，",
        "南桐绿竹南。",
        "竹林君早爱，",
        "桐树我初贪。",
        "凤栖桐不愧，",
        "凤食竹何惭。",
        "栖食更如此，",
        "馀非凤所堪。"
      ]
    }
  ]
}