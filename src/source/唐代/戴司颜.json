  "Poems": [
    {
      "_id": null,
      "Name": "塞上",
      "PoetId": "9134aaa6b8f9419882201b0e380316c2",
      "PoetName": "戴司颜",
      "Form": "五律",
      "Tags": [
        "唐代",
        "戴司颜"
      ],
      "Contents": [
        "空迹昼苍茫，",
        "沙腥古战场。",
        "逢春多霰雪，",
        "生计在牛羊。",
        "冷角吹乡泪，",
        "干榆落梦床。",
        "从来山水客，",
        "谁谓到渔阳。"
      ]
    },
    {
      "_id": null,
      "Name": "江上雨",
      "PoetId": "9134aaa6b8f9419882201b0e380316c2",
      "PoetName": "戴司颜",
      "Form": "五律",
      "Tags": [
        "唐代",
        "戴司颜"
      ],
      "Contents": [
        "非不欲前去，",
        "此情非自由。",
        "星辰照何处，",
        "风雨送凉秋。",
        "寒锁空江梦，",
        "声随黄叶愁。",
        "萧萧犹未已，",
        "早晚去蘋洲。"
      ]
    }
  ]
}