  "Poems": [
    {
      "_id": null,
      "Name": "元日望含元殿御扇开合",
      "PoetId": "7322364c1f1b425ead2f5ff4b9990d8c",
      "PoetName": "张莒",
      "Form": "未知",
      "Tags": [
        "唐代",
        "张莒"
      ],
      "Contents": [
        "万国来朝岁，",
        "千年觐圣君。",
        "辇迎仙仗出，",
        "扇匝御香焚。",
        "俯对朝容近，",
        "先知曙色分。",
        "冕旒开处见，",
        "钟磬合时闻。",
        "影动承朝日，",
        "花攒似庆云。",
        "蒲葵那可比，",
        "徒用隔炎氛。"
      ]
    }
  ]
}