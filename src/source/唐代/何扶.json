  "Poems": [
    {
      "_id": null,
      "Name": "寄旧同年",
      "PoetId": "b0a5289df9aa4ed9a98394265b607bc4",
      "PoetName": "何扶",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "何扶"
      ],
      "Contents": [
        "金榜题名墨尚新，",
        "今年依旧去年春。",
        "花间每被红妆问，",
        "何事重来只一人。"
      ]
    },
    {
      "_id": null,
      "Name": "送阆州妓人归老",
      "PoetId": "b0a5289df9aa4ed9a98394265b607bc4",
      "PoetName": "何扶",
      "Form": "七律",
      "Tags": [
        "唐代",
        "何扶"
      ],
      "Contents": [
        "竹翠婵娟草径幽，",
        "佳人归老傍汀洲。",
        "玉蟾露冷梁尘暗，",
        "金凤花开云鬓秋。",
        "十亩稻香新绿野，",
        "一声歌断旧青楼。",
        "芭蕉半卷西池雨，",
        "日暮门前双白鸥。"
      ]
    }
  ]
}