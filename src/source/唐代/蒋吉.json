{
  "_id": "7ddfab653b234d70bbf2639f570de4c2",
  "Name": "蒋吉",
  "Description": "",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "出塞",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "瘦马羸童行背秦，",
        "暮鸦撩乱入残云。",
        "北风吹起寒营角，",
        "直至榆关人尽闻。"
      ]
    },
    {
      "_id": null,
      "Name": "四老庙",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "无端舍钓学干名，",
        "不得溪山养性情。",
        "自省此身非达者，",
        "今朝羞拜四先生。"
      ]
    },
    {
      "_id": null,
      "Name": "大庾驿有怀",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "一囊书重百余斤，",
        "邮吏宁知去计贫。",
        "莫讶偏吟望乡句，",
        "明朝便见岭南人。"
      ]
    },
    {
      "_id": null,
      "Name": "寄进士贾希",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "恨苦泪不落，",
        "耿然东北心。",
        "空囊与瘦马，",
        "羁绁意应深。"
      ]
    },
    {
      "_id": null,
      "Name": "旅泊",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "霜月正高鹦鹉洲，",
        "美人清唱发红楼。",
        "乡心暗逐秋江水，",
        "直到吴山脚下流。"
      ]
    },
    {
      "_id": null,
      "Name": "昭君冢",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "曾为汉帝眼中人，",
        "今作狂胡陌上尘。",
        "身死不知多少载，",
        "冢花犹带洛阳春。"
      ]
    },
    {
      "_id": null,
      "Name": "樵翁",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "独入深山信脚行，",
        "惯当貙虎不曾惊。",
        "路傍花发无心看，",
        "惟见枯枝刮眼明。"
      ]
    },
    {
      "_id": null,
      "Name": "次商於感旧寄卢中丞",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "昔年簪组隘丘门，",
        "今日旌幢一院存。",
        "何事商於泪如雨，",
        "小儒偏受陆家恩。"
      ]
    },
    {
      "_id": null,
      "Name": "次青云驿",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "马转栎林山鸟飞，",
        "商溪流水背残晖。",
        "行人几在青云路，",
        "底事风尘犹满衣。"
      ]
    },
    {
      "_id": null,
      "Name": "汉东道中",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "九十九冈遥，",
        "天寒雪未消。",
        "羸童牵瘦马，",
        "不敢过危桥。"
      ]
    },
    {
      "_id": null,
      "Name": "石城",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "系缆石城下，",
        "恣吟怀暂开。",
        "江人桡艇子，",
        "将谓莫愁来。"
      ]
    },
    {
      "_id": null,
      "Name": "闻歌竹枝",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "巡堤听唱竹枝词，",
        "正是月高风静时。",
        "独向东南人不会，",
        "弟兄俱在楚江湄。"
      ]
    },
    {
      "_id": null,
      "Name": "题商山修路僧院",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "此地修行山几枯，",
        "草堂生计只瓶盂。",
        "支郎既解除艰险，",
        "试看人心平得无。"
      ]
    },
    {
      "_id": null,
      "Name": "题长安僧院",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "出门争走九衢尘，",
        "总是浮生不了身。",
        "惟有水田衣下客，",
        "大家忙处作闲人。"
      ]
    },
    {
      "_id": null,
      "Name": "高溪有怀",
      "PoetId": "7ddfab653b234d70bbf2639f570de4c2",
      "PoetName": "蒋吉",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "蒋吉"
      ],
      "Contents": [
        "驻马高溪侧，",
        "旅人千里情。",
        "雁山山下水，",
        "还作此泉声。"
      ]
    }
  ]
}