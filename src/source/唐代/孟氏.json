  "Name": "孟氏",
  "Description": "孟氏，本寿春妓，归维扬万贞为妻。诗二首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "独游家园",
      "PoetId": "d8326c4cb6c34c06a1f9a62865d5ae37",
      "PoetName": "孟氏",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "孟氏"
      ],
      "Contents": [
        "可惜春时节，",
        "依前独自游。",
        "无端两行泪，",
        "长只对花流。"
      ]
    },
    {
      "_id": null,
      "Name": "答少年",
      "PoetId": "d8326c4cb6c34c06a1f9a62865d5ae37",
      "PoetName": "孟氏",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "孟氏"
      ],
      "Contents": [
        "谁家少年儿，",
        "心中暗自欺。",
        "不道终不可，",
        "可即恐郎知。"
      ]
    }
  ]
}