    {
      "_id": null,
      "Name": "哭陆先生",
      "PoetId": "4a919f04233a43999210ca07cae87b22",
      "PoetName": "张白",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "张白"
      ],
      "Contents": [
        "六亲恸哭还复苏，",
        "我笑先生泪个无。",
        "脱履定归天上去，",
        "空坟留入武陵图。"
      ]
    },
    {
      "_id": null,
      "Name": "武陵春色",
      "PoetId": "4a919f04233a43999210ca07cae87b22",
      "PoetName": "张白",
      "Form": "五律",
      "Tags": [
        "唐代",
        "张白"
      ],
      "Contents": [
        "武陵春色好，",
        "十二酒家楼。",
        "大醉方回首，",
        "逢人不举头。",
        "是非都不采，",
        "名利混然休。",
        "戴个星冠子，",
        "浮沈逐世流。"
      ]
    },
    {
      "_id": null,
      "Name": "赠酒店崔氏",
      "PoetId": "4a919f04233a43999210ca07cae87b22",
      "PoetName": "张白",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "张白"
      ],
      "Contents": [
        "武陵城里崔家酒，",
        "地上应无天上有。",
        "南游道士饮一斗，",
        "卧向白云深洞口。"
      ]
    }
  ]
}