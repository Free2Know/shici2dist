  "_id": "6c1bdb3d83264e9f8b7fa44cd92dbba1",
  "Name": "郑世翼",
  "Description": "一作郑翼。荥阳人。弱冠有盛名。武德中，历万年丞、扬州录事参军，数以言辞忤物。贞观中，坐怨谤，流巂州卒。集多遗失，今存诗五首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "乐府杂曲·鼓吹曲辞·巫山高",
      "PoetId": "6c1bdb3d83264e9f8b7fa44cd92dbba1",
      "PoetName": "郑世翼",
      "Form": "五律",
      "Tags": [
        "唐代",
        "郑世翼"
      ],
      "Contents": [
        "巫山凌太清，",
        "岧峣类削成。",
        "霏霏暮雨合，",
        "霭霭朝云生。",
        "危峰入鸟道，",
        "深谷泻猿声。",
        "别有幽栖客，",
        "淹留攀桂情。"
      ]
    },
    {
      "_id": null,
      "Name": "登北邙还望京洛",
      "PoetId": "6c1bdb3d83264e9f8b7fa44cd92dbba1",
      "PoetName": "郑世翼",
      "Form": "未知",
      "Tags": [
        "唐代",
        "郑世翼"
      ],
      "Contents": [
        "步登北邙坂，",
        "踟蹰聊写望。",
        "宛洛盛皇居，",
        "规模穷大壮。",
        "三河分设险，",
        "两崤资巨防。",
        "飞观紫烟中，",
        "层台碧云上。",
        "青槐夹驰道，",
        "迢迢修且旷。",
        "左右多第宅，",
        "参差居将相。",
        "清晨谒帝返，",
        "车马相追访。",
        "胥徒各异流，",
        "文物纷殊状。",
        "嚣尘暗天起，",
        "箫管从风飏。",
        "伊余孤且直，",
        "生平独沦丧。",
        "山幽有桂丛，",
        "何为坐惆怅。"
      ]
    },
    {
      "_id": null,
      "Name": "看新婚",
      "PoetId": "6c1bdb3d83264e9f8b7fa44cd92dbba1",
      "PoetName": "郑世翼",
      "Form": "五律",
      "Tags": [
        "唐代",
        "郑世翼"
      ],
      "Contents": [
        "初笄梦桃李，",
        "新妆应标梅。",
        "疑逐朝云去，",
        "翻随暮雨来。",
        "杂佩含风响，",
        "丛花隔扇开。",
        "姮娥对此夕，",
        "何用久裴回。"
      ]
    },
    {
      "_id": null,
      "Name": "见佳人负钱出路",
      "PoetId": "6c1bdb3d83264e9f8b7fa44cd92dbba1",
      "PoetName": "郑世翼",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "郑世翼"
      ],
      "Contents": [
        "独负千金价，",
        "应从买笑来。",
        "只持难发口，",
        "经为几人开。"
      ]
    },
    {
      "_id": null,
      "Name": "过严君平古井",
      "PoetId": "6c1bdb3d83264e9f8b7fa44cd92dbba1",
      "PoetName": "郑世翼",
      "Form": "未知",
      "Tags": [
        "唐代",
        "郑世翼"
      ],
      "Contents": [
        "严平本高尚，",
        "远蹈古人风。",
        "卖卜成都市，",
        "流名大汉中。",
        "旧井改人世，",
        "寒泉久不通。",
        "年多既罢汲，",
        "无禽乃遂空。",
        "如何属秋气，",
        "唯见落双桐。"
      ]
    }
  ]
}