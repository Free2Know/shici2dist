  "Poems": [
    {
      "_id": null,
      "Name": "严陵祠",
      "PoetId": "8719beda5dbc49d58491bc1125716f63",
      "PoetName": "洪子舆",
      "Form": "未知",
      "Tags": [
        "唐代",
        "洪子舆"
      ],
      "Contents": [
        "汉主召子陵，",
        "归宿洛阳殿。",
        "客星今安在，",
        "隐迹犹可见。",
        "水石空潺湲，",
        "松篁尚葱蒨。",
        "岸深翠阴合，",
        "川回白云遍。",
        "幽径滋芜没，",
        "荒祠幂霜霰。",
        "垂钓想遗芳，",
        "掇蘋羞野荐。",
        "高风激终古，",
        "语理忘荣贱。",
        "方验道可尊，",
        "山林情不变。"
      ]
    }
  ]
}