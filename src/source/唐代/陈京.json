  "Poems": [
    {
      "_id": null,
      "Name": "郊庙歌辞·享文敬太子庙乐章·登歌",
      "PoetId": "5ecf15c571f840b886695a710d781483",
      "PoetName": "陈京",
      "Form": "未知",
      "Tags": [
        "唐代",
        "陈京"
      ],
      "Contents": [
        "歌以德发，",
        "声以乐贵。",
        "乐善名存，",
        "追仙礼异。",
        "鸾旌拱修，",
        "凤鸣合吹。",
        "神听皇慈，",
        "仲月皆至。"
      ]
    }
  ]
}