  "Poems": [
    {
      "_id": null,
      "Name": "长至日上公献寿",
      "PoetId": "fa294b258b4a4c5489e128e744f3fe93",
      "PoetName": "崔琮",
      "Form": "未知",
      "Tags": [
        "唐代",
        "崔琮"
      ],
      "Contents": [
        "应律三阳首，",
        "朝天万国同。",
        "斗边看子月，",
        "台上候祥风。",
        "五夜钟初动，",
        "千门日正融。",
        "玉阶文物盛，",
        "仙仗武貔雄。",
        "率舞皆群辟，",
        "称觞即上公。",
        "南山为圣寿，",
        "长对未央宫。"
      ]
    }
  ]
}