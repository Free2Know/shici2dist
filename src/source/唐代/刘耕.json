  "Poems": [
    {
      "_id": null,
      "Name": "和主司王起",
      "PoetId": "f2a7edb367384f1f85cb7a8ab5cc8b5e",
      "PoetName": "刘耕",
      "Form": "七律",
      "Tags": [
        "唐代",
        "刘耕"
      ],
      "Contents": [
        "孔门频建铸颜功，",
        "紫绶青衿感激同。",
        "一篑勤劳成太华，",
        "三年恩德仰维嵩。",
        "杨随前辈穿皆中，",
        "桂许平人折欲空。",
        "惭和周郎应见顾，",
        "感知大造竟无穷。"
      ]
    }
  ]
}