    {
      "_id": null,
      "Name": "晚步",
      "PoetId": "06fdf15c23c545efb68b31bb21d9b27a",
      "PoetName": "姚揆",
      "Form": "七律",
      "Tags": [
        "唐代",
        "姚揆"
      ],
      "Contents": [
        "陋巷贫疑本姓颜，",
        "晚来闲步出林间。",
        "数声长笛吹沈日，",
        "一片残云点破山。",
        "岛寺渐疏敲石磬，",
        "渔家方半掩柴关。",
        "迟回从此搜吟久，",
        "待得溪头月上还。"
      ]
    },
    {
      "_id": null,
      "Name": "村行",
      "PoetId": "06fdf15c23c545efb68b31bb21d9b27a",
      "PoetName": "姚揆",
      "Form": "五律",
      "Tags": [
        "唐代",
        "姚揆"
      ],
      "Contents": [
        "天淡雨初晴，",
        "游人恨不胜。",
        "乱山啼蜀魄，",
        "孤棹宿巴陵。",
        "影暗村桥柳，",
        "光寒水寺灯。",
        "罢吟思故国，",
        "窗外有渔罾。"
      ]
    },
    {
      "_id": null,
      "Name": "秋日江东晚行",
      "PoetId": "06fdf15c23c545efb68b31bb21d9b27a",
      "PoetName": "姚揆",
      "Form": "七律",
      "Tags": [
        "唐代",
        "姚揆"
      ],
      "Contents": [
        "迢迢驱马过江东，",
        "此际令人恨莫穷。",
        "一撮秋烟堤上白，",
        "半轮残日岭头红。",
        "路岐滋味犹如旧，",
        "乡曲声音渐不同。",
        "含思看看到梁苑，",
        "画楼丝竹彻遥空。"
      ]
    },
    {
      "_id": null,
      "Name": "颍川客舍",
      "PoetId": "06fdf15c23c545efb68b31bb21d9b27a",
      "PoetName": "姚揆",
      "Form": "七律",
      "Tags": [
        "唐代",
        "姚揆"
      ],
      "Contents": [
        "素琴孤剑尚闲游，",
        "谁共芳尊话唱酬。",
        "乡梦有时生枕上，",
        "客情终日在眉头。",
        "云拖雨脚连天去，",
        "树夹河声绕郡流。",
        "回首帝京归未得，",
        "不堪吟倚夕阳楼。"
      ]
    }
  ]
}