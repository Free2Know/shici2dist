{
  "_id": "6e39f40b3b3142c99c2fc31ab4ed160c",
  "Name": "陈去疾",
  "Description": "陈去疾，字文医，侯官人。元和十四年及第，历官邕管副使。诗十三首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "偶题",
      "PoetId": "6e39f40b3b3142c99c2fc31ab4ed160c",
      "PoetName": "陈去疾",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "陈去疾"
      ],
      "Contents": [
        "魂梦天南垂，",
        "宿昔万里道。",
        "池台花气深，",
        "到处生春草。"
      ]
    },
    {
      "_id": null,
      "Name": "元夕京城和欧阳衮",
      "PoetId": "6e39f40b3b3142c99c2fc31ab4ed160c",
      "PoetName": "陈去疾",
      "Form": "七律",
      "Tags": [
        "唐代",
        "陈去疾"
      ],
      "Contents": [
        "兰焰芳芬彻晓开，",
        "珠光新霭映人来。",
        "歌迎甲夜催银管，",
        "影动繁星缀玉台。",
        "别有朱门春澹荡，",
        "不妨芝火翠崔嵬。",
        "此时月色同沾醉，",
        "何处游轮陌上回。"
      ]
    },
    {
      "_id": null,
      "Name": "塞下曲",
      "PoetId": "6e39f40b3b3142c99c2fc31ab4ed160c",
      "PoetName": "陈去疾",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "陈去疾"
      ],
      "Contents": [
        "春至金河雪似花，",
        "萧条玉塞但胡沙。",
        "晓来重上关城望，",
        "惟见惊尘不见家。"
      ]
    },
    {
      "_id": null,
      "Name": "忆山中",
      "PoetId": "6e39f40b3b3142c99c2fc31ab4ed160c",
      "PoetName": "陈去疾",
      "Form": "未知",
      "Tags": [
        "唐代",
        "陈去疾"
      ],
      "Contents": [
        "长吟重悒然，",
        "为忆山中年。",
        "清瑟泛遥夜，",
        "乱花随暮烟。",
        "珠林馀露气，",
        "乳窦滴香泉。",
        "迹远尘埃外，",
        "花开绮藻前。",
        "岩罗云貌逸，",
        "竹抱水容妍。",
        "蕙磴飞英绕，",
        "萍潭片影悬。",
        "林藏诸曲胜，",
        "台擅一峰偏。",
        "会可标真寄，",
        "焚香对石筵。"
      ]
    },
    {
      "_id": null,
      "Name": "春宫曲",
      "PoetId": "6e39f40b3b3142c99c2fc31ab4ed160c",
      "PoetName": "陈去疾",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "陈去疾"
      ],
      "Contents": [
        "流莺春晓唤樱桃，",
        "花外传呼殿影高。",
        "抱里琵琶最承宠，",
        "君王敕赐玉檀槽。"
      ]
    },
    {
      "_id": null,
      "Name": "西上辞母坟",
      "PoetId": "6e39f40b3b3142c99c2fc31ab4ed160c",
      "PoetName": "陈去疾",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "陈去疾"
      ],
      "Contents": [
        "高盖山头日影微，",
        "黄昏独立宿禽稀。",
        "林间滴酒空垂泪，",
        "不见丁宁嘱早归。"
      ]
    },
    {
      "_id": null,
      "Name": "赋得骐骥长鸣",
      "PoetId": "6e39f40b3b3142c99c2fc31ab4ed160c",
      "PoetName": "陈去疾",
      "Form": "未知",
      "Tags": [
        "唐代",
        "陈去疾"
      ],
      "Contents": [
        "骐骥忻知己，",
        "嘶鸣忽异常。",
        "积悲摅怨抑，",
        "一举彻穹苍。",
        "迹类三年鸟，",
        "心驰五达庄。",
        "何言从蹇踬，",
        "今日逐腾骧。",
        "牛皂休维絷，",
        "天衢恣陆梁。",
        "向非逢伯乐，",
        "谁足见其长。"
      ]
    },
    {
      "_id": null,
      "Name": "踏歌行",
      "PoetId": "6e39f40b3b3142c99c2fc31ab4ed160c",
      "PoetName": "陈去疾",
      "Form": "七律",
      "Tags": [
        "唐代",
        "陈去疾"
      ],
      "Contents": [
        "鸳鸯楼下万花新，",
        "翡翠宫前百戏陈。",
        "夭矫翔龙衔火树，",
        "飞来瑞凤散芳春。",
        "仙跸初传紫禁香，",
        "瑞云开处夜花芳。",
        "繁弦促管升平调，",
        "绮缀丹莲借月光。"
      ]
    },
    {
      "_id": null,
      "Name": "送人谪幽州",
      "PoetId": "6e39f40b3b3142c99c2fc31ab4ed160c",
      "PoetName": "陈去疾",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "陈去疾"
      ],
      "Contents": [
        "临路深怀放废惭，",
        "梦中犹自忆江南。",
        "莫言塞北春风少，",
        "还胜炎荒入瘴岚。"
      ]
    },
    {
      "_id": null,
      "Name": "送林刺史简言之漳州",
      "PoetId": "6e39f40b3b3142c99c2fc31ab4ed160c",
      "PoetName": "陈去疾",
      "Form": "五律",
      "Tags": [
        "唐代",
        "陈去疾"
      ],
      "Contents": [
        "江树欲含曛，",
        "清歌一送君。",
        "征骖辞荔浦，",
        "别袂暗松云。",
        "路狭横柯度，",
        "山深坠叶闻。",
        "明朝宿何处，",
        "未忍醉中分。"
      ]
    },
    {
      "_id": null,
      "Name": "送韩将军之雁门",
      "PoetId": "6e39f40b3b3142c99c2fc31ab4ed160c",
      "PoetName": "陈去疾",
      "Form": "七律",
      "Tags": [
        "唐代",
        "陈去疾"
      ],
      "Contents": [
        "荒塞峰烟百道驰，",
        "雁门风色暗旌旗。",
        "破围铁骑长驱疾，",
        "饮血将军转战危。",
        "画角吹开边月静，",
        "缦缨不信虏尘窥。",
        "归来长揖功成后，",
        "黄石当年故有期。"
      ]
    },
    {
      "_id": null,
      "Name": "采莲曲",
      "PoetId": "6e39f40b3b3142c99c2fc31ab4ed160c",
      "PoetName": "陈去疾",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "陈去疾"
      ],
      "Contents": [
        "粉光花色叶中开，",
        "荷气衣香水上来。",
        "棹响清潭见斜领，",
        "双鸳何事亦相猜。"
      ]
    }
  ]
}