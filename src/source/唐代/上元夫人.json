  "Poems": [
    {
      "_id": null,
      "Name": "再赠",
      "PoetId": "b9dc9dfb9ec74b4dabc46b17a89e287d",
      "PoetName": "上元夫人",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "上元夫人"
      ],
      "Contents": [
        "弄玉有夫皆得道，",
        "刘纲兼室尽登仙。",
        "君能仔细窥朝露，",
        "须逐云车拜洞天。"
      ]
    },
    {
      "_id": null,
      "Name": "留别",
      "PoetId": "b9dc9dfb9ec74b4dabc46b17a89e287d",
      "PoetName": "上元夫人",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "上元夫人"
      ],
      "Contents": [
        "萧郎不顾凤楼人，",
        "云涩回车泪脸新。",
        "愁想蓬瀛归去路，",
        "难窥旧苑碧桃春。"
      ]
    },
    {
      "_id": null,
      "Name": "赠封陟",
      "PoetId": "b9dc9dfb9ec74b4dabc46b17a89e287d",
      "PoetName": "上元夫人",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "上元夫人"
      ],
      "Contents": [
        "谪居蓬岛别瑶池，",
        "春媚烟花有所思。",
        "为爱君心能洁白，",
        "愿操箕帚奉屏帏。"
      ]
    }
  ]
}