  "Description": "奚贾，富春人。诗三首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "严陵滩下寄常建",
      "PoetId": "08539efcfbd9463c930d120a30293086",
      "PoetName": "奚贾",
      "Form": "未知",
      "Tags": [
        "唐代",
        "奚贾"
      ],
      "Contents": [
        "日入溪水静，",
        "寻真此亦难。",
        "乃知沧洲人，",
        "道成仍钓竿。",
        "漾楫乘微月，",
        "振衣生早寒。",
        "纷吾成独往，",
        "自速耽考槃。",
        "已息汉阴诮，",
        "且同濠上观。",
        "旷然心无涯，",
        "谁问容膝安。"
      ]
    },
    {
      "_id": null,
      "Name": "句",
      "PoetId": "08539efcfbd9463c930d120a30293086",
      "PoetName": "奚贾",
      "Form": "未知",
      "Tags": [
        "唐代",
        "奚贾"
      ],
      "Contents": [
        "眠涧花自落，",
        "步林鸟不飞。",
        "谿谷何萧条，",
        "日入人独行。",
        "落日下平楚，",
        "孤烟生洞庭。",
        "（见《诗式》）。",
        "第二百九十六卷"
      ]
    },
    {
      "_id": null,
      "Name": "寻许山人亭子",
      "PoetId": "08539efcfbd9463c930d120a30293086",
      "PoetName": "奚贾",
      "Form": "五律",
      "Tags": [
        "唐代",
        "奚贾"
      ],
      "Contents": [
        "桃源若远近，",
        "渔子棹轻舟。",
        "川路行难尽，",
        "人家到渐幽。",
        "山禽拂席起，",
        "溪水入庭流。",
        "君是何年隐，",
        "如今成白头。"
      ]
    },
    {
      "_id": null,
      "Name": "谒李尊师",
      "PoetId": "08539efcfbd9463c930d120a30293086",
      "PoetName": "奚贾",
      "Form": "五律",
      "Tags": [
        "唐代",
        "奚贾"
      ],
      "Contents": [
        "万物返常性，",
        "惟道贵自然。",
        "先生容其微，",
        "隐几为列仙。",
        "炼魄闭琼户，",
        "养毛飞洞天。",
        "将知逍遥久，",
        "得道无岁年。"
      ]
    }
  ]
}