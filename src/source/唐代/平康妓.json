      "_id": null,
      "Name": "赠裴思谦",
      "PoetId": "924ec86ab4544723a722e4ab12391f66",
      "PoetName": "平康妓",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "平康妓"
      ],
      "Contents": [
        "银釭斜背解明珰，",
        "小语偷声贺玉郎。",
        "从此不知兰麝贵，",
        "夜来新惹桂枝香。"
      ]
    }
  ]
