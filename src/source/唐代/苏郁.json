  "Poems": [
    {
      "_id": null,
      "Name": "咏和亲",
      "PoetId": "f0fa328577674ad9b626851c1f481dd3",
      "PoetName": "苏郁",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "苏郁"
      ],
      "Contents": [
        "关月夜悬青冢镜，",
        "寒云秋薄汉宫罗。",
        "君王莫信和亲策，",
        "生得胡雏虏更多。"
      ]
    },
    {
      "_id": null,
      "Name": "步虚词",
      "PoetId": "f0fa328577674ad9b626851c1f481dd3",
      "PoetName": "苏郁",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "苏郁"
      ],
      "Contents": [
        "十二楼藏玉堞中，",
        "凤凰双宿碧芙蓉。",
        "流霞浅酌谁同醉，",
        "今夜笙歌第几重。"
      ]
    },
    {
      "_id": null,
      "Name": "鹦鹉词",
      "PoetId": "f0fa328577674ad9b626851c1f481dd3",
      "PoetName": "苏郁",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "苏郁"
      ],
      "Contents": [
        "莫把金笼闭鹦鹉，",
        "个个分明解人语。",
        "忽然更向君前言，",
        "三十六宫愁几许。"
      ]
    }
  ]
}