      "_id": null,
      "Name": "姑熟杂咏·丹阳湖",
      "PoetId": "9223fd2f249045fba6baa5ea5cbd3f2b",
      "PoetName": "李赤",
      "Form": "五律",
      "Tags": [
        "唐代",
        "李赤"
      ],
      "Contents": [
        "湖与元气通，",
        "风波浩难止。",
        "天外贾客归，",
        "云间片帆起。",
        "龟游莲叶上，",
        "鸟宿芦花里。",
        "少女棹舟归，",
        "歌声逐流水。"
      ]
    },
    {
      "_id": null,
      "Name": "姑熟杂咏·天门山",
      "PoetId": "9223fd2f249045fba6baa5ea5cbd3f2b",
      "PoetName": "李赤",
      "Form": "五律",
      "Tags": [
        "唐代",
        "李赤"
      ],
      "Contents": [
        "迥出江水上，",
        "双峰自相对。",
        "岸映松色寒，",
        "石分浪花碎。",
        "参差远天际，",
        "缥缈晴霞外。",
        "落日舟去遥，",
        "回首沈青霭。"
      ]
    }
  ]
}