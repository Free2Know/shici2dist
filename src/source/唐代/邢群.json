  "Poems": [
    {
      "_id": null,
      "Name": "郡中有怀寄上睦州员外杜十三兄",
      "PoetId": "05597d61a2644d039e5f2d9576b80ef6",
      "PoetName": "邢群",
      "Form": "七律",
      "Tags": [
        "唐代",
        "邢群"
      ],
      "Contents": [
        "城枕溪流更浅斜，",
        "丽谯连带邑人家。",
        "经冬野菜青青色，",
        "未腊山梅处处花。",
        "虽免嶂云生岭上，",
        "永无音信到天涯。",
        "如今岁晏从羁滞，",
        "心喜弹冠事不赊。"
      ]
    }
  ]
}