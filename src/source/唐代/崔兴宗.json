{
  "_id": "269f7032a94d4b4481383da2059c09a3",
  "Name": "崔兴宗",
  "Description": "崔兴宗，与王维、裴迪俱居终南，后官右补阙，诗五首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "同王右丞送瑗公南归",
      "PoetId": "269f7032a94d4b4481383da2059c09a3",
      "PoetName": "崔兴宗",
      "Form": "五律",
      "Tags": [
        "唐代",
        "崔兴宗"
      ],
      "Contents": [
        "行苦神亦秀，",
        "泠然谿上松。",
        "铜瓶与竹杖，",
        "来自祝融峰。",
        "常愿入灵岳，",
        "藏经访遗踪。",
        "南归见长老，",
        "且为说心胸。"
      ]
    },
    {
      "_id": null,
      "Name": "和王维敕赐百官樱桃",
      "PoetId": "269f7032a94d4b4481383da2059c09a3",
      "PoetName": "崔兴宗",
      "Form": "七律",
      "Tags": [
        "唐代",
        "崔兴宗"
      ],
      "Contents": [
        "未央朝谒正逶迤，",
        "天上樱桃锡此时。",
        "朱实初传九华殿，",
        "繁花旧杂万年枝。",
        "未胜晏子江南橘，",
        "莫比潘家大谷梨。",
        "闻道令人好颜色，",
        "神农本草自应知。"
      ]
    },
    {
      "_id": null,
      "Name": "留别王维",
      "PoetId": "269f7032a94d4b4481383da2059c09a3",
      "PoetName": "崔兴宗",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "崔兴宗"
      ],
      "Contents": [
        "驻马欲分襟，",
        "清寒御沟上。",
        "前山景气佳，",
        "独往还惆怅。"
      ]
    },
    {
      "_id": null,
      "Name": "酬王维卢象见过林亭",
      "PoetId": "269f7032a94d4b4481383da2059c09a3",
      "PoetName": "崔兴宗",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "崔兴宗"
      ],
      "Contents": [
        "穷巷空林常闭关，",
        "悠然独卧对前山。",
        "今朝忽枉嵇生驾，",
        "倒屣开门遥解颜。"
      ]
    },
    {
      "_id": null,
      "Name": "青雀歌",
      "PoetId": "269f7032a94d4b4481383da2059c09a3",
      "PoetName": "崔兴宗",
      "Form": "未知",
      "Tags": [
        "唐代",
        "崔兴宗"
      ],
      "Contents": [
        "青扈绕青林，",
        "翩翾陋体一微禽。",
        "不应常在藩篱下，",
        "他日凌云谁见心。"
      ]
    }
  ]
}