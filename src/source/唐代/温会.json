  "Name": "温会",
  "Description": "温会，以殿中侍御史为西川安抚判官。诗二首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "和段相公登武担寺西台",
      "PoetId": "c6841430a4ca486c85efd5967b1d34d4",
      "PoetName": "温会",
      "Form": "五律",
      "Tags": [
        "唐代",
        "温会"
      ],
      "Contents": [
        "桑台烟树中，",
        "台榭造云空。",
        "眺听逢秋兴，",
        "篇辞变国风。",
        "坐愁高鸟起，",
        "笑指远人同。",
        "始愧才情薄，",
        "跻攀继韵穷。"
      ]
    },
    {
      "_id": null,
      "Name": "奉陪段相公晚夏登张仪楼",
      "PoetId": "c6841430a4ca486c85efd5967b1d34d4",
      "PoetName": "温会",
      "Form": "五律",
      "Tags": [
        "唐代",
        "温会"
      ],
      "Contents": [
        "危轩重叠开，",
        "访古上裴回。",
        "有舌嗟秦策，",
        "飞梁驾楚材。",
        "云霄随凤到，",
        "物象为诗来。",
        "欲和关山意，",
        "巴歌调更哀。"
      ]
    }
  ]
}