{
  "_id": "13b23ded77b74a1a9f35621f4635b197",
  "Name": "史凤",
  "Description": "史凤，宣城妓也，诗七首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "传香枕",
      "PoetId": "13b23ded77b74a1a9f35621f4635b197",
      "PoetName": "史凤",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "史凤"
      ],
      "Contents": [
        "韩寿香从何处传，",
        "枕边芳馥恋婵娟。",
        "休疑粉黛加铤刃，",
        "玉女旃檀侍佛前。"
      ]
    },
    {
      "_id": null,
      "Name": "八分羊",
      "PoetId": "13b23ded77b74a1a9f35621f4635b197",
      "PoetName": "史凤",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "史凤"
      ],
      "Contents": [
        "党家风味足肥羊，",
        "绮阁留人漫较量。",
        "万羊亦是男儿事，",
        "莫学狂夫取次尝。"
      ]
    },
    {
      "_id": null,
      "Name": "神鸡枕",
      "PoetId": "13b23ded77b74a1a9f35621f4635b197",
      "PoetName": "史凤",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "史凤"
      ],
      "Contents": [
        "枕绘鸳鸯久与栖，",
        "新裁雾縠斗神鸡。",
        "与郎酣梦浑忘晓，",
        "鸡亦留连不肯啼。"
      ]
    },
    {
      "_id": null,
      "Name": "迷香洞",
      "PoetId": "13b23ded77b74a1a9f35621f4635b197",
      "PoetName": "史凤",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "史凤"
      ],
      "Contents": [
        "洞口飞琼佩羽霓，",
        "香风飘拂使人迷。",
        "自从邂逅芙蓉帐，",
        "不数桃花流水溪。"
      ]
    },
    {
      "_id": null,
      "Name": "锁莲灯",
      "PoetId": "13b23ded77b74a1a9f35621f4635b197",
      "PoetName": "史凤",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "史凤"
      ],
      "Contents": [
        "灯锁莲花花照罍，",
        "翠钿同醉楚台巍。",
        "残灰剔罢携纤手，",
        "也胜金莲送辙回。"
      ]
    },
    {
      "_id": null,
      "Name": "闭门羹",
      "PoetId": "13b23ded77b74a1a9f35621f4635b197",
      "PoetName": "史凤",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "史凤"
      ],
      "Contents": [
        "一豆聊供游冶郎，",
        "去时忙唤锁仓琅。",
        "入门独慕相如侣，",
        "欲拨瑶琴弹凤凰。"
      ]
    },
    {
      "_id": null,
      "Name": "鲛红被",
      "PoetId": "13b23ded77b74a1a9f35621f4635b197",
      "PoetName": "史凤",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "史凤"
      ],
      "Contents": [
        "肱被当年仅御寒，",
        "青楼惯染血猩纨。",
        "牙床舒卷鹓鸾共，",
        "正值窗棂月一团。"
      ]
    }
  ]
}