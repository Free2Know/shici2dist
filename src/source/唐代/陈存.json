  "Name": "陈存",
  "Description": "陈存，大历、贞元间诗人。诗六首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "丹阳作",
      "PoetId": "0560bcadca3c451985808f9d53c4250a",
      "PoetName": "陈存",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "陈存"
      ],
      "Contents": [
        "暂入新丰市，",
        "犹闻旧酒香。",
        "抱琴沽一醉，",
        "尽日卧垂杨。"
      ]
    },
    {
      "_id": null,
      "Name": "寓居武丁馆",
      "PoetId": "0560bcadca3c451985808f9d53c4250a",
      "PoetName": "陈存",
      "Form": "五律",
      "Tags": [
        "唐代",
        "陈存"
      ],
      "Contents": [
        "暑雨飘已过，",
        "凉飙触幽衿。",
        "虚馆无喧尘，",
        "绿槐多昼阴。",
        "俯视古苔积，",
        "仰聆早蝉吟。",
        "放卷一长想，",
        "闭门千里心。"
      ]
    },
    {
      "_id": null,
      "Name": "楚州赠别周愿侍御",
      "PoetId": "0560bcadca3c451985808f9d53c4250a",
      "PoetName": "陈存",
      "Form": "五律",
      "Tags": [
        "唐代",
        "陈存"
      ],
      "Contents": [
        "漂泊楚水来，",
        "舍舟坐高馆。",
        "途穷在中路，",
        "孤征慕前伴。",
        "风雨一留宿，",
        "关山去欲懒。",
        "淮南木叶飞，",
        "夜闻广陵散。"
      ]
    },
    {
      "_id": null,
      "Name": "清溪馆作",
      "PoetId": "0560bcadca3c451985808f9d53c4250a",
      "PoetName": "陈存",
      "Form": "五律",
      "Tags": [
        "唐代",
        "陈存"
      ],
      "Contents": [
        "指途清溪里，",
        "左右唯深林。",
        "云蔽望乡处，",
        "雨愁为客心。",
        "遇人多物役，",
        "听鸟时幽音。",
        "何必沧浪水，",
        "庶兹浣尘襟。"
      ]
    },
    {
      "_id": null,
      "Name": "穆陵路",
      "PoetId": "0560bcadca3c451985808f9d53c4250a",
      "PoetName": "陈存",
      "Form": "五律",
      "Tags": [
        "唐代",
        "陈存"
      ],
      "Contents": [
        "西游匣长剑，",
        "日暮湘楚间。",
        "歇马上秋草，",
        "逢人问故关。",
        "孤村绿塘水，",
        "旷野白云山。",
        "方念此中去，",
        "何时此路还。"
      ]
    }
  ]
}