  "Poems": [
    {
      "_id": null,
      "Name": "送沈芳谒李观察求仕进",
      "PoetId": "130f588a0081406ba00aefed1212adb8",
      "PoetName": "周万",
      "Form": "未知",
      "Tags": [
        "唐代",
        "周万"
      ],
      "Contents": [
        "往日长安路，",
        "欢游不惜年。",
        "为贪卢女曲，",
        "用尽沈郎钱。",
        "身老方投刺，",
        "途穷始著鞭。",
        "犹闻有知己，",
        "此去不徒然。",
        "第一百四十六卷"
      ]
    }
  ]
