  "Poems": [
    {
      "_id": null,
      "Name": "春雪",
      "PoetId": "017dc0ad06bc4a83aed3cfcdea663423",
      "PoetName": "焦郁",
      "Form": "未知",
      "Tags": [
        "唐代",
        "焦郁"
      ],
      "Contents": [
        "散漫天涯色，",
        "乘春四望平。",
        "不分残照影，",
        "何处断鸿声。",
        "缭绕先经塞，",
        "霏微近过城。",
        "因风低未敛，",
        "带雨重还轻。",
        "干吕知时泰，",
        "如膏候岁成。",
        "小儒同品物，",
        "无以答皇明。",
        "春雪空濛帘外斜，",
        "霏微半入野人家。",
        "长天远树山山白，",
        "不辨梅花与柳花。"
      ]
    },
    {
      "_id": null,
      "Name": "白云向空尽",
      "PoetId": "017dc0ad06bc4a83aed3cfcdea663423",
      "PoetName": "焦郁",
      "Form": "未知",
      "Tags": [
        "唐代",
        "焦郁"
      ],
      "Contents": [
        "白云升远岫，",
        "摇曳入晴空。",
        "乘化随舒卷，",
        "无心任始终。",
        "欲销仍带日，",
        "将断更因风。",
        "势薄飞难定，",
        "天高色易穷。",
        "影收元气表，",
        "光灭太虚中。",
        "倘若从龙去，",
        "还施济物功。"
      ]
    }
  ]
}