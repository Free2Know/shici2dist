  "Poems": [
    {
      "_id": null,
      "Name": "送妓人出家",
      "PoetId": "ef21751df3494a3181e3321c9f074d3c",
      "PoetName": "杨郇伯",
      "Form": "七律",
      "Tags": [
        "唐代",
        "杨郇伯"
      ],
      "Contents": [
        "尽出花钿与四邻，",
        "云鬟剪落厌残春。",
        "暂惊风烛难留世，",
        "便是莲花不染身。",
        "贝叶欲翻迷锦字，",
        "梵声初学误梁尘。",
        "从今艳色归空后，",
        "湘浦应无解珮人。"
      ]
    }
  ]
}