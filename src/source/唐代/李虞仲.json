      "_id": null,
      "Name": "初日照凤楼",
      "PoetId": "f775a7fffda14d4d9804a44ab7188879",
      "PoetName": "李虞仲",
      "Form": "未知",
      "Tags": [
        "唐代",
        "李虞仲"
      ],
      "Contents": [
        "旭日烟云殿，",
        "朝阳烛帝居。",
        "断霞生峻宇，",
        "通阁丽晴虚。",
        "流彩连朱槛，",
        "腾辉照绮疏。",
        "曈曨晨景里，",
        "明灭晓光初。",
        "户牖仙山近，",
        "轩楹凤翼舒。",
        "还如王母过，",
        "遥度五云车。"
      ]
    }
  ]
}