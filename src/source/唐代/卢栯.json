  "Poems": [
    {
      "_id": null,
      "Name": "和于中丞登越王楼作",
      "PoetId": "91aeb35daf074fbb99909484c6ce6f93",
      "PoetName": "卢栯",
      "Form": "五律",
      "Tags": [
        "唐代",
        "卢栯"
      ],
      "Contents": [
        "图画越王楼，",
        "开缄慰别愁。",
        "山光涵雪冷，",
        "水色带江秋。",
        "云岛孤征雁，",
        "烟帆一叶舟。",
        "向风舒霁景，",
        "如伴谢公游。"
      ]
    }
  ]
}