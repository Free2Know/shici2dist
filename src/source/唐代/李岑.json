  "Poems": [
    {
      "_id": null,
      "Name": "东峰亭各赋一物得栖烟鸟",
      "PoetId": "cecf997cf6cc429daa5dfb3be5c738cc",
      "PoetName": "李岑",
      "Form": "未知",
      "Tags": [
        "唐代",
        "李岑"
      ],
      "Contents": [
        "从来养毛羽，",
        "昔日曾飞迁。",
        "变转对朝阳，",
        "差池栖夕烟。",
        "遇此枝叶覆，",
        "夙举冀冲天。"
      ]
    },
    {
      "_id": null,
      "Name": "玄元皇帝应见贺圣祚无疆",
      "PoetId": "cecf997cf6cc429daa5dfb3be5c738cc",
      "PoetName": "李岑",
      "Form": "未知",
      "Tags": [
        "唐代",
        "李岑"
      ],
      "Contents": [
        "皇纲归有道，",
        "帝系祖玄元。",
        "运表南山祚，",
        "神通北极尊。",
        "大同齐日月，",
        "兴废应乾坤。",
        "圣后趋庭礼，",
        "宗臣稽首言。",
        "千官欣肆觐，",
        "万国贺深恩。",
        "锡宴云天接，",
        "飞声雷地喧。",
        "祥光浮紫阁，",
        "喜气绕皇轩。",
        "未预承天命，",
        "空勤望帝门。"
      ]
    },
    {
      "_id": null,
      "Name": "西河郡太原守张夫人挽歌",
      "PoetId": "cecf997cf6cc429daa5dfb3be5c738cc",
      "PoetName": "李岑",
      "Form": "五律",
      "Tags": [
        "唐代",
        "李岑"
      ],
      "Contents": [
        "鹊印庆仍传，",
        "鱼轩宠莫先。",
        "从夫元凯贵，",
        "训子孟轲贤。",
        "龙是双归日，",
        "鸾非独舞年。",
        "哀荣今共尽，",
        "凄怆杜陵田。"
      ]
    }
  ]
}