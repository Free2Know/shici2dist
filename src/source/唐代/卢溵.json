  "Poems": [
    {
      "_id": null,
      "Name": "和李尚书命妓饯崔侍御",
      "PoetId": "fd4083e5f2cb4df694e2e626c35dbfaa",
      "PoetName": "卢溵",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "卢溵"
      ],
      "Contents": [
        "乌台上客紫髯公，",
        "共捧天书静境中。",
        "桃朵不辞歌白苎，",
        "耶溪暮雨起樵风。"
      ]
    },
    {
      "_id": null,
      "Name": "金灯",
      "PoetId": "fd4083e5f2cb4df694e2e626c35dbfaa",
      "PoetName": "卢溵",
      "Form": "五律",
      "Tags": [
        "唐代",
        "卢溵"
      ],
      "Contents": [
        "疏茎秋拥翠，",
        "幽艳夕添红。",
        "有月长灯在，",
        "无烟烬火同。",
        "香浓初受露，",
        "势庳不知风。",
        "应笑金台上，",
        "先随晓漏终。"
      ]
    }
  ]
}