  "Name": "欧阳玭",
  "Description": "欧阳玭，衮之子。咸通十年，擢进士第，官书记。诗五首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "巴陵",
      "PoetId": "448e0e2062bf4236bd697cedde6831c2",
      "PoetName": "欧阳玭",
      "Form": "五律",
      "Tags": [
        "唐代",
        "欧阳玭"
      ],
      "Contents": [
        "孤城向夕原，",
        "春入景初暄。",
        "绿树低官舍，",
        "青山在县门。",
        "楼台疑结蜃，",
        "枕席更闻猿。",
        "客路何曾定，",
        "栖迟欲断魂。"
      ]
    },
    {
      "_id": null,
      "Name": "幽轩",
      "PoetId": "448e0e2062bf4236bd697cedde6831c2",
      "PoetName": "欧阳玭",
      "Form": "未知",
      "Tags": [
        "唐代",
        "欧阳玭"
      ],
      "Contents": [
        "幽轩斜映山，",
        "空涧复潺潺。",
        "重叠岩峦趣，",
        "遥来窗户间。",
        "桃花飘岫幌，",
        "燕子语松关。",
        "衣桁侵池翠，",
        "阶痕露藓斑。",
        "临风清瑟奏，",
        "对客白云闲。",
        "眷恋青春色，",
        "含毫俯碧湾。"
      ]
    },
    {
      "_id": null,
      "Name": "新岭临眺寄连总进士",
      "PoetId": "448e0e2062bf4236bd697cedde6831c2",
      "PoetName": "欧阳玭",
      "Form": "五律",
      "Tags": [
        "唐代",
        "欧阳玭"
      ],
      "Contents": [
        "关势遥临海，",
        "峰峦半入云。",
        "烟中独鸟下，",
        "潭上杂花熏。",
        "寄远悲春草，",
        "登临忆使君。",
        "此时还极目，",
        "离思更纷纷。"
      ]
    },
    {
      "_id": null,
      "Name": "榆溪道上",
      "PoetId": "448e0e2062bf4236bd697cedde6831c2",
      "PoetName": "欧阳玭",
      "Form": "五律",
      "Tags": [
        "唐代",
        "欧阳玭"
      ],
      "Contents": [
        "初日在斜溪，",
        "山云片片低。",
        "乡愁梦里失，",
        "马色望中迷。",
        "涧底凄泉气，",
        "岩前遍绿荑。",
        "非关秦塞去，",
        "无事候晨鸡。"
      ]
    },
    {
      "_id": null,
      "Name": "清晓卷帘",
      "PoetId": "448e0e2062bf4236bd697cedde6831c2",
      "PoetName": "欧阳玭",
      "Form": "五律",
      "Tags": [
        "唐代",
        "欧阳玭"
      ],
      "Contents": [
        "清晓意未惬，",
        "卷帘时一吟。",
        "槛虚花气密，",
        "地暖竹声深。",
        "秀色还朝暮，",
        "浮云自古今。",
        "石泉惊已跃，",
        "会可洗幽心。"
      ]
    }
  ]
}