      "_id": null,
      "Name": "章仇公席上咏真珠姬",
      "PoetId": "7fe3cb2851744886bcbe0ab6a6566b74",
      "PoetName": "范元凯",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "范元凯"
      ],
      "Contents": [
        "神女初离碧玉阶，",
        "彤云犹拥牡丹鞋。",
        "应知子建怜罗袜，",
        "顾步裴回拾翠钗。"
      ]
    }
  ]
