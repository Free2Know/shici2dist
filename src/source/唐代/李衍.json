  "Poems": [
    {
      "_id": null,
      "Name": "宫词",
      "PoetId": "ce2d942ca5964811bd08c4ed86aebace",
      "PoetName": "李衍",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "李衍"
      ],
      "Contents": [
        "辉辉赫赫浮玉云，",
        "宣华池上月华新。",
        "月华如水浸宫殿，",
        "有酒不醉真痴人。"
      ]
    },
    {
      "_id": null,
      "Name": "幸秦川上梓潼山",
      "PoetId": "ce2d942ca5964811bd08c4ed86aebace",
      "PoetName": "李衍",
      "Form": "五律",
      "Tags": [
        "唐代",
        "李衍"
      ],
      "Contents": [
        "乔岩簇冷烟，",
        "幽径上寒天。",
        "下瞰峨眉岭，",
        "上窥华岳巅。",
        "驱驰非取乐，",
        "按幸为忧边。",
        "此去如登陟，",
        "歌楼路几千。"
      ]
    },
    {
      "_id": null,
      "Name": "过白卫岭和韩昭",
      "PoetId": "ce2d942ca5964811bd08c4ed86aebace",
      "PoetName": "李衍",
      "Form": "七律",
      "Tags": [
        "唐代",
        "李衍"
      ],
      "Contents": [
        "先朝神武力开边，",
        "画断封疆四五千。",
        "前望陇山屯剑戟，",
        "后凭巫峡锁烽烟。",
        "轩皇尚自亲平寇，",
        "嬴政徒劳爱学仙。",
        "想到隗宫寻胜处，",
        "正应莺语暮春天。"
      ]
    },
    {
      "_id": null,
      "Name": "醉妆词",
      "PoetId": "ce2d942ca5964811bd08c4ed86aebace",
      "PoetName": "李衍",
      "Form": "词",
      "Tags": [
        "唐代",
        "李衍"
      ],
      "Contents": [
        "者边走，",
        "那边走，",
        "只是寻花柳。",
        "那边走，",
        "者边走，",
        "莫厌金杯酒。"
      ]
    },
    {
      "_id": null,
      "Name": "题剑门",
      "PoetId": "ce2d942ca5964811bd08c4ed86aebace",
      "PoetName": "李衍",
      "Form": "五律",
      "Tags": [
        "唐代",
        "李衍"
      ],
      "Contents": [
        "缓辔逾双剑，",
        "行行蹑石棱。",
        "作千寻壁垒，",
        "为万祀依凭。",
        "道德虽无取，",
        "江山粗可矜。",
        "回看城阙路，",
        "云叠树层层。"
      ]
    }
  ]
}