  "Poems": [
    {
      "_id": null,
      "Name": "圣果寺",
      "PoetId": "1afd152f340f4e599c520f4aea80eac2",
      "PoetName": "处默",
      "Form": "五律",
      "Tags": [
        "唐代",
        "处默"
      ],
      "Contents": [
        "路自中峰上，",
        "盘回出薜萝。",
        "到江吴地尽，",
        "隔岸越山多。",
        "古木丛青霭，",
        "遥天浸白波。",
        "下方城郭近，",
        "钟磬杂笙歌。"
      ]
    },
    {
      "_id": null,
      "Name": "山中作",
      "PoetId": "1afd152f340f4e599c520f4aea80eac2",
      "PoetName": "处默",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "处默"
      ],
      "Contents": [
        "席帘高卷枕高欹，",
        "门掩垂萝蘸碧溪。",
        "闲把史书眠一觉，",
        "起来山日过松西。"
      ]
    },
    {
      "_id": null,
      "Name": "忆庐山旧居",
      "PoetId": "1afd152f340f4e599c520f4aea80eac2",
      "PoetName": "处默",
      "Form": "七律",
      "Tags": [
        "唐代",
        "处默"
      ],
      "Contents": [
        "粗衣粝食老烟霞，",
        "勉把衰颜惜岁华。",
        "独鹤只为山客伴，",
        "闲云常在野僧家。",
        "丛生嫩蕨粘松粉，",
        "自落干薪带藓花。",
        "明月清风旧相得，",
        "十年归恨可能赊。"
      ]
    },
    {
      "_id": null,
      "Name": "织妇",
      "PoetId": "1afd152f340f4e599c520f4aea80eac2",
      "PoetName": "处默",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "处默"
      ],
      "Contents": [
        "蓬鬓蓬门积恨多，",
        "夜阑灯下不停梭。",
        "成缣犹自陪钱纳，",
        "未直青楼一曲歌。"
      ]
    },
    {
      "_id": null,
      "Name": "远烟",
      "PoetId": "1afd152f340f4e599c520f4aea80eac2",
      "PoetName": "处默",
      "Form": "五律",
      "Tags": [
        "唐代",
        "处默"
      ],
      "Contents": [
        "霭霭前山上，",
        "凝光满薜萝。",
        "高风吹不尽，",
        "远树得偏多。",
        "翠与晴云合，",
        "轻将淑气和。",
        "正堪流野目，",
        "朱阁意如何。"
      ]
    },
    {
      "_id": null,
      "Name": "送僧游西域",
      "PoetId": "1afd152f340f4e599c520f4aea80eac2",
      "PoetName": "处默",
      "Form": "五律",
      "Tags": [
        "唐代",
        "处默"
      ],
      "Contents": [
        "一盂兼一锡，",
        "只此度流沙。",
        "野性虽为客，",
        "禅心即是家。",
        "寺披云峤雪，",
        "路入晓天霞。",
        "自说游诸国，",
        "回应岁月赊。"
      ]
    },
    {
      "_id": null,
      "Name": "题栖霞寺僧房",
      "PoetId": "1afd152f340f4e599c520f4aea80eac2",
      "PoetName": "处默",
      "Form": "七律",
      "Tags": [
        "唐代",
        "处默"
      ],
      "Contents": [
        "名山不取买山钱，",
        "任构花宫近碧巅。",
        "松桧老依云里寺，",
        "楼台深锁洞中天。",
        "风经绝嶂回疏雨，",
        "石倚危屏挂落泉。",
        "欲结茅庵共师住，",
        "肯饶多少薜萝烟。"
      ]
    }
  ]
}