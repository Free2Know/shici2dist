      "_id": null,
      "Name": "郡中客舍",
      "PoetId": "22db8fa9c3394ab7a88c1aba39a4bdc3",
      "PoetName": "黄麟",
      "Form": "五律",
      "Tags": [
        "唐代",
        "黄麟"
      ],
      "Contents": [
        "虫响乱啾啾，",
        "更人正数筹。",
        "魂归洞庭夜，",
        "霜卧洛阳秋。",
        "微月有时隐，",
        "长河到晓流。",
        "起来还嘱雁，",
        "乡信在吴洲。"
      ]
    }
  ]
}