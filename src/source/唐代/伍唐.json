  "Poems": [
    {
      "_id": null,
      "Name": "上苏使君",
      "PoetId": "3ef64c60205f4859bbda0dce271fed07",
      "PoetName": "伍唐",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "伍唐"
      ],
      "Contents": [
        "江西昔日推韩注，",
        "袁水今朝数赵祥。",
        "纵使文翁能待客，",
        "终栽桃李不成行。"
      ]
    },
    {
      "_id": null,
      "Name": "寒食日献郡守",
      "PoetId": "3ef64c60205f4859bbda0dce271fed07",
      "PoetName": "伍唐",
      "Form": "七绝",
      "Tags": [
        "唐代",
        "伍唐"
      ],
      "Contents": [
        "入门堪笑复堪怜，",
        "三径苔荒一钓船。",
        "惭愧四邻教断火，",
        "不知厨里久无烟。"
      ]
    },
    {
      "_id": null,
      "Name": "山中卧病寄卢郎中",
      "PoetId": "3ef64c60205f4859bbda0dce271fed07",
      "PoetName": "伍唐",
      "Form": "七律",
      "Tags": [
        "唐代",
        "伍唐"
      ],
      "Contents": [
        "十年耕钓水云间，",
        "住僻家贫少往还。",
        "一径绿苔凝晓露，",
        "满头白发对青山。",
        "野僧采药来医病，",
        "樵客携觞为解颜。",
        "空恋旧时恩奖地，",
        "无因匍匐出柴关。"
      ]
    }
  ]
}