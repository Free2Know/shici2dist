  "Poems": [
    {
      "_id": null,
      "Name": "恩赐魏文贞公诸孙旧第以导直臣",
      "PoetId": "5ad86b16894942a3b77742b451e7b8e4",
      "PoetName": "陈彦博",
      "Form": "未知",
      "Tags": [
        "唐代",
        "陈彦博"
      ],
      "Contents": [
        "阿衡随逝水，",
        "池馆主他人。",
        "天意能酬德，",
        "云孙喜庇身。",
        "生前由直道，",
        "殁后振芳尘。",
        "雨露新恩日，",
        "芝兰旧里春。",
        "勋庸留十代，",
        "光彩映诸邻。",
        "共贺升平日，",
        "从兹得谏臣。"
      ]
    }
  ]
}