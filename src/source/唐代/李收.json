  "Poems": [
    {
      "_id": null,
      "Name": "和中书侍郎院壁画云",
      "PoetId": "54476b0486cf4e12801fb875efef4cb7",
      "PoetName": "李收",
      "Form": "五律",
      "Tags": [
        "唐代",
        "李收"
      ],
      "Contents": [
        "粉壁画云成，",
        "如能上太清。",
        "影从霄汉发，",
        "光照掖垣明。",
        "映筱多幽趣，",
        "临轩得野情。",
        "独思作霖雨，",
        "流润及生灵。"
      ]
    },
    {
      "_id": null,
      "Name": "幽情",
      "PoetId": "54476b0486cf4e12801fb875efef4cb7",
      "PoetName": "李收",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "李收"
      ],
      "Contents": [
        "幽人惜春暮，",
        "潭上折芳草。",
        "佳期何时还，",
        "欲寄千里道。"
      ]
    }
  ]
}