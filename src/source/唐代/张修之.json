      "_id": null,
      "Name": "相和歌辞·长门怨",
      "PoetId": "88e81b66f07b44d0afa0305d0a99239f",
      "PoetName": "张修之",
      "Form": "五律",
      "Tags": [
        "唐代",
        "张修之"
      ],
      "Contents": [
        "长门落景尽，",
        "洞房秋月明。",
        "玉阶草露积，",
        "金屋网尘生。",
        "妾妒今应改，",
        "君恩昔未平。",
        "寄语临邛客，",
        "何时作赋成。"
      ]
    }
  ]
}