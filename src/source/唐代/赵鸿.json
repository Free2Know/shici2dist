  "Description": "赵鸿，咸通中人。诗三首。",
  "Dynasty": "唐代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "杜甫同谷茅茨",
      "PoetId": "96f10a401b2241ad942a166f8422a8aa",
      "PoetName": "赵鸿",
      "Form": "五律",
      "Tags": [
        "唐代",
        "赵鸿"
      ],
      "Contents": [
        "工部栖迟后，",
        "邻家大半无。",
        "青羌迷道路，",
        "白社寄杯盂。",
        "大雅何人继，",
        "全生此地孤。",
        "孤云飞鸟什，",
        "空勒旧山隅。"
      ]
    },
    {
      "_id": null,
      "Name": "栗亭",
      "PoetId": "96f10a401b2241ad942a166f8422a8aa",
      "PoetName": "赵鸿",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "赵鸿"
      ],
      "Contents": [
        "杜甫栗亭诗，",
        "诗人多在口。",
        "悠悠二甲子，",
        "题纪今何有？"
      ]
    },
    {
      "_id": null,
      "Name": "泥功山",
      "PoetId": "96f10a401b2241ad942a166f8422a8aa",
      "PoetName": "赵鸿",
      "Form": "五绝",
      "Tags": [
        "唐代",
        "赵鸿"
      ],
      "Contents": [
        "立石泥功状，",
        "天然诡怪形。",
        "未尝私祸福，",
        "终不费丹青。"
      ]
    }
  ]
}