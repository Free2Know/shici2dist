  "Poems": [
    {
      "_id": null,
      "Name": "郊庙歌辞·让皇帝庙乐章·亚献终献",
      "PoetId": "68ed44afffd444ac97b3e56d67639ceb",
      "PoetName": "李舒",
      "Form": "未知",
      "Tags": [
        "唐代",
        "李舒"
      ],
      "Contents": [
        "秩礼有序，",
        "和音既同。",
        "九仪不忒，",
        "三揖将终。",
        "孝感藩后，",
        "相维辟公。",
        "四时之典，",
        "永永无穷。"
      ]
    },
    {
      "_id": null,
      "Name": "郊庙歌辞·让皇帝庙乐章·奠币",
      "PoetId": "68ed44afffd444ac97b3e56d67639ceb",
      "PoetName": "李舒",
      "Form": "未知",
      "Tags": [
        "唐代",
        "李舒"
      ],
      "Contents": [
        "惟帝时若，",
        "去而上仙。",
        "祀用商舞，",
        "乐备宫悬。",
        "白璧加荐，",
        "玄纁告虔。",
        "子孙拜后，",
        "承兹吉蠲。"
      ]
    },
    {
      "_id": null,
      "Name": "郊庙歌辞·让皇帝庙乐章·迎俎",
      "PoetId": "68ed44afffd444ac97b3e56d67639ceb",
      "PoetName": "李舒",
      "Form": "未知",
      "Tags": [
        "唐代",
        "李舒"
      ],
      "Contents": [
        "祀盛体荐，",
        "礼协粢盛。",
        "方周假庙，",
        "用鲁纯牲。",
        "捧彻祗敬，",
        "击拊和鸣。",
        "受釐归胙，",
        "既戒而平。"
      ]
    },
    {
      "_id": null,
      "Name": "郊庙歌辞·让皇帝庙乐章·迎神",
      "PoetId": "68ed44afffd444ac97b3e56d67639ceb",
      "PoetName": "李舒",
      "Form": "未知",
      "Tags": [
        "唐代",
        "李舒"
      ],
      "Contents": [
        "皇矣天宗，",
        "德先王季。",
        "因心则友，",
        "克让以位。",
        "爰命有司，",
        "式遵前志。",
        "神其降灵，",
        "昭飨祀事。"
      ]
    },
    {
      "_id": null,
      "Name": "郊庙歌辞·让皇帝庙乐章·送神",
      "PoetId": "68ed44afffd444ac97b3e56d67639ceb",
      "PoetName": "李舒",
      "Form": "未知",
      "Tags": [
        "唐代",
        "李舒"
      ],
      "Contents": [
        "奠献已事，",
        "昏昕载分。",
        "风摇雨散，",
        "灵卫絪缊。",
        "龙驾帝服，",
        "上腾五云。",
        "泮宫复閟，",
        "寂寞无闻。"
      ]
    },
    {
      "_id": null,
      "Name": "郊庙歌辞·让皇帝庙乐章·酌献",
      "PoetId": "68ed44afffd444ac97b3e56d67639ceb",
      "PoetName": "李舒",
      "Form": "未知",
      "Tags": [
        "唐代",
        "李舒"
      ],
      "Contents": [
        "八音具举，",
        "三寿既盥。",
        "洁兹宗彝，",
        "瑟彼圭瓒。",
        "兰肴重错，",
        "椒醑飘散。",
        "降胙维城，",
        "永为藩翰。"
      ]
    }
  ]
}