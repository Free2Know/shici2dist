  "Poems": [
    {
      "_id": null,
      "Name": "出东林六七里望庐山",
      "PoetId": "7a9bc5f2761d4650a16ba20dce0adfcc",
      "PoetName": "钱载",
      "Form": "七绝",
      "Tags": [
        "清代",
        "钱载"
      ],
      "Contents": [
        "连峰出云云半开，",
        "奔渠卷雪响春雷。",
        "云中屈曲明如玉，",
        "都是天池顶泻来。"
      ]
    },
    {
      "_id": null,
      "Name": "望石湖",
      "PoetId": "7a9bc5f2761d4650a16ba20dce0adfcc",
      "PoetName": "钱载",
      "Form": "七绝",
      "Tags": [
        "清代",
        "钱载"
      ],
      "Contents": [
        "治平寺南湖翠昏，",
        "柳枝茭叶见滩痕。",
        "楞伽不管无情雨，",
        "一夜吹花落范村。"
      ]
    },
    {
      "_id": null,
      "Name": "清流关",
      "PoetId": "7a9bc5f2761d4650a16ba20dce0adfcc",
      "PoetName": "钱载",
      "Form": "七律",
      "Tags": [
        "清代",
        "钱载"
      ],
      "Contents": [
        "广宁门外二千程，",
        "齐鲁河淮坦迤行。",
        "突据冈峦高垒险，",
        "全收吴楚大江横。",
        "南唐入宋沿州堞，",
        "西日回风度使旌。",
        "老我重题秣陵柳，",
        "不知犹似昔年情!"
      ]
    },
    {
      "_id": null,
      "Name": "葑门口号",
      "PoetId": "7a9bc5f2761d4650a16ba20dce0adfcc",
      "PoetName": "钱载",
      "Form": "七绝",
      "Tags": [
        "清代",
        "钱载"
      ],
      "Contents": [
        "灭渡桥回柳映塘，",
        "南风吹郭不胜香。",
        "湖田半种紫芒稻，",
        "麦笠时遮青苎娘。"
      ]
    }
  ]
}