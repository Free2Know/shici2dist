  "Poems": [
    {
      "_id": null,
      "Name": "出都留别诸公",
      "PoetId": "b872f4d2560d443d952738245cd566de",
      "PoetName": "康有为",
      "Form": "七律",
      "Tags": [
        "清代",
        "康有为"
      ],
      "Contents": [
        "天龙作骑万灵从，",
        "独立飞来缥缈峰。",
        "怀抱芳馨兰一握，",
        "纵横宙合雾千重。",
        "眼中战国成争鹿，",
        "海内人才孰卧龙？",
        "抚剑长号归去也，",
        "千山风雨啸青锋。"
      ]
    },
    {
      "_id": null,
      "Name": "闻意索三门湾以兵轮三艘迫浙江有",
      "PoetId": "b872f4d2560d443d952738245cd566de",
      "PoetName": "康有为",
      "Form": "七绝",
      "Tags": [
        "清代",
        "康有为"
      ],
      "Contents": [
        "凄凉白马市中箫，",
        "梦入西湖数六桥。",
        "绝好江山谁看取？",
        "涛声怒断浙江潮。"
      ]
    }
  ]
}