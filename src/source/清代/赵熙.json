      "_id": null,
      "Name": "龙门峡道中",
      "PoetId": "5b09d041ab294a6b93733d06c85afb58",
      "PoetName": "赵熙",
      "Form": "五律",
      "Tags": [
        "清代",
        "赵熙"
      ],
      "Contents": [
        "出郭二十里，",
        "入山千万重。",
        "遥寻瀑布水，",
        "忽听松林钟。",
        "石涸樵生路，",
        "云开雁过峰。",
        "传闻葛由侣，",
        "于此伏虬龙。"
      ]
    }
  ]
}