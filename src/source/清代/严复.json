      "_id": null,
      "Name": "戊戍八月感事",
      "PoetId": "3dcb95462a2548e8a2200c32ad345ea1",
      "PoetName": "严复",
      "Form": "五律",
      "Tags": [
        "清代",
        "严复"
      ],
      "Contents": [
        "求治翻为罪，",
        "明时误爱才。",
        "伏尸名士贱，",
        "称疾诏书哀。",
        "燕市天如晦，",
        "宣南雨又来。",
        "临河鸣犊叹，",
        "莫遣才心灰。"
      ]
    }
  ]
}