      "_id": null,
      "Name": "月夜游孤山",
      "PoetId": "320f3d8222b5438fb2f4c26313cf8c0b",
      "PoetName": "吴震",
      "Form": "五律",
      "Tags": [
        "清代",
        "吴震"
      ],
      "Contents": [
        "松顶一声鹤，",
        "露华满径流。",
        "静闻空翠滴，",
        "间与片云留。",
        "四面只通水，",
        "全山如在舟。",
        "明明楼阁夜，",
        "有客独寻秋。"
      ]
    }
  ]
}