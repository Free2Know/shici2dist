      "_id": null,
      "Name": "宣府逢立秋",
      "PoetId": "696e318f52f4490d9567c705c4d12120",
      "PoetName": "计东",
      "Form": "五律",
      "Tags": [
        "清代",
        "计东"
      ],
      "Contents": [
        "秋气吾所爱，",
        "边城太早寒。",
        "披裘三伏惯，",
        "拥被五更残。",
        "风自长城落，",
        "天连大漠宽。",
        "摩霄羡鹰隼，",
        "健翮尔飞搏。"
      ]
    }
  ]
}