  "Poems": [
    {
      "_id": null,
      "Name": "别老母",
      "PoetId": "4099a24e57b54507975921a29c7b7e40",
      "PoetName": "黄景仁",
      "Form": "七绝",
      "Tags": [
        "清代",
        "黄景仁"
      ],
      "Contents": [
        "搴帷拜母河梁去，",
        "白发愁看泪眼枯。",
        "惨惨柴门风雪夜，",
        "此时有子不如无。"
      ]
    },
    {
      "_id": null,
      "Name": "少年行",
      "PoetId": "4099a24e57b54507975921a29c7b7e40",
      "PoetName": "黄景仁",
      "Form": "未知",
      "Tags": [
        "清代",
        "黄景仁"
      ],
      "Contents": [
        "男儿作健向沙场，",
        "自爱登台不望乡。",
        " 太白高高天尺五，",
        "宝刀明月共辉光。"
      ]
    },
    {
      "_id": null,
      "Name": "山馆夜作",
      "PoetId": "4099a24e57b54507975921a29c7b7e40",
      "PoetName": "黄景仁",
      "Form": "七绝",
      "Tags": [
        "清代",
        "黄景仁"
      ],
      "Contents": [
        "长夜山窗面面开，",
        "江湖前后思悠哉。",
        "当窗试与然高烛，",
        "要看鱼龙啖影来。"
      ]
    },
    {
      "_id": null,
      "Name": "幼女",
      "PoetId": "4099a24e57b54507975921a29c7b7e40",
      "PoetName": "黄景仁",
      "Form": "七绝",
      "Tags": [
        "清代",
        "黄景仁"
      ],
      "Contents": [
        "汝父年来实鲜欢，",
        "牵衣故作别离难。",
        "此行不是长安客，",
        "莫向浮云直北看。"
      ]
    },
    {
      "_id": null,
      "Name": "新安滩",
      "PoetId": "4099a24e57b54507975921a29c7b7e40",
      "PoetName": "黄景仁",
      "Form": "五绝",
      "Tags": [
        "清代",
        "黄景仁"
      ],
      "Contents": [
        "一滩复一滩，",
        "一滩高十丈。",
        "三百六十滩，",
        "新安在天上。"
      ]
    },
    {
      "_id": null,
      "Name": "杂感",
      "PoetId": "4099a24e57b54507975921a29c7b7e40",
      "PoetName": "黄景仁",
      "Form": "七律",
      "Tags": [
        "清代",
        "黄景仁"
      ],
      "Contents": [
        "仙佛茫茫两未成，",
        "只知独夜不平鸣。",
        "风蓬飘尽悲歌气，",
        "泥絮沾来薄幸名。",
        "十有九人堪白眼，",
        "百无一用是书生。",
        "莫因诗卷愁成谶，",
        "春鸟秋虫自作声。"
      ]
    },
    {
      "_id": null,
      "Name": "江村",
      "PoetId": "4099a24e57b54507975921a29c7b7e40",
      "PoetName": "黄景仁",
      "Form": "五绝",
      "Tags": [
        "清代",
        "黄景仁"
      ],
      "Contents": [
        "江水绕孤村，",
        "芳菲在何处？",
        "春从啼鸟来，",
        "啼是春归去。"
      ]
    },
    {
      "_id": null,
      "Name": "癸巳除夕偶成",
      "PoetId": "4099a24e57b54507975921a29c7b7e40",
      "PoetName": "黄景仁",
      "Form": "七绝",
      "Tags": [
        "清代",
        "黄景仁"
      ],
      "Contents": [
        "千家笑语漏迟迟，",
        "忧患潜从物外知。",
        "悄立市桥人不识，",
        "一星如月看多时。"
      ]
    },
    {
      "_id": null,
      "Name": "绮怀诗",
      "PoetId": "4099a24e57b54507975921a29c7b7e40",
      "PoetName": "黄景仁",
      "Form": "七律",
      "Tags": [
        "清代",
        "黄景仁"
      ],
      "Contents": [
        "几回花下坐吹箫，",
        "银汉红墙入望遥。",
        "似此星辰非昨夜，",
        "为谁风露立中宵。",
        "缠绵思尽抽残茧，",
        "宛转心伤剥后蕉。",
        "三五年时三五月，",
        "可怜杯酒不曾消。"
      ]
    }
  ]
}