{
  "_id": "e07f3f92c4f049bd96de265d49d6dbeb",
  "Name": "屈大均",
  "Description": "（1630-1696），初名绍隆，字介子、翁山，广东番禺人。其诗富于民族意识，关怀人民疾苦，诗风高浑雄肆，兼有杜甫、李白之长。为\"岭南三大家\"之冠。",
  "Dynasty": "清代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "云州秋望",
      "PoetId": "e07f3f92c4f049bd96de265d49d6dbeb",
      "PoetName": "屈大均",
      "Form": "五律",
      "Tags": [
        "清代",
        "屈大均"
      ],
      "Contents": [
        "白草黄羊外，",
        "空闻觱篥哀。",
        "遥寻苏武庙，",
        "不上李陵台。",
        "风助群鹰击，",
        "云随万马来。",
        "关前无数柳，",
        "一夜落龙堆。"
      ]
    },
    {
      "_id": null,
      "Name": "天边",
      "PoetId": "e07f3f92c4f049bd96de265d49d6dbeb",
      "PoetName": "屈大均",
      "Form": "七绝",
      "Tags": [
        "清代",
        "屈大均"
      ],
      "Contents": [
        "天边明月迥含霜，",
        "夜夜哀笳怨望乡。",
        "一片愁心与鸿雁，",
        "秋风吹不到衡阳。"
      ]
    },
    {
      "_id": null,
      "Name": "念奴娇",
      "PoetId": "e07f3f92c4f049bd96de265d49d6dbeb",
      "PoetName": "屈大均",
      "Form": "词",
      "Tags": [
        "清代",
        "屈大均"
      ],
      "Contents": [
        "萧条如此，",
        "更何须，",
        "苦忆江南佳丽。",
        "花柳何曾迷六代，",
        "只为春光能醉。",
        "玉笛风朝，",
        "金笳霜夕，",
        "吹得天憔悴。",
        "秦淮波浅，",
        "忍含如许清泪。",
        "  任尔燕子无情，",
        "飞归旧国，",
        "又怎忘兴替。",
        "虎踞龙蟠那得久，",
        "莫 又苍苍王气。",
        "灵谷梅花，",
        "蒋山松树，",
        "未识何年岁。",
        "石人犹在，",
        "问君多少能记。"
      ]
    },
    {
      "_id": null,
      "Name": "梦江南",
      "PoetId": "e07f3f92c4f049bd96de265d49d6dbeb",
      "PoetName": "屈大均",
      "Form": "七绝",
      "Tags": [
        "清代",
        "屈大均"
      ],
      "Contents": [
        "悲落叶，",
        "叶落绝归期。",
        "纵使归来花满树，",
        "新枝不是旧时枝。",
        "且逐水流迟。"
      ]
    },
    {
      "_id": null,
      "Name": "江皋",
      "PoetId": "e07f3f92c4f049bd96de265d49d6dbeb",
      "PoetName": "屈大均",
      "Form": "五律",
      "Tags": [
        "清代",
        "屈大均"
      ],
      "Contents": [
        "翠微春更湿，",
        "烟雨欲无山。",
        "白鹭一溪影，",
        "桃花何处湾？",
        "渔村疏竹外，",
        "古渡夕阳间。",
        "田父不相识，",
        "相随谷口还。"
      ]
    },
    {
      "_id": null,
      "Name": "浣溪沙",
      "PoetId": "e07f3f92c4f049bd96de265d49d6dbeb",
      "PoetName": "屈大均",
      "Form": "词",
      "Tags": [
        "清代",
        "屈大均"
      ],
      "Contents": [
        "一片花含一片愁，",
        "愁随江水不东流。",
        "飞飞长傍景阳楼。",
        "  六代只余芳草在，",
        "三园空有乳莺留。",
        "白门容易白人头。"
      ]
    },
    {
      "_id": null,
      "Name": "潇湘神",
      "PoetId": "e07f3f92c4f049bd96de265d49d6dbeb",
      "PoetName": "屈大均",
      "Form": "七绝",
      "Tags": [
        "清代",
        "屈大均"
      ],
      "Contents": [
        "潇水深，",
        "湘水深，",
        "双双流水逐臣心。",
        "潇水不如湘水好，",
        "将愁送去洞庭阴。"
      ]
    },
    {
      "_id": null,
      "Name": "秣陵",
      "PoetId": "e07f3f92c4f049bd96de265d49d6dbeb",
      "PoetName": "屈大均",
      "Form": "五律",
      "Tags": [
        "清代",
        "屈大均"
      ],
      "Contents": [
        "牛首开天阙，",
        "龙冈抱帝宫。",
        "六朝春草里，",
        "万井落花中。",
        "访旧乌衣少，",
        "听歌玉树空。",
        "如何亡国恨，",
        "尽在大江东？"
      ]
    },
    {
      "_id": null,
      "Name": "紫萸香慢",
      "PoetId": "e07f3f92c4f049bd96de265d49d6dbeb",
      "PoetName": "屈大均",
      "Form": "词",
      "Tags": [
        "清代",
        "屈大均"
      ],
      "Contents": [
        "恨沙蓬，",
        "偏随人转，",
        "更怜雾柳难青。",
        "问征鸿南向，",
        "几时暖返龙庭。",
        "正有无边烟雪，",
        "与鲜飚千里，",
        "送度长城。",
        "向并门少待，",
        "白首牧羝人，",
        "正海上，",
        "手携李卿。",
        "  秋声，",
        "宿定还惊。",
        "愁里月，",
        "不分明。",
        "又哀笳四起，",
        "衣砧断续，",
        "终夜伤情。",
        "跨羊小儿争射，",
        "恁能到，",
        "白蘋汀。",
        "尽长天遍排人字，",
        "逆风飞去，",
        "毛羽随处飘零，",
        "书寄未成。"
      ]
    },
    {
      "_id": null,
      "Name": "虞美人",
      "PoetId": "e07f3f92c4f049bd96de265d49d6dbeb",
      "PoetName": "屈大均",
      "Form": "词",
      "Tags": [
        "清代",
        "屈大均"
      ],
      "Contents": [
        "无风亦向朱栏舞，",
        "情为君王苦。",
        "乌江不渡为红颜，",
        "忍使香魂无主独东还。",
        "  春含古血看犹暖，",
        "巧作红深浅。",
        "花前休唱楚人歌，",
        "恐惹英雄又唤奈虞何。"
      ]
    },
    {
      "_id": null,
      "Name": "读陈胜传",
      "PoetId": "e07f3f92c4f049bd96de265d49d6dbeb",
      "PoetName": "屈大均",
      "Form": "五律",
      "Tags": [
        "清代",
        "屈大均"
      ],
      "Contents": [
        "闾左称雄日，",
        "渔阳谪戍人。",
        "王侯宁有种？",
        "竿木足亡秦。",
        "大义呼豪杰，",
        "先声仗鬼神。",
        "驱除功第一，",
        "汉将可谁伦？"
      ]
    },
    {
      "_id": null,
      "Name": "长亭怨",
      "PoetId": "e07f3f92c4f049bd96de265d49d6dbeb",
      "PoetName": "屈大均",
      "Form": "词",
      "Tags": [
        "清代",
        "屈大均"
      ],
      "Contents": [
        "记烧烛、雁门高处。",
        "积雪封城，",
        "冻云迷路。",
        "添尽香煤，",
        "紫貂相拥、夜深语。",
        "苦寒如许，",
        "难和尔、凄凉句。",
        "一片望乡愁，",
        "饮不醉，",
        "垆头驼乳。",
        "  无处，",
        "问长城旧主，",
        "但见武灵遗墓。",
        "沙飞似箭，",
        "乱穿向，",
        "草中狐兔。",
        "那能使、口北关南、更重作，",
        "并州门户。",
        "且莫吊沙场，",
        "收拾秦弓归去。"
      ]
    }
  ]
}