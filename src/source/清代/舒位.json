    {
      "_id": null,
      "Name": "卧龙冈作",
      "PoetId": "1c0b119c58154d49adbbc46ce1a3af7b",
      "PoetName": "舒位",
      "Form": "七律",
      "Tags": [
        "清代",
        "舒位"
      ],
      "Contents": [
        "象床宝帐悄无言，",
        "草得降书又几番？",
        "两表涕零前出塞，",
        "一官安乐老称藩。",
        "祠官香火三间屋，",
        "大将星辰五丈原。",
        "异代萧条吾怅望，",
        "斜阳满树暮云蘩。"
      ]
    },
    {
      "_id": null,
      "Name": "杨花诗",
      "PoetId": "1c0b119c58154d49adbbc46ce1a3af7b",
      "PoetName": "舒位",
      "Form": "七律",
      "Tags": [
        "清代",
        "舒位"
      ],
      "Contents": [
        "歌残杨柳武昌城，",
        "扑面飞花管送迎。",
        "三月水流春太老，",
        "六朝人去雪无声。",
        "较量妾命谁当薄，",
        "吹落邻家尔许轻。",
        "我住天涯最飘荡，",
        "看渠如此不胜情。"
      ]
    },
    {
      "_id": null,
      "Name": "舟过山塘闻笛",
      "PoetId": "1c0b119c58154d49adbbc46ce1a3af7b",
      "PoetName": "舒位",
      "Form": "七绝",
      "Tags": [
        "清代",
        "舒位"
      ],
      "Contents": [
        "红穗疏灯水上楼，",
        "笛声纤远指痕留。",
        "定知吹笛人双髻，",
        "可惜湘帘不上钩。"
      ]
    },
    {
      "_id": null,
      "Name": "花生日诗魏塘道中作",
      "PoetId": "1c0b119c58154d49adbbc46ce1a3af7b",
      "PoetName": "舒位",
      "Form": "七绝",
      "Tags": [
        "清代",
        "舒位"
      ],
      "Contents": [
        "啼莺如梦送归艎，",
        "日子平分夜未央。",
        "愿取鸳鸯湖里水，",
        "酿成春酒寄花尝。"
      ]
    }
  ]
}