      "PoetId": "22daf5d51208488295f745b656a8498a",
      "PoetName": "宋湘",
      "Form": "七绝",
      "Tags": [
        "清代",
        "宋湘"
      ],
      "Contents": [
        "老屋柴门树打头，",
        "青山屋后水自流。",
        "受书十日九逃学，",
        "恨不先生命牧牛。"
      ]
    }
