{
  "_id": "c6257b3aa8f44c54b080f563fbc0a8de",
  "Name": "朱彝尊",
  "Description": "（1629-1709），字锡鬯，号竹垞，浙江嘉兴人。有《曝书亭集》。博通经史，擅长诗词，为浙派的创始者，诗与王士禛齐名。",
  "Dynasty": "清代",
  "Birth": "",
  "Death": "",
  "Poems": [
    {
      "_id": null,
      "Name": "出居庸关",
      "PoetId": "c6257b3aa8f44c54b080f563fbc0a8de",
      "PoetName": "朱彝尊",
      "Form": "七绝",
      "Tags": [
        "清代",
        "朱彝尊"
      ],
      "Contents": [
        "居庸关上子规啼，",
        "饮马流泉落日低。",
        "雨雪自飞千嶂外，",
        "榆林只隔数峰西。"
      ]
    },
    {
      "_id": null,
      "Name": "卖花声",
      "PoetId": "c6257b3aa8f44c54b080f563fbc0a8de",
      "PoetName": "朱彝尊",
      "Form": "七律",
      "Tags": [
        "清代",
        "朱彝尊"
      ],
      "Contents": [
        "衰柳白门湾，",
        "潮打城还，",
        "小长干接大长干。",
        "歌板酒旗零落尽，",
        "剩有鱼竿。",
        "秋草六朝寒，",
        "花雨空坛，",
        "更无人处一凭栏。",
        "燕子斜阳来又去，",
        "如此江山。"
      ]
    },
    {
      "_id": null,
      "Name": "桂殿秋",
      "PoetId": "c6257b3aa8f44c54b080f563fbc0a8de",
      "PoetName": "朱彝尊",
      "Form": "七绝",
      "Tags": [
        "清代",
        "朱彝尊"
      ],
      "Contents": [
        "思往事，",
        "渡江干。",
        "青娥低映越山看。",
        "共眠一舸听秋雨，",
        "小簟轻衾各自寒。"
      ]
    },
    {
      "_id": null,
      "Name": "鸳鸯湖棹歌",
      "PoetId": "c6257b3aa8f44c54b080f563fbc0a8de",
      "PoetName": "朱彝尊",
      "Form": "七绝",
      "Tags": [
        "清代",
        "朱彝尊"
      ],
      "Contents": [
        "穆湖莲叶小于钱，",
        "卧柳虽多不碍船。",
        "两岸新苗才过雨，",
        "夕阳沟水响溪田。"
      ]
    }
  ]
}