      "_id": null,
      "Name": "崖门谒三忠祠",
      "PoetId": "364c3c01961a4607b7f0951771d66dc7",
      "PoetName": "陈恭尹",
      "Form": "未知",
      "Tags": [
        "清代",
        "陈恭尹"
      ],
      "Contents": [
        "山木萧萧风更吹，",
        "两崖波浪至今悲。",
        "一声望帝啼荒殿，",
        "十载愁人来古祠。",
        "海水有门分上下，",
        "江山无地限化夷。",
        "停舟我亦艰难日，",
        "畏向苍苔读旧碑。",
        "?"
      ]
    }
  ]
