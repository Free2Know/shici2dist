    {
      "_id": null,
      "Name": "向常侍",
      "PoetId": "4283d5d48ba94ad59267dbb71c4fe607",
      "PoetName": "颜延之",
      "Form": "五律",
      "Tags": [
        "南北朝",
        "颜延之"
      ],
      "Contents": [
        "向秀甘淡薄，",
        "深心托豪素。",
        "探道好渊玄，",
        "观书鄙章句。",
        "交吕既鸿轩，",
        "攀嵇亦凤举。",
        "流连河里游，",
        "恻怆山阳赋。"
      ]
    },
    {
      "_id": null,
      "Name": "嵇中散",
      "PoetId": "4283d5d48ba94ad59267dbb71c4fe607",
      "PoetName": "颜延之",
      "Form": "五律",
      "Tags": [
        "南北朝",
        "颜延之"
      ],
      "Contents": [
        "中散不偶世，",
        "本自餐霞人。",
        "形解验默仙，",
        "吐论知凝神。",
        "立俗迕流议，",
        "寻山洽隐沦。",
        "鸾翮有时铩，",
        "龙性谁能驯。"
      ]
    },
    {
      "_id": null,
      "Name": "阮步兵",
      "PoetId": "4283d5d48ba94ad59267dbb71c4fe607",
      "PoetName": "颜延之",
      "Form": "五律",
      "Tags": [
        "南北朝",
        "颜延之"
      ],
      "Contents": [
        "阮公虽沦迹，",
        "识密鉴亦洞。",
        "沉醉似埋照，",
        "寓词类托讽。",
        "长啸若怀人，",
        "越礼自惊众。",
        "物故不可论，",
        "途穷能无恸。"
      ]
    }
  ]
}