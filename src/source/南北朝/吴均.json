  "Poems": [
    {
      "_id": null,
      "Name": "山中杂诗",
      "PoetId": "182e92e81f9943d5b7c96162fe1100a6",
      "PoetName": "吴均",
      "Form": "五绝",
      "Tags": [
        "南北朝",
        "吴均"
      ],
      "Contents": [
        "山际见来烟，",
        "竹中窥落日。",
        "鸟向檐上飞，",
        "云从窗里出。"
      ]
    },
    {
      "_id": null,
      "Name": "答柳恽",
      "PoetId": "182e92e81f9943d5b7c96162fe1100a6",
      "PoetName": "吴均",
      "Form": "未知",
      "Tags": [
        "南北朝",
        "吴均"
      ],
      "Contents": [
        "清晨发陇西，",
        "日暮飞狐谷。",
        "秋月照层岭，",
        "寒风扫高木。",
        "雾露夜侵衣，",
        "关山晓催轴。",
        "君去欲何之，",
        "参差间原陆。",
        "一见终无缘，",
        "怀悲空满目。"
      ]
    },
    {
      "_id": null,
      "Name": "行路难",
      "PoetId": "182e92e81f9943d5b7c96162fe1100a6",
      "PoetName": "吴均",
      "Form": "词",
      "Tags": [
        "南北朝",
        "吴均"
      ],
      "Contents": [
        "洞庭水上一株桐，",
        "经霜触浪困严风。",
        "昔时抽心耀白日，",
        "今旦卧死黄沙中。",
        "洛阳名工见咨嗟，",
        "一翦一刻作琵琶。",
        "白璧规心学明月，",
        "珊瑚映面作风花。",
        "帝王见赏不见忘，",
        "提携把握登建章。",
        "掩抑摧藏张女弹，",
        "殷勤促柱楚明光。",
        "年年月月对君子，",
        "遥遥夜夜宿未央。",
        "未央采女弃鸣篪，",
        "争先拂拭生光仪。",
        "茱萸锦衣玉作匣，",
        "安念昔日枯树枝。",
        "不学衡山南岭桂，",
        "至今千年犹未知。"
      ]
    },
    {
      "_id": null,
      "Name": "赠周散骑兴嗣",
      "PoetId": "182e92e81f9943d5b7c96162fe1100a6",
      "PoetName": "吴均",
      "Form": "五律",
      "Tags": [
        "南北朝",
        "吴均"
      ],
      "Contents": [
        "子云好饮酒，",
        "家在成都县。",
        "制赋已百篇，",
        "弹琴复千转。",
        "敬通不富豪，",
        "相如本贫贱。",
        "共作失职人，",
        "包山一相见。"
      ]
    },
    {
      "_id": null,
      "Name": "赠杜容成",
      "PoetId": "182e92e81f9943d5b7c96162fe1100a6",
      "PoetName": "吴均",
      "Form": "未知",
      "Tags": [
        "南北朝",
        "吴均"
      ],
      "Contents": [
        "一燕海上来，",
        "一燕高堂息。",
        "一朝相逢遇，",
        "依然旧相识。",
        "问我来何迟，",
        "山川几纡直。",
        "答言海路长，",
        "风驶飞无力。",
        "昔别缝罗衣，",
        "春风初入帏。",
        "今来夏欲晚，",
        "桑扈薄树飞。"
      ]
    },
    {
      "_id": null,
      "Name": "赠王桂阳",
      "PoetId": "182e92e81f9943d5b7c96162fe1100a6",
      "PoetName": "吴均",
      "Form": "五律",
      "Tags": [
        "南北朝",
        "吴均"
      ],
      "Contents": [
        "松生数寸时，",
        "遂为草所没。",
        "未见笼云心，",
        "谁知负霜骨。",
        "弱干可摧残，",
        "纤茎易陵忽。",
        "何当数千尺，",
        "为君覆明月。"
      ]
    },
    {
      "_id": null,
      "Name": "酬别江主簿屯骑",
      "PoetId": "182e92e81f9943d5b7c96162fe1100a6",
      "PoetName": "吴均",
      "Form": "未知",
      "Tags": [
        "南北朝",
        "吴均"
      ],
      "Contents": [
        "有客告将离，",
        "赠言重兰蕙。",
        "泛舟当泛济，",
        "结交当结桂。",
        "济水有清源，",
        "桂树多芳根。",
        "毛公与朱亥，",
        "俱在信陵门。",
        "赵瑟凤凰柱，",
        "吴醥金罍樽。",
        "我有北山志，",
        "留连为报恩。",
        "夫君皆逸翮，",
        "抟景复凌骞。",
        "白云间海树，",
        "秋日暗平原。",
        "寒虫鸣趯趯，",
        "落叶飞翻翻。",
        "何用赠分手，",
        "自有北堂萱。"
      ]
    }
  ]
}