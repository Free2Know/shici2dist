  "Poems": [
    {
      "_id": null,
      "Name": "光华殿侍宴赋竞病韵诗",
      "PoetId": "30d2ee4ef8c246b580397feaa5ac472a",
      "PoetName": "曹景宗",
      "Form": "五绝",
      "Tags": [
        "南北朝",
        "曹景宗"
      ],
      "Contents": [
        "去时儿女悲，",
        "归来笳鼓竞。",
        "借问行路人，",
        "何如霍去病？"
      ]
    }
  ]
