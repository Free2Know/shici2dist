  "Poems": [
    {
      "_id": null,
      "Name": "狱中赠邹容",
      "PoetId": "9f002c2914564a20ab993a4107a85e9b",
      "PoetName": "章炳麟",
      "Form": "五律",
      "Tags": [
        "近现代",
        "章炳麟"
      ],
      "Contents": [
        "邹容吾小弟，",
        "被发下瀛洲。",
        "快剪刀除辫，",
        "干牛肉作糇。",
        "英雄一入狱，",
        "天地亦悲秋。",
        "临命须掺手，",
        "乾坤只两头。"
      ]
    }
  ]
}