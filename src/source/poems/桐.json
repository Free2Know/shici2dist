{
  "_id": "3c4b4b6dd7e046fda73c0fcccac61fc7",
  "name": "桐",
  "contents": [
    "孤秀峰阳岑，",
    "亭亭出众林。",
    "春光杂凤影，",
    "秋月弄圭阴。",
    "高映龙门迥，",
    "双依玉井深。",
    "不因将入爨，",
    "谁谓作鸣琴。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "5f3c01f5ddab4ebdb2d9748023f40391",
  "form": "五律",
