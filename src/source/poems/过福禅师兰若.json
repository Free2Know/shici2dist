  "_id": "22c82c45b2954bc9babbad3a14f388a1",
  "name": "过福禅师兰若",
  "contents": [
    "岩壑转微径，",
    "云林隐法堂。",
    "羽人飞奏乐，",
    "天女跪焚香。",
    "竹外峰偏曙，",
    "藤阴水更凉。",
    "欲知禅坐久，",
    "行路长春芳。"
  ],
  "ptype": null,
  "poetName": "王维",
  "poetId": "37d898722e054cb28508509ddcd22139",
  "form": "五律",
