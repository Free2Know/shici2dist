{
  "_id": "e19f2f0b438c4e9f9ac58ce1b1e523bf",
  "name": "神女祠",
  "contents": [
    "停车祀圣女，",
    "凉叶下阴风。",
    "龙气石床湿，",
    "鸟声山庙空。",
    "长眉留桂绿，",
    "丹脸寄莲红。",
    "莫学阳台畔，",
    "朝云暮雨中。"
  ],
  "ptype": null,
  "poetName": "许浑",
  "poetId": "ba84f30a9b344330943f2fbe4d128d8d",
  "form": "五律",
