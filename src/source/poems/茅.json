{
  "_id": "786c453161574a4b966c54d1cadeb000",
  "name": "茅",
  "contents": [
    "楚甸供王日，",
    "衡阳入贡年。",
    "麏包青野外，",
    "鸱啸绮楹前。",
    "尧帝成茨罢，",
    "殷汤祭雨旋。",
    "方期大君锡，",
    "不惧小巫捐。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "5f3c01f5ddab4ebdb2d9748023f40391",
