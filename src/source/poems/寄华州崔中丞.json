{
  "_id": "0d0f96f65fce4564bc53f13a7ab3f66f",
  "name": "寄华州崔中丞",
  "contents": [
    "莲华峰下郡，",
    "仙洞亦难胜。",
    "闾里苍苔水，",
    "虚空瀑布冰。",
    "酒香和药熟，",
    "山峭过云登。",
    "清净黎人泰，",
    "唯忧急诏征。"
  ],
  "ptype": null,
  "poetName": "姚合",
  "poetId": "9a0ddeb7f13a437d8090ccf3996de1f3",
  "form": "五律",
