  "_id": "c524e0244a5e4fb4a9de47adf1a6c138",
  "name": "答端",
  "contents": [
    "郊园夏雨歇，",
    "闲院绿阴生。",
    "职事方无效，",
    "幽赏独违情。",
    "物色坐如见，",
    "离抱怅多盈。",
    "况感夕凉气，",
    "闻此乱蝉鸣。"
  ],
  "ptype": null,
  "poetName": "韦应物",
  "poetId": "45a80c2fc73a4437b0b6c41a5fc10227",
