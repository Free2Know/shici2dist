{
  "_id": "86cdff4df03b4201b629e5f16b48f053",
  "name": "病马",
  "contents": [
    "乘尔亦已久，",
    "天寒关塞深。",
    "尘中老尽力，",
    "岁晚病伤心。",
    "毛骨岂殊众，",
    "驯良犹至今。",
    "物微意不浅，",
    "感动一沉吟。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
