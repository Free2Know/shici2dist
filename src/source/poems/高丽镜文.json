{
  "_id": "04ded816cbef494a81d34aea28f06db3",
  "name": "高丽镜文",
  "contents": [
    "三水中，",
    "四维下，",
    "上帝降子于辰马。",
    "先操鸡，",
    "后搏鸭。",
    "己年中，",
    "二龙见。",
    "一则藏身青木中，",
    "一则见形黑金东。"
  ],
  "ptype": null,
  "poetName": "唐无名氏",
  "poetId": "5fb2c729b42a4746a5b376b70c9d5229",
  "form": "五律",
