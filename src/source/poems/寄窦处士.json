{
  "_id": "dd3cd480d61a4841b8e0d5a26a43fdf5",
  "name": "寄窦处士",
  "contents": [
    "漳水醉中别，",
    "今来犹未醒。",
    "半生因酒废，",
    "大国几时宁。",
    "海畔将军柳，",
    "天边处士星。",
    "游人不可见，",
    "春入乱山青。"
  ],
  "ptype": null,
  "poetName": "杜荀鹤",
  "poetId": "4a8bb60fa6f54cc49debda0988ff139d",
  "form": "五律",
