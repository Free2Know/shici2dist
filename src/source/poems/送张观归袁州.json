{
  "_id": "ac62a02eff664ac7819f69b6add57b80",
  "name": "送张观归袁州",
  "contents": [
    "羡尔湘东去，",
    "烟花尚可亲。",
    "绿芳深映鸟，",
    "远岫递迎人。",
    "饥狖啼初日，",
    "残莺惜暮春。",
    "遥怜谢客兴，",
    "佳句又应新。"
  ],
  "ptype": null,
  "poetName": "李嘉祐",
  "poetId": "c256d35ea8de4cbb869292361f80ed95",
  "form": "五律",
