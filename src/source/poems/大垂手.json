  "_id": "88cd0e60aa8c4e6b92e16d803b1d6fd1",
  "name": "大垂手",
  "contents": [
    "金刀剪轻云，",
    "盘用黄金缕。",
    "装束赵飞燕，",
    "教来掌上舞。",
    "舞罢飞燕死，",
    "片片随风去。"
  ],
  "ptype": null,
  "poetName": "聂夷中",
