  "_id": "3211f20a4b8b4c19bc07a3be1d7f27a3",
  "name": "咏山",
  "contents": [
    "灵山峙千仞，",
    "蔽日且嵯峨。",
    "紫盖云阴远，",
    "香炉烟气多。",
    "石梁高鸟路，",
    "瀑水近天河。",
    "欲知闻道里，",
    "别自有仙歌。"
  ],
  "ptype": null,
  "poetName": "刘斌",
  "poetId": "87ad8e274fd24eb3a4ac36174aeb925d",
