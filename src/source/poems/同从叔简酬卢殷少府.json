{
  "_id": "43797feb5f2d454a8efe5c6d03b7b051",
  "name": "同从叔简酬卢殷少府",
  "contents": [
    "梅尉吟楚声，",
    "竹风为凄清。",
    "深虚冰在性，",
    "高洁云入情。",
    "借水洗闲貌，",
    "寄蕉书逸名。",
    "羞将片石文，",
    "斗此双琼英。"
  ],
  "ptype": null,
  "poetName": "孟郊",
  "poetId": "03c94ff99104486fbd1c223eade4ad35",
  "form": "五律",
