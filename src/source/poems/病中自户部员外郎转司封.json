{
  "_id": "79591d108d9e4d028eb8b0c06e5f284a",
  "name": "病中自户部员外郎转司封",
  "contents": [
    "羸卧承新命，",
    "优容获所安。",
    "遣儿迎贺客，",
    "无力拂尘冠。",
    "偃仰晴轩暖，",
    "支离晓镜寒。",
    "那堪报恩去，",
    "感激对衰兰。"
  ],
  "ptype": null,
  "poetName": "吕温",
  "poetId": "9ba522d8a1704d6ca3c941a5b20de720",
  "form": "五律",
