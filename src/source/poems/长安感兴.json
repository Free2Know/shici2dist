{
  "_id": "81f7d2df2c9440d9953bebe1b58c8a4c",
  "name": "长安感兴",
  "contents": [
    "徒劳悲丧乱，",
    "自古戒繁华。",
    "落日狐兔径，",
    "近年公相家。",
    "可悲闻玉笛，",
    "不见走香车。",
    "寂寞墙匡里，",
    "春阴挫杏花。"
  ],
  "ptype": null,
  "poetName": "郑谷",
  "poetId": "87cb83f765db4575a2bdcb67a72bc6d7",
