  "_id": "8c72e3bdf72c4052ac97da79969b7405",
  "name": "上侍御七兄",
  "contents": [
    "天人俟明路，",
    "益稷分尧心。",
    "利器必先举，",
    "非贤安可任。",
    "吾兄执严宪，",
    "时佐能钓深。"
  ],
  "ptype": null,
  "poetName": "王昌龄",
