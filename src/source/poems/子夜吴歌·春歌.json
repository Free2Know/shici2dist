{
  "_id": "8fa7d69d1e9e4c7e9cf0d1b7f5ef3ede",
  "name": "子夜吴歌·春歌",
  "contents": [
    "秦地罗敷女，",
    "采桑绿水边。",
    "素手青条上，",
    "红妆白日鲜。",
    "蚕饥妾欲去，",
    "五马莫留连。"
  ],
  "ptype": null,
  "poetName": "李白",
  "poetId": "1b59d554dd594c8497d9cceefaf73d61",
  "form": "五律",
