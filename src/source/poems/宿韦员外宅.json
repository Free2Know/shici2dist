  "_id": "5cb507c5b3fc4b37a2a307c66b9de0b8",
  "name": "宿韦员外宅",
  "contents": [
    "传经韦相后，",
    "赐笔汉家郎。",
    "幽阁诸生会，",
    "寒宵几刻长。",
    "座中灯泛酒，",
    "檐外月如霜。",
    "人事多飘忽，",
    "邀欢讵可忘。"
  ],
  "ptype": null,
  "poetName": "耿湋",
  "poetId": "d62460239e1244dcad61df818057bd89",
