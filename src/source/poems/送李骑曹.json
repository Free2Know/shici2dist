{
  "_id": "3a97d17e159e487e8b1b4071bb745c5c",
  "name": "送李骑曹",
  "contents": [
    "归骑双旌远，",
    "欢生此别中。",
    "萧关分碛路，",
    "嘶马背寒鸿。",
    "朔色晴天北，",
    "河源落日东。",
    "贺兰山顶草，",
    "时动卷帆风。"
  ],
  "ptype": null,
  "poetName": "贾岛",
  "poetId": "03638acd71554861ad66405ecf420eb4",
  "form": "五律",
