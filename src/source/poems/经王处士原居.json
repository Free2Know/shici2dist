  "_id": "426e117971f844439696a7096da2ef3a",
  "name": "经王处士原居",
  "contents": [
    "旧宅谁相近，",
    "唯僧近竹关。",
    "庭闲云满井，",
    "窗晓雪通山。",
    "来客半留宿，",
    "借书多寄还。",
    "明时未中岁，",
    "莫便一生闲。"
  ],
  "ptype": null,
  "poetName": "张籍",
  "poetId": "0810aa6ff9a34a96aba9c12caed115ae",
