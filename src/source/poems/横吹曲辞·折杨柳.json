{
  "_id": "7540bac04a5f4a2dbf865428e17d73a1",
  "name": "横吹曲辞·折杨柳",
  "contents": [
    "万里边城地，",
    "三春杨柳节。",
    "叶似镜中眉，",
    "花如关外雪。",
    "征人远乡思，",
    "倡妇高楼别。",
    "不忍掷年华，",
    "含情寄攀折。"
  ],
  "ptype": null,
  "poetName": "韦承庆",
  "poetId": "9eac0531c0364b6bbdb169a29826f994",
  "form": "五律",
