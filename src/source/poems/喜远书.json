  "_id": "e1a5b2831c4b4d1c96f5c10e333c5932",
  "name": "喜远书",
  "contents": [
    "端居换时节，",
    "离恨隔龙泷。",
    "苔色上春阁，",
    "柳阴移晚窗。",
    "寄怀因桂水，",
    "流泪极枫江。",
    "此日南来使，",
    "金盘鱼一双。"
  ],
  "ptype": null,
  "poetName": "许浑",
  "poetId": "ba84f30a9b344330943f2fbe4d128d8d",
