  "_id": "bb8552795f614b6c8dfee80c0f6f8442",
  "name": "送韦员外还京",
  "contents": [
    "十年离乱后，",
    "此去若为情。",
    "春晚香山绿，",
    "人稀豫水清。",
    "野陂看独树，",
    "关路逐残莺。",
    "前殿朝明主，",
    "应怜白发生。"
  ],
  "ptype": null,
  "poetName": "崔峒",
