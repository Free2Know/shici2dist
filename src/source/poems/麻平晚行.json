{
  "_id": "572d49d21b064e99a11315358f13a694",
  "name": "麻平晚行",
  "contents": [
    "百年怀土望，",
    "千里倦游情。",
    "高低寻戍道，",
    "远近听泉声。",
    "漳叶才分色，",
    "山花不辨名。",
    "羁心何处尽，",
    "风急暮猿清。"
  ],
  "ptype": null,
  "poetName": "王勃",
  "poetId": "c7670ea82d5d4d89b2e94df234298b4c",
