{
  "_id": "5fe8c29f129f47de86a8edd4db2e09cc",
  "name": "送罗侍御归西台",
  "contents": [
    "归台新柱史，",
    "辞府旧英髦。",
    "劝酒莲幕贵，",
    "望尘骢马高。",
    "诗情分绣段，",
    "剑彩拂霜毫。",
    "此举关风化，",
    "谁云别恨劳。"
  ],
  "ptype": null,
  "poetName": "鲍溶",
  "poetId": "573cd37f2fa84977bd69787588224a32",
