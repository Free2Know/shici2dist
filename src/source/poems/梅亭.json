  "_id": "ab5af35406584902a6528c97999be432",
  "name": "梅亭",
  "contents": [
    "东海穷诗客，",
    "西风古驿亭。",
    "发从残岁白，",
    "山入故乡青。",
    "世事徒三窟，",
    "儿曹且一经。",
    "丁宁速赊酒，",
    "煮栗试砂瓶。"
  ],
  "ptype": null,
  "poetName": "唐彦谦",
  "poetId": "bed3d2dc8e014c25950dab9d7fad3ef4",
  "form": "五律",
