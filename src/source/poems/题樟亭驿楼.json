  "_id": "46fd65f67ab940259ce735601ecb3b07",
  "name": "题樟亭驿楼",
  "contents": [
    "危槛倚山城，",
    "风帆槛外行。",
    "日生沧海赤，",
    "潮落浙江清。",
    "秋晚遥峰出，",
    "沙干细草平。",
    "西陵烟树色，",
    "长见伍员情。"
  ],
  "ptype": null,
  "poetName": "喻坦之",
  "poetId": "39b164d7bed64e5e937387611e36c4bd",
  "form": "五律",
