  "_id": "ab1c15d176134dc0be68c0b2eddebef2",
  "name": "享太庙乐章",
  "contents": [
    "于铄丕嗣，",
    "惟帝之光。",
    "羽籥象德，",
    "金石荐祥。",
    "圣系无极，",
    "景命永昌。",
    "神降上哲，",
    "维天配长。"
  ],
  "ptype": null,
  "poetName": "萧仿",
