{
  "_id": "0348094a107749dc87c7d5b7d6ee97c9",
  "name": "陈氏园林",
  "contents": [
    "当门三四峰，",
    "高兴几人同。",
    "寻鹤新泉外，",
    "留僧古木中。",
    "蝉鸣槐叶雨，",
    "鱼散芰荷风。",
    "多喜陪幽赏，",
    "清吟绕石丛。"
  ],
  "ptype": null,
  "poetName": "郑巢",
  "poetId": "3c6cf70867af413299df013644fcd454",
