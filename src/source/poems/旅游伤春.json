{
  "_id": "e3f627ebbc3a412b8a53deee3c3d2bbc",
  "name": "旅游伤春",
  "contents": [
    "酒醒乡关远，",
    "迢迢听漏终。",
    "曙分林影外，",
    "春尽雨声中。",
    "鸟思江村路，",
    "花残野岸风。",
    "十年成底事，",
    "羸马倦西东。"
  ],
  "ptype": null,
  "poetName": "李昌符",
  "poetId": "ca15f36b5e124414b993931a1677a016",
  "form": "五律",
