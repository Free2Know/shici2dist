  "_id": "39d06e92a1b14f8e8d5451ac76a59e21",
  "name": "秋日寄李支使",
  "contents": [
    "秋思朝来起，",
    "侵人暑稍微。",
    "晓眠离北户，",
    "午饭尚生衣。",
    "山静云初白，",
    "枝高果渐稀。",
    "闻君家海上，",
    "莫与燕同归。"
  ],
  "ptype": null,
  "poetName": "姚合",
  "poetId": "9a0ddeb7f13a437d8090ccf3996de1f3",
