  "_id": "6e55ab5561b8424ea9205d14da14e461",
  "name": "路旁树",
  "contents": [
    "古树何人种，",
    "清阴减昔时。",
    "莓苔根半露，",
    "风雨节偏危。",
    "虫蠹心将穴，",
    "蝉催叶向衰。",
    "樵童不须翦，",
    "聊起邵公思。"
  ],
  "ptype": null,
  "poetName": "马戴",
  "poetId": "db14ec8dfa7a4a92987b2e2357aaa5b6",
