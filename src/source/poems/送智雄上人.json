  "_id": "31d047782dc14ea69b42114d604f67d6",
  "name": "送智雄上人",
  "contents": [
    "忽起游方念，",
    "飘然不可留。",
    "未知携一锡，",
    "乘兴向何州。",
    "古岸春云散，",
    "遥天晚雨收。",
    "想应重会面，",
    "风月又清秋。"
  ],
  "ptype": null,
  "poetName": "李中",
  "poetId": "5b8a00819d324861a0a7ee4ab6bd3e48",
