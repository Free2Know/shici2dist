  "_id": "c497cd0119a349ef925626f554a8d54a",
  "name": "新春",
  "contents": [
    "老境三年病，",
    "新元十日阴。",
    "疏篱枯蔓缀，",
    "坏壁绿苔侵。",
    "忧国孤臣泪，",
    "平胡壮士心。",
    "吾非儿女辈，",
    "肯赋白头吟？"
  ],
  "ptype": null,
  "poetName": "陆游",
