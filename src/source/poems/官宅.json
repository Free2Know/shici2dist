{
  "_id": "760a306796da4232bab09ca7f4e260bb",
  "name": "官宅",
  "contents": [
    "红紫共纷纷，",
    "只承老使君。",
    "移舟木兰棹，",
    "行酒石榴裙。",
    "水色窗窗见，",
    "花香院院闻。",
    "恋他官舍住，",
    "双鬓白如云。"
  ],
  "ptype": null,
  "poetName": "白居易",
  "poetId": "61c661a769a04e409e99836b8f4a8f79",
