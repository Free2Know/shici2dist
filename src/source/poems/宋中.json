  "_id": "e6e9ee8bfc8b43c0a44feebe513c2c99",
  "name": "宋中",
  "contents": [
    "日暮黄云合，",
    "年深白骨稀。",
    "旧村乔木在，",
    "秋草远人归。",
    "废井莓苔厚，",
    "荒田路径微。",
    "唯馀近山色，",
    "相对似依依。"
  ],
  "ptype": null,
  "poetName": "耿湋",
  "poetId": "d62460239e1244dcad61df818057bd89",
