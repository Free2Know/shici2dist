  "_id": "e73e0fb1d65b4eadaf1f43bcce36d10c",
  "name": "寄姚合郎中",
  "contents": [
    "转刺名山郡，",
    "连年别省曹。",
    "分题得客少，",
    "著价买书高。",
    "晚柳蝉和角，",
    "寒城烛照涛。",
    "鄱溪卧疾久，",
    "未获后乘骚。"
  ],
  "ptype": null,
  "poetName": "周贺",
  "poetId": "1dcebe7978b74148a639d775fa448e03",
