  "_id": "27981d03f5964cccab557dd98d109616",
  "name": "游龙门奉先寺",
  "contents": [
    "已从招提游，",
    "更宿招提境。",
    "阴壑生虚籁，",
    "月林散清影。",
    "天阙象纬逼，",
    "云卧衣裳冷。",
    "欲觉闻晨钟，",
    "令人发深省。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
