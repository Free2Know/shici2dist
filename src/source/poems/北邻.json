{
  "_id": "70b231761b2f4d27bcc23f68605c5f69",
  "name": "北邻",
  "contents": [
    "明府岂辞满，",
    "藏身方告劳。",
    "青钱买野竹，",
    "白帻岸江皋。",
    "爱酒晋山简，",
    "能诗何水曹。",
    "时来访老疾，",
    "步屟到蓬蒿。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
  "form": "五律",
