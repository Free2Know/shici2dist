  "_id": "ab921783b81f44ffa85ffce52c79ded7",
  "name": "送宗密上人",
  "contents": [
    "门前九陌尘，",
    "石上定中身。",
    "近放辽天鹤，",
    "曾为南岳人。",
    "腊高松叶换，",
    "雪尽茗芽新。",
    "一自传香后，",
    "名山愿卜邻。"
  ],
  "ptype": null,
  "poetName": "马戴",
  "poetId": "db14ec8dfa7a4a92987b2e2357aaa5b6",
