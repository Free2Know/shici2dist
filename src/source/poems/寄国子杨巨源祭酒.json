  "_id": "439dee3357d24f9c8a5926c1282858db",
  "name": "寄国子杨巨源祭酒",
  "contents": [
    "日日新诗出，",
    "城中写不禁。",
    "清高疑对竹，",
    "闲雅胜闻琴。",
    "门户饶秋景，",
    "儿童解冷吟。",
    "云山今作主，",
    "还借外人寻。"
  ],
  "ptype": null,
  "poetName": "姚合",
  "poetId": "9a0ddeb7f13a437d8090ccf3996de1f3",
