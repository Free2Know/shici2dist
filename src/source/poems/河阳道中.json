  "_id": "a79798f225944fbb92e81a425efc9ddb",
  "name": "河阳道中",
  "contents": [
    "冲风仍蹑冻，",
    "提辔手频呵。",
    "得事应须早，",
    "愁人不在多。",
    "雪田平入塞，",
    "烟郭曲随河。",
    "翻忆江涛里，",
    "船中睡盖蓑。"
  ],
  "ptype": null,
  "poetName": "李洞",
  "poetId": "4eeec622c9a342139df6f02d3b9f134b",
