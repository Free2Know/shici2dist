{
  "_id": "5e5fbc229cd340d9a2961ed74536d72b",
  "name": "宿甘棠馆",
  "contents": [
    "尘迹入门尽，",
    "悄然江海心。",
    "水声巫峡远，",
    "山色洞庭深。",
    "风桂落寒子，",
    "岚烟凝夕阴。",
    "前轩鹤归处，",
    "萝月思沈沈。"
  ],
  "ptype": null,
  "poetName": "储嗣宗",
  "poetId": "31341e7447c14337b59e67bd53708bf9",
  "form": "五律",
