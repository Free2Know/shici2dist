{
  "_id": "2f321b3671af442da1d5865c138d1074",
  "name": "石笋",
  "contents": [
    "千古海门石，",
    "移归吟叟居。",
    "窍腥蛟出后，",
    "形瘦浪冲余。",
    "工语宁无玉，",
    "僧知忽有书。",
    "好期仙者叱，",
    "变化向庭隅。"
  ],
  "ptype": null,
  "poetName": "刘昭禹",
  "poetId": "25832508b4e74648a41c95525b5a5202",
  "form": "五律",
