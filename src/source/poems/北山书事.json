  "_id": "5ecdd1c88f0b482fa423adca76fdfffb",
  "name": "北山书事",
  "contents": [
    "黄河一曲山，",
    "天半锁重关。",
    "圣日雄藩静，",
    "秋风老将闲。",
    "车舆穿谷口，",
    "市井响云间。",
    "大野无飞鸟，",
    "元戎校猎还。"
  ],
  "ptype": null,
  "poetName": "张乔",
  "poetId": "746ea2e1efcd41db9a5d67eddf753f46",
