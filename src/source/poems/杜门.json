{
  "_id": "e5870d2303014efe9a5e5b7840152184",
  "name": "杜门",
  "contents": [
    "且咏闲居赋，",
    "飞翔去未能。",
    "春风花屿酒，",
    "秋雨竹溪灯。",
    "世路变陵谷，",
    "时情验友朋。",
    "达生书一卷，",
    "名利付春冰。"
  ],
  "ptype": null,
  "poetName": "李群玉",
  "poetId": "533477cc5b0045399dfdc5f641132458",
