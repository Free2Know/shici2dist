{
  "_id": "7a0d7c7d5528494a9d806a14aadec260",
  "name": "冬深",
  "contents": [
    "花叶随天意，",
    "江溪共石根。",
    "早霞随类影，",
    "寒水各依痕。",
    "易下杨朱泪，",
    "难招楚客魂。",
    "风涛暮不稳，",
    "舍棹宿谁门。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
  "form": "五律",
