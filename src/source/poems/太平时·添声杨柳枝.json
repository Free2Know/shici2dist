{
  "_id": "18faa6f7952e448c8332bf93a4544a12",
  "name": "太平时·添声杨柳枝",
  "contents": [
    "竹里房栊一径深。",
    "静愔愔。",
    "乱红飞尽绿成阴。",
    "有鸣禽。",
    "临罢兰亭无一事，",
    "自修琴。",
    "铜炉袅袅海南沉。",
    "洗尘襟。"
  ],
  "ptype": null,
  "poetName": "陆游",
  "poetId": "1c6bea7a14844f16a71b37c195c82973",
  "form": "五律",
