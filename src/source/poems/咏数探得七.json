  "_id": "c20c71ee2972454b96f9e3dfa7f9ccc6",
  "name": "咏数探得七",
  "contents": [
    "邹子谭天岁，",
    "黄童对日年。",
    "求真初作传，",
    "炼魄已成仙。",
    "鹤驾迎缑岭，",
    "星桥下蜀川。",
    "逢君竹林客，",
    "相对弄清弦。"
  ],
  "ptype": null,
  "poetName": "皎然",
  "poetId": "a5afe9c958b94f8fb0cef564a238e23e",
