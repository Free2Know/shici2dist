  "_id": "12b34a97d92a47e2b3c277149a9c3117",
  "name": "赋昭君冢",
  "contents": [
    "万里关山冢，",
    "明妃旧死心。",
    "恨为秋色晚，",
    "愁结暮云阴。",
    "夜切胡风起，",
    "天高汉月临。",
    "已知无玉貌，",
    "何事送黄金。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "3a86db0ed7e4461794e5c0aa5ffb339c",
