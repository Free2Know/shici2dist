{
  "_id": "1fe6ac5b7f2e4acc8e82b8b3207c54fb",
  "name": "同僧宿道者院",
  "contents": [
    "携文过水宿，",
    "拂席四廊尘。",
    "坠果敲楼瓦，",
    "高萤映鹤身。",
    "点灯吹叶火，",
    "谈佛悟山人。",
    "尽有栖霞志，",
    "好谋三教邻。"
  ],
  "ptype": null,
  "poetName": "李洞",
  "poetId": "4eeec622c9a342139df6f02d3b9f134b",
  "form": "五律",
