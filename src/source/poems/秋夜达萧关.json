  "_id": "28231a1f9e4547d58c43c016eeb8f102",
  "name": "秋夜达萧关",
  "contents": [
    "扰扰浮梁路，",
    "人忙月自闲。",
    "去年为塞客，",
    "今夜宿萧关。",
    "辞国几经岁，",
    "望乡空见山。",
    "不知江叶下，",
    "又作布衣还。"
  ],
  "ptype": null,
  "poetName": "于邺",
  "poetId": "d0aa9bcad74f4376b04a950b49bdd3d3",
