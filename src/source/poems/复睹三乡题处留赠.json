  "_id": "5130e8e66b6849098c6f84ba0eaf5d6a",
  "name": "复睹三乡题处留赠",
  "contents": [
    "壁古字未灭，",
    "声长响不绝。",
    "蕙质本如云，",
    "松心应耐雪。",
    "耿耿离幽谷，",
    "悠悠望瓯越。",
    "杞妇哭夫时，",
    "城崩无此说。"
  ],
  "ptype": null,
  "poetName": "贾驰",
  "poetId": "a9e1ae858cb740f69169516fb6a0c691",
  "form": "五律",
