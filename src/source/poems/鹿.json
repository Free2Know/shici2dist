{
  "_id": "d96d28a035ce45dda89c03a8d1b7cdba",
  "name": "鹿",
  "contents": [
    "涿鹿闻中冀，",
    "秦原辟帝畿。",
    "柰花开旧苑，",
    "萍叶蔼前诗。",
    "道士乘仙日，",
    "先生折角时。",
    "方怀丈夫志，",
    "抗首别心期。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "5f3c01f5ddab4ebdb2d9748023f40391",
