{
  "_id": "49a17ee9865345e5b7ab6a327ef49f80",
  "name": "送房处士闲游",
  "contents": [
    "采药陶贞白，",
    "寻山许远游。",
    "刀圭藏妙用，",
    "岩洞契冥搜。",
    "花月三江水，",
    "琴尊一叶舟。",
    "羡君随野鹤，",
    "长揖稻粱愁。"
  ],
  "ptype": null,
  "poetName": "李群玉",
  "poetId": "533477cc5b0045399dfdc5f641132458",
  "form": "五律",
