  "_id": "cc54611b296a4764bc03d0340cbd8305",
  "name": "白头吟",
  "contents": [
    "汉家天宇阔，",
    "日月不暂闲。",
    "常将古今骨，",
    "裨作北邙山。",
    "山高势已极，",
    "犹自凋朱颜。"
  ],
  "ptype": null,
  "poetName": "邵谒",
