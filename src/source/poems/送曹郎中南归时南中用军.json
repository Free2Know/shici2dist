{
  "_id": "9713f06ef2bd4d29bef736041b3634f3",
  "name": "送曹郎中南归时南中用军",
  "contents": [
    "桂水净和天，",
    "南归似谪仙。",
    "系绦轻象笏，",
    "买布接蛮船。",
    "海气蒸鼙软，",
    "江风激箭偏。",
    "罢郎吟乱里，",
    "帝远岂知贤。"
  ],
  "ptype": null,
  "poetName": "李洞",
  "poetId": "4eeec622c9a342139df6f02d3b9f134b",
  "form": "五律",
