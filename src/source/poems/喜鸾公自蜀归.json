  "_id": "79f83507e8ea48f2957dfff30089d451",
  "name": "喜鸾公自蜀归",
  "contents": [
    "禁院对生台，",
    "寻师到绿槐。",
    "寺高猿看讲，",
    "钟动鸟知斋。",
    "扫石月盈帚，",
    "滤泉花满筛。",
    "归来逢圣节，",
    "吟步上尧阶。"
  ],
  "ptype": null,
  "poetName": "李洞",
  "poetId": "4eeec622c9a342139df6f02d3b9f134b",
