  "_id": "c8b22f86cf7a48e5bf76ba0513224535",
  "name": "赠宋校书",
  "contents": [
    "曾伴元戎猎，",
    "寒来梦北军。",
    "闲身不计日，",
    "病鹤放归林。",
    "石上铺棋势，",
    "船中赌酒分。",
    "长言买天姥，",
    "高卧谢人群。"
  ],
  "ptype": null,
  "poetName": "李洞",
  "poetId": "4eeec622c9a342139df6f02d3b9f134b",
