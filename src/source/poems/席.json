{
  "_id": "191d04f120544d84a62db0883e6c9eb2",
  "name": "席",
  "contents": [
    "辟席承宣父，",
    "重筵揖戴公。",
    "桂香浮半月，",
    "兰气袭回风。",
    "舞拂丹霞上，",
    "歌清白雪中。",
    "伫将文绮色，",
    "舒卷帝王宫。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "5f3c01f5ddab4ebdb2d9748023f40391",
