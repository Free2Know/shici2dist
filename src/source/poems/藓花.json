{
  "_id": "f1ae8973e46f4d619a762469a6851e7d",
  "name": "藓花",
  "contents": [
    "寂寞人偏重，",
    "无心愧牡丹。",
    "秋风凋不得，",
    "流水泛应难。",
    "怪石纵教遍，",
    "幽庭一任盘。",
    "若逢公子顾，",
    "重叠是朱栏。"
  ],
  "ptype": null,
  "poetName": "文丙",
  "poetId": "bec69fe0f36741cea8071355ca2711d2",
