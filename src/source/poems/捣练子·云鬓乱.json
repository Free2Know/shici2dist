  "_id": "442d86627c544cd1ba38e83c895e6c7e",
  "name": "捣练子·云鬓乱",
  "contents": [
    "云鬓乱，",
    "晚妆残，",
    "带恨眉儿远岫攒。",
    "斜托香腮春笋嫩，",
    "为谁和泪倚阑干。"
  ],
  "ptype": null,
  "poetName": "李煜",
  "poetId": "3973cc8d8e7c4a2c82065a694458d2f6",
  "form": "七绝",
