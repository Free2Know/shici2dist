  "_id": "24788847924344b1bdc5a7ac2874dc7b",
  "name": "留别江陵王少府",
  "contents": [
    "迢迢山上路，",
    "病客独行迟。",
    "况此分手处，",
    "当君失意时。",
    "寒林远路驿，",
    "晚烧过荒陂。",
    "别后空回首，",
    "相逢未有期。"
  ],
  "ptype": null,
  "poetName": "张籍",
  "poetId": "0810aa6ff9a34a96aba9c12caed115ae",
