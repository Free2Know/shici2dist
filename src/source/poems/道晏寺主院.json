  "_id": "82f21bbe2db0498e805d054295a042d5",
  "name": "道晏寺主院",
  "contents": [
    "北邻有幽竹，",
    "潜筠穿我庐。",
    "往来地已密，",
    "心乐道者居。",
    "残花回往节，",
    "轻条荫夏初。",
    "闻钟北窗起，",
    "啸傲永日馀。"
  ],
  "ptype": null,
  "poetName": "韦应物",
  "poetId": "45a80c2fc73a4437b0b6c41a5fc10227",
