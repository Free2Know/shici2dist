{
  "_id": "be62c2d109f84a9d9462577d7d1d1eba",
  "name": "至陈仓晓晴望京邑",
  "contents": [
    "拂曙驱飞传，",
    "初晴带晓凉。",
    "雾敛长安树，",
    "云归仙帝乡。",
    "涧流漂素沫，",
    "岩景霭朱光。",
    "今朝好风色，",
    "延瞰极天庄。"
  ],
  "ptype": null,
  "poetName": "卢照邻",
  "poetId": "605828d5fc0c456498ec2ddb7fec0cb4",
  "form": "五律",
