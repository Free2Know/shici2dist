  "_id": "1fff9b1ccc5341dea4d46413bd67319f",
  "name": "故相国苗公挽歌",
  "contents": [
    "灞陵谁宠葬，",
    "汉主念萧何。",
    "盛业留青史，",
    "浮荣逐逝波。",
    "陇云仍作雨，",
    "薤露已成歌。",
    "凄怆平津阁，",
    "秋风吊客过。"
  ],
  "ptype": null,
  "poetName": "钱起",
  "poetId": "a0ef0c5bc91742a1842f4e8974008f2f",
