{
  "_id": "ab8f1ed8e6f24e7bb8cfe7ba35e0b94b",
  "name": "题山僧院",
  "contents": [
    "谿路曾来日，",
    "年多与旧同。",
    "地寒松影里，",
    "僧老磬声中。",
    "远水清风落，",
    "闲云别院通。",
    "心源若无碍，",
    "何必更论空。"
  ],
  "ptype": null,
  "poetName": "张乔",
  "poetId": "746ea2e1efcd41db9a5d67eddf753f46",
