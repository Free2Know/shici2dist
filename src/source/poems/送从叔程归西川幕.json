{
  "_id": "0dfccb7b80744a3bb3f460c2c53a6cfc",
  "name": "送从叔程归西川幕",
  "contents": [
    "千山冰雪晴，",
    "山静锦花明。",
    "群鹤栖莲府，",
    "诸戎拜柳营。",
    "浪依巴字息，",
    "风入蜀关清。",
    "岂念在贫巷，",
    "竹林鸣鸟声。"
  ],
  "ptype": null,
  "poetName": "卢纶",
  "poetId": "10008e7cd9434079b60c4f3e941d4f4f",
  "form": "五律",
