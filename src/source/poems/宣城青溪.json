  "_id": "935e45f3aa25424d9eaa8c0ec16de9ae",
  "name": "宣城青溪",
  "contents": [
    "青溪胜桐庐，",
    "水木有佳色。",
    "山貌日高古，",
    "石容天倾侧。",
    "彩鸟昔未名，",
    "白猿初相识。",
    "不见同怀人，",
    "对之空叹息。"
  ],
  "ptype": null,
  "poetName": "李白",
  "poetId": "1b59d554dd594c8497d9cceefaf73d61",
