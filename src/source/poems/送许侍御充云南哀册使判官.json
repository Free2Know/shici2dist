{
  "_id": "13f72ad4fd504009b2e98d483c5e4c79",
  "name": "送许侍御充云南哀册使判官",
  "contents": [
    "万里永昌城，",
    "威仪奉圣明。",
    "冰心瘴江冷，",
    "霜宪漏天晴。",
    "荒外开亭候，",
    "云南降旆旌。",
    "他时功自许，",
    "绝域转哀荣。"
  ],
  "ptype": null,
  "poetName": "杨巨源",
  "poetId": "37f6157b39fb437780684fb935928971",
  "form": "五律",
