  "_id": "a1a595c4d51b43f3a474d39211d60215",
  "name": "旅夜怀远客",
  "contents": [
    "异乡多远情，",
    "梦断落江城。",
    "病起惭书癖，",
    "贫家负酒名。",
    "过春花自落，",
    "竟晓月空明。",
    "独此一长啸，",
    "故人天际行。"
  ],
  "ptype": null,
  "poetName": "许浑",
  "poetId": "ba84f30a9b344330943f2fbe4d128d8d",
