  "_id": "883c5e7d24f64bc985b438942430e256",
  "name": "送人尉闽中",
  "contents": [
    "可叹芳菲日，",
    "分为万里情。",
    "阊门折垂柳，",
    "御苑听残莺。",
    "海戍通闽邑，",
    "江航过楚城。",
    "客心君莫问，",
    "春草是王程。"
  ],
  "ptype": null,
  "poetName": "李颀",
  "poetId": "a6c5740481924010b2b4eb097caa19df",
