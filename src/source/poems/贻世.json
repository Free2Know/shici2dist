{
  "_id": "74f1a8251d7242ac9f38d5069f6ca47f",
  "name": "贻世",
  "contents": [
    "至理不误物，",
    "悠悠自不明。",
    "黄金烧欲尽，",
    "白发火边生。",
    "苦惑神仙谜，",
    "难收日月精。",
    "捕风兼系影，",
    "信矣不须争。"
  ],
  "ptype": null,
  "poetName": "贯休",
  "poetId": "35ef826ebca24b318ada7e612c75eda6",
