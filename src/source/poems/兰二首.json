{
  "_id": "55d9492257764755b6f2aef6f9d71e4f",
  "name": "兰二首",
  "contents": [
    "清风摇翠环，",
    "凉露滴苍玉。",
    "美人胡不纫，",
    "幽香蔼空谷。",
    "谢庭漫芳草，",
    "楚畹多绿莎。",
    "于焉忽相见，",
    "岁晏将如何?"
  ],
  "ptype": null,
  "poetName": "唐彦谦",
  "poetId": "bed3d2dc8e014c25950dab9d7fad3ef4",
  "form": "五律",
