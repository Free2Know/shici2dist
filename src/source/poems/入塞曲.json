  "_id": "2379791eb93d44a9acb743d132ea2d34",
  "name": "入塞曲",
  "contents": [
    "留滞边庭久，",
    "归思岁月赊。",
    "黄云同入塞，",
    "白首独还家。",
    "宛马随秦草，",
    "胡人问汉花。",
    "还伤李都尉，",
    "独自没黄沙。"
  ],
  "ptype": null,
  "poetName": "郑鏦",
  "poetId": "11d1c02f55b84c30a4a68be0cf914167",
  "form": "五律",
