  "_id": "1ba4574ae26f4dd0a70da346005abc9d",
  "name": "吊造微上人",
  "contents": [
    "至人随化往，",
    "遗路自堪伤。",
    "白塔收真骨，",
    "青山闭影堂。",
    "钟残含细韵，",
    "烟灭有余香。",
    "松上斋乌在，",
    "迟迟立夕阳。"
  ],
  "ptype": null,
  "poetName": "张乔",
  "poetId": "746ea2e1efcd41db9a5d67eddf753f46",
