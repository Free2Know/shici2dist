{
  "_id": "b1c79870cafc4e20a8d26bcebafc13d4",
  "name": "郡斋平望江山",
  "contents": [
    "水路东连楚，",
    "人烟北接巴。",
    "山光围一郡，",
    "江月照千家。",
    "庭树纯栽橘，",
    "园畦半种茶。",
    "梦魂知忆处，",
    "无夜不京华。"
  ],
  "ptype": null,
  "poetName": "岑参",
  "poetId": "77a58f32ae514e76913c578aae0230d3",
  "form": "五律",
