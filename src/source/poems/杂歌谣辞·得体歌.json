{
  "_id": "c1bb6325b80f40aaa256c012380d4fcf",
  "name": "杂歌谣辞·得体歌",
  "contents": [
    "得体纥那也，",
    "纥囊得体那。",
    "潭里船车闹，",
    "扬州铜器多。",
    "三郎当殿坐，",
    "听唱得体歌。"
  ],
  "ptype": null,
  "poetName": "唐无名氏",
