{
  "_id": "aaec496b31c643aabb87a164dadbbd18",
  "name": "车",
  "contents": [
    "天子驭金根，",
    "蒲轮辟四门。",
    "五神趋雪至，",
    "双毂似雷奔。",
    "丹凤栖金辖，",
    "非熊载宝轩。",
    "无阶忝虚左，",
    "珠乘奉王言。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "5f3c01f5ddab4ebdb2d9748023f40391",
  "form": "五律",
