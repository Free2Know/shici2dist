{
  "_id": "0edbfd5b43014f86acdea2c42feef6d7",
  "name": "即席",
  "contents": [
    "家住丛台旧，",
    "名参绛圃新。",
    "醉波疑夺烛，",
    "娇态欲沈春。",
    "伴雨聊过楚，",
    "归云定占秦。",
    "桃花正浓暖，",
    "争不浪迷人。"
  ],
  "ptype": null,
  "poetName": "吴融",
  "poetId": "c8fcad51e3264dc8a7c8162344d3b0e9",
  "form": "五律",
