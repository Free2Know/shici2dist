{
  "_id": "d4386a7d5b954c94a8f2fdcda1cf0beb",
  "name": "卜算子·黄州定惠院寓居作",
  "contents": [
    "缺月挂疏桐，",
    "漏断人初静。",
    "谁见幽人独往来，",
    "缥缈孤鸿影。",
    "惊起却回头，",
    "有恨无人省。",
    "拣尽寒枝不肯栖，",
    "寂寞沙洲冷。"
  ],
  "ptype": null,
  "poetName": "苏轼",
  "poetId": "a2dfeee3453b44a986f42154d40f53a5",
