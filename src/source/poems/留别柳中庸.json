  "_id": "cf9e670a7e444f24877aa41c385f3528",
  "name": "留别柳中庸",
  "contents": [
    "惆怅流水时，",
    "萧条背城路。",
    "离人出古亭，",
    "嘶马入寒树。",
    "江海正风波，",
    "相逢在何处。"
  ],
  "ptype": null,
  "poetName": "李端",
