  "_id": "f6f6b6cb595c4e59adb52410d1ae64b4",
  "name": "题金山",
  "contents": [
    "灵山一峰秀，",
    "岌然殊众山。",
    "盘根大江底，",
    "插影浮云间。",
    "雷霆常间作，",
    "风雨时往还。",
    "象外悬清影，",
    "千载长跻攀。"
  ],
  "ptype": null,
  "poetName": "韩垂",
  "poetId": "0224663ff117408c9a8b39430c21eef3",
