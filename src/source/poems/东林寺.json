{
  "_id": "03b94f29cbcc4931a21c797cbc407ecb",
  "name": "东林寺",
  "contents": [
    "欲去不忍去，",
    "徘徊吟绕廊。",
    "水光秋澹荡，",
    "僧好语寻常。",
    "碑古苔文叠，",
    "山晴钟韵长。",
    "翻思南岳上，",
    "欠此白莲香。"
  ],
  "ptype": null,
  "poetName": "修睦",
  "poetId": "8622d7b6d4a44b4899b4814c97950042",
