{
  "_id": "42e4adb6586d4002b597da7d33ff7fe9",
  "name": "洛妃怨·昭君怨",
  "contents": [
    "拾翠当年延伫。",
    "解佩感君诚素。",
    "微步过南冈。",
    "献明珰。",
    "襟上泪难再会。",
    "惆怅幽兰心事。",
    "心事永难忘。",
    "寄君王。"
  ],
  "ptype": null,
  "poetName": "朱敦儒",
  "poetId": "a9627566399d4a579de5b14f464c69f5",
  "form": "五律",
