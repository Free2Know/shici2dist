{
  "_id": "aa19927a88a24476b4e241ce01969cda",
  "name": "伤心行",
  "contents": [
    "咽咽学楚吟，",
    "病骨伤幽素。",
    "秋姿白发生，",
    "木叶啼风雨。",
    "灯青兰膏歇，",
    "落照飞蛾舞。",
    "古壁生凝尘，",
    "羁魂梦中语。"
  ],
  "ptype": null,
  "poetName": "李贺",
  "poetId": "a9fc5f44a7474857a8c6fe39ffbad132",
  "form": "五律",
