{
  "_id": "9e924aa239e74145a8b34d538f740ae3",
  "name": "姑孰溪",
  "contents": [
    "爱此溪水闲。",
    "乘流兴无极。",
    "漾楫怕鸥惊。",
    "垂竿待鱼食。",
    "波翻晓霞影。",
    "岸叠春山色。",
    "何处浣纱人。",
    "红颜未相识。"
  ],
  "ptype": null,
  "poetName": "李白",
  "poetId": "1b59d554dd594c8497d9cceefaf73d61",
