{
  "_id": "b5506f895b40427abbba7f43bd000e2e",
  "name": "北庭作",
  "contents": [
    "雁塞通盐泽，",
    "龙堆接醋沟。",
    "孤城天北畔，",
    "绝域海西头。",
    "秋雪春仍下，",
    "朝风夜不休。",
    "可知年四十，",
    "犹自未封侯。"
  ],
  "ptype": null,
  "poetName": "岑参",
  "poetId": "77a58f32ae514e76913c578aae0230d3",
  "form": "五律",
