{
  "_id": "97d34bb5c7d6424394b3c38087e78f5a",
  "name": "送谢石先辈归宣州",
  "contents": [
    "名随春色远，",
    "湖外已先知。",
    "花尽方辞醉，",
    "莺残是放时。",
    "天寒千尺岳，",
    "颔白半联诗。",
    "笋蕨犹堪采，",
    "荣归及养期。"
  ],
  "ptype": null,
  "poetName": "林宽",
  "poetId": "1f28658b77bc42b6bbb13991f69311a1",
  "form": "五律",
