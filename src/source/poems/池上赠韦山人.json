  "_id": "94e7370fac484fc58decbac9bffb56a9",
  "name": "池上赠韦山人",
  "contents": [
    "新竹夹平流，",
    "新荷拂小舟。",
    "众皆嫌好拙，",
    "谁肯伴闲游？",
    "客为忙多去，",
    "僧因饭暂留。",
    "独怜韦处士，",
    "尽日共悠悠。"
  ],
  "ptype": null,
  "poetName": "白居易",
  "poetId": "61c661a769a04e409e99836b8f4a8f79",
