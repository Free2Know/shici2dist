  "_id": "33618ce7a3c1487b8ea44743511c26d4",
  "name": "病僧",
  "contents": [
    "三年病不出，",
    "苔藓满藤鞋。",
    "倚壁看经坐，",
    "闻钟吃药斋。",
    "茶烟熏杀竹，",
    "檐雨滴穿阶。",
    "无暇频相访，",
    "秋风寂寞怀。"
  ],
  "ptype": null,
  "poetName": "姚合",
  "poetId": "9a0ddeb7f13a437d8090ccf3996de1f3",
