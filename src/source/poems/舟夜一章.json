  "_id": "572d226620964e7ea71d4759c06ca9e2",
  "name": "舟夜一章",
  "contents": [
    "水色连天色，",
    "风声益浪声。",
    "旅人归思苦，",
    "渔叟梦魂惊。",
    "举棹云先到，",
    "移舟月逐行。",
    "旋吟诗句罢，",
    "犹见远山横。"
  ],
  "ptype": null,
  "poetName": "海印",
  "poetId": "01f816ccada84e04a548fb2158e36f0a",
