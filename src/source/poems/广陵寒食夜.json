  "_id": "5e95864c3e424090998bf55ac9d4c001",
  "name": "广陵寒食夜",
  "contents": [
    "广陵寒食夜，",
    "豪贵足佳期。",
    "紫陌人归后，",
    "红楼月上时。",
    "绮罗香未歇，",
    "丝竹韵犹迟。",
    "明日踏青兴，",
    "输他轻薄儿。"
  ],
  "ptype": null,
  "poetName": "李中",
  "poetId": "5b8a00819d324861a0a7ee4ab6bd3e48",
