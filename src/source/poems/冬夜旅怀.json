  "_id": "b03a8883b4594d02bd116686067f4805",
  "name": "冬夜旅怀",
  "contents": [
    "寒窗危竹枕，",
    "月过半床阴。",
    "嫩叶不归梦，",
    "晴虫成苦吟。",
    "酒无通夜力，",
    "事满五更心。",
    "寂寞谁相似，",
    "残灯与素琴。"
  ],
  "ptype": null,
  "poetName": "刘威",
  "poetId": "4927ea064d7641a482e34f79af93553d",
