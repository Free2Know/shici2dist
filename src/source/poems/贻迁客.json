  "_id": "3de21fb41cd544fea0df96abd1502fd9",
  "name": "贻迁客",
  "contents": [
    "无机还得罪，",
    "直道不伤情。",
    "微雨昏山色，",
    "疏笼闭鹤声。",
    "闲居多野客，",
    "高枕见江城。",
    "门外长溪水，",
    "怜君又濯缨。"
  ],
  "ptype": null,
  "poetName": "杜牧",
  "poetId": "372769721d354f479c45de7931dd507c",
