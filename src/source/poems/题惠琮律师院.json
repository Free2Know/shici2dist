{
  "_id": "a300b695fc2b4333b82d7a6b8584329c",
  "name": "题惠琮律师院",
  "contents": [
    "苦节兼青目，",
    "公卿话有馀。",
    "唯传黄叶喻，",
    "还似白泉居。",
    "猿拨孤云破，",
    "钟撞众木疏。",
    "社坛踪迹在，",
    "重结复何如。"
  ],
  "ptype": null,
  "poetName": "贯休",
  "poetId": "35ef826ebca24b318ada7e612c75eda6",
