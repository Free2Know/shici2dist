  "_id": "f43655c961fc4685b1058037d2271028",
  "name": "宿淮阴水馆",
  "contents": [
    "积水自成阴，",
    "昏昏月映林。",
    "五更离浦棹，",
    "一夜隔淮砧。",
    "漂母乡非远，",
    "王孙道岂沉。",
    "不当无健妪，",
    "谁肯效前心。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "3a86db0ed7e4461794e5c0aa5ffb339c",
