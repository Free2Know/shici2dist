{
  "_id": "84c388477b904beaa69a2c1ab21c583c",
  "name": "寄台省知己",
  "contents": [
    "久怀声籍甚，",
    "千里致双鱼。",
    "宦路终推毂，",
    "亲帏且著书。",
    "才名贾太傅，",
    "文学马相如。",
    "辙迹东巡海，",
    "何时适我闾？"
  ],
  "ptype": null,
  "poetName": "唐彦谦",
  "poetId": "bed3d2dc8e014c25950dab9d7fad3ef4",
  "form": "五律",
