  "_id": "df72203b91014ad68522107868c2ca77",
  "name": "江乡故人偶集客舍",
  "contents": [
    "天秋月又满，",
    "城阙夜千重。",
    "还作江南会，",
    "翻疑梦里逢。",
    "风枝惊暗鹊，",
    "露草覆寒蛩。",
    "羁旅长堪醉，",
    "相留畏晓钟。"
  ],
  "ptype": null,
  "poetName": "戴叔伦",
  "poetId": "b045bb7d35dd4370aad75204e2b55843",
