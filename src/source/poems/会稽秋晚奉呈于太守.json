{
  "_id": "0c0ea4d818824e9b868a1ad36ddc5b33",
  "name": "会稽秋晚奉呈于太守",
  "contents": [
    "寂寂讼庭幽，",
    "森森戟户秋。",
    "山光隐危堞，",
    "湖色上高楼。",
    "禹穴探书罢，",
    "天台作赋游。",
    "云浮将越客，",
    "岁晚共淹留。"
  ],
  "ptype": null,
  "poetName": "张继",
  "poetId": "19418e9d21d942d882c2f53ac712f1d4",
  "form": "五律",
