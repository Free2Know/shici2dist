{
  "_id": "95a8339dd4a84b059aec47a0d2bdf9d2",
  "name": "赠吏部孙员外济",
  "contents": [
    "天子爱贤才，",
    "星郎入拜来。",
    "明光朝半下，",
    "建礼直初回。",
    "名带含香发，",
    "文随绮幕开。",
    "披云自有镜，",
    "从此照仙台。"
  ],
  "ptype": null,
  "poetName": "张谔",
  "poetId": "fb4c7a18b4da41f39d092bb8cf4e9ab7",
  "form": "五律",
