  "_id": "b44952fa8920431fad1b4355436d460e",
  "name": "伤辽东战亡",
  "contents": [
    "凿门初奉律，",
    "仗战始临戎。",
    "振鳞方跃浪，",
    "骋翼正凌风。",
    "未展六奇术，",
    "先亏一篑功。",
    "防身岂乏智，",
    "殉命有余忠。"
  ],
  "ptype": null,
  "poetName": "李世民",
  "poetId": "10cc5cc32b3c450d8b73272986bf9569",
