{
  "_id": "5094b0cf05a54a6193e5f7feefe954b7",
  "name": "伤我马词",
  "contents": [
    "生于碛砺善驰走，",
    "万里南来困丘阜。",
    "青菰寒菽非适口，",
    "病闻北风犹举首。",
    "金台已平骨空朽，",
    "投之龙渊从尔友。"
  ],
  "ptype": null,
  "poetName": "刘禹锡",
  "poetId": "f708c5d8cf5d4324bdc3a709ea718fd3",
  "form": "五律",
