  "_id": "f1b91f7e0d9d428b9645db9627449adc",
  "name": "题友人屋",
  "contents": [
    "松底诗人宅，",
    "闲门远岫孤。",
    "数家分小径，",
    "一水截平芜。",
    "竹节偶相对，",
    "鸟名多自呼。",
    "爱君真静者，",
    "欲去又踟蹰。"
  ],
  "ptype": null,
  "poetName": "李昌符",
  "poetId": "ca15f36b5e124414b993931a1677a016",
