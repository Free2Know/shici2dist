{
  "_id": "7b694417e43847e0bcc2e9def9b129c7",
  "name": "客旧馆",
  "contents": [
    "陈迹随人事，",
    "初秋别此亭。",
    "重来梨叶赤，",
    "依旧竹林青。",
    "风幔何时卷，",
    "寒砧昨夜声。",
    "无由出江汉，",
    "愁绪月冥冥。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
