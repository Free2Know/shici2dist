{
  "_id": "707300031cea41a3a6d508a1a525a38e",
  "name": "元日早朝呈故省诸公",
  "contents": [
    "万戟凌霜布，",
    "森森瑞气间。",
    "垂衣当晓日，",
    "上寿对南山。",
    "济济延多士，",
    "跹跹舞百蛮。",
    "小臣无事谏，",
    "空愧伴鸣环。"
  ],
  "ptype": null,
  "poetName": "卢纶",
  "poetId": "10008e7cd9434079b60c4f3e941d4f4f",
  "form": "五律",
