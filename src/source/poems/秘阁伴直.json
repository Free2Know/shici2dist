  "_id": "05aca8a198e94281a751593b5440a3db",
  "name": "秘阁伴直",
  "contents": [
    "秘阁锁书深，",
    "墙南列晚岑。",
    "吏人同野鹿，",
    "庭木似山林。",
    "浅井寒芜入，",
    "回廊叠藓侵。",
    "闲看薛稷鹤，",
    "共起五湖心。"
  ],
  "ptype": null,
  "poetName": "郑谷",
  "poetId": "87cb83f765db4575a2bdcb67a72bc6d7",
