{
  "_id": "95535082cb404f0abee89d29a5e1fae3",
  "name": "送狄参军赴杭州",
  "contents": [
    "新官非次受，",
    "圣主宠前勋。",
    "关雪发车晚，",
    "风涛挂席闻。",
    "海门山叠翠，",
    "湖岸郡藏云。",
    "执简从公后，",
    "髯参岂胜君。"
  ],
  "ptype": null,
  "poetName": "马戴",
  "poetId": "db14ec8dfa7a4a92987b2e2357aaa5b6",
  "form": "五律",
