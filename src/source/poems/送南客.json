  "_id": "2bf88a66cff04f00873e45b5a71da124",
  "name": "送南客",
  "contents": [
    "行路雨修修，",
    "青山尽海头。",
    "天涯人去远，",
    "岭北水空流。",
    "夜市连铜柱，",
    "巢居属象州。",
    "来时旧相识，",
    "谁向日南游。"
  ],
  "ptype": null,
  "poetName": "张籍",
  "poetId": "0810aa6ff9a34a96aba9c12caed115ae",
