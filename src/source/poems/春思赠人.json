{
  "_id": "4a54b1b3e9b74a879ee11a20ec9e95ab",
  "name": "春思赠人",
  "contents": [
    "红粉当三五，",
    "青娥艳一双。",
    "绮罗回锦陌，",
    "弦管入花江。",
    "落雁惊金弹，",
    "抛杯泻玉缸。",
    "谁知褐衣客，",
    "憔悴在书窗。"
  ],
  "ptype": null,
  "poetName": "柳中庸",
  "poetId": "c150155a53d3456c8f1f8085991108d3",
  "form": "五律",
