{
  "_id": "9dd4829ed5684b7bb8fb06626a2be9b3",
  "name": "洛",
  "contents": [
    "九洛韶光媚，",
    "三川物候新。",
    "花明丹凤浦，",
    "日映玉鸡津。",
    "元礼期仙客，",
    "陈王睹丽人。",
    "神龟方锡瑞，",
    "绿字重来臻。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "5f3c01f5ddab4ebdb2d9748023f40391",
