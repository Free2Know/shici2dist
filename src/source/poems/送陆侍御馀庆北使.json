  "_id": "b8353fb69c314141aec34d7d123d2164",
  "name": "送陆侍御馀庆北使",
  "contents": [
    "古人贵将命，",
    "之子出輶轩。",
    "受委当不辱，",
    "随时敢赠言。",
    "朔途际辽海，",
    "春思绕轘辕。",
    "安得回白日，",
    "留欢尽绿樽。"
  ],
  "ptype": null,
  "poetName": "沈佺期",
  "poetId": "07e42c6b04e64ead9dddc08f9967e4a3",
