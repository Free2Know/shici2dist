{
  "_id": "5463082676b045ca938dd94cf97016fb",
  "name": "过崔驸马山池",
  "contents": [
    "画楼吹笛妓，",
    "金碗酒家胡。",
    "锦石称贞女，",
    "青松学大夫。",
    "脱貂贳桂醑，",
    "射雁与山厨。",
    "闻道高阳会，",
    "愚公谷正愚。"
  ],
  "ptype": null,
  "poetName": "王维",
  "poetId": "37d898722e054cb28508509ddcd22139",
