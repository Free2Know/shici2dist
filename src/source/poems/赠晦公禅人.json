{
  "_id": "01fa2f7413ae45d3959957b508881eb0",
  "name": "赠晦公禅人",
  "contents": [
    "流阳为役者，",
    "相访叶纷纷。",
    "有句虽如我，",
    "无心未似君。",
    "构林青及竹，",
    "茆屋暖于云。",
    "何日相将去，",
    "千山麋鹿群。"
  ],
  "ptype": null,
  "poetName": "贯休",
  "poetId": "35ef826ebca24b318ada7e612c75eda6",
