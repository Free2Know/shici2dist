  "_id": "a85753586fbd48b7b71347484e327a65",
  "name": "周平西墓",
  "contents": [
    "英威今寂寞，",
    "陈迹对崇丘。",
    "壮志清风在，",
    "荒坟白日愁。",
    "穷泉那复晓，",
    "乔木不知秋。",
    "岁岁寒塘侧，",
    "无人水自流。"
  ],
  "ptype": null,
  "poetName": "权德舆",
  "poetId": "6f790147bfc643018268252c6189e5eb",
