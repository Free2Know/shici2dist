{
  "_id": "b8a28efee1b14546988d69f1ac4c0573",
  "name": "郑国夫人挽歌词",
  "contents": [
    "淑德延公胄，",
    "宜家接帝姻。",
    "桂宫男掌仆，",
    "兰殿女升嫔。",
    "恩泽昭前命，",
    "盈虚变此辰。",
    "万年今已矣，",
    "彤管列何人。"
  ],
  "ptype": null,
  "poetName": "赵良器",
  "poetId": "a97fa5ab7a004973b2e1471dd0020eac",
  "form": "五律",
