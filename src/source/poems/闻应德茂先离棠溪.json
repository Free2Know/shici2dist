{
  "_id": "eaf37b3ce5544413b5a28d6cc1afb913",
  "name": "闻应德茂先离棠溪",
  "contents": [
    "落日芦花雨，",
    "行人谷树村。",
    "青山时问路，",
    "红叶自知门。",
    "苜蓿穷诗味，",
    "芭蕉醉墨痕。",
    "端知弃城市，",
    "经席许频温。"
  ],
  "ptype": null,
  "poetName": "唐彦谦",
  "poetId": "bed3d2dc8e014c25950dab9d7fad3ef4",
  "form": "五律",
