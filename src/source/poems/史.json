{
  "_id": "1e1ab23f7d6442e79b7a5397a8ca65f8",
  "name": "史",
  "contents": [
    "马记天官设，",
    "班图地里新。",
    "善谈方亹亹，",
    "青简见彬彬。",
    "方朔初闻汉，",
    "荆轲昔向秦。",
    "正辞堪载笔，",
    "终冀作良臣。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "5f3c01f5ddab4ebdb2d9748023f40391",
