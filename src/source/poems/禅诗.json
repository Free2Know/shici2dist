{
  "_id": "3126f65cde3f47dea8fbbc73896f64b7",
  "name": "禅诗",
  "contents": [
    "万法出无门，",
    "纷纷使智昏。",
    "徒称谁氏子，",
    "独立天地元。",
    "实际且何有，",
    "物先安可存。",
    "须知不动念，",
    "照出万重源。"
  ],
  "ptype": null,
  "poetName": "皎然",
  "poetId": "a5afe9c958b94f8fb0cef564a238e23e",
