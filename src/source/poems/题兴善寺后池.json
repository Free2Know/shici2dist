{
  "_id": "1f1a864a55f543c48dcc4edd0ca7a57b",
  "name": "题兴善寺后池",
  "contents": [
    "隔窗栖白鹤，",
    "似与镜湖邻。",
    "月照何年树，",
    "花逢几遍人。",
    "岸莎青有路，",
    "苔径绿无尘。",
    "永愿容依止，",
    "僧中老此身。"
  ],
  "ptype": null,
  "poetName": "卢纶",
  "poetId": "10008e7cd9434079b60c4f3e941d4f4f",
  "form": "五律",
