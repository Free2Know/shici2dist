{
  "_id": "c45de155b62a4cf0bdb588d7e31f2cea",
  "name": "瓜",
  "contents": [
    "欲识东陵味，",
    "青门五色瓜。",
    "龙蹄远珠履，",
    "女臂动金花。",
    "六子方呈瑞，",
    "三仙实可嘉。",
    "终朝奉絺绤，",
    "谒帝伫非赊。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "5f3c01f5ddab4ebdb2d9748023f40391",
  "form": "五律",
