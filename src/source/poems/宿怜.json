{
  "_id": "dc2b7800d3db48e0a3f7de7e1a3e6ed9",
  "name": "宿怜",
  "contents": [
    "重公旧相识，",
    "一夕话劳生。",
    "药裹关身病，",
    "经函寄道情。",
    "岳寒当寺色，",
    "滩夜入楼声。",
    "不待移文诮，",
    "三年别赤城。"
  ],
  "ptype": null,
  "poetName": "李郢",
  "poetId": "f97df69e1d814cd5866fdbdac2fbac14",
