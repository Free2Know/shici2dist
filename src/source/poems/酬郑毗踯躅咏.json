{
  "_id": "6d2fc06d4a6f4fc18e586719a5273eab",
  "name": "酬郑毗踯躅咏",
  "contents": [
    "不似人手致，",
    "岂关地势偏。",
    "孤光袅馀翠，",
    "独影舞多妍。",
    "迸火烧闲地，",
    "红星堕青天。",
    "忽惊物表物，",
    "嘉客为留连。"
  ],
  "ptype": null,
  "poetName": "孟郊",
  "poetId": "03c94ff99104486fbd1c223eade4ad35",
  "form": "五律",
