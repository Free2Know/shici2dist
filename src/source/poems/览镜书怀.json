  "_id": "200c0f1f4da34fb1abf3a92204ede838",
  "name": "览镜书怀",
  "contents": [
    "得道无古今，",
    "失道还衰老。",
    "自笑镜中人，",
    "白发如霜草。",
    "扪心空叹息，",
    "问影何枯槁。",
    "桃李竟何言，",
    "终成南山皓。"
  ],
  "ptype": null,
  "poetName": "李白",
  "poetId": "1b59d554dd594c8497d9cceefaf73d61",
