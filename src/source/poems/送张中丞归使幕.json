{
  "_id": "70a747e8ce6e4f4cbcfa4021e7e8c0cb",
  "name": "送张中丞归使幕",
  "contents": [
    "独受主恩归，",
    "当朝似者稀。",
    "玉壶分御酒，",
    "金殿赐春衣。",
    "拂席流莺醉，",
    "鸣鞭骏马肥。",
    "满台簪白笔，",
    "捧手恋清辉。"
  ],
  "ptype": null,
  "poetName": "张继",
  "poetId": "19418e9d21d942d882c2f53ac712f1d4",
  "form": "五律",
