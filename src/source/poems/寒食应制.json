{
  "_id": "28a868d9ec7f42f49aa3b614e31f5e34",
  "name": "寒食应制",
  "contents": [
    "凤城春色晚，",
    "龙禁早晖通。",
    "旧火收槐燧，",
    "馀寒入桂宫。",
    "莺啼正隐叶，",
    "鸡斗始开笼。",
    "蔼蔼瑶山满，",
    "仙歌始乐风。"
  ],
  "ptype": null,
  "poetName": "韦承庆",
  "poetId": "9eac0531c0364b6bbdb169a29826f994",
  "form": "五律",
