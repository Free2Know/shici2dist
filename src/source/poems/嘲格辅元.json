{
  "_id": "e65baf98aaf24d248eefec8408428759",
  "name": "嘲格辅元",
  "contents": [
    "有耻宿龙门，",
    "精彩先瞰浑。",
    "眼瘦呈近店，",
    "睡响彻遥林。",
    "埒囊将旧识，",
    "制被异新婚。",
    "谁言骢马使，",
    "翻作蛰熊蹲。"
  ],
  "ptype": null,
  "poetName": "杜易简",
  "poetId": "c48a4fb3c5ef4b518f719ccc69f465c6",
