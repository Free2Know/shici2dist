{
  "_id": "5488ebe3980643278f235a70fb5455b4",
  "name": "憨郭郎",
  "contents": [
    "休要强贪名利，",
    "休要恋妻男。",
    "免轮回，",
    "生死苦，",
    "做痴憨。",
    "清净自然明道，",
    "神气自相参。",
    "功成朝玉帝，",
    "跨云骖。"
  ],
  "ptype": null,
  "poetName": "马钰",
  "poetId": "6f79865ac305486ba0011c7eff4b976f",
