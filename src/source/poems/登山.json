{
  "_id": "8e20a5f6875c4c9abeff47349c1c1f15",
  "name": "登山",
  "contents": [
    "信步上鸟道，",
    "不知身忽高。",
    "近空无世界，",
    "当楚见波涛。",
    "顶峭松多瘦，",
    "崖悬石尽牢。",
    "猕猴呼独散，",
    "隔水向人号。"
  ],
  "ptype": null,
  "poetName": "许棠",
  "poetId": "79e0a1e55d8d45d6a3aa421b0ade4bd3",
  "form": "五律",
