  "_id": "83d6e089d96649e6a8868c63fadbc245",
  "name": "寻桃源",
  "contents": [
    "武林春草齐，",
    "花影隔澄溪。",
    "路远无人去，",
    "山空有鸟啼。",
    "水垂青霭断，",
    "松偃绿萝低。",
    "世上迷途客，",
    "经兹尽不迷。"
  ],
  "ptype": null,
  "poetName": "张乔",
  "poetId": "746ea2e1efcd41db9a5d67eddf753f46",
  "form": "五律",
