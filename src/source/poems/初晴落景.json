  "_id": "df9103866ced41eba4f448f803223eae",
  "name": "初晴落景",
  "contents": [
    "晚霞聊自怡，",
    "初晴弥可喜。",
    "日晃百花色，",
    "风动千林翠。",
    "池鱼跃不同，",
    "园鸟声还异。",
    "寄言博通者，",
    "知予物外志。"
  ],
  "ptype": null,
  "poetName": "李世民",
