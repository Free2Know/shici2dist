  "_id": "fa9e45ce4739468b9422997020a08087",
  "name": "赠梁将军",
  "contents": [
    "曾经黑山虏，",
    "一剑出重围。",
    "年长穷书意，",
    "时清隐钓矶。",
    "高斋云外住，",
    "瘦马月中归。",
    "唯说乡心苦，",
    "春风雁北飞。"
  ],
  "ptype": null,
  "poetName": "许浑",
  "poetId": "ba84f30a9b344330943f2fbe4d128d8d",
