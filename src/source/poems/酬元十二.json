  "_id": "2e3a61354b2641c980bbfff766560d8c",
  "name": "酬元十二",
  "contents": [
    "偶寻乌府客，",
    "同醉习家池。",
    "积雪初迷径，",
    "孤云遂失期。",
    "风前劳引领，",
    "月下重相思。",
    "何必因尊酒，",
    "幽心两自知。"
  ],
  "ptype": null,
  "poetName": "武元衡",
