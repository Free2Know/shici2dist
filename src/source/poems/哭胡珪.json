  "_id": "2296d6d3c5c743cea758a9e1c9daf12a",
  "name": "哭胡珪",
  "contents": [
    "才高登上第，",
    "孝极殁庐茔。",
    "一命何无定，",
    "片言徒有声。",
    "故园花自发，",
    "新冢月初明。",
    "寂寞重泉里，",
    "岂知春物荣。"
  ],
  "ptype": null,
  "poetName": "方干",
  "poetId": "fb2c8db4ee504545b4b8cf96199091b0",
