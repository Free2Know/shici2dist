{
  "_id": "3ae268565b3b480a8e44c3f37f5aaba9",
  "name": "题寿安甘棠馆二首",
  "contents": [
    "公馆似仙家，",
    "池清竹径斜。",
    "山禽忽惊起，",
    "冲落半岩花。",
    "门前洛阳道，",
    "门里桃花路。",
    "尘土与烟霞，",
    "其间十馀步。"
  ],
  "ptype": null,
  "poetName": "刘禹锡",
  "poetId": "f708c5d8cf5d4324bdc3a709ea718fd3",
  "form": "五律",
