  "_id": "0cd9bd78c06a4e539e42c5636bb726af",
  "name": "度秋",
  "contents": [
    "夏律昨留灰，",
    "秋箭今移晷。",
    "峨嵋岫初出，",
    "洞庭波渐起。",
    "桂白发幽岩，",
    "菊黄开灞涘。",
    "运流方可叹，",
    "含毫属微理。"
  ],
  "ptype": null,
  "poetName": "李世民",
  "poetId": "10cc5cc32b3c450d8b73272986bf9569",
