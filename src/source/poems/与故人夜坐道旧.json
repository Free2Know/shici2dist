  "_id": "6737210172664dbca8f8956d661ab54b",
  "name": "与故人夜坐道旧",
  "contents": [
    "笑语欢今夕，",
    "烟霞怆昔游。",
    "清羸还对月，",
    "迟暮更逢秋。",
    "胜理方自得，",
    "浮名不在求。",
    "终当制初服，",
    "相与卧林丘。"
  ],
  "ptype": null,
  "poetName": "权德舆",
