{
  "_id": "037b945d56e54151b5c6d4801c62483c",
  "name": "秋寒",
  "contents": [
    "雪鬓年颜老，",
    "霜庭景气秋。",
    "病看妻捡药，",
    "寒遣婢梳头。",
    "身外名何有，",
    "人间事且休。",
    "澹然方寸内，",
    "唯拟学虚舟。"
  ],
  "ptype": null,
  "poetName": "白居易",
  "poetId": "61c661a769a04e409e99836b8f4a8f79",
