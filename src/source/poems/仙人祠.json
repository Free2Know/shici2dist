  "_id": "18e8944276da4133ad88c187b4e97656",
  "name": "仙人祠",
  "contents": [
    "苍岑古仙子，",
    "清庙閟华容。",
    "千载去寥廓，",
    "白云遗旧踪。",
    "归来灞陵上，",
    "犹见最高峰。"
  ],
  "ptype": null,
  "poetName": "韦应物",
