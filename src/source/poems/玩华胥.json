{
  "_id": "adb5a124b2b44e57a2bbaea6b5614cb3",
  "name": "玩华胥",
  "contents": [
    "古岸蟠桃初绽。",
    "新月素光交灿。",
    "尘世几人窥，",
    "我留心高盼。",
    "长叹。",
    "长叹。",
    "回首远游仙梵。"
  ],
  "ptype": null,
  "poetName": "侯善渊",
