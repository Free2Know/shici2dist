{
  "_id": "6177f4f0d3a44da6a0c41c9170677178",
  "name": "归田",
  "contents": [
    "陶君三十七，",
    "挂绶出都门。",
    "我亦今年去，",
    "商山淅岸村。",
    "冬修方丈室，",
    "春种桔槔园。",
    "千万人间事，",
    "从兹不复言。"
  ],
  "ptype": null,
  "poetName": "元稹",
  "poetId": "e04ddf3e16fd40a3bc351fd163098dda",
