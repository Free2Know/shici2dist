{
  "_id": "4eaaeaa7d2ad419093b83a930fbee3c9",
  "name": "题豪家故池",
  "contents": [
    "岁久无泉引，",
    "春来仰雨流。",
    "萍枯黏朽槛，",
    "沙浅露沉舟。",
    "照影人何在，",
    "持竿客寄游。",
    "翛然兴废外，",
    "回首谢眠鸥。"
  ],
  "ptype": null,
  "poetName": "吴融",
  "poetId": "c8fcad51e3264dc8a7c8162344d3b0e9",
  "form": "五律",
