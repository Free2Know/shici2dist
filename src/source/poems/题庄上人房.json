  "_id": "147d483bed5a4901ba6032e7fb228396",
  "name": "题庄上人房",
  "contents": [
    "不语焚香坐，",
    "心知道已成。",
    "流年衰此世，",
    "定力见他生。",
    "暮雪馀春冷，",
    "寒灯续昼明。",
    "寻常五侯至，",
    "敢望下阶迎。"
  ],
  "ptype": null,
  "poetName": "耿湋",
  "poetId": "d62460239e1244dcad61df818057bd89",
