{
  "_id": "ef8227cc7b7a47e3adcfff611a792e3a",
  "name": "送深法师游南岳",
  "contents": [
    "师在白云乡，",
    "名登善法堂。",
    "十方传句偈，",
    "八部会坛场。",
    "飞锡无定所，",
    "宝书留旧房。",
    "唯应衔果雁，",
    "相送至衡阳。"
  ],
  "ptype": null,
  "poetName": "刘禹锡",
  "poetId": "f708c5d8cf5d4324bdc3a709ea718fd3",
