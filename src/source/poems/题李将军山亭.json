  "_id": "c70c4ffc92d240c4a2b4fa7c3a4b1811",
  "name": "题李将军山亭",
  "contents": [
    "凤辖将军位，",
    "龙门司隶家。",
    "衣冠为隐逸，",
    "山水作繁华。",
    "径出重林草，",
    "池摇两岸花。",
    "谁知贵公第，",
    "亭院有烟霞。"
  ],
  "ptype": null,
  "poetName": "郭良",
  "poetId": "3da3930fad3e49509aef3dc3f54aaab6",
  "form": "五律",
