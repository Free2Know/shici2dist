  "_id": "f3478839d8244e70afa3290938588606",
  "name": "馀干旅舍",
  "contents": [
    "摇落暮天迥，",
    "青枫霜叶稀。",
    "孤城向水闭，",
    "独鸟背人飞。",
    "渡口月初上，",
    "邻家渔未归。",
    "乡心正欲绝，",
    "何处捣寒衣。"
  ],
  "ptype": null,
  "poetName": "刘长卿",
  "poetId": "8fb91cedc6274daba50e589c0dde93b7",
