{
  "_id": "aa393272c3024a6db964a4d1da3c5901",
  "name": "哭陈庾",
  "contents": [
    "系马向山立，",
    "一杯聊奠君。",
    "野烟孤客路，",
    "寒草故人坟。",
    "琴韵归流水，",
    "诗情寄白云。",
    "日斜休哭后，",
    "松韵不堪闻。"
  ],
  "ptype": null,
  "poetName": "周朴",
  "poetId": "6235ef5ef90046f5b18becd7b6b1dae8",
