{
  "_id": "53c3d9ca9d7143d5926d1c1b79bfe4aa",
  "name": "送田道士使蜀投龙",
  "contents": [
    "风驭忽泠然，",
    "云台路几千。",
    "蜀门峰势断，",
    "巴字水形连。",
    "人隔壶中地，",
    "龙游洞里天。",
    "赠言回驭日，",
    "图画彼山川。"
  ],
  "ptype": null,
  "poetName": "宋之问",
  "poetId": "7c291600d311418b82540c7aad3f8f70",
  "form": "五律",
