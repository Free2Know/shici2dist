  "_id": "7e87511fad8d4928af7d2f01cef7ba22",
  "name": "庭柳",
  "contents": [
    "袅袅柳杨枝，",
    "当轩杂珮垂。",
    "交阴总共密，",
    "分条各自宜。",
    "因依似永久，",
    "揽结更伤离。",
    "爱此阳春色，",
    "秋风莫遽吹。"
  ],
  "ptype": null,
  "poetName": "姚系",
  "poetId": "cd1305caf08a45db83a2a300ca1f9f40",
