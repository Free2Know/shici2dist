  "_id": "bbc119e973c34594915e8585de90c625",
  "name": "五郊乐章·雍和",
  "contents": [
    "金悬夕肆，",
    "玉俎朝陈。",
    "飨荐黄道，",
    "芬流紫辰。",
    "乃诚乃敬，",
    "载享载諲。",
    "崇荐斯在，",
    "惟皇是宾。"
  ],
  "ptype": null,
  "poetName": "魏征",
