  "_id": "9240b2e3d83341358297482867798517",
  "name": "安邑王校书居",
  "contents": [
    "秋来池馆清，",
    "夜闻宫漏声。",
    "迢递玉山迥，",
    "泛滟银河倾。",
    "琴上松风至，",
    "窗里竹烟生。",
    "多君不家食，",
    "孰云事岩耕。"
  ],
  "ptype": null,
  "poetName": "耿湋",
  "poetId": "d62460239e1244dcad61df818057bd89",
