{
  "_id": "f7900dd83d9e4d04ae944fc4c83c1846",
  "name": "宿寿安甘棠馆",
  "contents": [
    "行人方倦役，",
    "到此似还乡。",
    "流水来关外，",
    "青山近洛阳。",
    "溪云归洞鹤，",
    "松月半轩霜。",
    "坐恐晨钟动，",
    "天涯道路长。"
  ],
  "ptype": null,
  "poetName": "司马扎",
  "poetId": "193f51c2a3e44c868d3be6cd409b4c18",
  "form": "五律",
