{
  "_id": "1ff6e27ac2064c9c8b839a4d90d613b2",
  "name": "使院中新栽柏树子，呈李十五栖筠",
  "contents": [
    "爱尔青青色，",
    "移根此地来。",
    "不曾台上种，",
    "留向碛中栽。",
    "脆叶欺门柳，",
    "狂花笑院梅。",
    "不须愁岁晚，",
    "霜露岂能摧。"
  ],
  "ptype": null,
  "poetName": "岑参",
  "poetId": "77a58f32ae514e76913c578aae0230d3",
  "form": "五律",
