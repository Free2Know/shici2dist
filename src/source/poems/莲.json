  "_id": "2729e7b584ac45b3b814669fae23868f",
  "name": "莲",
  "contents": [
    "新莲映多浦，",
    "迢递绿塘东。",
    "静影摇波日，",
    "寒香映水风。",
    "金尘飘落蕊，",
    "玉露洗残红。",
    "看著余芳少，",
    "无人问的中。"
  ],
  "ptype": null,
  "poetName": "唐彦谦",
