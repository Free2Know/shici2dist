  "_id": "dfc34bf5319749028538b1f1c2aed2d3",
  "name": "银结条冠子",
  "contents": [
    "日下征良匠，",
    "宫中赠阿娇。",
    "瑞莲开二孕，",
    "琼缕织千条。",
    "蝉翼轻轻结，",
    "花纹细细挑。",
    "舞时红袖举，",
    "纤影透龙绡。"
  ],
  "ptype": null,
  "poetName": "徐夤",
  "poetId": "52ec99675887475ea1a5adf0625ec087",
  "form": "五律",
