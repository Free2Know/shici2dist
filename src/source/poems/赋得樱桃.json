  "_id": "42b17adedc47437fa620e7dcbd4981ea",
  "name": "赋得樱桃",
  "contents": [
    "华林满芳景，",
    "洛阳遍阳春。",
    "朱颜含远日，",
    "翠色影长津。",
    "乔柯啭娇鸟，",
    "低枝映美人。",
    "昔作园中实，",
    "今来席上珍。"
  ],
  "ptype": null,
  "poetName": "李世民",
  "poetId": "10cc5cc32b3c450d8b73272986bf9569",
