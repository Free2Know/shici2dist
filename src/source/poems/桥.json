{
  "_id": "28c925c866ac426699a1e39d44e95920",
  "name": "桥",
  "contents": [
    "乌鹊填应满，",
    "黄公去不归。",
    "势疑虹始见，",
    "形似雁初飞。",
    "妙应七星制，",
    "高分半月辉。",
    "秦王空构石，",
    "仙岛远难依。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "5f3c01f5ddab4ebdb2d9748023f40391",
