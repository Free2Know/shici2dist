{
  "_id": "3c6139c8c98a4f98bf75b294cc90e3d1",
  "name": "送惟良上人",
  "contents": [
    "高斋洒寒水，",
    "是夕山僧至。",
    "玄牝无关锁，",
    "琼书舍文字。",
    "灯明香满室，",
    "月午霜凝地。",
    "语到不言时，",
    "世间人尽睡。"
  ],
  "ptype": null,
  "poetName": "刘禹锡",
  "poetId": "f708c5d8cf5d4324bdc3a709ea718fd3",
