{
  "_id": "ae96b25483de4b14a4e9a62db6f60033",
  "name": "步登夏口古城作",
  "contents": [
    "平芜连古堞，",
    "远客此沾衣。",
    "高树朝光上，",
    "空城秋气归。",
    "微明汉水极，",
    "摇落楚人稀。",
    "但见荒郊外，",
    "寒鸦暮暮飞。"
  ],
  "ptype": null,
  "poetName": "刘长卿",
  "poetId": "8fb91cedc6274daba50e589c0dde93b7",
  "form": "五律",
