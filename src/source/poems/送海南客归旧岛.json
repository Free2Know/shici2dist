  "_id": "38bcf49935eb4812ac33901d85884df6",
  "name": "送海南客归旧岛",
  "contents": [
    "海上去应远，",
    "蛮家云岛孤。",
    "竹船来桂浦，",
    "山市卖鱼须。",
    "入国自献宝，",
    "逢人多赠珠。",
    "却归春洞口，",
    "斩象祭天吴。"
  ],
  "ptype": null,
  "poetName": "张籍",
