  "_id": "82e712a261304875b85d48194df0810a",
  "name": "玉清公主挽歌",
  "contents": [
    "学凤年犹小，",
    "乘龙日尚赊。",
    "初封千户邑，",
    "忽驾五云车。",
    "地接金人岸，",
    "山通玉女家。",
    "秋风何太早，",
    "吹落禁园花。"
  ],
  "ptype": null,
  "poetName": "张谓",
  "poetId": "10dd3923dfc74032888245249533ffc0",
