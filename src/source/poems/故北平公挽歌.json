{
  "_id": "1499841635b842b7a1f287d23ac2d001",
  "name": "故北平公挽歌",
  "contents": [
    "木落园林旷，",
    "庭虚风露寒。",
    "北里清音绝，",
    "南陔芳草残。",
    "远气犹标剑，",
    "浮云尚写冠。",
    "寂寂琴台晚，",
    "秋阴入井干。"
  ],
  "ptype": null,
  "poetName": "上官仪",
  "poetId": "5172f35ac6194b0ea91b59f61618f357",
  "form": "五律",
