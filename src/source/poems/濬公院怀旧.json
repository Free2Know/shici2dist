  "_id": "96c78b459705483ca68d5fe368282b1f",
  "name": "濬公院怀旧",
  "contents": [
    "远公传教毕，",
    "身没向他方。",
    "吊客来何见，",
    "门人闭影堂。",
    "纱灯临古砌，",
    "尘札在空床。",
    "寂寞疏钟后，",
    "秋天有夕阳。"
  ],
  "ptype": null,
  "poetName": "耿湋",
  "poetId": "d62460239e1244dcad61df818057bd89",
