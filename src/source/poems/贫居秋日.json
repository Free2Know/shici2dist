{
  "_id": "dc85eaa5b0724bad97a2451c31f0301d",
  "name": "贫居秋日",
  "contents": [
    "亭午头未冠，",
    "端坐独愁予。",
    "贫家烟爨稀，",
    "灶底阴虫语。",
    "门小愧车马，",
    "廪空惭雀鼠。",
    "尽室未寒衣，",
    "机声羡邻女。"
  ],
  "ptype": null,
  "poetName": "皮日休",
  "poetId": "a55da5ef62d14b4c8b1e70d4fb417653",
