{
  "_id": "dab855dbe86142f28f45c4740c492323",
  "name": "雍和",
  "contents": [
    "夙夜宥密，",
    "不敢宁宴。",
    "五齐既陈，",
    "八音在县。",
    "粢盛以洁，",
    "房俎斯荐。",
    "惟德惟馨，",
    "尚兹克遍。"
  ],
  "ptype": null,
  "poetName": "贺知章",
