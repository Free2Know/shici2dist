  "_id": "1fa5c4fb10914d1480c3789fdbcbc219",
  "name": "寄陆龟蒙",
  "contents": [
    "龙楼李丞相，",
    "昔岁仰高文。",
    "黄阁寻无主，",
    "青山竟未焚。",
    "夜船乘海月，",
    "秋寺伴江云。",
    "却恐尘埃里，",
    "浮名点污君。"
  ],
  "ptype": null,
  "poetName": "罗隐",
