{
  "_id": "0f654f252804469bb8a3f95f08c1ac0a",
  "name": "永嘉别张子容",
  "contents": [
    "旧国余归楚，",
    "新年子北征。",
    "挂帆愁海路，",
    "分手恋朋情。",
    "日夕故园意，",
    "汀洲春草生。",
    "何时一杯酒，",
    "重与季鹰倾。"
  ],
  "ptype": null,
  "poetName": "孟浩然",
  "poetId": "613f112c3eb94ff58f235d9e7cbb5975",
  "form": "五律",
