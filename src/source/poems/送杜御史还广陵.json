{
  "_id": "35a8a4c1d56b4ac7882cccf0d0dddf6d",
  "name": "送杜御史还广陵",
  "contents": [
    "惭君从弱岁，",
    "顾我比诸昆。",
    "同事元戎久，",
    "俱承国士恩。",
    "随莺过淮水，",
    "看柳向辕门。",
    "草色金陵岸，",
    "思心那可论。"
  ],
  "ptype": null,
  "poetName": "李嘉祐",
  "poetId": "c256d35ea8de4cbb869292361f80ed95",
  "form": "五律",
