{
  "_id": "e2814faaafd54fba9fb22c5fc5855a27",
  "name": "酬乐天闲卧见寄",
  "contents": [
    "散诞向阳眠，",
    "将闲敌地仙。",
    "诗情茶助爽，",
    "药力酒能宣。",
    "风碎竹间日，",
    "露明池底天。",
    "同年未同隐，",
    "缘欠买山钱。"
  ],
  "ptype": null,
  "poetName": "刘禹锡",
  "poetId": "f708c5d8cf5d4324bdc3a709ea718fd3",
  "form": "五律",
