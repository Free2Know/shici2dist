{
  "_id": "46e0631243ec499f99a648a314844c38",
  "name": "奉和圣制次琼岳应制",
  "contents": [
    "陆海披晴雪，",
    "千旗猎早阳。",
    "岳临秦路险，",
    "河绕汉垣长。",
    "行漏通鳷鹊，",
    "离宫接建章。",
    "都门信宿近，",
    "歌舞从周王。"
  ],
  "ptype": null,
  "poetName": "韦济",
  "poetId": "8ff0f7a532b3470db5a653e4a734d2e8",
  "form": "五律",
