  "_id": "bbc45163f7eb476da8e400dc5a6cfba4",
  "name": "送道士",
  "contents": [
    "千年山上行，",
    "山上无遗踪。",
    "一日人间游，",
    "六合人皆逢。",
    "自有意中侣，",
    "白寒徒相从。"
  ],
  "ptype": null,
  "poetName": "孟郊",
