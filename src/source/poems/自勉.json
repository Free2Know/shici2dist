  "_id": "b6c5b77ec44d4a76bafaaa7229420a1e",
  "name": "自勉",
  "contents": [
    "试算平生事，",
    "中年欠五年。",
    "知非未落后，",
    "读易尚加前。",
    "分受诗魔役，",
    "宁容俗态牵。",
    "闲吟见秋水，",
    "数只钓鱼船。"
  ],
  "ptype": null,
  "poetName": "齐己",
