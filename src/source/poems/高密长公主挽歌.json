  "_id": "34ed8911408f40b3a71ec451d3dd5165",
  "name": "高密长公主挽歌",
  "contents": [
    "湘渚韬灵迹，",
    "娥台静瑞音。",
    "凤逐清箫远，",
    "鸾随幽镜沉。",
    "霜处华芙落，",
    "风前银烛侵。",
    "寂寞平阳宅，",
    "月冷洞房深。"
  ],
  "ptype": null,
  "poetName": "上官仪",
  "poetId": "5172f35ac6194b0ea91b59f61618f357",
