  "_id": "1cd9da5a58b14803b7347eaf557325fa",
  "name": "后唐宗庙乐舞辞",
  "contents": [
    "艰难王业，",
    "返正皇唐。",
    "先天再造，",
    "却日重光。",
    "汉绍世祖，",
    "夏资少康。",
    "功成德茂，",
    "率祀无疆。"
  ],
  "ptype": null,
  "poetName": "崔居俭",
