  "_id": "302edc9b1ec142cba22739d5e53c125e",
  "name": "成王挽歌",
  "contents": [
    "幽山悲旧桂，",
    "长坂怆馀兰。",
    "地底孤灯冷，",
    "泉中一镜寒。",
    "铭旌门客送，",
    "骑吹路人看。",
    "漫作琉璃碗，",
    "淮王误合丹。"
  ],
  "ptype": null,
  "poetName": "岑参",
  "poetId": "77a58f32ae514e76913c578aae0230d3",
