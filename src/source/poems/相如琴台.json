  "_id": "86e362fbd2da45729571f8a8dfd09c2d",
  "name": "相如琴台",
  "contents": [
    "闻有雍容地，",
    "千年无四邻。",
    "园院风烟古，",
    "池台松槚春。",
    "云疑作赋客，",
    "月似听琴人。",
    "寂寂啼莺处，",
    "空伤游子神。"
  ],
  "ptype": null,
  "poetName": "卢照邻",
  "poetId": "605828d5fc0c456498ec2ddb7fec0cb4",
