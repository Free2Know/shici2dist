  "_id": "4e6a580b17cb47a7967aec3dded69f47",
  "name": "银州北书事",
  "contents": [
    "南辞采石远，",
    "北背乞银深。",
    "碛路虽多险，",
    "江人不废吟。",
    "雕依孤堠立，",
    "鸥向迥沙沈。",
    "因共边人熟，",
    "行行起战心。"
  ],
  "ptype": null,
  "poetName": "许棠",
  "poetId": "79e0a1e55d8d45d6a3aa421b0ade4bd3",
