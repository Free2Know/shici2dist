  "_id": "58e830e8dcbd46e88e8497af5c7b805b",
  "name": "寓居武丁馆",
  "contents": [
    "暑雨飘已过，",
    "凉飙触幽衿。",
    "虚馆无喧尘，",
    "绿槐多昼阴。",
    "俯视古苔积，",
    "仰聆早蝉吟。",
    "放卷一长想，",
    "闭门千里心。"
  ],
  "ptype": null,
  "poetName": "陈存",
  "poetId": "d92e9dcddbe14af5bfff3da17aa3f80a",
