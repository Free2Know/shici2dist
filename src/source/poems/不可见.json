{
  "_id": "3263d78c846840d4aedde1239c0f5caa",
  "name": "不可见",
  "contents": [
    "常闻贫贱夫，",
    "头白终相待。",
    "自从嫁黔娄，",
    "终岁长不在。",
    "君梦有双影，",
    "妾梦空四邻。",
    "常思劲北风，",
    "吹折双车轮。"
  ],
  "ptype": null,
  "poetName": "曹邺",
  "poetId": "7ee93682f15e45668bf2f4303b1835b6",
  "form": "五律",
