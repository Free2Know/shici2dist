  "_id": "b150e00be3234f1f9760449b53be4967",
  "name": "题僧梦微房",
  "contents": [
    "东海日未出，",
    "九衢人已行。",
    "吾师无事坐，",
    "苔藓入门生。",
    "雨过闲花落，",
    "风来古木声。",
    "天台频说法，",
    "石壁欠题名。"
  ],
  "ptype": null,
  "poetName": "修睦",
