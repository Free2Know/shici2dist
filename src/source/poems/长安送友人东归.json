  "_id": "9be0d67dfca043b7857b81483deffc4d",
  "name": "长安送友人东归",
  "contents": [
    "白社思归处，",
    "青门见去人。",
    "乡遥茂苑树，",
    "路入广陵尘。",
    "海日潮浮晓，",
    "湖山雪露春。",
    "犹期来帝里，",
    "未是得闲人。"
  ],
  "ptype": null,
  "poetName": "李频",
  "poetId": "427dc1231d8140679d9bcf34613ef360",
