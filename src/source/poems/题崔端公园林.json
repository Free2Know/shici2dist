{
  "_id": "8fae5e9caf3b4d638cd43fbb582a049b",
  "name": "题崔端公园林",
  "contents": [
    "上士爱清辉，",
    "开门向翠微。",
    "抱琴看鹤去，",
    "枕石待云归。",
    "野坐苔生席，",
    "高眠竹挂衣。",
    "旧山东望远，",
    "惆怅暮花飞。"
  ],
  "ptype": null,
  "poetName": "李端",
  "poetId": "d49a87724e4b436aab7e04e56ad3d93e",
  "form": "五律",
