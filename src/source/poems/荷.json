{
  "_id": "1c8072fc788a46029ea799f4d50a9f01",
  "name": "荷",
  "contents": [
    "新溜满澄陂，",
    "圆荷影若规。",
    "风来香气远，",
    "日落盖阴移。",
    "鱼戏排缃叶，",
    "龟浮见绿池。",
    "魏朝难接采，",
    "楚服但同披。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "5f3c01f5ddab4ebdb2d9748023f40391",
