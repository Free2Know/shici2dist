{
  "_id": "97078469865b4e8e8a8a46e61767b5c7",
  "name": "敕勒歌",
  "contents": [
    "敕勒川，",
    "阴山下，",
    "天似穹庐，",
    "笼盖四野。",
    "天苍苍，",
    "野茫茫，",
    "风吹草低见牛羊。"
  ],
  "ptype": null,
  "poetName": "南北朝无名氏",
  "poetId": "361f68a6e9d946259f774b99a21d8a86",
