  "_id": "7ae6ab93aaef4f75a78dd98a10750aea",
  "name": "送友失意南归",
  "contents": [
    "衣挥京洛尘，",
    "完璞伴归人。",
    "故国青山遍，",
    "沧江白发新。",
    "邻荒收酒幔，",
    "屋古布苔茵。",
    "不用通名姓，",
    "渔樵共主宾。"
  ],
  "ptype": null,
  "poetName": "顾况",
  "poetId": "645ccbd5285149148c88a84eee44d640",
