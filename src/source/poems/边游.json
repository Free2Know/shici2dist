  "_id": "e5b89019034347f0a943a709a37b123b",
  "name": "边游",
  "contents": [
    "古镇门前去，",
    "长安路在东。",
    "天寒明堠火，",
    "日晚裂旗风。",
    "塞馆皆无事，",
    "儒装亦有弓。",
    "防秋故乡卒，",
    "暂喜语音同。"
  ],
  "ptype": null,
  "poetName": "项斯",
