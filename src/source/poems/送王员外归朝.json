  "_id": "0f33978c4eb94d879102c77543f45689",
  "name": "送王员外归朝",
  "contents": [
    "往来无尽目，",
    "离别要逢春。",
    "海内罹多事，",
    "天涯见近臣。",
    "芳时万里客，",
    "乡路独归人。",
    "魏阙心常在，",
    "随君亦向秦。"
  ],
  "ptype": null,
  "poetName": "刘长卿",
  "poetId": "8fb91cedc6274daba50e589c0dde93b7",
