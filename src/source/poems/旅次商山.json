  "_id": "1ed9c50389a74998a5f3dea17cbb6780",
  "name": "旅次商山",
  "contents": [
    "役役依山水，",
    "何曾似问津。",
    "断崖时避马，",
    "芳树欲留人。",
    "日夕猿鸟伴，",
    "古今京洛尘。",
    "一枝甘已失，",
    "辜负故园春。"
  ],
  "ptype": null,
  "poetName": "赵嘏",
  "poetId": "acc20a3400f440d598db9b84c3159cf2",
