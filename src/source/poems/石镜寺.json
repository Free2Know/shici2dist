{
  "_id": "5330c7a979944105950d0c8d4a5cc322",
  "name": "石镜寺",
  "contents": [
    "古墓芙蓉塔，",
    "神铭松柏烟。",
    "鸾沉仙镜底，",
    "花没梵轮前。",
    "铢衣千古佛，",
    "宝月两重圆。",
    "稳隐香台夜，",
    "钟声彻九天。"
  ],
  "ptype": null,
  "poetName": "卢照邻",
  "poetId": "605828d5fc0c456498ec2ddb7fec0cb4",
