{
  "_id": "7a2a855cc1e64e0b807119697827a013",
  "name": "送惟良上人归江南",
  "contents": [
    "落日映危樯，",
    "归僧向岳阳。",
    "注瓶寒浪静，",
    "读律夜船香。",
    "苦雾沈山影，",
    "阴霾发海光。",
    "群生一何负，",
    "多病礼医王。"
  ],
  "ptype": null,
  "poetName": "卢纶",
  "poetId": "10008e7cd9434079b60c4f3e941d4f4f",
  "form": "五律",
