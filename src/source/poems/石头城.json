  "_id": "759af8465e994abe93965b55bdf9c269",
  "name": "石头城",
  "contents": [
    "伯业随流水，",
    "寒芜上古城。",
    "长空横海色，",
    "断岸落潮声。",
    "八极悲扶拄，",
    "五湖来止倾。",
    "东南天子气，",
    "扫地入函京。"
  ],
  "ptype": null,
  "poetName": "李群玉",
  "poetId": "533477cc5b0045399dfdc5f641132458",
