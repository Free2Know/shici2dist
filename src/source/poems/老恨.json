  "_id": "c9e170ffe6304886a84e065d87d45772",
  "name": "老恨",
  "contents": [
    "无子抄文字，",
    "老吟多飘零。",
    "有时吐向床，",
    "枕席不解听。",
    "斗蚁甚微细，",
    "病闻亦清泠。",
    "小大不自识，",
    "自然天性灵。"
  ],
  "ptype": null,
  "poetName": "孟郊",
  "poetId": "03c94ff99104486fbd1c223eade4ad35",
