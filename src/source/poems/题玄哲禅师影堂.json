{
  "_id": "7fc118c1f0a24e82b34c75b230e7a8f5",
  "name": "题玄哲禅师影堂",
  "contents": [
    "吾师视化身，",
    "一念即遗尘。",
    "岩谷藏虚塔，",
    "江湖散学人。",
    "云迷禅处石，",
    "院掩写来真。",
    "寂寞焚香后，",
    "闲阶细草生。"
  ],
  "ptype": null,
  "poetName": "张乔",
  "poetId": "746ea2e1efcd41db9a5d67eddf753f46",
  "form": "五律",
