  "_id": "bd71b43a8b774c11a6067bd852c75870",
  "name": "赠韦赞善别",
  "contents": [
    "扶病送君发，",
    "自怜犹不归。",
    "只应尽客泪，",
    "复作掩荆扉。",
    "江汉故人少，",
    "音书从此稀。",
    "往还二十载，",
    "岁晚寸心违。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
