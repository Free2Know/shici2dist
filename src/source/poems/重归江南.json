  "_id": "7b4e6db5cd034da0bdefaa1f5a2ad446",
  "name": "重归江南",
  "contents": [
    "无成归故里，",
    "不似在他乡。",
    "岁月逐流水，",
    "山川空夕阳。",
    "回潮迷古渡，",
    "迸竹过邻墙。",
    "耆旧休存省，",
    "胡为止泪行。"
  ],
  "ptype": null,
  "poetName": "许棠",
  "poetId": "79e0a1e55d8d45d6a3aa421b0ade4bd3",
