  "_id": "d6f1a9df40724e8b8f6a65f8c3fec5d6",
  "name": "赠章仇将军",
  "contents": [
    "将军不夸剑，",
    "才气为英雄。",
    "五岳拽力内，",
    "百川倾意中。",
    "本立谁敢拔，",
    "飞文自难穷。",
    "前时天地翻，",
    "已有扶正功。"
  ],
  "ptype": null,
  "poetName": "孟郊",
  "poetId": "03c94ff99104486fbd1c223eade4ad35",
