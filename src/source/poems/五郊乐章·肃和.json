  "_id": "2b89812eaec945b7a56c8cfdaea272ef",
  "name": "五郊乐章·肃和",
  "contents": [
    "律周玉琯，",
    "星回金度。",
    "次极阳乌，",
    "纪穷阴兔。",
    "火林散雪，",
    "阳泉凝冱。",
    "八蜡已登，",
    "三农息务。"
  ],
  "ptype": null,
  "poetName": "魏征",
