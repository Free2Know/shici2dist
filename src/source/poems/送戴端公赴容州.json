  "_id": "377a3d4359864826bbd7e85a21fe909e",
  "name": "送戴端公赴容州",
  "contents": [
    "分命诸侯重，",
    "葳蕤绣服香。",
    "八蛮治险阻，",
    "千骑蹋繁霜。",
    "山断旌旗出，",
    "天晴剑珮光。",
    "还将小戴礼，",
    "远出化南方。"
  ],
  "ptype": null,
  "poetName": "陈羽",
  "poetId": "9ca8bee1f2034d4ea59b066d3cef56bb",
  "form": "五律",
