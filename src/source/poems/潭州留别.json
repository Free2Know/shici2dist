  "_id": "7b246dc90e724ad799a775450e024a60",
  "name": "潭州留别",
  "contents": [
    "贤达不相识，",
    "偶然交已深。",
    "宿帆谒郡佐，",
    "怅别依禅林。",
    "湘水流入海，",
    "楚云千里心。",
    "望君杉松夜，",
    "山月清猿吟。"
  ],
  "ptype": null,
  "poetName": "常建",
