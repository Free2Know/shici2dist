{
  "_id": "15e5d94db35e4e9d978c3c2f75414e51",
  "name": "行路难",
  "contents": [
    "下浸与高盘，",
    "不为行路难。",
    "是非真险恶，",
    "翻覆作峰峦。",
    "漆愧同时黑，",
    "朱惭巧处丹。",
    "令人畏相识，",
    "欲画白云看。"
  ],
  "ptype": null,
  "poetName": "齐己",
