{
  "_id": "764f9c7848674d12b62d5f86b507c583",
  "name": "扬州送人",
  "contents": [
    "桂楫木兰舟，",
    "枫江竹箭流。",
    "故人从此去，",
    "望远不胜愁。",
    "落日低帆影，",
    "归风引棹讴。",
    "思君折杨柳，",
    "泪尽武昌楼。"
  ],
  "ptype": null,
  "poetName": "刘绮庄",
  "poetId": "8d194035fb974dda898ae25e42d24e4b",
  "form": "五律",
