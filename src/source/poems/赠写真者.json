  "_id": "6284e7f42f224c8db9034ff8bdd23a84",
  "name": "赠写真者",
  "contents": [
    "子骋丹青日，",
    "予当丑老时。",
    "无劳役神思，",
    "更画病容仪。",
    "迢递麒麟阁，",
    "图功未有期。",
    "区区尺素上，",
    "焉用写真为？"
  ],
  "ptype": null,
  "poetName": "白居易",
  "poetId": "61c661a769a04e409e99836b8f4a8f79",
