{
  "_id": "dbf3fb30a64c412cbef749ac30ea00ee",
  "name": "新荷",
  "contents": [
    "田田八九叶，",
    "散点绿池初。",
    "嫩碧才平水，",
    "圆阴已蔽鱼。",
    "浮萍遮不合，",
    "弱荇绕犹疏。",
    "增在春波底，",
    "芳心卷未舒。"
  ],
  "ptype": null,
  "poetName": "李群玉",
  "poetId": "533477cc5b0045399dfdc5f641132458",
