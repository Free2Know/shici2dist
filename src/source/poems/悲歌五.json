  "_id": "7a10b9f777ac4591b74af75ba2569854",
  "name": "悲歌五",
  "contents": [
    "临春风，",
    "听春鸟；",
    "别时多，",
    "见时少。",
    "愁人夜永不得眠，",
    "瑶井玉绳相对晓。"
  ],
  "ptype": null,
  "poetName": "顾况",
