{
  "_id": "c8d5f3760298432b8734fcfd8d62423c",
  "name": "登白马潭",
  "contents": [
    "水生春缆没，",
    "日出野船开。",
    "宿鸟行犹去，",
    "丛花笑不来。",
    "人人伤白首，",
    "处处接金杯。",
    "莫道新知要，",
    "南征且未回。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
  "form": "五律",
