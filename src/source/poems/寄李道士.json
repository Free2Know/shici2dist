  "_id": "89b2ae7fa8ff491bb8907647efb2e462",
  "name": "寄李道士",
  "contents": [
    "常见高人说，",
    "犹来不偶然。",
    "致身同槁木，",
    "话道出忘诠。",
    "长啸仙钟外，",
    "眠楂海月边。",
    "倘修阴姹姹，",
    "一望寄余焉。"
  ],
  "ptype": null,
  "poetName": "贯休",
