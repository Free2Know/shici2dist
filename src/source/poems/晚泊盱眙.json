{
  "_id": "984cdf3b1d17480092f421463d41d1b1",
  "name": "晚泊盱眙",
  "contents": [
    "广苇夹深流，",
    "萧萧到海秋。",
    "宿船横月浦，",
    "惊鸟绕霜洲。",
    "云湿淮南树，",
    "笳清泗水楼。",
    "徒悬乡国思，",
    "羁迹尚东游。"
  ],
  "ptype": null,
  "poetName": "喻坦之",
  "poetId": "39b164d7bed64e5e937387611e36c4bd",
  "form": "五律",
