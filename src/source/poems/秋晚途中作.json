  "_id": "8ea0f8afa2bd4634b074444415523bf2",
  "name": "秋晚途中作",
  "contents": [
    "落日驰车道，",
    "秋郊思不胜。",
    "水云遥断绪，",
    "山日半衔棱。",
    "远吠邻村处，",
    "计想羡他能。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "3a86db0ed7e4461794e5c0aa5ffb339c",
