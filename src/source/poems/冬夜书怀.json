  "_id": "01ae7d98948d4296b431e5696c272f07",
  "name": "冬夜书怀",
  "contents": [
    "冬宵寒且永。",
    "夜漏宫中发。",
    "草白霭繁霜。",
    "木衰澄清月。",
    "丽服映颓颜。",
    "朱灯照华发。",
    "汉家方尚少。",
    "顾影惭朝谒。"
  ],
  "ptype": null,
  "poetName": "王维",
  "poetId": "37d898722e054cb28508509ddcd22139",
