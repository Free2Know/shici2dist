  "_id": "4de0d3eaf6e74ecd8dab15a5d377b7bb",
  "name": "作诗",
  "contents": [
    "作诗无知音，",
    "作不如不作。",
    "未逢赓载人，",
    "此道终寂寞。",
    "有虞今已殁，",
    "来者谁为托。",
    "朗咏豁心胸，",
    "笔与泪俱落。"
  ],
  "ptype": null,
  "poetName": "刘叉",
