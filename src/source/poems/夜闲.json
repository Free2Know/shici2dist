{
  "_id": "0b5da8d3c1e149d7b748f7c96d51efcb",
  "name": "夜闲",
  "contents": [
    "感极都无梦，",
    "魂销转易惊。",
    "风帘半钩落，",
    "秋月满床明。",
    "怅望临阶坐，",
    "沉吟绕树行。",
    "孤琴在幽匣，",
    "时迸断弦声。"
  ],
  "ptype": null,
  "poetName": "元稹",
  "poetId": "e04ddf3e16fd40a3bc351fd163098dda",
