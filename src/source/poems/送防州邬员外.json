{
  "_id": "8fcfa80f25b9433a8c81c27c33d2b035",
  "name": "送防州邬员外",
  "contents": [
    "千溪与万嶂，",
    "缭绕复峥嵘。",
    "太守劳车马，",
    "何从驻旆旌。",
    "椒香近满郭，",
    "漆货远通京。",
    "唯涤双尘耳，",
    "东南听政声。"
  ],
  "ptype": null,
  "poetName": "许棠",
  "poetId": "79e0a1e55d8d45d6a3aa421b0ade4bd3",
  "form": "五律",
