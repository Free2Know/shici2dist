{
  "_id": "8eaf3db73f184e08af09a5c8322abd17",
  "name": "酬张少府",
  "contents": [
    "晚年惟好静，",
    "万事不关心。",
    "自顾无长策，",
    "空知返旧林。",
    "松风吹解带，",
    "山月照弹琴。",
    "君问穷通理，",
    "渔歌入浦深。"
  ],
  "ptype": null,
  "poetName": "王维",
  "poetId": "37d898722e054cb28508509ddcd22139",
