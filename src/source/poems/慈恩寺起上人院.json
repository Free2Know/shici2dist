{
  "_id": "de6a1b1f975d4a31b7546f2f74c40ef7",
  "name": "慈恩寺起上人院",
  "contents": [
    "禅堂支许同，",
    "清论道源穷。",
    "起灭秋云尽，",
    "虚无夕霭空。",
    "池澄山倒影，",
    "林动叶翻风。",
    "他日焚香待，",
    "还来礼惠聪。"
  ],
  "ptype": null,
  "poetName": "武元衡",
  "poetId": "8f0c555aaef7468e98bbbeae1f023b4d",
  "form": "五律",
