  "_id": "247b48788081498d997c10edbe222605",
  "name": "闲居寄苗发",
  "contents": [
    "渐向浮生老，",
    "前期竟若何。",
    "独身居处静，",
    "永夜坐时多。",
    "厌逐青林客，",
    "休吟白雪歌。",
    "支公有遗寺，",
    "重与谢安过。"
  ],
  "ptype": null,
  "poetName": "司空曙",
