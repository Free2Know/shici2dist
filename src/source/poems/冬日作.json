  "_id": "270fe220c62942bb92da99de1390844b",
  "name": "冬日作",
  "contents": [
    "粝食拥败絮，",
    "苦吟吟过冬。",
    "稍寒人却健，",
    "太饱事多慵。",
    "树老生烟薄，",
    "墙阴贮雪重。",
    "安能只如此，",
    "公道会相容。"
  ],
  "ptype": null,
  "poetName": "裴说",
