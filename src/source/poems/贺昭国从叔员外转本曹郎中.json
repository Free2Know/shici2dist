{
  "_id": "d2268100038a4ed2bdcfcc84c7d4ae83",
  "name": "贺昭国从叔员外转本曹郎中",
  "contents": [
    "苔砌塔阴浓，",
    "朝回尚叫蛩。",
    "粟征山县欠，",
    "官转水曹重。",
    "灯照楼中雨，",
    "书求海上峰。",
    "诗家无骤显，",
    "一一古人踪。"
  ],
  "ptype": null,
  "poetName": "李洞",
  "poetId": "4eeec622c9a342139df6f02d3b9f134b",
  "form": "五律",
