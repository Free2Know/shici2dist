{
  "_id": "93640f3bbcc54dd19454714e9187d103",
  "name": "祀雨师乐章·奠币登歌",
  "contents": [
    "岁正朱明，",
    "礼布玄制。",
    "惟乐能感，",
    "与神合契。",
    "阴雾离披，",
    "灵驭摇裔。",
    "膏泽之庆，",
    "期于稔岁。"
  ],
  "ptype": null,
  "poetName": "包佶",
