{
  "_id": "4af14b74a6d842638f7de75f6a78502b",
  "name": "古砚",
  "contents": [
    "癖性爱古物，",
    "终岁求不得。",
    "昨朝得古砚，",
    "兰河滩之侧。",
    "波涛所击触，",
    "背面生隟隙。",
    "质状朴且丑，",
    "令人作不得。"
  ],
  "ptype": null,
  "poetName": "唐无名氏",
  "poetId": "5fb2c729b42a4746a5b376b70c9d5229",
