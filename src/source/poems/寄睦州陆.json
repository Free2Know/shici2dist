{
  "_id": "ff175e4c64d448fe9fc30505ea5c5986",
  "name": "寄睦州陆",
  "contents": [
    "下国多高趣，",
    "终年半是吟。",
    "海涛通越分，",
    "部伍杂闽音。",
    "晓郭云藏市，",
    "春山鸟护林。",
    "东浮虽未遂，",
    "日日至中心。"
  ],
  "ptype": null,
  "poetName": "许棠",
  "poetId": "79e0a1e55d8d45d6a3aa421b0ade4bd3",
