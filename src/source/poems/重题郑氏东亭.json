{
  "_id": "5c82be805d6d47599ea79f6a3ae722c2",
  "name": "重题郑氏东亭",
  "contents": [
    "华亭入翠微，",
    "秋日乱清晖。",
    "崩石欹山树，",
    "清涟曳水衣。",
    "紫鳞冲岸跃，",
    "苍隼护巢归。",
    "向晚寻征路，",
    "残云傍马飞。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
  "form": "五律",
