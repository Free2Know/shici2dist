{
  "_id": "4097c0e53e434a37a9a61d6c13776980",
  "name": "唐封泰山乐章·豫和",
  "contents": [
    "礼乐终，",
    "烟燎上。",
    "怀灵惠，",
    "结皇想。",
    "归风疾，",
    "回风爽。",
    "百神来，",
    "众神往。"
  ],
  "ptype": null,
  "poetName": "张说",
  "poetId": "5db142df4e374fd7921a571e505f6f23",
