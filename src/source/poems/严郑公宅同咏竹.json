  "_id": "94f71d5f5bfc4b00a67cd2c885ab07cc",
  "name": "严郑公宅同咏竹",
  "contents": [
    "绿竹半含箨，",
    "新梢才出墙。",
    "色侵书帙晚，",
    "阴过酒樽凉。",
    "雨洗娟娟净，",
    "风吹细细香。",
    "但令无剪伐，",
    "会见拂云长。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
