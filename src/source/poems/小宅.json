{
  "_id": "28b2a90a6c5a4e3398c3cbb9a804fb36",
  "name": "小宅",
  "contents": [
    "小宅里闾接，",
    "疏篱鸡犬通。",
    "渠分南巷水，",
    "窗借北家风。",
    "庾信园殊小，",
    "陶潜屋不丰。",
    "何劳问宽窄，",
    "宽窄在心中。"
  ],
  "ptype": null,
  "poetName": "白居易",
  "poetId": "61c661a769a04e409e99836b8f4a8f79",
