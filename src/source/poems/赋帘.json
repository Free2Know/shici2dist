{
  "_id": "e55ff539be2f408795c0f2963f92d8e6",
  "name": "赋帘",
  "contents": [
    "参差垂玉阙，",
    "舒卷映兰宫。",
    "珠光摇素月，",
    "竹影乱清风。",
    "彩散银钩上，",
    "文斜桂户中。",
    "惟当杂罗绮，",
    "相与媚房栊。"
  ],
  "ptype": null,
  "poetName": "李世民",
  "poetId": "10cc5cc32b3c450d8b73272986bf9569",
