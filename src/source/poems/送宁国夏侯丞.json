{
  "_id": "63e7e5f0c9a84f4da8de1ba64f617fb3",
  "name": "送宁国夏侯丞",
  "contents": [
    "楚国青芜上，",
    "秋云似白波。",
    "五湖长路少，",
    "九派乱山多。",
    "谢守通诗宴，",
    "陶公许醉过。",
    "怃然饯离阻，",
    "年鬓两蹉跎。"
  ],
  "ptype": null,
  "poetName": "卢纶",
  "poetId": "10008e7cd9434079b60c4f3e941d4f4f",
  "form": "五律",
