  "_id": "dfc27b150bc646d2bd56ad0cf018c661",
  "name": "秋夜喜友人宿",
  "contents": [
    "莫说春闱事，",
    "清宵且共吟。",
    "频年遗我辈，",
    "何日遇知音。",
    "逼曙天倾斗，",
    "将寒叶坠林。",
    "无将簪绂意，",
    "只损壮夫心。"
  ],
  "ptype": null,
  "poetName": "刘得仁",
