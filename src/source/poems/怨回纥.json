{
  "_id": "bb68b40ecac540ee9616d8813c476781",
  "name": "怨回纥",
  "contents": [
    "祖席驻征棹，",
    "开帆候信潮。",
    "隔筵桃叶泣，",
    "吹管杏花飘。",
    "船去鸥飞阁，",
    "人归尘上桥。",
    "别离惆怅泪，",
    "江路湿红蕉。"
  ],
  "ptype": null,
  "poetName": "皇甫松",
  "poetId": "638687e351b54b2c8ef66b58a1e59d74",
