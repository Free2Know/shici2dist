{
  "_id": "366d9281ef7a416bb17a0a8e87f1ac59",
  "name": "饮马歌",
  "contents": [
    "边头春未到。",
    "雪满交河道。",
    "暮沙明残照。",
    "塞烽云间小。",
    "断鸿悲。",
    "陇月低。",
    "泪湿征衣悄。",
    "岁华老。"
  ],
  "ptype": null,
  "poetName": "曹勋",
  "poetId": "1fb617acb3124806b75c4e78e2d499e8",
