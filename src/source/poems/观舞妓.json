{
  "_id": "d9560e62313f42ae9203b4e52fad3cda",
  "name": "观舞妓",
  "contents": [
    "朔音悲嘒管，",
    "瑶踏动芳尘。",
    "总袖时增怨，",
    "听破复含嚬。",
    "凝腰倚风软，",
    "花题照锦春。",
    "朱弦固凄紧，",
    "琼树亦迷人。"
  ],
  "ptype": null,
  "poetName": "温庭筠",
  "poetId": "c510983e29d441038ad19df73266762d",
