  "_id": "aab5699a88ca4cd797e29b864f12f03f",
  "name": "安稳眠",
  "contents": [
    "家虽日渐贫，",
    "犹未苦饥冻。",
    "身虽日渐老，",
    "幸无急病痛。",
    "眼逢闹处合，",
    "心向闲时用。",
    "既得安稳眠，",
    "亦无颠倒梦。"
  ],
  "ptype": null,
  "poetName": "白居易",
