  "_id": "e60ae99eb25349ee9f728dbbe011d963",
  "name": "秋晚郊居",
  "contents": [
    "远声霜后树，",
    "秋色水边村。",
    "野径无来客，",
    "寒风自动门。",
    "海山藏日影，",
    "江月落潮痕。",
    "惆怅高飞晚，",
    "年年别故园。"
  ],
  "ptype": null,
  "poetName": "任翻",
  "poetId": "401135652e004165b16fd6575db6bd1d",
