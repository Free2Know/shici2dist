  "_id": "1ad1db0cf11240b7bff567a35185f0c3",
  "name": "赠送崔子向",
  "contents": [
    "旅食来江上，",
    "求名赴洛阳。",
    "新诗踪谢守，",
    "内学似支郎。",
    "行怯秦为客，",
    "心依越是乡。",
    "何人作知己，",
    "送尔泪浪浪。"
  ],
  "ptype": null,
  "poetName": "严维",
  "poetId": "2cf32a5a735b45d6b3a6931b3c38faa3",
