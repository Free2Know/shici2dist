{
  "_id": "9068f5e52dfa4ae39caa92d9d3be702b",
  "name": "宿山居",
  "contents": [
    "山店灯前客，",
    "酬身未有媒。",
    "乡关贫后别，",
    "风雨夜深来。",
    "上国求丹桂，",
    "衡门长绿苔。",
    "堪惊双鬓雪，",
    "不待岁寒催。"
  ],
  "ptype": null,
  "poetName": "朱庆馀",
  "poetId": "1d2ece7d434747758f0f1f12e6228187",
