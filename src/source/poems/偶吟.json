{
  "_id": "7f25d0ca6da043b2974ba2b48efab47f",
  "name": "偶吟",
  "contents": [
    "里巷多通水，",
    "林园尽不扃。",
    "松身为外户，",
    "池面是中庭。",
    "元氏诗三帙，",
    "陈家酒一瓶。",
    "醉来狂发咏，",
    "邻女映篱听。"
  ],
  "ptype": null,
  "poetName": "白居易",
  "poetId": "61c661a769a04e409e99836b8f4a8f79",
