{
  "_id": "6ef9f819871144fa81b5b4a11efb3a28",
  "name": "寄镜湖朱处士",
  "contents": [
    "澄霁晚流阔，",
    "微风吹绿蘋。",
    "鳞鳞远峰见，",
    "淡淡平湖春。",
    "芳草日堪把，",
    "白云心所亲。",
    "何时可为乐，",
    "梦里东山人。"
  ],
  "ptype": null,
  "poetName": "李颀",
  "poetId": "a6c5740481924010b2b4eb097caa19df",
  "form": "五律",
