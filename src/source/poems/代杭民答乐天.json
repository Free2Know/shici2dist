  "_id": "e2456bf47a194b5b953c9ae6071b1799",
  "name": "代杭民答乐天",
  "contents": [
    "翠幕笼斜日，",
    "朱衣俨别筵。",
    "管弦凄欲罢，",
    "城郭望依然。",
    "路溢新城市，",
    "农开旧废田。",
    "春坊幸无事，",
    "何惜借三年。"
  ],
  "ptype": null,
  "poetName": "元稹",
  "poetId": "e04ddf3e16fd40a3bc351fd163098dda",
