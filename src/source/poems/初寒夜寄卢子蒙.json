  "_id": "66f31635ceba44ec823269b696ed11cf",
  "name": "初寒夜寄卢子蒙",
  "contents": [
    "月是阴秋镜，",
    "寒为寂寞资。",
    "轻寒酒醒后，",
    "斜月枕前时。",
    "倚壁思闲事，",
    "回灯检旧诗。",
    "闻君亦同病，",
    "终夜远相悲。"
  ],
  "ptype": null,
  "poetName": "元稹",
