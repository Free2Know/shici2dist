  "_id": "a36b674bf64f49f98f0314d2b472406c",
  "name": "途中逢友人",
  "contents": [
    "长大有南北，",
    "山川各所之。",
    "相逢孤馆夜，",
    "共忆少年时。",
    "烂醉百花酒，",
    "狂题几首诗。",
    "来朝又分袂，",
    "后会鬓应丝。"
  ],
  "ptype": null,
  "poetName": "项斯",
  "poetId": "3806b4720d3e4654bc75b06816f786cd",
