{
  "_id": "35a07e2c07a1460f9328cc3dca0fcc78",
  "name": "西阁口号",
  "contents": [
    "山木抱云稠，",
    "寒江绕上头。",
    "雪崖才变石，",
    "风幔不依楼。",
    "社稷堪流涕，",
    "安危在运筹。",
    "看君话王室，",
    "感动几销忧。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
  "form": "五律",
