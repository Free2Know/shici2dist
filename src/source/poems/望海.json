{
  "_id": "3a94da917e2e4a5485918a1ec2e2dad8",
  "name": "望海",
  "contents": [
    "苍茫空泛日，",
    "四顾绝人烟。",
    "半浸中华岸，",
    "旁通异域船。",
    "岛间应有国，",
    "波外恐无天。",
    "欲作乘槎客，",
    "翻愁去隔年。"
  ],
  "ptype": null,
  "poetName": "周繇",
  "poetId": "a3896509202e411098944b7fe677db11",
