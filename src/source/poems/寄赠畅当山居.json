{
  "_id": "ffae48e494714e06a292bf3ddb8e3ca8",
  "name": "寄赠畅当山居",
  "contents": [
    "古村荒石路，",
    "岁晏独言归。",
    "山雪厚三尺，",
    "社榆粗十围。",
    "虬龙宁守蛰，",
    "鸾鹤岂矜飞。",
    "君子固安分，",
    "毋听劳者讥。"
  ],
  "ptype": null,
  "poetName": "卢纶",
  "poetId": "10008e7cd9434079b60c4f3e941d4f4f",
