{
  "_id": "79e8e2fca1cf4152b1427cbffe030d75",
  "name": "题湖上友人居",
  "contents": [
    "岂得恋樵渔，",
    "全家湖畔居。",
    "远无潮客信，",
    "闲寄岳僧书。",
    "野白梅繁后，",
    "山明雨散初。",
    "逍遥向云水，",
    "莫与宦情疏。"
  ],
  "ptype": null,
  "poetName": "张乔",
  "poetId": "746ea2e1efcd41db9a5d67eddf753f46",
  "form": "五律",
