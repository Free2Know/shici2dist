{
  "_id": "b6b16bbf71084b7288e464e329a12dfc",
  "name": "经漂母墓",
  "contents": [
    "昔贤怀一饭，",
    "兹事已千秋。",
    "古墓樵人识，",
    "前朝楚水流。",
    "渚蘋行客荐，",
    "山木杜鹃愁。",
    "春草茫茫绿，",
    "王孙旧此游。"
  ],
  "ptype": null,
  "poetName": "刘长卿",
  "poetId": "8fb91cedc6274daba50e589c0dde93b7",
  "form": "五律",
