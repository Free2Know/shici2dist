  "_id": "ba9211f926b54ae5aa5aeabc940fe767",
  "name": "赠章八元",
  "contents": [
    "相见谈经史，",
    "江楼坐夜阑。",
    "风声吹户响，",
    "灯影照人寒。",
    "俗薄交游尽，",
    "时危出处难。",
    "衰年逢二妙，",
    "亦得闷怀宽。"
  ],
  "ptype": null,
  "poetName": "张继",
  "poetId": "19418e9d21d942d882c2f53ac712f1d4",
