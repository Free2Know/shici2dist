{
  "_id": "e8dab5e9aae8410f9f8a05e9cc87a2c3",
  "name": "精舍纳凉",
  "contents": [
    "山景寂已晦，",
    "野寺变苍苍。",
    "夕风吹高殿，",
    "露叶散林光。",
    "清钟始戒夜，",
    "幽禽尚归翔。",
    "谁复掩扉卧，",
    "不咏南轩凉。"
  ],
  "ptype": null,
  "poetName": "韦应物",
  "poetId": "45a80c2fc73a4437b0b6c41a5fc10227",
  "form": "五律",
