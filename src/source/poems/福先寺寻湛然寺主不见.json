{
  "_id": "8d53ed2f2e12486e9346db2d67ee64ff",
  "name": "福先寺寻湛然寺主不见",
  "contents": [
    "寂然空伫立，",
    "往往报疏钟。",
    "高馆谁留客，",
    "东南二室峰。",
    "川原通霁色，",
    "田野变春容。",
    "惆怅层城暮，",
    "犹言归路逢。"
  ],
  "ptype": null,
  "poetName": "皇甫冉",
  "poetId": "2ba2e98104ff4f82b7c02921e5cd0e6a",
  "form": "五律",
