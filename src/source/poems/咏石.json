{
  "_id": "3e3c8ad52b26408c8e69f8954ab1a9e2",
  "name": "咏石",
  "contents": [
    "济北甄神贶，",
    "河西濯锦文。",
    "声应天池雨，",
    "影触岱宗云。",
    "燕归犹可候，",
    "羊起自成群。",
    "何当握灵髓，",
    "高枕绝嚣氛。"
  ],
  "ptype": null,
  "poetName": "苏味道",
  "poetId": "6739ba80d90e4e04931143ec5482c69f",
