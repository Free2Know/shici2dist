{
  "_id": "ff9c563eb1f84564b26f755a952ceaa0",
  "name": "酬于訢校书见贻",
  "contents": [
    "能□得从军，",
    "清嬴□□□。",
    "绮罗徒满目，",
    "山水不离心。",
    "暂别愁花老，",
    "相思倚竹阴。",
    "家贫无以养，",
    "未可话抽簪。"
  ],
  "ptype": null,
  "poetName": "朱庆馀",
  "poetId": "1d2ece7d434747758f0f1f12e6228187",
  "form": "五律",
