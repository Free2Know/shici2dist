  "_id": "4e62ed031aaf4c5db4185400e2c165c6",
  "name": "西游书怀",
  "contents": [
    "渭川通野戍，",
    "有路上桑干。",
    "独鸟青天暮，",
    "惊麏赤烧残。",
    "高秋辞故国，",
    "昨日梦长安。",
    "客意自如此，",
    "非关行路难。"
  ],
  "ptype": null,
  "poetName": "温庭筠",
  "poetId": "c510983e29d441038ad19df73266762d",
