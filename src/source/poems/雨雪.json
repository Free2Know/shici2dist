{
  "_id": "a6b06a39a1444c58b27f608eebbc7973",
  "name": "雨雪",
  "contents": [
    "风沙悲久戍，",
    "雨雪更劳师。",
    "绝漠无人境，",
    "将军苦战时。",
    "山川迷向背，",
    "氛雾失旌旗。",
    "徒念天涯隔，",
    "中人芳草期。"
  ],
  "ptype": null,
  "poetName": "皇甫冉",
  "poetId": "2ba2e98104ff4f82b7c02921e5cd0e6a",
  "form": "五律",
