{
  "_id": "efec57b1caa24e788a030a8b401dc74d",
  "name": "哭长孙侍御",
  "contents": [
    "道为谋书重，",
    "名因赋颂雄。",
    "礼闱曾擢桂，",
    "宪府旧乘骢。",
    "流水生涯尽，",
    "浮云世事空。",
    "唯馀旧台柏，",
    "萧瑟九原中。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
  "form": "五律",
