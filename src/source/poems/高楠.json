  "_id": "2e117ba6231f4565828dfa77824ea252",
  "name": "高楠",
  "contents": [
    "楠树色冥冥，",
    "江边一盖青。",
    "近根开药圃，",
    "接叶制茅亭。",
    "落景阴犹合，",
    "微风韵可听。",
    "寻常绝醉困，",
    "卧此片时醒。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
