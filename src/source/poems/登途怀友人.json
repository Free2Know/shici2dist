  "_id": "df6877ab585d425db076b2a4fbb31ee3",
  "name": "登途怀友人",
  "contents": [
    "日落野原秀，",
    "雨余云物闲。",
    "清时正愁绝，",
    "高处正跻攀。",
    "京洛遥天外，",
    "江河战鼓间。",
    "孤怀欲谁寄，",
    "应望塞鸿还。"
  ],
  "ptype": null,
  "poetName": "吴融",
  "poetId": "c8fcad51e3264dc8a7c8162344d3b0e9",
