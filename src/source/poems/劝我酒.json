{
  "_id": "957d7a45685d4346a782c6cfd6d4dcde",
  "name": "劝我酒",
  "contents": [
    "劝我酒，",
    "我不辞。",
    "请君歌，",
    "歌莫迟。",
    "歌声长，",
    "辞亦切，",
    "此辞听者堪愁绝。",
    "洛阳女儿面似花，",
    "河南大尹头如雪。"
  ],
  "ptype": null,
  "poetName": "白居易",
  "poetId": "61c661a769a04e409e99836b8f4a8f79",
  "form": "五律",
