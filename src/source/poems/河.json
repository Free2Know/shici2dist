{
  "_id": "db9dc8110c004f92921b1dc366b73310",
  "name": "河",
  "contents": [
    "源出昆仑中，",
    "长波接汉空。",
    "桃花来马颊，",
    "竹箭入龙宫。",
    "德水千年变，",
    "荣光五色通。",
    "若披兰叶检，",
    "□□□□□。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "5f3c01f5ddab4ebdb2d9748023f40391",
