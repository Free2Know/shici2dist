  "_id": "2b9b768389d846ebafc028eb4891655c",
  "name": "赠富平李宰",
  "contents": [
    "夫君清且贫，",
    "琴鹤最相亲。",
    "简肃诸曹事，",
    "安闲一境人。",
    "陵山云里拜，",
    "渠路雨中巡。",
    "易得连宵醉，",
    "千缸石冻春。"
  ],
  "ptype": null,
  "poetName": "郑谷",
  "poetId": "87cb83f765db4575a2bdcb67a72bc6d7",
