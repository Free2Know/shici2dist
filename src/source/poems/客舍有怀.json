  "_id": "a5cb7492f78f40c49aa9aedcc9499199",
  "name": "客舍有怀",
  "contents": [
    "旅人无事喜，",
    "终日思悠悠。",
    "逢酒嫌杯浅，",
    "寻书怕字稠。",
    "贫来许钱圣，",
    "梦觉见身愁。",
    "寂寞中林下，",
    "饥鹰望到秋。"
  ],
  "ptype": null,
  "poetName": "姚合",
  "poetId": "9a0ddeb7f13a437d8090ccf3996de1f3",
