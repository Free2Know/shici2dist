{
  "_id": "5a09a1cc0eaa4650b0b5301c65eb561e",
  "name": "题春明门外镇国禅院",
  "contents": [
    "空门临大道，",
    "师坐此中禅。",
    "过客自生敬，",
    "焚香惟默然。",
    "书灯明象外，",
    "古木覆檐前。",
    "不得如驯鸽，",
    "人间万虑牵。"
  ],
  "ptype": null,
  "poetName": "顾非熊",
  "poetId": "7d10bda5d33144fab1cdf0dba4c0bced",
  "form": "五律",
