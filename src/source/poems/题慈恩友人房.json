  "_id": "b908d2da7fa943cc853dc1eba190efeb",
  "name": "题慈恩友人房",
  "contents": [
    "贾生耽此寺，",
    "胜事入诗多。",
    "鹤宿星千树，",
    "僧归烧一坡。",
    "塔棱垂雪水，",
    "江色映茶锅。",
    "长久堪栖息，",
    "休言忆镜波。"
  ],
  "ptype": null,
  "poetName": "李洞",
  "poetId": "4eeec622c9a342139df6f02d3b9f134b",
