  "_id": "492e60c594044ff097ace5c712ee9470",
  "name": "湖西逸人",
  "contents": [
    "老隐洞庭西，",
    "渔樵共一溪。",
    "琴前孤鹤影，",
    "石上远僧题。",
    "橘柚园林熟，",
    "蒹葭径路迷。",
    "君能许邻并，",
    "分药劚春畦。"
  ],
  "ptype": null,
  "poetName": "齐己",
  "poetId": "759693a9a6584b838ea6a6bfa0b9e490",
