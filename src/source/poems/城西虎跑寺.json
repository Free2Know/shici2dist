{
  "_id": "030f74b65cc840ce80a335c53087646d",
  "name": "城西虎跑寺",
  "contents": [
    "石势虎蹲伏，",
    "山形龙屈盘。",
    "寺开梁殿阁，",
    "坟掩晋衣冠。",
    "出涧泉声细，",
    "斜阳塔影寒。",
    "近城多□□，",
    "栖息此中安。"
  ],
  "ptype": null,
  "poetName": "张继",
  "poetId": "19418e9d21d942d882c2f53ac712f1d4",
  "form": "五律",
