{
  "_id": "b8e73dfc1c2d4c4c82bb746aff13b23e",
  "name": "寄怀西蟾师弟",
  "contents": [
    "万里八九月，",
    "一身西北风。",
    "自从相示后，",
    "长记在吟中。",
    "见说南游远，",
    "堪怀我姓同。",
    "江边忽得信，",
    "回到岳门东。"
  ],
  "ptype": null,
  "poetName": "齐己",
  "poetId": "759693a9a6584b838ea6a6bfa0b9e490",
  "form": "五律",
