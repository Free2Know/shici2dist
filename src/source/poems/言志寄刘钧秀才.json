{
  "_id": "ae7ca11adf7d4117a1f582e50f73cad4",
  "name": "言志寄刘钧秀才",
  "contents": [
    "童稚亲儒墨，",
    "时平喜道存。",
    "酬身指书剑，",
    "赋命委乾坤。",
    "秋爽鼓琴兴，",
    "月清搜句魂。",
    "与君同此志，",
    "终待至公论。"
  ],
  "ptype": null,
  "poetName": "李中",
  "poetId": "5b8a00819d324861a0a7ee4ab6bd3e48",
  "form": "五律",
