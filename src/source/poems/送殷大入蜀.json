{
  "_id": "3eb3c1f5da2f44f58f26d0d314e87f35",
  "name": "送殷大入蜀",
  "contents": [
    "禺山金碧路，",
    "此地饶英灵。",
    "送君一为别，",
    "栖断故乡情。",
    "片云生极浦，",
    "斜日隐离亭。",
    "坐看征骑没，",
    "惟见远山青。"
  ],
  "ptype": null,
  "poetName": "陈子昂",
  "poetId": "4f8b9816b598482bb7011d0ac90378f3",
  "form": "五律",
