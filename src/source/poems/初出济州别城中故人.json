{
  "_id": "233148d1ea5b4d25a2e9d41e6f4ce23e",
  "name": "初出济州别城中故人",
  "contents": [
    "微官易得罪，",
    "谪去济川阴。",
    "执政方持法，",
    "明君照此心。",
    "闾阎河润上，",
    "井邑海云深。",
    "纵有归来日，",
    "各愁年鬓侵。"
  ],
  "ptype": null,
  "poetName": "王维",
  "poetId": "37d898722e054cb28508509ddcd22139",
