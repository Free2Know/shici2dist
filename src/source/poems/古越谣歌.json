{
  "_id": "1ecd3c858acf45919016bae9a7c83a68",
  "name": "古越谣歌",
  "contents": [
    "君乘车，",
    "我戴笠，",
    "他日相逢下车揖。",
    "君担簦，",
    "我跨马，",
    "他日相逢为君下。"
  ],
  "ptype": null,
  "poetName": "汉无名氏",
  "poetId": "f5490a8f45f64fc4b56a0f50076493e1",
