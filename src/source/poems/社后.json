  "_id": "625fe3b9f23c4e7291d8b1d140042c2c",
  "name": "社后",
  "contents": [
    "社后重阳近，",
    "云天澹薄间。",
    "目随棋客静，",
    "心共睡僧闲。",
    "归鸟城衔日，",
    "残虹雨在山。",
    "寂寥思晤语，",
    "何夕款柴关？"
  ],
  "ptype": null,
  "poetName": "韩偓",
  "poetId": "c48ef178cc064285901e04ad31dffe69",
