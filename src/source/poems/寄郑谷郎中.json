  "_id": "e9453a018d4d48d8985928bbfa78fbdf",
  "name": "寄郑谷郎中",
  "contents": [
    "高名喧省闼，",
    "雅颂出吾唐。",
    "叠巘供秋望，",
    "无云到夕阳。",
    "自封修药院，",
    "别扫著僧床。",
    "几梦中朝事，",
    "依依鹓鹭行。"
  ],
  "ptype": null,
  "poetName": "齐己",
  "poetId": "759693a9a6584b838ea6a6bfa0b9e490",
