  "_id": "6c0a695467184b609efb6ede4d00a65a",
  "name": "哭孟郊",
  "contents": [
    "身死声名在，",
    "多应万古传。",
    "寡妻无子息，",
    "破宅带林泉。",
    "冢近登山道，",
    "诗随过海船。",
    "故人相吊后，",
    "斜日下寒天。"
  ],
  "ptype": null,
  "poetName": "贾岛",
