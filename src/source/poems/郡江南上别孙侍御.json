{
  "_id": "195d6223e91c4d79860fe8e5a1e5d8c0",
  "name": "郡江南上别孙侍御",
  "contents": [
    "云嶂天涯尽，",
    "川途海县穷。",
    "何言此地僻，",
    "忽与故人同。",
    "身负邦君弩，",
    "情纡御史骢。",
    "王程不我驻，",
    "离思逐秋风。"
  ],
  "ptype": null,
  "poetName": "张九龄",
  "poetId": "a899cc0a679f4d6cadeee32c92c54761",
  "form": "五律",
