{
  "_id": "57366bebaddb4b47bfa01c63115d48b0",
  "name": "吊膳曹从叔郎中",
  "contents": [
    "华省支残俸，",
    "寒蔬办祭稀。",
    "安坟对白阁，",
    "买石折朱衣。",
    "蜀客弹琴哭，",
    "江鸥入宅飞。",
    "帆吹佳句远，",
    "不独遍王畿。"
  ],
  "ptype": null,
  "poetName": "李洞",
  "poetId": "4eeec622c9a342139df6f02d3b9f134b",
  "form": "五律",
