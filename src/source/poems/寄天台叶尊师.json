  "_id": "591235a00be8434b819c03e9bf952bb3",
  "name": "寄天台叶尊师",
  "contents": [
    "师住天台久，",
    "长闻过石桥。",
    "晴峰见沧海，",
    "深洞彻丹霄。",
    "采药霞衣湿，",
    "煎芝古鼎焦。",
    "念予无俗骨，",
    "频与鹤书招。"
  ],
  "ptype": null,
  "poetName": "王贞白",
  "poetId": "a91d77ba2e2f457bbd9f206dacdf822a",
  "form": "五律",
