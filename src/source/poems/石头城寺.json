{
  "_id": "b3db16811773467bae38e5c75b2e037e",
  "name": "石头城寺",
  "contents": [
    "山势抱烟光，",
    "重门突兀傍。",
    "连檐金像阁，",
    "半壁石龛廊。",
    "碧树丛高顶，",
    "清池占下方。",
    "徒悲宦游意，",
    "尽日老僧房。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "3a86db0ed7e4461794e5c0aa5ffb339c",
  "form": "五律",
