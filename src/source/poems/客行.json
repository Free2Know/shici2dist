{
  "_id": "f2b49d4d92674de2838b9b7ea5f654e7",
  "name": "客行",
  "contents": [
    "路岐长不尽，",
    "客恨杳难通。",
    "芦荻晚汀雨，",
    "柳花南浦风。",
    "乱钟嘶马急，",
    "残日半帆红。",
    "却羡渔樵侣，",
    "闲歌落照中。"
  ],
  "ptype": null,
  "poetName": "马戴",
  "poetId": "db14ec8dfa7a4a92987b2e2357aaa5b6",
  "form": "五律",
