  "_id": "682705a5d41940e0be442be64a623b46",
  "name": "郊庙歌辞·祭汾阴乐章·太簇角",
  "contents": [
    "坤元载物，",
    "阳乐发生。",
    "播殖资始，",
    "品汇咸亨。",
    "列俎棋布，",
    "方坛砥平。",
    "神歆禋祀，",
    "后德惟明。"
  ],
  "ptype": null,
  "poetName": "卢从愿",
