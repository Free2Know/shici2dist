{
  "_id": "eff4f5e8d3d941f88b062d36f30daf5c",
  "name": "题寄王秘书",
  "contents": [
    "唯求买药价，",
    "此外更无机。",
    "扶病看红叶，",
    "辞官著白衣。",
    "断篱通野径，",
    "高树荫邻扉。",
    "时馥留僧宿，",
    "余人得见稀。"
  ],
  "ptype": null,
  "poetName": "朱庆馀",
  "poetId": "1d2ece7d434747758f0f1f12e6228187",
  "form": "五律",
