{
  "_id": "e7f5403c775e4a81b6ca8626842f58fe",
  "name": "美人春卧",
  "contents": [
    "妾家巫峡阳，",
    "罗幌寝兰堂。",
    "晓日临窗久，",
    "春风引梦长。",
    "落钗仍挂鬓，",
    "微汗欲消黄。",
    "纵使朦胧觉，",
    "魂犹逐楚王。"
  ],
  "ptype": null,
  "poetName": "梁锽",
  "poetId": "015fda90f10d46e3ad1233f29e891d6b",
  "form": "五律",
