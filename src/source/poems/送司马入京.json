{
  "_id": "b1d25adc266e46e8892e3a97e1819fa3",
  "name": "送司马入京",
  "contents": [
    "群盗至今日，",
    "先朝忝从臣。",
    "叹君能恋主，",
    "久客羡归秦。",
    "黄阁长司谏，",
    "丹墀有故人。",
    "向来论社稷，",
    "为话涕沾巾。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
  "form": "五律",
