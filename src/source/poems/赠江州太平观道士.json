{
  "_id": "6460f02fe2324cc28e87235b5a5d2dce",
  "name": "赠江州太平观道士",
  "contents": [
    "落魄薛高士，",
    "年高无白髭。",
    "云中闲卧石，",
    "山里冷寻碑。",
    "夸我饮大酒，",
    "嫌人说小诗。",
    "不知甚么汉，",
    "一任辈流嗤。"
  ],
  "ptype": null,
  "poetName": "吕岩",
  "poetId": "b72ba9b2176f44df9f2f829dbc577be1",
  "form": "五律",
