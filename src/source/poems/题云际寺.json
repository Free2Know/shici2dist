  "_id": "4c81c80df48242828bd63d2854dccb66",
  "name": "题云际寺",
  "contents": [
    "开门风雪顶，",
    "上彻困飞禽。",
    "猿戏青冥里，",
    "人行紫阁阴。",
    "腊泉冰下出，",
    "夜磬月中寻。",
    "尽欲居岩室，",
    "如何不住心。"
  ],
  "ptype": null,
  "poetName": "李洞",
  "poetId": "4eeec622c9a342139df6f02d3b9f134b",
