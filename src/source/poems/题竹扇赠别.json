  "_id": "3867bc88d47844268ae35e17c3e02350",
  "name": "题竹扇赠别",
  "contents": [
    "湘竹殊堪制，",
    "齐纨且未工。",
    "幸亲芳袖日，",
    "犹带旧林风。",
    "掩笑歌筵里，",
    "传书卧阁中。",
    "竟将为别赠，",
    "宁与合欢同。"
  ],
  "ptype": null,
  "poetName": "皇甫冉",
  "poetId": "2ba2e98104ff4f82b7c02921e5cd0e6a",
