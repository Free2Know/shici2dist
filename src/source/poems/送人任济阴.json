  "_id": "daa33d2a4ab44f28a09265fc4223283a",
  "name": "送人任济阴",
  "contents": [
    "黄绶在腰下，",
    "知君非旅行。",
    "将书报旧里，",
    "留褐与诸生。",
    "赠别尽沽酒，",
    "惜欢多出城。",
    "春风济水上，",
    "候吏听车声。"
  ],
  "ptype": null,
  "poetName": "张籍",
  "poetId": "0810aa6ff9a34a96aba9c12caed115ae",
