  "_id": "fc56332492a24039823a69b852ff6b98",
  "name": "池边",
  "contents": [
    "柳老香丝宛，",
    "荷新钿扇圆。",
    "残春深树里，",
    "斜日小楼前。",
    "醉遣收杯杓，",
    "闲听理管弦。",
    "池边更无事，",
    "看补采莲船。"
  ],
  "ptype": null,
  "poetName": "白居易",
