{
  "_id": "977342ff28a04c2caf1a30a735c6a61b",
  "name": "幽院早春",
  "contents": [
    "草短花初拆，",
    "苔青柳半黄。",
    "隔帘春雨细，",
    "高枕晓莺长。",
    "无事含闲梦，",
    "多情识异香。",
    "欲寻苏小小，",
    "何处觅钱塘。"
  ],
  "ptype": null,
  "poetName": "柳中庸",
  "poetId": "c150155a53d3456c8f1f8085991108d3",
  "form": "五律",
