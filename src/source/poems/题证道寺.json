  "_id": "25f1cf62a9de4b169585f6072bbcdc33",
  "name": "题证道寺",
  "contents": [
    "弯环青径斜，",
    "自是野僧家。",
    "满涧洗岩液，",
    "插天排石牙。",
    "炉寒余柏子，",
    "架静落藤花。",
    "记得逃兵日，",
    "门多贵客车。"
  ],
  "ptype": null,
  "poetName": "唐彦谦",
  "poetId": "bed3d2dc8e014c25950dab9d7fad3ef4",
  "form": "五律",
