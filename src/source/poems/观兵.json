{
  "_id": "b7a326c4dc3c490ca62a42c190b68d05",
  "name": "观兵",
  "contents": [
    "北庭送壮士，",
    "貔虎数尤多。",
    "精锐旧无敌，",
    "边隅今若何。",
    "妖氛拥白马，",
    "元帅待雕戈。",
    "莫守邺城下，",
    "斩鲸辽海波。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
  "form": "五律",
