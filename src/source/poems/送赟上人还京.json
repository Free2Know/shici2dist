  "_id": "1f0f53a100bb42068cdef443e13eefcc",
  "name": "送赟上人还京",
  "contents": [
    "久游春草尽，",
    "还寄北船归。",
    "沙鸟窥中食，",
    "江云入净衣。",
    "秦原山色近，",
    "楚寺磬声微。",
    "见说翻经馆，",
    "多闻似者稀。"
  ],
  "ptype": null,
  "poetName": "皎然",
  "poetId": "a5afe9c958b94f8fb0cef564a238e23e",
