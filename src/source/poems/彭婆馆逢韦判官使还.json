  "_id": "86c19081f7be4627a814ce43ef7ce1dd",
  "name": "彭婆馆逢韦判官使还",
  "contents": [
    "受辞分路远，",
    "会府见君稀。",
    "雨雪经年去，",
    "轩车此日归。",
    "暮春愁见别，",
    "久客顺相依。",
    "寂寞伊川上，",
    "杨花空自飞。"
  ],
  "ptype": null,
  "poetName": "戴叔伦",
  "poetId": "b045bb7d35dd4370aad75204e2b55843",
  "form": "五律",
