{
  "_id": "97218a61c9d14dbabaf127d9c5595f5e",
  "name": "敬简王明府",
  "contents": [
    "叶县郎官宰，",
    "周南太史公。",
    "神仙才有数，",
    "流落意无穷。",
    "骥病思偏秣，",
    "鹰愁怕苦笼。",
    "看君用高义，",
    "耻与万人同。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
