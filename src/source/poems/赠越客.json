{
  "_id": "79024d977a82451b81f19e1ecfe68292",
  "name": "赠越客",
  "contents": [
    "故国波涛隔，",
    "明时已久留。",
    "献书双阙晚，",
    "看月五陵秋。",
    "南棹何时返，",
    "长江忆共游。",
    "遥知钓船畔，",
    "相望在汀洲。"
  ],
  "ptype": null,
  "poetName": "马戴",
  "poetId": "db14ec8dfa7a4a92987b2e2357aaa5b6",
