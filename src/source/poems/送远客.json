  "_id": "e27c745121d34e1aad0d9809bc035176",
  "name": "送远客",
  "contents": [
    "南原相送处，",
    "秋水草还生。",
    "同作忆乡客，",
    "如今分路行。",
    "因谁寄归信，",
    "渐远问前程。",
    "明日重阳节，",
    "无人上古城。"
  ],
  "ptype": null,
  "poetName": "张籍",
  "poetId": "0810aa6ff9a34a96aba9c12caed115ae",
