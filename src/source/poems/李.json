{
  "_id": "9102f5399c2e45c1b0a69d941e43b311",
  "name": "李",
  "contents": [
    "君子盈天阶，",
    "小人甘自免。",
    "方随炼金客，",
    "林上家绝巘。",
    "背岭花未开，",
    "入云树深浅。",
    "清昼犹自眠，",
    "山鸟时一啭。"
  ],
  "ptype": null,
  "poetName": "王维",
