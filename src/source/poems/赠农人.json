{
  "_id": "98504d0fb15441b8a462403d66a55b93",
  "name": "赠农人",
  "contents": [
    "劝尔勤耕田，",
    "盈尔仓中粟。",
    "劝尔伐桑株，",
    "减尔身上服。",
    "清霜一委地，",
    "万草色不绿。",
    "狂飙一入林，",
    "万叶不著木。",
    "青春如不耕，",
    "何以自结束。"
  ],
  "ptype": null,
  "poetName": "孟郊",
  "poetId": "03c94ff99104486fbd1c223eade4ad35",
