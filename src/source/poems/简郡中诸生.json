  "_id": "71f4582a42284c1b81ee61d3c4dcb9e2",
  "name": "简郡中诸生",
  "contents": [
    "守郡卧秋阁，",
    "四面尽荒山。",
    "此时听夜雨，",
    "孤灯照窗间。",
    "药园日芜没，",
    "书帷长自闲。",
    "惟当上客至，",
    "论诗一解颜。"
  ],
  "ptype": null,
  "poetName": "韦应物",
  "poetId": "45a80c2fc73a4437b0b6c41a5fc10227",
