  "_id": "fd404ed7cb704c3b85480a587e22e027",
  "name": "裴六书堂",
  "contents": [
    "闲堂闭空阴，",
    "竹林但清响。",
    "窗下长啸客，",
    "区中无遗想。",
    "经纶精微言，",
    "兼济当独往。"
  ],
  "ptype": null,
  "poetName": "王昌龄",
