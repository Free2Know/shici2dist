{
  "_id": "7e51952047b949f8bde1ef483a7ae6d1",
  "name": "还自会稽歌",
  "contents": [
    "野粉椒壁黄，",
    "湿萤满梁殿。",
    "台城应教人，",
    "秋衾梦铜辇。",
    "吴霜点归鬓，",
    "身与塘蒲晚。",
    "脉脉辞金鱼，",
    "羁臣守迍贱。"
  ],
  "ptype": null,
  "poetName": "李贺",
  "poetId": "a9fc5f44a7474857a8c6fe39ffbad132",
  "form": "五律",
