  "_id": "5faa752ff93c4b0bbbee097c110b7976",
  "name": "腊夜对酒",
  "contents": [
    "琥珀杯中物，",
    "琼枝席上人。",
    "乐声方助醉，",
    "烛影已含春。",
    "自顾行将老，",
    "何辞坐达晨。",
    "传觞称厚德，",
    "不问吐车茵。"
  ],
  "ptype": null,
  "poetName": "羊士谔",
