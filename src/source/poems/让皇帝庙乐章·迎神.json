{
  "_id": "43304de3dea044d88189bfeb5db7a432",
  "name": "让皇帝庙乐章·迎神",
  "contents": [
    "皇矣天宗，",
    "德先王季。",
    "因心则友，",
    "克让以位。",
    "爰命有司，",
    "式尊前志。",
    "神其降灵，",
    "昭飨祀事。"
  ],
  "ptype": null,
  "poetName": "李纾",
