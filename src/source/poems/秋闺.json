{
  "_id": "19e0721f100c4435b2795fa23fc6854b",
  "name": "秋闺",
  "contents": [
    "征客向轮台，",
    "幽闺寂不开。",
    "音书秋雁断，",
    "机杼夜蛩催。",
    "虚幌风吹叶，",
    "闲阶露湿苔。",
    "自怜愁思影，",
    "常共月裴回。"
  ],
  "ptype": null,
  "poetName": "郑愔",
  "poetId": "844b95a00ecd4c37a24afcf3992aad31",
