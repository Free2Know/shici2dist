{
  "_id": "d38087f31cf64fa8a5918a3881f68c3f",
  "name": "送刘侍御赴陕州",
  "contents": [
    "金羁映骕骦，",
    "后骑佩干将。",
    "把酒春城晚，",
    "鸣鞭晓路长。",
    "带冰新溜涩，",
    "间雪早梅香。",
    "明日怀贤处，",
    "依依御史床。"
  ],
  "ptype": null,
  "poetName": "韩翃",
  "poetId": "135a4148450d4726a0b138479ebc3f64",
  "form": "五律",
