{
  "_id": "e91f8267cb004b4aa8a2d11206826173",
  "name": "过龙湾五王阁访友人不遇",
  "contents": [
    "野桥秋水落，",
    "江阁暝烟微。",
    "白日又欲午，",
    "高人犹未归。",
    "青林依古塔，",
    "虚馆静柴扉。",
    "坐久思题字，",
    "翻怜柿叶稀。"
  ],
  "ptype": null,
  "poetName": "戴叔伦",
  "poetId": "b045bb7d35dd4370aad75204e2b55843",
  "form": "五律",
