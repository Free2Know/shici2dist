{
  "_id": "8a2ef301c548481d94aebd5f84c904c4",
  "name": "中秋",
  "contents": [
    "清香闻晓莲，",
    "水国雨余天。",
    "天气正得所，",
    "客心刚悄然。",
    "乱兵何日息，",
    "故老几人全。",
    "此际难消遣，",
    "从来未学禅。"
  ],
  "ptype": null,
  "poetName": "郑谷",
  "poetId": "87cb83f765db4575a2bdcb67a72bc6d7",
