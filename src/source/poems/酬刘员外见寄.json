  "_id": "ca88f49804424d48a7c6141fe18937fe",
  "name": "酬刘员外见寄",
  "contents": [
    "苏耽佐郡时，",
    "近出白云司。",
    "药补清羸疾，",
    "窗吟绝妙词。",
    "柳塘春水慢，",
    "花坞夕阳迟。",
    "欲识怀君意，",
    "明朝访楫师。"
  ],
  "ptype": null,
  "poetName": "严维",
  "poetId": "2cf32a5a735b45d6b3a6931b3c38faa3",
