  "_id": "7523e89600154ed5a6413026d3760016",
  "name": "寄刘钧秀才",
  "contents": [
    "掩户当春昼，",
    "知君志在诗。",
    "闲花半落处，",
    "幽客未来时。",
    "野鸟穿莎径，",
    "江云过竹篱。",
    "会须明月夜，",
    "与子水边期。"
  ],
  "ptype": null,
  "poetName": "李中",
  "poetId": "5b8a00819d324861a0a7ee4ab6bd3e48",
