{
  "_id": "399e3c7e387d41f8a4c50201b73b302c",
  "name": "送陆秀才归觐省",
  "contents": [
    "武陵何处在，",
    "南指楚云阴。",
    "花萼连枝近，",
    "桃源去路深。",
    "啼莺徒寂寂，",
    "征马已駸駸。",
    "堤上千年柳，",
    "条条挂我心。"
  ],
  "ptype": null,
  "poetName": "戎昱",
  "poetId": "1875ef3b39b9441ea237f7168c2db830",
  "form": "五律",
