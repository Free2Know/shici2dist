{
  "_id": "0171e1d841cb4e5493d116adfb4f90c2",
  "name": "戊辰岁江南感怀",
  "contents": [
    "忽忽动中私，",
    "人间何所之。",
    "老过离乱世，",
    "生在太平时。",
    "桃李春无主，",
    "杉松寺有期。",
    "曾吟子山赋，",
    "何啻旧凌迟。"
  ],
  "ptype": null,
  "poetName": "齐己",
  "poetId": "759693a9a6584b838ea6a6bfa0b9e490",
