{
  "_id": "b2e436a4ad814c219a1e5a260b21768f",
  "name": "周先生",
  "contents": [
    "寥寥空山岑，",
    "冷冷风松林。",
    "流月垂鳞光，",
    "悬泉扬高音。",
    "希夷周先生，",
    "烧香调琴心。",
    "神力盈三千，",
    "谁能还黄金。"
  ],
  "ptype": null,
  "poetName": "元稹",
  "poetId": "e04ddf3e16fd40a3bc351fd163098dda",
