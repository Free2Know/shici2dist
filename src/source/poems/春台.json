  "_id": "f25b3e092e0b4da09a8479de460b07eb",
  "name": "春台",
  "contents": [
    "台上树阴合，",
    "台前流水多。",
    "青春不出门，",
    "坐见野田花。",
    "谁能学公子，",
    "走马逐香车。",
    "六街尘满衣，",
    "鼓绝方还家。"
  ],
  "ptype": null,
  "poetName": "刘驾",
  "poetId": "41306bf7eb664addb27d8b1258f18b40",
