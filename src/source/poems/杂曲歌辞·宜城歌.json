{
  "_id": "39ea278fa09b431a982df1676f2f6f40",
  "name": "杂曲歌辞·宜城歌",
  "contents": [
    "野水绕空城，",
    "行尘起孤驿。",
    "花台侧生树，",
    "石碣阳镌额。",
    "靡靡度行人，",
    "温风吹宿麦。",
    "知"
  ],
  "ptype": null,
  "poetName": "刘禹锡",
  "poetId": "f708c5d8cf5d4324bdc3a709ea718fd3",
