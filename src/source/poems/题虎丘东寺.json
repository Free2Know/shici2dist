  "_id": "5fc465670eef4662bc77951d93dda9b3",
  "name": "题虎丘东寺",
  "contents": [
    "云树拥崔嵬，",
    "深行异俗埃。",
    "寺门山外入，",
    "石壁地中开。",
    "仰砌池光动，",
    "登楼海气来。",
    "伤心万古意，",
    "金玉葬寒灰。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "3a86db0ed7e4461794e5c0aa5ffb339c",
