{
  "_id": "b118f6a09c444bc08d9be26f3cb1d1fa",
  "name": "客思吟",
  "contents": [
    "禾黍正离离，",
    "南园剪白芝。",
    "细腰沈赵女，",
    "高髻唱蛮姬。",
    "路愧前冈月，",
    "梳惭一颔丝。",
    "乡人不可语，",
    "独念畏人知。"
  ],
  "ptype": null,
  "poetName": "杨敬之",
  "poetId": "6f85f2fe72b1420b939fc8615c256d8b",
  "form": "五律",
