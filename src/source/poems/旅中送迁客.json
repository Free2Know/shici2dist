  "_id": "a3f2ab7254304ed29f43e2a73c53e4ab",
  "name": "旅中送迁客",
  "contents": [
    "天南不可去，",
    "君去吊灵均。",
    "落日青山路，",
    "秋风白发人。",
    "言危无继者，",
    "道在有明神。",
    "满目尽胡越，",
    "平生何处陈。"
  ],
  "ptype": null,
  "poetName": "吴融",
  "poetId": "c8fcad51e3264dc8a7c8162344d3b0e9",
