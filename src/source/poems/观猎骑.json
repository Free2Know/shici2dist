{
  "_id": "193d6511e69443e98a764eb1e2de4e60",
  "name": "观猎骑",
  "contents": [
    "缠臂绣纶巾，",
    "貂裘窄称身。",
    "射禽风助箭，",
    "走马雪翻尘。",
    "金埒争开道，",
    "香车为驻轮。",
    "翩翩不知处，",
    "传是霍家亲。"
  ],
  "ptype": null,
  "poetName": "司空曙",
  "poetId": "5790ee951f5a4a438124284e6c69ee7a",
  "form": "五律",
