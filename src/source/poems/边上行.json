{
  "_id": "df5e02eea5b04f1ab24fb95b0d9b9fd6",
  "name": "边上行",
  "contents": [
    "黑松林外路，",
    "风角远嗈嗈。",
    "朔气生荒堡，",
    "秋尘满病容。",
    "豺掊沙底骨，",
    "人上月边烽。",
    "休作西行计，",
    "西行地渐凶。"
  ],
  "ptype": null,
  "poetName": "贯休",
  "poetId": "35ef826ebca24b318ada7e612c75eda6",
