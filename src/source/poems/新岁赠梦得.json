  "_id": "da7f216e73e143ab8bc277de51349999",
  "name": "新岁赠梦得",
  "contents": [
    "暮齿忽将及，",
    "同心私自怜。",
    "渐衰宜减食，",
    "已喜更加年。",
    "紫绶行联袂，",
    "篮舆出比肩。",
    "与君同甲子，",
    "岁酒合谁先？"
  ],
  "ptype": null,
  "poetName": "白居易",
  "poetId": "61c661a769a04e409e99836b8f4a8f79",
