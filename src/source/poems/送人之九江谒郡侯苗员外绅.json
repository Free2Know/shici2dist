{
  "_id": "170a7dda572346479d6b2471eaffd829",
  "name": "送人之九江谒郡侯苗员外绅",
  "contents": [
    "泽国寻知己，",
    "南浮不偶游。",
    "湓城分楚塞，",
    "庐岳对江州。",
    "晓饭临孤屿，",
    "春帆入乱流。",
    "双旌相望处，",
    "月白庾公楼。"
  ],
  "ptype": null,
  "poetName": "郑谷",
  "poetId": "87cb83f765db4575a2bdcb67a72bc6d7",
