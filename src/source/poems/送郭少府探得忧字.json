{
  "_id": "2c6ffad639754d3f88d6c4e7d4f71798",
  "name": "送郭少府探得忧字",
  "contents": [
    "开筵枕德水，",
    "辍棹舣仙舟。",
    "贝阙桃花浪，",
    "龙门竹箭流。",
    "当歌凄别曲，",
    "对酒泣离忧。",
    "还望青门外，",
    "空见白云浮。"
  ],
  "ptype": null,
  "poetName": "骆宾王",
  "poetId": "fc99ba1506f7440b99b0946b24173672",
  "form": "五律",
