{
  "_id": "dc0a3e161021487a9a032a6d0007d236",
  "name": "山中兴作",
  "contents": [
    "高高丹桂枝，",
    "袅袅女萝衣。",
    "密叶浮云过，",
    "幽阴暮鸟归。",
    "月和风翠动，",
    "花落瀑泉飞。",
    "欲剪兰为佩，",
    "中林露未(日希)。"
  ],
  "ptype": null,
  "poetName": "马戴",
  "poetId": "db14ec8dfa7a4a92987b2e2357aaa5b6",
