{
  "_id": "c3f57c2cbc4649d0b99a307f12943157",
  "name": "纸",
  "contents": [
    "妙迹蔡侯施，",
    "芳名左伯驰。",
    "云飞锦绮落，",
    "花发缥红披。",
    "舒卷随幽显，",
    "廉方合轨仪。",
    "莫惊反掌字，",
    "当取葛洪规。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "5f3c01f5ddab4ebdb2d9748023f40391",
  "form": "五律",
