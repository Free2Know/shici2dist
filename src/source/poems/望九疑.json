{
  "_id": "32d7cc4d9c1643f4884279b10390c243",
  "name": "望九疑",
  "contents": [
    "浮生犹役役，",
    "未得便寻真。",
    "白日如无路，",
    "青山岂有人。",
    "烟收遥岫小，",
    "雨过晚川新。",
    "倚杖何凝望，",
    "中宵梦往频。"
  ],
  "ptype": null,
  "poetName": "朱庆馀",
