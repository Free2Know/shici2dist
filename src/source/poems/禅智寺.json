{
  "_id": "0cbdd03ebe694a5d8faa31acc35a1d8b",
  "name": "禅智寺",
  "contents": [
    "宝殿依山嶮，",
    "临虚势若吞。",
    "画檐齐木末，",
    "香砌压云根。",
    "远景窗中岫，",
    "孤烟竹里村。",
    "凭高聊一望，",
    "乡思隔吴门。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "3a86db0ed7e4461794e5c0aa5ffb339c",
