{
  "_id": "116f910a4d494bf4ae612b58e9956ce4",
  "name": "赠杨先辈",
  "contents": [
    "平生闲放久，",
    "野鹿许为群。",
    "居止邻西岳，",
    "轩窗度白云。",
    "斋心饭松子，",
    "话道接茅君。",
    "汉主思清净，",
    "休书谏猎文。"
  ],
  "ptype": null,
  "poetName": "马戴",
  "poetId": "db14ec8dfa7a4a92987b2e2357aaa5b6",
  "form": "五律",
