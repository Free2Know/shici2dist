{
  "_id": "4988a708a497470b9cd144ada81c1a98",
  "name": "代边将",
  "contents": [
    "持戈簇边日，",
    "战罢浮云收。",
    "露草泣寒霁，",
    "夜泉鸣陇头。",
    "三尺握中铁，",
    "气冲星斗牛。",
    "报国不拘贵，",
    "愤将平虏雠。"
  ],
  "ptype": null,
  "poetName": "贾岛",
  "poetId": "03638acd71554861ad66405ecf420eb4",
