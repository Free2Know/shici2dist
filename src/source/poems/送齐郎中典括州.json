{
  "_id": "2feaafc92fa14d22a7c9e76ed973827e",
  "name": "送齐郎中典括州",
  "contents": [
    "星象移何处，",
    "旌麾独向东。",
    "劝耕沧海畔，",
    "听讼白云中。",
    "树色双溪合，",
    "猿声万岭同。",
    "石门康乐住，",
    "几里枉帆通。"
  ],
  "ptype": null,
  "poetName": "刘长卿",
  "poetId": "8fb91cedc6274daba50e589c0dde93b7",
  "form": "五律",
