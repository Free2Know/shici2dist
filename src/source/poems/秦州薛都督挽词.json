{
  "_id": "350b2b5614264ce0a1dd227f3331faed",
  "name": "秦州薛都督挽词",
  "contents": [
    "十里绛山幽，",
    "千年汾水流。",
    "碑传门客见，",
    "剑是故人留。",
    "陇树烟含夕，",
    "山门月照秋。",
    "古来钟鼎盛，",
    "共尽一蒿丘。"
  ],
  "ptype": null,
  "poetName": "崔湜",
  "poetId": "62c13d270dfe4c1fafb71a4e6f7a02ab",
  "form": "五律",
