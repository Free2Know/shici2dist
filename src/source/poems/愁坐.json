  "_id": "3d5d4c5165b24197b1fff747a8bc1a1e",
  "name": "愁坐",
  "contents": [
    "高斋常见野，",
    "愁坐更临门。",
    "十月山寒重，",
    "孤城月水昏。",
    "葭萌氐种迥，",
    "左担犬戎存。",
    "终日忧奔走，",
    "归期未敢论。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
  "form": "五律",
