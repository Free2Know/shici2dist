{
  "_id": "dbe592dd8c21416fb09c0d7fcc4f6c5c",
  "name": "东湖赠僧子兰",
  "contents": [
    "名利了无时，",
    "何人暂访师。",
    "道情闲外见，",
    "心地语来知。",
    "竹落穿窗叶，",
    "松寒荫井枝。",
    "匡山许同社，",
    "愿卜挂帆期。"
  ],
  "ptype": null,
  "poetName": "张乔",
  "poetId": "746ea2e1efcd41db9a5d67eddf753f46",
  "form": "五律",
