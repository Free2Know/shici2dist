  "_id": "ec54f6b284194566a58bc98e05950824",
  "name": "伤韦宾客",
  "contents": [
    "韦公八十馀，",
    "位至六尚书。",
    "五福唯无富，",
    "一生谁得如。",
    "桂枝攀最久，",
    "兰省出仍初。",
    "海内时流尽，",
    "何人动素车。"
  ],
  "ptype": null,
  "poetName": "刘禹锡",
