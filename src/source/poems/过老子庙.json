{
  "_id": "cdb3658a792b4c4192eee8f3a7ae0c3e",
  "name": "过老子庙",
  "contents": [
    "仙居怀圣德，",
    "灵庙肃神心。",
    "草合人踪断，",
    "尘浓鸟迹深。",
    "流沙丹灶没，",
    "关路紫烟沉。",
    "独伤千载后，",
    "空馀松柏林。"
  ],
  "ptype": null,
  "poetName": "李隆基",
  "poetId": "5da139a0d736493ea446ab4eabc2d54c",
  "form": "五律",
