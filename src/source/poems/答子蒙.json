  "_id": "744aaa1067504782bb35b6ce341ca69f",
  "name": "答子蒙",
  "contents": [
    "报卢君，",
    "门外雪纷纷。",
    "纷纷门外雪，",
    "城中鼓声绝。",
    "强梁御史人觑步，",
    "安得夜开沽酒户。"
  ],
  "ptype": null,
  "poetName": "元稹",
