  "_id": "ece3c312393741098ec9189dc0d952c9",
  "name": "过洞庭湖",
  "contents": [
    "惊波常不定，",
    "半日鬓堪斑。",
    "四顾疑无地，",
    "中流忽有山。",
    "鸟高恒畏坠，",
    "帆远却如闲。",
    "渔父闲相引，",
    "时歌浩渺间。"
  ],
  "ptype": null,
  "poetName": "许棠",
  "poetId": "79e0a1e55d8d45d6a3aa421b0ade4bd3",
