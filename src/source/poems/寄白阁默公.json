  "_id": "f031afbf782f431aac49a7c3d2585f73",
  "name": "寄白阁默公",
  "contents": [
    "已知归白阁，",
    "山远晚晴看。",
    "石室人心静，",
    "冰潭月影残。",
    "微云分片灭，",
    "古木落薪干。",
    "后夜谁闻磬，",
    "西峰绝顶寒。"
  ],
  "ptype": null,
  "poetName": "贾岛",
  "poetId": "03638acd71554861ad66405ecf420eb4",
