{
  "_id": "574755f928ab4c8a9a623e30e969f608",
  "name": "送程德琳郎中学士",
  "contents": [
    "瓜步妖氛灭，",
    "昆冈草树青。",
    "终朝空望极，",
    "今日送君行。",
    "报政秋云静，",
    "微吟晓月生。",
    "楼中长可见，",
    "特用灭离情。"
  ],
  "ptype": null,
  "poetName": "徐锴",
  "poetId": "d28c6e2a165747a989322d226dd185e5",
  "form": "五律",
