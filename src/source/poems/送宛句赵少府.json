{
  "_id": "99e71e44203949b99bc7a60e1d5cbea6",
  "name": "送宛句赵少府",
  "contents": [
    "解巾行作吏，",
    "尊酒谢离居。",
    "修竹含清景，",
    "华池澹碧虚。",
    "地将幽兴惬，",
    "人与旧游疏。",
    "林下纷相送，",
    "多逢长者车。"
  ],
  "ptype": null,
  "poetName": "张九龄",
  "poetId": "a899cc0a679f4d6cadeee32c92c54761",
  "form": "五律",
