{
  "_id": "b946beeef1a1414b8241fc88aa9b770c",
  "name": "新春江次",
  "contents": [
    "浦干潮未应，",
    "堤湿冻初销。",
    "粉片妆梅朵，",
    "金丝刷柳条。",
    "鸭头新绿水，",
    "雁齿小红桥。",
    "莫怪珂声碎，",
    "春来五马骄。"
  ],
  "ptype": null,
  "poetName": "白居易",
  "poetId": "61c661a769a04e409e99836b8f4a8f79",
