{
  "_id": "7fce1414dda9470d9bd3692aa3f64eb9",
  "name": "咏星",
  "contents": [
    "历历东井舍，",
    "昭昭右掖垣。",
    "云际龙文出，",
    "池中鸟色翻。",
    "流辉下月路，",
    "坠影入河源。",
    "方知颍川集，",
    "别有太丘门。"
  ],
  "ptype": null,
  "poetName": "董思恭",
  "poetId": "961dbace4d88411998ce3c5cafc079a0",
