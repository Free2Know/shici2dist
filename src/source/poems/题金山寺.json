  "_id": "333fed89a6e043e8a375e78b5bd886d6",
  "name": "题金山寺",
  "contents": [
    "四面波涛匝，",
    "中楼日月邻。",
    "上穷如出世，",
    "下瞷忽惊神。",
    "刹碍长空鸟，",
    "船通外国人。",
    "房房皆叠石，",
    "风扫永无尘。"
  ],
  "ptype": null,
  "poetName": "许棠",
