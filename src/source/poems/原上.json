  "_id": "d38356386a644803aa51c9aa471c273a",
  "name": "原上",
  "contents": [
    "危桥横古渡，",
    "村野带平林。",
    "野鹜寒塘静，",
    "山禽晓树深。",
    "雨微风矗□，",
    "云暗雪侵寻。",
    "安道门前水，",
    "清游岂独吟。"
  ],
  "ptype": null,
  "poetName": "唐彦谦",
  "poetId": "bed3d2dc8e014c25950dab9d7fad3ef4",
