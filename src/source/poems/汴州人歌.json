{
  "_id": "73b4aa16e5694c608d514fa1fb8b8fae",
  "name": "汴州人歌",
  "contents": [
    "浊流洋洋，",
    "有辟其郛。",
    "阗道嚾呼，",
    "公来之初。",
    "今公之归，",
    "公在丧车。",
    "公既来止，",
    "东人以完。",
    "今公殁矣，",
    "人谁与安？"
  ],
  "ptype": null,
  "poetName": "唐无名氏",
  "poetId": "5fb2c729b42a4746a5b376b70c9d5229",
