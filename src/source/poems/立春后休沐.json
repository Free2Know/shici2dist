  "_id": "a1bc35b00ca4434d8c77cec167fdada0",
  "name": "立春后休沐",
  "contents": [
    "心与青春背，",
    "新年亦掩扉。",
    "渐穷无相学，",
    "惟避不材讥。",
    "积病攻难愈，",
    "衔恩报转微。",
    "定知书课日，",
    "优诏许辞归。"
  ],
  "ptype": null,
  "poetName": "包佶",
  "poetId": "c367a308996045d4a91f2ec3f6ade755",
