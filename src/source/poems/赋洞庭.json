  "_id": "1f669ad19e2b434fb37cdacca2fc49c5",
  "name": "赋洞庭",
  "contents": [
    "周极八百里，",
    "凝眸望则劳。",
    "水涵天影阔，",
    "山拔地形高。",
    "贾客停非久，",
    "渔翁转几遭。",
    "飒然风起处，",
    "又是鼓波涛。"
  ],
  "ptype": null,
  "poetName": "可朋",
  "poetId": "7ee987456c4c4ca5877d0f8e4e5e81ed",
