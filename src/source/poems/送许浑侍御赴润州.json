  "_id": "a107d797d0484d5ead775fdf75ca8599",
  "name": "送许浑侍御赴润州",
  "contents": [
    "家山近石头，",
    "遂意恣东游。",
    "祖席离乌府，",
    "归帆转蜃楼。",
    "阴氛出海散，",
    "落月向潮流。",
    "别有为霖日，",
    "孤云未自由。"
  ],
  "ptype": null,
  "poetName": "李频",
  "poetId": "427dc1231d8140679d9bcf34613ef360",
