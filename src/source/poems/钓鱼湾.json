  "_id": "5e3d811ad4a54285a33675027a248603",
  "name": "钓鱼湾",
  "contents": [
    "垂钓绿湾春，",
    "春深杏花乱。",
    "潭清疑水浅，",
    "荷动知鱼散。",
    "日暮待情人，",
    "维舟绿杨岸。"
  ],
  "ptype": null,
  "poetName": "储光羲",
  "poetId": "05d2da3113d344b2b2150e220400d08f",
  "form": "五律",
