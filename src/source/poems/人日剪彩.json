{
  "_id": "4d174dcaab9a4bbbbd8ce4a2e7569d59",
  "name": "人日剪彩",
  "contents": [
    "闺妇持刀坐，",
    "自怜裁剪新。",
    "叶催情缀色，",
    "花寄手成春。",
    "帖燕留妆户，",
    "黏鸡待饷人。",
    "擎来问夫婿，",
    "何处不如真。"
  ],
  "ptype": null,
  "poetName": "徐延寿",
  "poetId": "aded2cd202f54625a947e5aa3494699a",
