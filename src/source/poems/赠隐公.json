{
  "_id": "f7ee584f2ee4487db3a5e0be80c71893",
  "name": "赠隐公",
  "contents": [
    "世间无近远，",
    "定里遍曾过。",
    "东海经长在，",
    "南朝寺最多。",
    "暮年聊化俗，",
    "初地即摧魔。",
    "今日忘尘虑，",
    "看心义若何。"
  ],
  "ptype": null,
  "poetName": "耿湋",
  "poetId": "d62460239e1244dcad61df818057bd89",
