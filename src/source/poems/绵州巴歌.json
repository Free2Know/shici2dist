  "_id": "2302568c8acd495ea33b5ee0a5dfb190",
  "name": "绵州巴歌",
  "contents": [
    "子山，",
    "打瓦鼓。",
    "扬平山，",
    "撒白雨。",
    "下白雨，",
    "取龙女。",
    "织得绢，",
    "二丈五。",
    "一半属罗江，",
    "一半属玄武。"
  ],
  "ptype": null,
  "poetName": "隋无名氏",
