{
  "_id": "4abdc5cae7a7468bb5519050b4b0e83d",
  "name": "读始兴公传",
  "contents": [
    "殁世议方存，",
    "升平道几论。",
    "诗情光日月，",
    "笔力动乾坤。",
    "乱首光雄算，",
    "朝纲在典坟。",
    "明时封禅绩，",
    "山下见丘门。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "3a86db0ed7e4461794e5c0aa5ffb339c",
