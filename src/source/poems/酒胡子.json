  "_id": "757eebd29bf5472a9a68687312a437a2",
  "name": "酒胡子",
  "contents": [
    "红筵丝竹合，",
    "用尔作欢娱。",
    "直指宁偏党，",
    "无私绝觊觎。",
    "当歌谁擐袖，",
    "应节渐轻躯。",
    "恰与真相似，",
    "毡裘满颔须。"
  ],
  "ptype": null,
  "poetName": "徐夤",
