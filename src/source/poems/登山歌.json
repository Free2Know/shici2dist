  "_id": "cfccb53fd7354f2c9b803d189871e878",
  "name": "登山歌",
  "contents": [
    "青山前，",
    "青山后，",
    "登高望两处，",
    "两处今何有。",
    "烟景满川原，",
    "离人堪白首。"
  ],
  "ptype": null,
  "poetName": "皇甫冉",
