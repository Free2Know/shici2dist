{
  "_id": "f6d93ced56294fef8f031b0a9cea3149",
  "name": "送外甥",
  "contents": [
    "衰年生侄少，",
    "唯尔最关心。",
    "偶作魏舒别，",
    "聊为殷浩吟。",
    "白波舟不定，",
    "黄叶路难寻。",
    "自此尊中物，",
    "谁当更共斟。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "3a86db0ed7e4461794e5c0aa5ffb339c",
