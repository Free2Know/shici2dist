{
  "_id": "bf99e131ebb042879d74325a47cd078d",
  "name": "赓歌",
  "contents": [
    "采江之鱼兮，",
    "朝船有鲈。",
    "采江之蔬兮，",
    "暮筐有蒲。",
    "左图且书，",
    "右琴与壶。",
    "寿欤夭欤，",
    "贵欤贱欤。"
  ],
  "ptype": null,
  "poetName": "陆龟蒙",
  "poetId": "8441f39c230844b2bff5fac0a3703003",
