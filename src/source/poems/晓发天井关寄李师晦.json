{
  "_id": "da3a9d6fc7b6449e95b2a02fdff07f6a",
  "name": "晓发天井关寄李师晦",
  "contents": [
    "山在水滔滔，",
    "流年欲二毛。",
    "湘潭归梦远，",
    "燕赵客程劳。",
    "露晓红兰重，",
    "云晴碧树高。",
    "逢秋正多感，",
    "万里别同袍。"
  ],
  "ptype": null,
  "poetName": "许浑",
  "poetId": "ba84f30a9b344330943f2fbe4d128d8d",
  "form": "五律",
