{
  "_id": "c7965dc5fc3046ef866ef14a457debaf",
  "name": "误桃源",
  "contents": [
    "砥柱勒铭赋，",
    "本赞禹功勋。",
    "试官亲处分，",
    "赞唐文。",
    "秀文冥子里，",
    "銮驾幸并汾。",
    "恰似郑州去，",
    "出曹门。"
  ],
  "ptype": null,
  "poetName": "宋无名氏",
