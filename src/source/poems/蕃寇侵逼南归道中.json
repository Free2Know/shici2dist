{
  "_id": "1d9bdefa790c49b48eaa4e5d4cb0481e",
  "name": "蕃寇侵逼南归道中",
  "contents": [
    "云州三万骑，",
    "南走疾飞鹰。",
    "回碛星低雁，",
    "孤城月伴僧。",
    "敲关通汉节，",
    "倾府守河冰。",
    "无处论边事，",
    "归溪夜结罾。"
  ],
  "ptype": null,
  "poetName": "李洞",
  "poetId": "4eeec622c9a342139df6f02d3b9f134b",
  "form": "五律",
