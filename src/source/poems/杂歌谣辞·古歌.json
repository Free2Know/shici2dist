{
  "_id": "9dd04794f2294ab88eb42c0d5afedbf3",
  "name": "杂歌谣辞·古歌",
  "contents": [
    "美人怨何深，",
    "含情倚金阁。",
    "不嚬复不语，",
    "红泪双双落。",
    "美人闭红烛，",
    "烛坐裁新锦。",
    "频放翦刀声，",
    "夜寒知未寝。"
  ],
  "ptype": null,
  "poetName": "薛维翰",
  "poetId": "a6b0f586d03a4a05ba173caf9ee86bd8",
  "form": "五律",
