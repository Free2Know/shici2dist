{
  "_id": "87d8822f0b5e48f7914c5bc5bc26e822",
  "name": "奉陪韦润州游鹤林寺",
  "contents": [
    "野寺江城近，",
    "双旌五马过。",
    "禅心超忍辱，",
    "梵语问多罗。",
    "松竹闲僧老，",
    "云烟晚日和。",
    "寒塘归路转，",
    "清磬隔微波。"
  ],
  "ptype": null,
  "poetName": "李嘉祐",
  "poetId": "c256d35ea8de4cbb869292361f80ed95",
  "form": "五律",
