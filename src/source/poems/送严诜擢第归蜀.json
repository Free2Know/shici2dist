  "_id": "831d84975d1e4ee5bf518cdaa402b880",
  "name": "送严诜擢第归蜀",
  "contents": [
    "巴江秋月新，",
    "阁道发征轮。",
    "战胜真才子，",
    "名高功世人。",
    "工文能似舅，",
    "擢第去荣亲。",
    "十月天官待，",
    "应须早赴秦。"
  ],
  "ptype": null,
  "poetName": "岑参",
  "poetId": "77a58f32ae514e76913c578aae0230d3",
