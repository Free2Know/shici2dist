  "_id": "9edaa6f18b99461a80cf4a418eecd16a",
  "name": "赠郓州马使君",
  "contents": [
    "东方千万骑，",
    "出望使君时。",
    "暮雪行看尽，",
    "春城到莫迟。",
    "路人趋墨帻，",
    "官柳度青丝。",
    "他日铃斋内，",
    "知君亦赋诗。"
  ],
  "ptype": null,
  "poetName": "韩翃",
  "poetId": "135a4148450d4726a0b138479ebc3f64",
  "form": "五律",
