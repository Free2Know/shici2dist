{
  "_id": "41455b3b4e0f4e3a9e1cf0e230ad9243",
  "name": "幸秦始皇陵",
  "contents": [
    "眷言君失德，",
    "骊邑想秦馀。",
    "政烦方改篆，",
    "愚俗乃焚书。",
    "阿房久已灭，",
    "阁道遂成墟。",
    "欲厌东南气，",
    "翻伤掩鲍车。"
  ],
  "ptype": null,
  "poetName": "李显",
  "poetId": "00fdca0d240947e2b001e6c7410f4e01",
  "form": "五律",
