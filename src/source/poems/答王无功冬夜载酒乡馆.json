{
  "_id": "733cafc02b244292abf7a3c8949b5b75",
  "name": "答王无功冬夜载酒乡馆",
  "contents": [
    "颁条忝贵郡，",
    "悬榻久相望。",
    "处士同杨郑，",
    "邦君谢李疆。",
    "讵知方拥彗，",
    "逢子敬惟桑。",
    "明朝蓬户侧，",
    "会自谒任棠。"
  ],
  "ptype": null,
  "poetName": "崔善为",
  "poetId": "9ee5df81c6da4f33be4e0d5312787bcc",
  "form": "五律",
