{
  "_id": "f929b576c31b49a2a328e16b3c34f1d0",
  "name": "酬元秘书晚出蓝溪见寄",
  "contents": [
    "野兴引才子，",
    "独行幽径迟。",
    "云留下山处，",
    "鸟静出溪时。",
    "拙宦不忘隐，",
    "归休常在兹。",
    "知音倘相访，",
    "炊黍扫茅茨。"
  ],
  "ptype": null,
  "poetName": "钱起",
  "poetId": "a0ef0c5bc91742a1842f4e8974008f2f",
  "form": "五律",
