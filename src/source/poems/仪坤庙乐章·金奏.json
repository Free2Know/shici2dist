  "_id": "def0def203cb47b9a1def110f7cd9382",
  "name": "仪坤庙乐章·金奏",
  "contents": [
    "阴灵效祉，",
    "轩曜降精。",
    "祥符淑气，",
    "庆集柔明。",
    "瑶俎既列，",
    "雕桐发声。",
    "徽猷永远，",
    "比德皇英。"
  ],
  "ptype": null,
  "poetName": "徐彦伯",
