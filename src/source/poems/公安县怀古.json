{
  "_id": "a6c8c6472eb7488a8fb8970c93c5ecb6",
  "name": "公安县怀古",
  "contents": [
    "野旷吕蒙营，",
    "江深刘备城。",
    "寒天催日短，",
    "风浪与云平。",
    "洒落君臣契，",
    "飞腾战伐名。",
    "维舟倚前浦，",
    "长啸一含情。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
  "form": "五律",
