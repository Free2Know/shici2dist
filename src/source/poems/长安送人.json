  "_id": "7ad50643d2104f22afa9f72e3798a5b1",
  "name": "长安送人",
  "contents": [
    "上国多离别，",
    "年年渭水滨。",
    "空将未归意，",
    "说向欲行人。",
    "雁度池塘月，",
    "山连井邑春。",
    "临岐惜分手，",
    "日暮一沾巾。"
  ],
  "ptype": null,
  "poetName": "周贺",
  "poetId": "1dcebe7978b74148a639d775fa448e03",
