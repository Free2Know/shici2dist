{
  "_id": "30fee6dfb7c64546a14528a3f9aa8613",
  "name": "效徐陵体赠更衣",
  "contents": [
    "密帐真珠络，",
    "温帏翡翠装。",
    "楚腰知便宠，",
    "宫眉正斗强。",
    "结带悬栀子，",
    "绣领刺鸳鸯。",
    "轻寒衣省夜，",
    "金斗熨沈香。"
  ],
  "ptype": null,
  "poetName": "李商隐",
  "poetId": "2771bb31ded849bdac8207fbb5785a3a",
  "form": "五律",
