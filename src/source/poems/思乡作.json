{
  "_id": "22bc5acb04d14384a29ef5c0b44a66f3",
  "name": "思乡作",
  "contents": [
    "泸北行人绝，",
    "云南信未还。",
    "庭前花不扫，",
    "门外柳谁攀。",
    "坐久销银烛，",
    "愁多减玉颜。",
    "悬心秋夜月，",
    "万里照关山。"
  ],
  "ptype": null,
  "poetName": "布燮",
  "poetId": "16c79f53e1ba4f1cbcb8d20ae7c85b49",
