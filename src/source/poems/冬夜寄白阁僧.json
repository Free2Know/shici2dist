  "_id": "0b0be5775aa144288b5675da5d324b35",
  "name": "冬夜寄白阁僧",
  "contents": [
    "营营不自息，",
    "暌阔数年情。",
    "林下期难遂，",
    "人间事旋生。",
    "鸟栖寒水迥，",
    "月映积冰清。",
    "石室焚香坐，",
    "悬知不为名。"
  ],
  "ptype": null,
  "poetName": "刘得仁",
  "poetId": "d023386d977440f8bbea3bbd47049cbb",
