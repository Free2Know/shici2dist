  "_id": "1e573ddd61ac40489b536513d10558d5",
  "name": "别灵武令狐校书",
  "contents": [
    "北风吹别思，",
    "落月度关河。",
    "树隐流沙短，",
    "山平近塞多。",
    "雁池戎马饮，",
    "雕帐戍人过。",
    "莫虑行军苦，",
    "华夷道正和。"
  ],
  "ptype": null,
  "poetName": "马戴",
  "poetId": "db14ec8dfa7a4a92987b2e2357aaa5b6",
  "form": "五律",
