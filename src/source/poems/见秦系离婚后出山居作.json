{
  "_id": "46f456f3fa914e0587e9f92633d56396",
  "name": "见秦系离婚后出山居作",
  "contents": [
    "岂知偕老重，",
    "垂老绝良姻。",
    "郗氏诚难负，",
    "朱家自愧贫。",
    "绽衣留欲故，",
    "织锦罢经春。",
    "何况蘼芜绿，",
    "空山不见人。"
  ],
  "ptype": null,
  "poetName": "刘长卿",
  "poetId": "8fb91cedc6274daba50e589c0dde93b7",
  "form": "五律",
