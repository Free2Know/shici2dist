{
  "_id": "0379e55a5f3f4db39803083fb8dcec7a",
  "name": "题李三书斋",
  "contents": [
    "灼灼青春仲。",
    "悠悠白日升。",
    "声容何足恃。",
    "荣吝坐相矜。",
    "愿与金庭会。",
    "将待玉书征。",
    "还丹应有术。",
    "烟驾共君乘。"
  ],
  "ptype": null,
  "poetName": "陈子昂",
  "poetId": "4f8b9816b598482bb7011d0ac90378f3",
