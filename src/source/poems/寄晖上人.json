{
  "_id": "f30c7b33db2041faade62c2ee602e46b",
  "name": "寄晖上人",
  "contents": [
    "日出月复没，",
    "悠悠昏与明。",
    "修持经几劫，",
    "清净到今生。",
    "林下知无相，",
    "人间苦是情。",
    "终期逐师去，",
    "不拟老尘缨。"
  ],
  "ptype": null,
  "poetName": "姚合",
  "poetId": "9a0ddeb7f13a437d8090ccf3996de1f3",
