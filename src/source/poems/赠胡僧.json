  "_id": "e934d2f36e364ad3b8d53e5157ca9a49",
  "name": "赠胡僧",
  "contents": [
    "瘦形无血色，",
    "草屦著行穿。",
    "闲话似持咒，",
    "不眠同坐禅。",
    "背经来汉地，",
    "袒膊过冬天。",
    "情性人难会，",
    "游方应信缘。"
  ],
  "ptype": null,
  "poetName": "周贺",
  "poetId": "1dcebe7978b74148a639d775fa448e03",
