{
  "_id": "64f2daf1299e45db89cb4bd6917ac0ef",
  "name": "经宣城元员外山居",
  "contents": [
    "无人袭仙隐，",
    "石室闭空山。",
    "避烧猿犹到，",
    "随云鹤不还。",
    "涧荒岩影在，",
    "桥断树阴闲。",
    "但有黄河赋，",
    "长留在世间。"
  ],
  "ptype": null,
  "poetName": "张乔",
  "poetId": "746ea2e1efcd41db9a5d67eddf753f46",
  "form": "五律",
