{
  "_id": "8ff5ac4d14534928addcb66d5daa93d9",
  "name": "送李长史纵之任常州",
  "contents": [
    "不与名利隔，",
    "且为江汉游。",
    "吴山本佳丽，",
    "谢客旧淹留。",
    "狭道通陵口，",
    "贫家住蒋州。",
    "思归复怨别，",
    "寥落讵关秋。"
  ],
  "ptype": null,
  "poetName": "戴叔伦",
  "poetId": "b045bb7d35dd4370aad75204e2b55843",
  "form": "五律",
