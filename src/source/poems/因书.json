{
  "_id": "16bd92f49a164dc4a9d6cf053f872bbb",
  "name": "因书",
  "contents": [
    "绝徼南通栈，",
    "孤城北枕江。",
    "猿声连月槛，",
    "鸟影落天窗。",
    "海石分棋子，",
    "郫筒当酒缸。",
    "生归话辛苦，",
    "别夜对凝釭。"
  ],
  "ptype": null,
  "poetName": "李商隐",
  "poetId": "2771bb31ded849bdac8207fbb5785a3a",
