  "_id": "5b22e273f3ff4e338fa98350dbc706f0",
  "name": "题李山人幽居",
  "contents": [
    "襄阳南郭外，",
    "茅屋一书生。",
    "无事焚香坐，",
    "有时寻竹行。",
    "画苔藤杖细，",
    "踏石笋鞋轻。",
    "应笑风尘客，",
    "区区逐世名。"
  ],
  "ptype": null,
  "poetName": "张籍",
  "poetId": "0810aa6ff9a34a96aba9c12caed115ae",
