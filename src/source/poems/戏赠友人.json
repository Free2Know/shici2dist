  "_id": "a09db04bc09844a489b2690268e81e93",
  "name": "戏赠友人",
  "contents": [
    "一日不作诗，",
    "心源如废井。",
    "笔砚为辘轳，",
    "吟咏作縻绠。",
    "朝来重汲引，",
    "依旧得清冷。",
    "书赠同怀人，",
    "词中多苦辛。"
  ],
  "ptype": null,
  "poetName": "贾岛",
  "poetId": "03638acd71554861ad66405ecf420eb4",
