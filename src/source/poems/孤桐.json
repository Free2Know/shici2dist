{
  "_id": "19f5e88d5d16432498580eca671e33be",
  "name": "孤桐",
  "contents": [
    "天质自森森，",
    "孤高几百寻。",
    "凌霄不屈己，",
    "得地本虚心。",
    "岁老根弥壮，",
    "阳骄叶更阴。",
    "明时思解愠，",
    "愿斫五弦琴。"
  ],
  "ptype": null,
  "poetName": "王安石",
  "poetId": "96069f8ce7e24ccbb57de41bedace092",
