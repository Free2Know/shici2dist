{
  "_id": "74282d53c77640509c709cdd7c042a9b",
  "name": "送卢评事婺州省觐",
  "contents": [
    "知向东阳去，",
    "晨装见彩衣。",
    "客愁青眼别，",
    "家喜玉人归。",
    "漠漠水烟晚，",
    "萧萧枫叶飞。",
    "双溪泊船处，",
    "候吏拜胡威。"
  ],
  "ptype": null,
  "poetName": "权德舆",
  "poetId": "6f790147bfc643018268252c6189e5eb",
