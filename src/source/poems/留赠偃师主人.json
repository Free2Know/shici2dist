{
  "_id": "a1e831e676394d32821f18a4b38bf7c3",
  "name": "留赠偃师主人",
  "contents": [
    "孤城漏未残，",
    "徒侣拂征鞍。",
    "洛北去游远，",
    "淮南归梦阑。",
    "晓灯回壁暗，",
    "晴雪卷帘寒。",
    "强尽主人酒，",
    "出门行路难。"
  ],
  "ptype": null,
  "poetName": "许浑",
  "poetId": "ba84f30a9b344330943f2fbe4d128d8d",
  "form": "五律",
