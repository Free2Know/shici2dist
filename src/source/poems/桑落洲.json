{
  "_id": "df05a6561805400da5d7c428eff46449",
  "name": "桑落洲",
  "contents": [
    "九江寒露夕，",
    "微浪北风生。",
    "浦屿渔人火，",
    "蒹葭凫雁声。",
    "颓云晦庐岳，",
    "微鼓辨湓城。",
    "远忆天边弟，",
    "曾从此路行。"
  ],
  "ptype": null,
  "poetName": "李群玉",
  "poetId": "533477cc5b0045399dfdc5f641132458",
