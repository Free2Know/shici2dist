{
  "_id": "50c9186a7a6747329e8105f6f8b267ff",
  "name": "题贾岛墓",
  "contents": [
    "倚恃才难继，",
    "昂藏貌不恭。",
    "骑驴冲大尹，",
    "夺卷忤宣宗。",
    "驰誉超先辈，",
    "居官下我侬。",
    "司仓旧曹署，",
    "一见一心忡。"
  ],
  "ptype": null,
  "poetName": "安锜",
  "poetId": "e8ee77e2a5594397af08189f95e77416",
  "form": "五律",
