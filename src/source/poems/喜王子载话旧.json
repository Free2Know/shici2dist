  "_id": "72c62ccae5cd470380574faeb1dcf88c",
  "name": "喜王子载话旧",
  "contents": [
    "相逢青眼日，",
    "相叹白头时。",
    "累话三朝事，",
    "重看一局棋。",
    "欢娱非老大，",
    "成长是婴儿。",
    "且尽尊中物，",
    "无烦更后期。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "3a86db0ed7e4461794e5c0aa5ffb339c",
