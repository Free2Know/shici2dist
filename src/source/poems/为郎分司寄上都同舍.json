{
  "_id": "4aad1db2900b4263a20f6ad35413246c",
  "name": "为郎分司寄上都同舍",
  "contents": [
    "籍通金马门，",
    "家在铜驼陌。",
    "省闼昼无尘，",
    "宫树朝凝碧。",
    "荒街浅深辙，",
    "古渡潺湲石。",
    "唯有嵩丘云，",
    "堪夸早朝客。"
  ],
  "ptype": null,
  "poetName": "刘禹锡",
  "poetId": "f708c5d8cf5d4324bdc3a709ea718fd3",
  "form": "五律",
