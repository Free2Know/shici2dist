{
  "_id": "a423c1469ded4c93a646c6019cead3c2",
  "name": "归国遥",
  "contents": [
    "初雨歇。",
    "照水绿腰裙带熟。",
    "杨花不做人情雪。",
    "风流欲过前村蝶。",
    "羞成别。",
    "回头却恨春三月。"
  ],
  "ptype": null,
  "poetName": "刘辰翁",
