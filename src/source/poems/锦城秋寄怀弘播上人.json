{
  "_id": "8e6587e630f94ed3834e93fcc8c2be2f",
  "name": "锦城秋寄怀弘播上人",
  "contents": [
    "极顶云兼冻，",
    "孤城露洗初。",
    "共辞嵩少雪，",
    "久绝贝多书。",
    "远照雁行细，",
    "寒条狖挂虚。",
    "分泉煎月色，",
    "忆就茗林居。"
  ],
  "ptype": null,
  "poetName": "李洞",
  "poetId": "4eeec622c9a342139df6f02d3b9f134b",
  "form": "五律",
