{
  "_id": "bddddaaaee5d4fdd9d34643a30a2932d",
  "name": "祀雨师乐章·亚献终献",
  "contents": [
    "奠既备，",
    "献将终。",
    "神行令，",
    "瑞飞空。",
    "迎乾德，",
    "祈岁功。",
    "乘烟燎，",
    "俨从风。"
  ],
  "ptype": null,
  "poetName": "包佶",
  "poetId": "c367a308996045d4a91f2ec3f6ade755",
  "form": "七绝",
