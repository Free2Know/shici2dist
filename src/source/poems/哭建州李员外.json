  "_id": "92f71dc213164f1d888961e7a0cb7c41",
  "name": "哭建州李员外",
  "contents": [
    "诗名不易出，",
    "名出又何为。",
    "捷到重科早，",
    "官终一郡卑。",
    "素风无后嗣，",
    "遗迹有生祠。",
    "自罢羊公市，",
    "溪猿哭旧时。"
  ],
  "ptype": null,
  "poetName": "张蠙",
  "poetId": "97a6812b9e414e86b1d7809d0b164787",
