{
  "_id": "980b5bfe74f640efb7b5b50b866c794e",
  "name": "病中寓直代书题寄",
  "contents": [
    "愚夫何所任，",
    "多病感君深。",
    "自谓青春壮，",
    "宁知白发侵。",
    "寝兴劳善祝，",
    "疏懒愧良箴。",
    "寂寞闻宫漏，",
    "那堪直夜心。"
  ],
  "ptype": null,
  "poetName": "权德舆",
  "poetId": "6f790147bfc643018268252c6189e5eb",
  "form": "五律",
