{
  "_id": "74df6c6b5f9245f4a1e6d8b0aa8f1c2c",
  "name": "送杨府李功曹",
  "contents": [
    "平生属良友，",
    "结绶望光辉。",
    "何知人事拙，",
    "相与宦情非。",
    "别路穿林尽，",
    "征帆际海归。",
    "居然已多意，",
    "况复两乡违。"
  ],
  "ptype": null,
  "poetName": "张九龄",
  "poetId": "a899cc0a679f4d6cadeee32c92c54761",
  "form": "五律",
