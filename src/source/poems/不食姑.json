  "_id": "d619cad9c4cd47f582076b0169e16041",
  "name": "不食姑",
  "contents": [
    "几年山里住，",
    "已作绿毛身。",
    "护气常稀语，",
    "存思自见神。",
    "养龟同不食，",
    "留药任生尘。",
    "要问西王母，",
    "仙中第几人。"
  ],
  "ptype": null,
  "poetName": "张籍",
