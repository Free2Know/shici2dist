{
  "_id": "1f4d3f5d8c684c7a8a160c9cbdd5c908",
  "name": "送卫司法河中觐省",
  "contents": [
    "出身因强学，",
    "不以外家荣。",
    "年少无遗事，",
    "官闲有政声。",
    "晓山临野渡，",
    "落日照军营。",
    "共赏高堂下，",
    "连行弟与兄。"
  ],
  "ptype": null,
  "poetName": "卢纶",
  "poetId": "10008e7cd9434079b60c4f3e941d4f4f",
  "form": "五律",
