  "_id": "3bfded82a349485ca92537288124d8a9",
  "name": "寄郑谷",
  "contents": [
    "五百首新诗，",
    "缄封寄去时。",
    "只凭夫子鉴，",
    "不要俗人知。",
    "火鼠重收布，",
    "冰蚕乍吐丝。",
    "直须天上手，",
    "裁作领巾披。"
  ],
  "ptype": null,
  "poetName": "王贞白",
  "poetId": "a91d77ba2e2f457bbd9f206dacdf822a",
