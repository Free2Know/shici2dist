{
  "_id": "393f757be3db4f86b57f1632d3ed65b4",
  "name": "寄绩溪陈明府",
  "contents": [
    "古邑猿声里，",
    "空城只半存。",
    "岸移无旧路，",
    "沙涨别成村。",
    "鼓角喧京口，",
    "江山尽汝濆。",
    "六朝兴废地，",
    "行子一销魂。"
  ],
  "ptype": null,
  "poetName": "张乔",
  "poetId": "746ea2e1efcd41db9a5d67eddf753f46",
  "form": "五律",
