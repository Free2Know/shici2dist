  "_id": "ba4c559b8e7c474fb0e1478eb98c4825",
  "name": "寄白头陀",
  "contents": [
    "近见头陀伴，",
    "云师老更慵。",
    "性灵闲似鹤，",
    "颜壮古于松。",
    "山里犹难觅，",
    "人间岂易逢。",
    "仍闻移住处，",
    "太白最高峰。"
  ],
  "ptype": null,
  "poetName": "白居易",
  "poetId": "61c661a769a04e409e99836b8f4a8f79",
