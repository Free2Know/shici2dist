{
  "_id": "abf9bd4b3f7143d8a652819ec6ae3dcd",
  "name": "湓城赠别",
  "contents": [
    "楚岸青枫树，",
    "长随送远心。",
    "九江春水阔，",
    "三峡暮云深。",
    "气调桓伊笛，",
    "才华蔡琰琴。",
    "迢迢嫁湘汉，",
    "谁不重黄金。"
  ],
  "ptype": null,
  "poetName": "陈陶",
  "poetId": "ef39201e27bb4bc5ac905fba9fe1fb60",
  "form": "五律",
