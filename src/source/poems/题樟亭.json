{
  "_id": "a2c2d09882974d1c9d3a6e8fc2a53aaf",
  "name": "题樟亭",
  "contents": [
    "晓霁凭虚槛，",
    "云山四望通。",
    "地盘江岸绝，",
    "天映海门空。",
    "树色连秋霭，",
    "潮声入夜风。",
    "年年此光景，",
    "催尽白头翁。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "3a86db0ed7e4461794e5c0aa5ffb339c",
  "form": "五律",
