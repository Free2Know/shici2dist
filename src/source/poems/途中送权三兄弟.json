{
  "_id": "17ff986e8b3841b9997fb4975ad520c2",
  "name": "途中送权三兄弟",
  "contents": [
    "淮海风涛起，",
    "江关忧思长。",
    "同悲鹊绕树，",
    "独作雁随阳。",
    "山晚云初雪，",
    "汀寒月照霜。",
    "由来濯缨处，",
    "渔父爱沧浪。"
  ],
  "ptype": null,
  "poetName": "皇甫冉",
  "poetId": "2ba2e98104ff4f82b7c02921e5cd0e6a",
  "form": "五律",
