  "_id": "292d79121166416793ccf1075e68c78f",
  "name": "秋日过徐氏园林",
  "contents": [
    "回塘分越水，",
    "古树积吴烟。",
    "扫竹催铺席，",
    "垂萝待系船。",
    "鸟窥新罅栗，",
    "龟上半欹莲。",
    "屡入忘归地，",
    "长嗟俗事牵。"
  ],
  "ptype": null,
  "poetName": "包佶",
  "poetId": "c367a308996045d4a91f2ec3f6ade755",
