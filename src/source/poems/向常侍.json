  "_id": "cffc180e5a1e4535b4862aa42f5b43b2",
  "name": "向常侍",
  "contents": [
    "向秀甘淡薄，",
    "深心托豪素。",
    "探道好渊玄，",
    "观书鄙章句。",
    "交吕既鸿轩，",
    "攀嵇亦凤举。",
    "流连河里游，",
    "恻怆山阳赋。"
  ],
  "ptype": null,
  "poetName": "颜延之",
  "poetId": "9a2e9a667ff147459aa25412c8d625c5",
