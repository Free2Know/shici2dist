  "_id": "66bde73eabd241efb583cdae829e8703",
  "name": "题韩少府水亭",
  "contents": [
    "梅福幽栖处，",
    "佳期不忘还。",
    "鸟吟当户竹，",
    "花绕傍池山。",
    "水气侵阶冷，",
    "松阴覆座闲。",
    "宁知武陵趣，",
    "宛在市朝间。"
  ],
  "ptype": null,
  "poetName": "祖咏",
  "poetId": "b05b92b88f7b44fe9c6a65aaef7252d6",
