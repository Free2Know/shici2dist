{
  "_id": "975f626d2a764b7ab350622d3d801255",
  "name": "过野叟居",
  "contents": [
    "野人闲种树，",
    "树老野人前。",
    "居止白云内，",
    "渔樵沧海边。",
    "呼儿采山药，",
    "放犊饮溪泉。",
    "自著养生论，",
    "无烦忧暮年。"
  ],
  "ptype": null,
  "poetName": "马戴",
  "poetId": "db14ec8dfa7a4a92987b2e2357aaa5b6",
  "form": "五律",
