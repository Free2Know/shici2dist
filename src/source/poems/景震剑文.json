{
  "_id": "f9ad9796fb7d4c8c8b9f20471464039f",
  "name": "景震剑文",
  "contents": [
    "撝雷电，",
    "运玄星。",
    "摧凶恶，",
    "亨利贞。",
    "乾降精，",
    "坤应灵。",
    "日月象，",
    "岳渎形。"
  ],
  "ptype": null,
  "poetName": "唐无名氏",
  "poetId": "5fb2c729b42a4746a5b376b70c9d5229",
