{
  "_id": "1c7e1987f0484d1b9af9c01a3295a8ca",
  "name": "哭宝月三藏大禅师",
  "contents": [
    "五峰习圣罢，",
    "乾竺化身归。",
    "帝子传真印，",
    "门人哭宝衣。",
    "一囊穷海没，",
    "三藏故园稀。",
    "无复天花落，",
    "悲风满铁围。"
  ],
  "ptype": null,
  "poetName": "陈陶",
  "poetId": "ef39201e27bb4bc5ac905fba9fe1fb60",
  "form": "五律",
