{
  "_id": "58cd7c8028d24ea4a304bcd033265b41",
  "name": "春水曲",
  "contents": [
    "鸭鸭，",
    "觜唼唼。",
    "青蒲生，",
    "春水狭。",
    "荡漾木兰船，",
    "中有双少年。",
    "少年醉，",
    "鸭不起。"
  ],
  "ptype": null,
  "poetName": "张籍",
  "poetId": "0810aa6ff9a34a96aba9c12caed115ae",
