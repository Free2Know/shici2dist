  "_id": "456b84337e8d43fb80da8a13543fd3e4",
  "name": "赠李金州",
  "contents": [
    "绮里祠前后，",
    "山程践白云。",
    "溯流随大旆，",
    "登岸见全军。",
    "晓角吹人梦，",
    "秋风卷雁群。",
    "雾开方露日，",
    "汉水底沙分。"
  ],
  "ptype": null,
  "poetName": "贾岛",
  "poetId": "03638acd71554861ad66405ecf420eb4",
