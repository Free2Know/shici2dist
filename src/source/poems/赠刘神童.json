  "_id": "ddf5be7be6c04892890b8e37452ed7f2",
  "name": "赠刘神童",
  "contents": [
    "习读在前生，",
    "僧谈足可明。",
    "还家虽解喜，",
    "登第未知荣。",
    "时果曾沾赐，",
    "春闱不挂情。",
    "灯前犹恶睡，",
    "寤语读书声。"
  ],
  "ptype": null,
  "poetName": "郑谷",
  "poetId": "87cb83f765db4575a2bdcb67a72bc6d7",
