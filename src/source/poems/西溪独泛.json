  "_id": "129549cb89c44ea497c85b8a43ecb906",
  "name": "西溪独泛",
  "contents": [
    "道情何所寄，",
    "素舸漫流间。",
    "真性怜高鹤，",
    "无名羡野山。",
    "经寒丛竹秀，",
    "人静片云闲。",
    "泛泛谁为侣，",
    "唯应共月还。"
  ],
  "ptype": null,
  "poetName": "皎然",
  "poetId": "a5afe9c958b94f8fb0cef564a238e23e",
