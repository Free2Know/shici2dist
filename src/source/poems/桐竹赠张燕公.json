{
  "_id": "cc6fba1d32a6428a93c29d8b92263d95",
  "name": "桐竹赠张燕公",
  "contents": [
    "北竹青桐北，",
    "南桐绿竹南。",
    "竹林君早爱，",
    "桐树我初贪。",
    "凤栖桐不愧，",
    "凤食竹何惭。",
    "栖食更如此，",
    "馀非凤所堪。"
  ],
  "ptype": null,
  "poetName": "李伯鱼",
  "poetId": "7c6a81b313364a1187714ebd3c461a8d",
  "form": "五律",
