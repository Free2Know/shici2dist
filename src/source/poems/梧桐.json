  "_id": "97cdc4487d554b38922f67c0037746e2",
  "name": "梧桐",
  "contents": [
    "亭亭南轩外，",
    "贞干修且直。",
    "广叶结青阴，",
    "繁花连素色。",
    "天资韶雅性，",
    "不愧知音识。"
  ],
  "ptype": null,
  "poetName": "戴叔伦",
