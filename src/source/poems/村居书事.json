  "_id": "1b0f17e9fc9f4c83b9d74a1b6ec23c47",
  "name": "村居书事",
  "contents": [
    "年年耕与钓，",
    "鸥鸟已相依。",
    "砌长苍苔厚，",
    "藤抽紫蔓肥。",
    "风莺移树啭，",
    "雨燕入楼飞。",
    "不觉春光暮，",
    "绕篱红杏稀。"
  ],
  "ptype": null,
  "poetName": "韦庄",
  "poetId": "73c980ef1fac470ea54eac39c66f7e4e",
