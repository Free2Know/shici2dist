{
  "_id": "028b2b8878c94109b07e8e445af1e057",
  "name": "夏日会修行段将军宅",
  "contents": [
    "爱君书院静，",
    "莎覆藓阶浓。",
    "连穗古藤暗，",
    "领雏幽鸟重。",
    "樽前迎远客，",
    "林杪见晴峰。",
    "谁谓朱门内，",
    "云山满座逢。"
  ],
  "ptype": null,
  "poetName": "顾非熊",
  "poetId": "7d10bda5d33144fab1cdf0dba4c0bced",
  "form": "五律",
