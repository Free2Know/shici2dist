{
  "_id": "d0f0f2d27cf84c5dab45bd94a95f52e5",
  "name": "舟",
  "contents": [
    "征棹三江暮，",
    "连樯万里回。",
    "相乌风际转，",
    "画鹢浪前开。",
    "羽客乘霞至，",
    "仙人弄月来。",
    "何当同传说，",
    "特展巨川材。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "5f3c01f5ddab4ebdb2d9748023f40391",
