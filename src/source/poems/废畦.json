  "_id": "b79c1f7abf7c4ab9932ed9a7781fb73f",
  "name": "废畦",
  "contents": [
    "秋蔬拥霜露，",
    "岂敢惜凋残。",
    "暮景数枝叶，",
    "天风吹汝寒。",
    "绿沾泥滓尽，",
    "香与岁时阑。",
    "生意春如昨，",
    "悲君白玉盘。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
  "form": "五律",
