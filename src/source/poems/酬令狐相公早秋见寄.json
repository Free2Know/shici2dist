{
  "_id": "9a751dfb053341e3b7cd7b8270cb1685",
  "name": "酬令狐相公早秋见寄",
  "contents": [
    "公来第四秋，",
    "乐国号无愁。",
    "军士游书肆，",
    "商人占酒楼。",
    "熊罴交黑槊，",
    "宾客满青油。",
    "今日文章主，",
    "梁王不姓刘。"
  ],
  "ptype": null,
  "poetName": "刘禹锡",
  "poetId": "f708c5d8cf5d4324bdc3a709ea718fd3",
  "form": "五律",
