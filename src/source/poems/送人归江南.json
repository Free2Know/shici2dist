  "_id": "fc6ddb8868114215b05f65a43926e251",
  "name": "送人归江南",
  "contents": [
    "贫归无定程，",
    "水宿与山行。",
    "未有安亲计，",
    "难为去国情。",
    "岛烟孤寺磬，",
    "江月远船筝。",
    "思苦秋回日，",
    "多应吟更清。"
  ],
  "ptype": null,
  "poetName": "张乔",
  "poetId": "746ea2e1efcd41db9a5d67eddf753f46",
