{
  "_id": "f27c850b44c148b7a753afb0ecbdf6e2",
  "name": "示外生",
  "contents": [
    "牵役非吾好，",
    "宽情尔在傍。",
    "经过悲井邑，",
    "起坐倦舟航。",
    "相宅生应贵，",
    "逢时学可强。",
    "无轻吾未用，",
    "世事有行藏。"
  ],
  "ptype": null,
  "poetName": "严维",
  "poetId": "2cf32a5a735b45d6b3a6931b3c38faa3",
