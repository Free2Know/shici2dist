{
  "_id": "fc5922dbd4224049a083b5871d4d7fec",
  "name": "元日",
  "contents": [
    "爆竹声中一岁除，",
    "春风送暖入屠苏。",
    "千门万户瞳瞳日，",
    "总把新桃换旧符。"
  ],
  "ptype": null,
  "poetName": "王安石",
  "poetId": "96069f8ce7e24ccbb57de41bedace092",
  "form": "七绝",
