  "_id": "5baf9c009bf74b41a88843078f66d455",
  "name": "冬日骆家亭子",
  "contents": [
    "亭台腊月时，",
    "松竹见贞姿。",
    "林积烟藏日，",
    "风吹水合池。",
    "恨无人此住，",
    "静有鹤相窥。",
    "是景吟诗遍，",
    "真于野客宜。"
  ],
  "ptype": null,
  "poetName": "刘得仁",
  "poetId": "d023386d977440f8bbea3bbd47049cbb",
