  "_id": "d7246862087f44ada7bb337924307ddb",
  "name": "避暑",
  "contents": [
    "当暑忆归林，",
    "陶家借柳阴。",
    "蝉从初伏噪，",
    "客向晚凉吟。",
    "白日欺玄鬓，",
    "沧江负素心。",
    "神仙倘有术，",
    "引我出幽岑。"
  ],
  "ptype": null,
  "poetName": "李频",
