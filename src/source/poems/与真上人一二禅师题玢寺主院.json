{
  "_id": "7cbf8dde9dac4804a2f46ce2df1d87e3",
  "name": "与真上人一二禅师题玢寺主院",
  "contents": [
    "杖屦相随任处便，",
    "不唯空寄上方眠。",
    "归时亦取湖边路，",
    "晚映枫林共上船。"
  ],
  "ptype": null,
  "poetName": "朱庆馀",
  "poetId": "1d2ece7d434747758f0f1f12e6228187",
