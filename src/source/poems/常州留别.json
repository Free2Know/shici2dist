  "_id": "78bcfd4f4c004d52b813e72237f5873f",
  "name": "常州留别",
  "contents": [
    "万里南天外，",
    "求书禹穴间。",
    "往来成白首，",
    "旦暮见青山。",
    "夜浦凉云过，",
    "秋塘好月闲。",
    "殷勤阳羡桂，",
    "别此几时攀。"
  ],
  "ptype": null,
  "poetName": "耿湋",
  "poetId": "d62460239e1244dcad61df818057bd89",
