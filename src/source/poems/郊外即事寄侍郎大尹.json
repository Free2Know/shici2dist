{
  "_id": "3326dd482f9b4d788342137fd94805d9",
  "name": "郊外即事寄侍郎大尹",
  "contents": [
    "高秋惭非隐，",
    "闲林喜退居。",
    "老农争席坐，",
    "稚子带经锄。",
    "竹径难回骑，",
    "仙舟但跂予。",
    "岂知陶靖节，",
    "只自爱吾庐。"
  ],
  "ptype": null,
  "poetName": "李德裕",
  "poetId": "9f0e42dd07124a388bf5bf42e01ed4a1",
  "form": "五律",
