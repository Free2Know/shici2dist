  "_id": "291bdb9724d0445d9af600e840715c1f",
  "name": "酬九经者",
  "contents": [
    "九经三史学，",
    "穷妙又穷微。",
    "长白山初出，",
    "青云路欲飞。",
    "江僧酬雪句，",
    "沙鹤识麻衣。",
    "家在黄河北，",
    "南来偶未归。"
  ],
  "ptype": null,
  "poetName": "齐己",
