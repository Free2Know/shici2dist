{
  "_id": "2406d68b545e4a2582acef909b39d807",
  "name": "奉和杜相公初发京城作",
  "contents": [
    "按节辞黄阁，",
    "登坛恋赤墀。",
    "衔恩期报主，",
    "授律远行师。",
    "野鹊迎金印，",
    "郊云拂画旗。",
    "叨陪幕中客，",
    "敢和出车诗。"
  ],
  "ptype": null,
  "poetName": "岑参",
  "poetId": "77a58f32ae514e76913c578aae0230d3",
  "form": "五律",
