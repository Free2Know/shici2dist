{
  "_id": "01d87ee50f65426f81534c15c5c11d9f",
  "name": "哭友人",
  "contents": [
    "病向名场得，",
    "终为善误身。",
    "无儿承后嗣，",
    "有女托何人。",
    "葬礼难求备，",
    "交情好者贫。",
    "惟余旧文集，",
    "一览一沾巾。"
  ],
  "ptype": null,
  "poetName": "杜荀鹤",
  "poetId": "4a8bb60fa6f54cc49debda0988ff139d",
