{
  "_id": "9be309f68179479e94fbaa702662ffba",
  "name": "北原别业",
  "contents": [
    "野外车骑绝，",
    "古村桑柘阴。",
    "流莺出谷静，",
    "春草闭门深。",
    "学稼农为业，",
    "忘情道作心。",
    "因知上皇日，",
    "凿井在灵林。"
  ],
  "ptype": null,
  "poetName": "释泚",
