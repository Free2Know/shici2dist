{
  "_id": "878875b5db4f44ecb1edf2e8c53a6bdd",
  "name": "谢都督挽歌",
  "contents": [
    "漠漠佳城幽，",
    "苍苍松槚暮。",
    "鲁幕飘欲卷，",
    "宛驷悲还顾。",
    "楚挽绕庐山，",
    "胡笳临武库。",
    "怅然郊原静，",
    "烟生归鸟度。"
  ],
  "ptype": null,
  "poetName": "上官仪",
  "poetId": "5172f35ac6194b0ea91b59f61618f357",
  "form": "五律",
