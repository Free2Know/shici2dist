  "_id": "785e744c3c01410391d12602551de0bc",
  "name": "蓟北寒月作",
  "contents": [
    "蓟门寒到骨，",
    "战碛雁相悲。",
    "古屋不胜雪，",
    "严风欲断髭。",
    "清吟得冷句，",
    "远念失佳期。",
    "寂寞谁相问，",
    "迢迢天一涯。"
  ],
  "ptype": null,
  "poetName": "贯休",
  "poetId": "35ef826ebca24b318ada7e612c75eda6",
