  "_id": "4e8ed754f2ba4f28ad0ed4c983e4bb13",
  "name": "送胡校书秩满归河中",
  "contents": [
    "古树汾阴道，",
    "悠悠东去长。",
    "位卑仍解印，",
    "身老又还乡。",
    "河水平秋岸，",
    "关门向夕阳。",
    "音书须数附，",
    "莫学晋嵇康。"
  ],
  "ptype": null,
  "poetName": "耿湋",
  "poetId": "d62460239e1244dcad61df818057bd89",
