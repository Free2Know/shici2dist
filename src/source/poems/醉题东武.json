  "_id": "e8d770dc5c46474389dbfa2a7bf5361b",
  "name": "醉题东武",
  "contents": [
    "役役行人事，",
    "纷纷碎簿书。",
    "功夫两衙尽，",
    "留滞七年馀。",
    "病痛梅天发，",
    "亲情海岸疏。",
    "因循未归得，",
    "不是忆鲈鱼。"
  ],
  "ptype": null,
  "poetName": "元稹",
  "poetId": "e04ddf3e16fd40a3bc351fd163098dda",
