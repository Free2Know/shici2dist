{
  "_id": "59dbf4385d294bd3891c0c636f87cfbd",
  "name": "泛舟侍宴应制",
  "contents": [
    "平明出御沟，",
    "解缆坐回舟。",
    "绿水澄明月，",
    "红罗结绮楼。",
    "弦歌争浦入，",
    "冠盖逐川流。",
    "白鱼臣作伴，",
    "相对舞王舟。"
  ],
  "ptype": null,
  "poetName": "张易之",
  "poetId": "6c9f435b537f443da1a1643e0571c2ae",
