{
  "_id": "00a3bcd8290744a9b5980c4c39f1be03",
  "name": "山居偶作",
  "contents": [
    "趋名逐利身，",
    "终日走风尘。",
    "还到水边宅，",
    "却为山下人。",
    "僧教开竹户，",
    "客许戴纱巾。",
    "且喜琴书在，",
    "苏生未厌贫。"
  ],
  "ptype": null,
  "poetName": "唐求",
  "poetId": "1bbb0b907006407ca57b5b184e25df9e",
