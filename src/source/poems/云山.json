  "_id": "b49d579da75746caa36bb39864933aba",
  "name": "云山",
  "contents": [
    "京洛云山外，",
    "音书静不来。",
    "神交作赋客，",
    "力尽望乡台。",
    "衰疾江边卧，",
    "亲朋日暮回。",
    "白鸥元水宿，",
    "何事有馀哀。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
