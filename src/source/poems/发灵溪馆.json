{
  "_id": "f3b12d8659c14d76b8d71f6915043aef",
  "name": "发灵溪馆",
  "contents": [
    "山多水不穷，",
    "一叶似渔翁。",
    "鸟浴寒潭雨，",
    "猿吟暮岭风。",
    "杂英垂锦绣，",
    "众籁合丝桐。",
    "应有曹溪路，",
    "千岩万壑中。"
  ],
  "ptype": null,
  "poetName": "许浑",
  "poetId": "ba84f30a9b344330943f2fbe4d128d8d",
  "form": "五律",
