  "_id": "c777783aa5c543ee8d43b9dbe4cf9951",
  "name": "通道",
  "contents": [
    "通道复通玄，",
    "名留四海传。",
    "交亲一拄杖，",
    "活计两空拳。",
    "要果逡巡种，",
    "思茶逐旋煎。",
    "岂知来混世，",
    "不久却回天。"
  ],
  "ptype": null,
  "poetName": "吕岩",
