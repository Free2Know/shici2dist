  "_id": "957ebe0c772446348478aa076e671732",
  "name": "首春",
  "contents": [
    "寒随穷律变，",
    "春逐鸟声开。",
    "初风飘带柳，",
    "晚雪间花梅。",
    "碧林青旧竹，",
    "绿沼翠新苔。",
    "芝田初雁去，",
    "绮树巧莺来。"
  ],
  "ptype": null,
  "poetName": "李世民",
  "poetId": "10cc5cc32b3c450d8b73272986bf9569",
