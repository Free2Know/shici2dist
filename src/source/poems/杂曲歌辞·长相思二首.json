{
  "_id": "f836f02c79404930aaa396e03cd3f356",
  "name": "杂曲歌辞·长相思二首",
  "contents": [
    "君行登陇上，",
    "妾梦在闺中。",
    "玉箸千行落，",
    "银床一半空。",
    "绮席春眠觉，",
    "纱窗晓望迷。",
    "朦胧残梦里，",
    "犹自在辽西。"
  ],
  "ptype": null,
  "poetName": "令狐楚",
  "poetId": "98300c3321da4b4cafe0410ee86bda3c",
  "form": "五律",
