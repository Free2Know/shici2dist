  "_id": "593be1fff4ba488cb7c6b7b9e11326af",
  "name": "湖边采莲妇",
  "contents": [
    "小姑织白纻，",
    "未解将人语。",
    "大嫂采芙蓉，",
    "溪湖千万重。",
    "长兄行不在，",
    "莫使外人逢。",
    "愿学秋胡妇，",
    "贞心比古松。"
  ],
  "ptype": null,
  "poetName": "李白",
  "poetId": "1b59d554dd594c8497d9cceefaf73d61",
