{
  "_id": "1ef92e2ddd8d4751ab2f9d751b660d4e",
  "name": "桃",
  "contents": [
    "独有成蹊处，",
    "秾华发井傍。",
    "山风凝笑脸，",
    "朝露泫啼妆。",
    "隐士颜应改，",
    "仙人路渐长。",
    "还欣上林苑，",
    "千岁奉君王。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "5f3c01f5ddab4ebdb2d9748023f40391",
  "form": "五律",
