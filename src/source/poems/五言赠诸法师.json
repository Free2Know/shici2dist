{
  "_id": "04445bd503104f12a38e72f93b5a59ab",
  "name": "五言赠诸法师",
  "contents": [
    "驾鹤排朱雾，",
    "乘鸾入紫烟。",
    "凌晨味潭菊，",
    "薄暮玩峰莲。",
    "玉叶低梁下，",
    "金飗引窗前。",
    "啸傲云霞际，",
    "留情鳞羽年。"
  ],
  "ptype": null,
  "poetName": "公孙杲",
  "poetId": "462e365e44ff4cf9ad99044ef7697542",
  "form": "五律",
