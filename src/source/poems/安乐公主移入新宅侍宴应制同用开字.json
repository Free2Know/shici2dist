{
  "_id": "9fbdd8cf9b5f4c19a090f5f04083dcfb",
  "name": "安乐公主移入新宅侍宴应制同用开字",
  "contents": [
    "云物中京晓，",
    "天人外馆开。",
    "飞桥象河汉，",
    "悬榜学蓬莱。",
    "北阙临仙槛，",
    "南山送寿杯。",
    "一窥轮奂毕，",
    "惭恧栋梁材。"
  ],
  "ptype": null,
  "poetName": "赵彦昭",
  "poetId": "7f6e3b5edd034f9fb7f0bdd444b8e3c8",
  "form": "五律",
