{
  "_id": "ca9cb563103c4c7ab4701e99b836c7d7",
  "name": "送远上人归京",
  "contents": [
    "夏腊岁方深，",
    "思归彻曙吟。",
    "未离销雪院，",
    "已有过云心。",
    "寒磬清函谷，",
    "孤钟宿华阴。",
    "别京游旧寺，",
    "月色似双林。"
  ],
  "ptype": null,
  "poetName": "冷朝阳",
  "poetId": "e4da664605a74e579884c36de964cf3e",
