  "_id": "f675ced66b604f8b9becf4c7313190e4",
  "name": "霁后作",
  "contents": [
    "新霁辨草木，",
    "晚塘明衣衿。",
    "乳燕不归宿，",
    "双双飞向林。",
    "微照露花影，",
    "轻云浮麦阴。",
    "无人可招隐，",
    "尽日登山吟。"
  ],
  "ptype": null,
  "poetName": "曹邺",
  "poetId": "7ee93682f15e45668bf2f4303b1835b6",
