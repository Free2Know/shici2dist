  "_id": "c8a134b78fc24aeeafb26677ced2fa60",
  "name": "五噫歌",
  "contents": [
    "陟彼北芒兮，",
    "噫！",
    "顾瞻帝京兮，",
    "噫！",
    "宫阙崔巍兮，",
    "噫！",
    "民之劬劳兮，",
    "噫！",
    "辽辽未央兮，",
    "噫！"
  ],
  "ptype": null,
  "poetName": "梁鸿",
