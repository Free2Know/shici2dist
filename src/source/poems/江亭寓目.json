  "_id": "26a7c47f576b4f23b63a80e12503f508",
  "name": "江亭寓目",
  "contents": [
    "江郭带林峦，",
    "津亭倚槛看。",
    "水风蒲叶战，",
    "沙雨鹭鸶寒。",
    "晚木初凋柳，",
    "秋丛欲败兰。",
    "哀猿自相叫，",
    "乡泪好无端。"
  ],
  "ptype": null,
  "poetName": "卢拱",
  "poetId": "c61e41c9b46446d9847421fa38977006",
