{
  "_id": "4e9065fb57a64f8b862f2d4e08d90c43",
  "name": "月夜有怀简诸同病",
  "contents": [
    "闲庭落景尽，",
    "疏帘夜月通。",
    "山灵响似应，",
    "水净望如空。",
    "栖枝犹绕鹊，",
    "遵渚未来鸿。",
    "可叹高楼妇，",
    "悲思杳难终。"
  ],
  "ptype": null,
  "poetName": "骆宾王",
  "poetId": "fc99ba1506f7440b99b0946b24173672",
  "form": "五律",
