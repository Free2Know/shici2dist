  "_id": "3b119636af7f482c9186664e9d3ada97",
  "name": "侍宴归雁堂",
  "contents": [
    "歌堂面渌水，",
    "舞馆接金塘。",
    "竹开霜后翠，",
    "梅动雪前香。",
    "凫归初命侣，",
    "雁起欲分行。",
    "刷羽同栖集，",
    "怀恩愧稻粱。"
  ],
  "ptype": null,
  "poetName": "虞世南",
  "poetId": "5f8fbaabf00e48c59559484811d09871",
