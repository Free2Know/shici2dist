  "_id": "cf896dbba4224efd83bda77fde9506ad",
  "name": "题终南麻先生寂禅师石室",
  "contents": [
    "因居石室贫，",
    "五十二回春。",
    "拥褐冥心客，",
    "穷经暮齿人。",
    "翠沉空水定，",
    "雨绝片云新。",
    "危细秋峰径，",
    "相随到顶频。"
  ],
  "ptype": null,
  "poetName": "刘得仁",
  "poetId": "d023386d977440f8bbea3bbd47049cbb",
  "form": "五律",
