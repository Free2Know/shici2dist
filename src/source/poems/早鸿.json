  "_id": "b460885587574898987a7813902948c4",
  "name": "早鸿",
  "contents": [
    "木落波浪动，",
    "南飞闻夜鸿。",
    "参差天汉雾，",
    "嘹唳月明风。",
    "野水莲茎折，",
    "寒泥稻穗空。",
    "无令一行侣，",
    "相失五湖中。"
  ],
  "ptype": null,
  "poetName": "李群玉",
  "poetId": "533477cc5b0045399dfdc5f641132458",
