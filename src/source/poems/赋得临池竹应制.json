{
  "_id": "96b4e1100da04b2496d1a7618d31a674",
  "name": "赋得临池竹应制",
  "contents": [
    "葱翠梢云质，",
    "垂彩映清池。",
    "波泛含风影，",
    "流摇防露枝。",
    "龙鳞漾嶰谷，",
    "凤翅拂涟漪。",
    "欲识凌冬性，",
    "唯有岁寒知。"
  ],
  "ptype": null,
  "poetName": "虞世南",
  "poetId": "5f8fbaabf00e48c59559484811d09871",
  "form": "五律",
