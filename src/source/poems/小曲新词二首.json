  "_id": "26246177b8e9411f83a30c08b378ade1",
  "name": "小曲新词二首",
  "contents": [
    "霁色鲜宫殿，",
    "秋声脆管弦。",
    "圣明千岁乐，",
    "岁岁似今年。",
    "红裾明月夜，",
    "碧簟早秋时。",
    "好向昭阳宿，",
    "天凉玉漏迟。"
  ],
  "ptype": null,
  "poetName": "白居易",
  "poetId": "61c661a769a04e409e99836b8f4a8f79",
