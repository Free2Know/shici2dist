{
  "_id": "063a46380cef4dbdadd17329118b5e6a",
  "name": "久雨",
  "contents": [
    "云影蔽遥空，",
    "无端淡复浓。",
    "两旬绵密雨，",
    "二月似深冬。",
    "诗酒从教数，",
    "帘帏一任重。",
    "孰知春有地，",
    "微露小桃红。"
  ],
  "ptype": null,
  "poetName": "殷尧藩",
