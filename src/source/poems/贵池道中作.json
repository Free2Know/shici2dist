{
  "_id": "a91ded806bc94cea81bf194a626ccb60",
  "name": "贵池道中作",
  "contents": [
    "羸骖驱野岸，",
    "山远路盘盘。",
    "清露月华晓，",
    "碧江星影寒。",
    "离群徒长泣，",
    "去国自加餐。",
    "霄汉宁无旧，",
    "相哀自语端。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "3a86db0ed7e4461794e5c0aa5ffb339c",
  "form": "五律",
