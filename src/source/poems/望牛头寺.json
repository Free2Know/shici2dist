  "_id": "b2e092b082ab4192bb1db19707db1a0a",
  "name": "望牛头寺",
  "contents": [
    "牛头见鹤林，",
    "梯迳绕幽深。",
    "春色浮山外，",
    "天河宿殿阴。",
    "传灯无白日，",
    "布地有黄金。",
    "休作狂歌老，",
    "回看不住心。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
