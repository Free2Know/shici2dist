{
  "_id": "8850b1a1bed9481bba994b37c3ffdbcb",
  "name": "裴侍御见赠斑竹杖",
  "contents": [
    "骚人夸竹杖，",
    "赠我意何深。",
    "万点湘妃泪，",
    "三年贾谊心。",
    "愿持终白首，",
    "谁道贵黄金。",
    "他日归愚谷，",
    "偏宜绿绮琴。"
  ],
  "ptype": null,
  "poetName": "李嘉祐",
  "poetId": "c256d35ea8de4cbb869292361f80ed95",
  "form": "五律",
