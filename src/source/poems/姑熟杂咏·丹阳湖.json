{
  "_id": "f2ec6bcd145f421e95779e9ed8a9f2a6",
  "name": "姑熟杂咏·丹阳湖",
  "contents": [
    "湖与元气通，",
    "风波浩难止。",
    "天外贾客归，",
    "云间片帆起。",
    "龟游莲叶上，",
    "鸟宿芦花里。",
    "少女棹舟归，",
    "歌声逐流水。"
  ],
  "ptype": null,
  "poetName": "李赤",
  "poetId": "c7eeded1db4d4eaaa9195525253de068",
  "form": "五律",
