  "_id": "193192b46ae6433eabf4dda2683d43b0",
  "name": "书边事",
  "contents": [
    "朔野烟尘起，",
    "天军又举戈。",
    "阴风向晚急，",
    "杀气入秋多。",
    "树尽禽栖草，",
    "冰坚路在河。",
    "汾阳无继者，",
    "羌虏肯先和。"
  ],
  "ptype": null,
  "poetName": "李昌符",
  "poetId": "ca15f36b5e124414b993931a1677a016",
