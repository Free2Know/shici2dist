{
  "_id": "bb168595dc1642db8e1a5f2893cb37e5",
  "name": "送金城公主适西蕃应制",
  "contents": [
    "下嫁戎庭远，",
    "和亲汉礼优。",
    "笳声出虏塞，",
    "箫曲背秦楼。",
    "贵主悲黄鹤，",
    "征人怨紫骝。",
    "皇情眷亿兆，",
    "割念俯怀柔。"
  ],
  "ptype": null,
  "poetName": "郑愔",
  "poetId": "844b95a00ecd4c37a24afcf3992aad31",
  "form": "五律",
