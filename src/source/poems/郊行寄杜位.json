{
  "_id": "72a2ec9b68b345f9a1ddc46f8d0dbe14",
  "name": "郊行寄杜位",
  "contents": [
    "崷崒空城烟，",
    "凄清寒山景。",
    "秋风引归梦，",
    "昨夜到汝颍。",
    "近寺闻钟声，",
    "映陂见树影。",
    "所思何由见，",
    "东北徒引领。"
  ],
  "ptype": null,
  "poetName": "岑参",
  "poetId": "77a58f32ae514e76913c578aae0230d3",
  "form": "五律",
