{
  "_id": "4cf66f2164604a309bbd9f3ed9f06c5a",
  "name": "建州人歌",
  "contents": [
    "令我州郡泰，",
    "令我户口裕，",
    "令我活计大，",
    "陆员外。",
    "令我家不分，",
    "令我马成群，",
    "令我稻满囷，",
    "陆使君。"
  ],
  "ptype": null,
  "poetName": "唐无名氏",
