{
  "_id": "4e3e87d86c71441b86d5a6a1d50f1f80",
  "name": "宪台出絷寒夜有怀",
  "contents": [
    "独坐怀明发，",
    "长谣苦未安。",
    "自应迷北叟，",
    "谁肯问南冠。",
    "生死交情异，",
    "殷忧岁序阑。",
    "空馀朝夕鸟，",
    "相伴夜啼寒。"
  ],
  "ptype": null,
  "poetName": "骆宾王",
  "poetId": "fc99ba1506f7440b99b0946b24173672",
  "form": "五律",
