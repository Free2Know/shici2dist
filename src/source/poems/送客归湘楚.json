  "_id": "8a052b9637314a7584d67179f88da252",
  "name": "送客归湘楚",
  "contents": [
    "无辞一杯酒，",
    "昔日与君深。",
    "秋色换归鬓，",
    "曙光生别心。",
    "桂花山庙冷，",
    "枫树水楼阴。",
    "此路千余里，",
    "应劳楚客吟。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "3a86db0ed7e4461794e5c0aa5ffb339c",
