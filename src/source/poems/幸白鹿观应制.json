  "_id": "bc408997c3d749958f5e9bb50e3a2a2c",
  "name": "幸白鹿观应制",
  "contents": [
    "碧虚清吹下，",
    "蔼蔼入仙宫。",
    "松磴攀云绝，",
    "花源接涧空。",
    "受符邀羽使，",
    "传诀注香童。",
    "讵似闲居日，",
    "徒闻有顺风。"
  ],
  "ptype": null,
  "poetName": "苏颋",
  "poetId": "a19db5ae647c4aa790f71abd80406865",
