  "_id": "ddcf915d62974b358ff427c9beb0d601",
  "name": "落日怅望",
  "contents": [
    "孤云与归鸟，",
    "千里片时间。",
    "念我一何滞，",
    "辞家久未还。",
    "微阳下乔木，",
    "远色隐秋山。",
    "临水不敢照，",
    "恐惊平昔颜。"
  ],
  "ptype": null,
  "poetName": "马戴",
  "poetId": "db14ec8dfa7a4a92987b2e2357aaa5b6",
