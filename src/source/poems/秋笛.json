{
  "_id": "4e5054a426f34f2ea03ed37075f571f0",
  "name": "秋笛",
  "contents": [
    "清商欲尽奏，",
    "奏苦血沾衣。",
    "他日伤心极，",
    "征人白骨归。",
    "相逢恐恨过，",
    "故作发声微。",
    "不见秋云动，",
    "悲风稍稍飞。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "8d8a925e68914c4e93ba72c4de5b17cd",
