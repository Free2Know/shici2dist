{
  "_id": "5ba8d979c7fb46fbb64387223838c9a0",
  "name": "山居晚眺赠王道士",
  "contents": [
    "金坛疏俗宇，",
    "玉洞侣仙群。",
    "花枝栖晚露，",
    "峰叶度晴云。",
    "斜照移山影，",
    "回沙拥籀文。",
    "琴尊方待兴，",
    "竹树已迎曛。"
  ],
  "ptype": null,
  "poetName": "王勃",
  "poetId": "c7670ea82d5d4d89b2e94df234298b4c",
