  "_id": "16326713aa9d4a2db75ae0a2404b0f8a",
  "name": "寄恒璨",
  "contents": [
    "心绝去来缘，",
    "迹顺人间事。",
    "独寻秋草径，",
    "夜宿寒山寺。",
    "今日郡斋闲，",
    "思问楞伽字。"
  ],
  "ptype": null,
  "poetName": "韦应物",
