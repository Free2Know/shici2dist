  "_id": "23fb27747b4f44ddbde5088ff1cdda44",
  "name": "戴光弓",
  "contents": [
    "潞府筋角劲，",
    "戴光因合成。",
    "因君怀胆气，",
    "赠我定交情。",
    "不拟闲穿叶，",
    "那能枉始生。",
    "唯调一只箭，",
    "飞入破聊城。"
  ],
  "ptype": null,
  "poetName": "元稹",
  "poetId": "e04ddf3e16fd40a3bc351fd163098dda",
