  "_id": "fbd2a826d6d54f4e92f3172c0e5ecd26",
  "name": "道院",
  "contents": [
    "白日人稀到，",
    "帘垂道院深。",
    "雨苔生古壁，",
    "雪雀聚寒林。",
    "忘虑凭三乐，",
    "消闲信五禽。",
    "谁知是官府，",
    "烟缕满炉沈。"
  ],
  "ptype": null,
  "poetName": "王周",
  "poetId": "ec59ee5aaa504708b97886774ce64687",
