  "_id": "5e4720e275db4873b1baeb9f04597ecb",
  "name": "太康六年三月三日后园会",
  "contents": [
    "暮春元日，",
    "阳气清明，",
    "祁祁甘雨，",
    "膏泽流盈。",
    "习习祥风，",
    "启滞异生，",
    "禽鸟翔逸，",
    "卉木滋荣。"
  ],
  "ptype": null,
  "poetName": "张华",
  "poetId": "5aa5de7f2df34500bd93bfe05eaefcfb",
