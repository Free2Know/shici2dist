{
  "_id": "9f625d60e0a14a39b0475f2706a7d4ca",
  "name": "又和游光睦院",
  "contents": [
    "寺门山水际，",
    "清浅照孱颜。",
    "客棹晚维岸，",
    "僧房犹掩关。",
    "日华穿竹静，",
    "云影过阶闲。",
    "箕踞一长啸，",
    "忘怀物我间。"
  ],
  "ptype": null,
  "poetName": "徐铉",
  "poetId": "f57e972c16904a6e8b4b47397470ec67",
  "form": "五律",
