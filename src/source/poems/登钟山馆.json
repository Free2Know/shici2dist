  "_id": "c7e54816893a456b93ac47520f860104",
  "name": "登钟山馆",
  "contents": [
    "匹马宜春路，",
    "萧条背馆心。",
    "涧花寒夕雨，",
    "潭水黑朝林。",
    "野市鱼盐隘，",
    "江村竹苇深。",
    "子规何处发，",
    "青树满高岑。"
  ],
  "ptype": null,
  "poetName": "耿湋",
  "poetId": "d62460239e1244dcad61df818057bd89",
