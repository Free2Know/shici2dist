{
  "_id": "3384d8a2510a4c7ab73d5c6bfcf6e097",
  "name": "赠欧阳明府",
  "contents": [
    "贤宰宰斯邑，",
    "政闻闾里间。",
    "都缘民讼少，",
    "长觉吏徒闲。",
    "帆落樽前浦，",
    "钟鸣枕上山。",
    "回舟却惆怅，",
    "数宿钓鱼湾。"
  ],
  "ptype": null,
  "poetName": "杜荀鹤",
  "poetId": "4a8bb60fa6f54cc49debda0988ff139d",
  "form": "五律",
