  "_id": "7935476e793f4f689d784b4aaf7d8d7c",
  "name": "寄会稽公徐侍郎",
  "contents": [
    "摇落淮南叶，",
    "秋风想越吟。",
    "邹枚入梁苑，",
    "逸少在山阴。",
    "老鹤无衰貌，",
    "寒松有本心。",
    "圣朝难税驾，",
    "惆怅白云深。"
  ],
  "ptype": null,
  "poetName": "刘长卿",
  "poetId": "8fb91cedc6274daba50e589c0dde93b7",
  "form": "五律",
