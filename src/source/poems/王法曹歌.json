{
  "_id": "1c17dc5bdbb44354a0f863f6835c4b54",
  "name": "王法曹歌",
  "contents": [
    "前得尹佛子，",
    "后得王癞獭。",
    "判事驴咬瓜，",
    "唤人牛嚼沫。",
    "见钱满面喜，",
    "无镪从头喝。",
    "常逢饿夜叉，",
    "百姓不可活。"
  ],
  "ptype": null,
  "poetName": "唐无名氏",
  "poetId": "5fb2c729b42a4746a5b376b70c9d5229",
  "form": "五律",
