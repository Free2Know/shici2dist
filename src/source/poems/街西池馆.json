{
  "_id": "f2288c2a5b914bbb955ae9a7ed83e6d6",
  "name": "街西池馆",
  "contents": [
    "白阁他年别，",
    "朱门此夜过。",
    "疏帘留月魄，",
    "珍簟接烟波。",
    "太守三刀梦，",
    "将军一箭歌。",
    "国租容客旅，",
    "香熟玉山禾。"
  ],
  "ptype": null,
  "poetName": "李商隐",
  "poetId": "2771bb31ded849bdac8207fbb5785a3a",
  "form": "五律",
