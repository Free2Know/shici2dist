{
  "_id": "7eaaf62df35e449f95daf70366fb3b08",
  "name": "道成归",
  "contents": [
    "年苦志不求奇。",
    "公寻者希。",
    "夷猛悟笑微微。",
    "华枉用机。",
    "予偃，",
    "婴肥。",
    "宫祥瑞围。",
    "传心印谨相依。",
    "灵神自飞。"
  ],
  "ptype": null,
  "poetName": "马钰",
  "poetId": "6f79865ac305486ba0011c7eff4b976f",
