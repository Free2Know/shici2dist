  "_id": "5d59096f51834587a35d3617c29901f3",
  "name": "怀博陵故人",
  "contents": [
    "孤城易水头，",
    "不忘旧交游。",
    "雪压围棋石，",
    "风吹饮酒楼。",
    "路遥千万里，",
    "人别十三秋。",
    "吟苦相思处，",
    "天寒水急流。"
  ],
  "ptype": null,
  "poetName": "贾岛",
  "poetId": "03638acd71554861ad66405ecf420eb4",
