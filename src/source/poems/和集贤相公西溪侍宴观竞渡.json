{
  "_id": "08aec9176ca1424f9e32c359356e185a",
  "name": "和集贤相公西溪侍宴观竞渡",
  "contents": [
    "片水耸层桥，",
    "祥烟霭庆霄。",
    "昼花铺广宴，",
    "晴电闪飞桡。",
    "浪叠摇仙仗，",
    "风微定彩标。",
    "都人同盛观，",
    "不觉在行朝。"
  ],
  "ptype": null,
  "poetName": "吴融",
  "poetId": "c8fcad51e3264dc8a7c8162344d3b0e9",
  "form": "五律",
