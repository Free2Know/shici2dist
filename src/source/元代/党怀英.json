  "Poems": [
    {
      "_id": null,
      "Name": "感皇恩",
      "PoetId": "1b524259b8a945689ccca638da59050b",
      "PoetName": "党怀英",
      "Form": "词",
      "Tags": [
        "元代",
        "党怀英"
      ],
      "Contents": [
        "碧玉捻柔条，",
        "蓝袍裁叶。",
        "明艳黄深软金叠，",
        "道装仙子，",
        "谪堕蕊珠仙阙。",
        "为春闲管领，",
        "花时节。",
        "汉额妆，",
        "楚腰舞怯。",
        "襞积裙馀旧宫褶。",
        "东君著意，",
        "留伴小庭风月。",
        "任教唤，",
        "群芳歇。"
      ]
    },
    {
      "_id": null,
      "Name": "月上海棠",
      "PoetId": "1b524259b8a945689ccca638da59050b",
      "PoetName": "党怀英",
      "Form": "词",
      "Tags": [
        "元代",
        "党怀英"
      ],
      "Contents": [
        "傲霜枝袅团珠蕾。",
        "冷香霏、烟雨晚秋意。",
        "萧散绕东篱，",
        "尚仿佛、见山清气。",
        "西风外，",
        "梦到斜川栗里。",
        "断霞鱼尾明秋水。",
        "带三两、飞鸿点烟际。",
        "疏林飒秋声，",
        "似知人、倦游无味。",
        "家何处，",
        "落日西山紫翠。"
      ]
    },
    {
      "_id": null,
      "Name": "青玉案",
      "PoetId": "1b524259b8a945689ccca638da59050b",
      "PoetName": "党怀英",
      "Form": "词",
      "Tags": [
        "元代",
        "党怀英"
      ],
      "Contents": [
        "红莎绿蒻春风饼，",
        "趁梅驿，",
        "来云岭，",
        "紫桂岩空琼窦冷。",
        "佳人却恨，",
        "等闲分破，",
        "缥缈双鸾影。",
        "一瓯月露心魂醒，",
        "更送清歌助清兴。",
        "痛饮休辞今夕永。",
        "与君洗尽，",
        "满襟烦暑，",
        "别作高寒境。"
      ]
    },
    {
      "_id": null,
      "Name": "鹧鸪天",
      "PoetId": "1b524259b8a945689ccca638da59050b",
      "PoetName": "党怀英",
      "Form": "词",
      "Tags": [
        "元代",
        "党怀英"
      ],
      "Contents": [
        "云步凌波小凤钩。",
        "年年星汉踏清秋。",
        "只缘巧极稀相见，",
        "底用人间乞巧楼。",
        "天外事，",
        "两悠悠。",
        "不应也作可怜愁。",
        "开帘放入窥窗月，",
        "且尽新凉睡美"
      ]
    }
  ]
}