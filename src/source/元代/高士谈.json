  "Poems": [
    {
      "_id": null,
      "Name": "减字木兰花",
      "PoetId": "0335ceddf95f4c60ae611fc1868a4343",
      "PoetName": "高士谈",
      "Form": "词",
      "Tags": [
        "元代",
        "高士谈"
      ],
      "Contents": [
        "西湖睡起。",
        "飞絮游丝春老矣。",
        "涨绿涵空。",
        "十顷玻璃四面风。",
        "时平事少。",
        "天与湖山供坐啸。",
        "他日西州。",
        "却怕羊昙感旧游。"
      ]
    },
    {
      "_id": null,
      "Name": "好事近",
      "PoetId": "0335ceddf95f4c60ae611fc1868a4343",
      "PoetName": "高士谈",
      "Form": "词",
      "Tags": [
        "元代",
        "高士谈"
      ],
      "Contents": [
        "谁打玉川门，",
        "白娟斜封团月。",
        "晴日小窗活火，",
        "响一壶春雪。",
        "可怜桑一生颠，",
        "文字更清绝。",
        "直拟驾风归去，",
        "把三山登彻。"
      ]
    },
    {
      "_id": null,
      "Name": "朝中措",
      "PoetId": "0335ceddf95f4c60ae611fc1868a4343",
      "PoetName": "高士谈",
      "Form": "词",
      "Tags": [
        "元代",
        "高士谈"
      ],
      "Contents": [
        "琅琊山色最清雄。",
        "心赏待衰翁。",
        "路转峰回如画，",
        "新亭半湿青红。",
        "风流宾从，",
        "清闲岁月，",
        "且共从容。",
        "莫笑尊前老大，",
        "犹堪管领春风。"
      ]
    },
    {
      "_id": null,
      "Name": "玉楼春",
      "PoetId": "0335ceddf95f4c60ae611fc1868a4343",
      "PoetName": "高士谈",
      "Form": "词",
      "Tags": [
        "元代",
        "高士谈"
      ],
      "Contents": [
        "少年人物江山秀。",
        "流落天涯今白首。",
        "形容憔悴不如初，",
        "文采风流仍似旧。",
        "百花元是仙家酒。",
        "千岁灵根能益寿。",
        "都将万事付天公。",
        "且伴老人开笑"
      ]
    }
  ]
}