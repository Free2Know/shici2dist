  "Poems": [
    {
      "_id": null,
      "Name": "太常引",
      "PoetId": "4a10ecabb47d4a6fa6c10d5ac0f7eda4",
      "PoetName": "薛昂夫",
      "Form": "词",
      "Tags": [
        "元代",
        "薛昂夫"
      ],
      "Contents": [
        "冷烟千顷酿寒威。",
        "晓霜重压征衣。",
        "休教六花飞。",
        "忆尚有、游人未归。",
        "江空岁晚，",
        "故园秋老，",
        "行色莫依违。",
        "特地与君期。",
        "趁南浦、莼鲈正肥。"
      ]
    },
    {
      "_id": null,
      "Name": "山坡羊",
      "PoetId": "4a10ecabb47d4a6fa6c10d5ac0f7eda4",
      "PoetName": "薛昂夫",
      "Form": "词",
      "Tags": [
        "元代",
        "薛昂夫"
      ],
      "Contents": [
        "晴云轻漾，",
        "薰风无浪，",
        "开樽避暑争相向。",
        "映湖光，",
        "逞新妆，",
        "笙歌鼎沸南湖荡，",
        "今夜且休回画舫。",
        "风，",
        "满座凉；",
        "莲，",
        "入梦香。"
      ]
    },
    {
      "_id": null,
      "Name": "最高楼",
      "PoetId": "4a10ecabb47d4a6fa6c10d5ac0f7eda4",
      "PoetName": "薛昂夫",
      "Form": "词",
      "Tags": [
        "元代",
        "薛昂夫"
      ],
      "Contents": [
        "登高懒，",
        "且平地过重阳。",
        "风雨又何妨。",
        "问牛山悲泪又何苦，",
        "龙山佳会又何狂。",
        "笑渊明，",
        "便归去，",
        "又何忙。",
        "也休说、玉堂金马乐。",
        "也休说、花与酒，",
        "一般香。",
        "西风莫放秋容老，",
        "时时留待客徜徉。",
        "便百年，",
        "浑是醉，",
        "几千"
      ]
    },
    {
      "_id": null,
      "Name": "楚天遥带过清江引",
      "PoetId": "4a10ecabb47d4a6fa6c10d5ac0f7eda4",
      "PoetName": "薛昂夫",
      "Form": "未知",
      "Tags": [
        "元代",
        "薛昂夫"
      ],
      "Contents": [
        "屈指数春来，",
        "弹指惊春去。",
        "蛛丝网落花，",
        "也要留春住。",
        "几日喜春晴，",
        "几夜愁春雨。",
        "六曲小山屏，",
        "题满伤春句。",
        "春若有情应解语，",
        "问着无凭据。",
        "江东日暮云，",
        "渭北春天树，",
        "不知那答儿是春住处？"
      ]
    },
    {
      "_id": null,
      "Name": "水仙子",
      "PoetId": "4a10ecabb47d4a6fa6c10d5ac0f7eda4",
      "PoetName": "薛昂夫",
      "Form": "词",
      "Tags": [
        "元代",
        "薛昂夫"
      ],
      "Contents": [
        "几年无事傍江湖，",
        "醉倒黄公旧酒垆。",
        "人间纵有伤心处，",
        "也不到刘伶坟上土，",
        "醉乡中不辨贤愚。",
        "对风流人物，",
        "看江山画图，",
        "便醉倒何如！"
      ]
    }
  ]
}