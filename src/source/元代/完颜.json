  "Poems": [
    {
      "_id": null,
      "Name": "临江仙",
      "PoetId": "fe152e7a78d544c886a14e4eb0bc31e1",
      "PoetName": "完颜",
      "Form": "词",
      "Tags": [
        "元代",
        "完颜"
      ],
      "Contents": [
        "倦客更遭尘事冗，",
        "故寻闲地婆娑。",
        "一尊芳酒一声歌。",
        "卢郎心未老，",
        "潘令鬓行皤。",
        "醉向繁台台上问，",
        "满川细柳新荷。",
        "薰风楼阁夕阳多。",
        "倚阑凝思久，",
        "渔笛起烟波。"
      ]
    },
    {
      "_id": null,
      "Name": "春草碧",
      "PoetId": "fe152e7a78d544c886a14e4eb0bc31e1",
      "PoetName": "完颜",
      "Form": "词",
      "Tags": [
        "元代",
        "完颜"
      ],
      "Contents": [
        "几番风雨西城陌。",
        "不见海棠红、梨花白。",
        "底事胜赏匆匆，",
        "正自天付酒肠窄。",
        "更笑老东君、人间客。",
        "赖有玉管新翻，",
        "罗襟醉墨。",
        "望中倚阑人，",
        "如曾识。",
        "旧梦回首何堪，",
        "故苑春光又陈迹。",
        "落尽后庭花，",
        "春草碧。"
      ]
    },
    {
      "_id": null,
      "Name": "朝中措",
      "PoetId": "fe152e7a78d544c886a14e4eb0bc31e1",
      "PoetName": "完颜",
      "Form": "词",
      "Tags": [
        "元代",
        "完颜"
      ],
      "Contents": [
        "襄阳古道灞陵桥。",
        "诗兴与秋高。",
        "千古风流人物，",
        "一时多少雄豪。",
        "霜清玉塞，",
        "云飞陇首，",
        "风落江皋。",
        "梦到凤凰台上，",
        "山围故国周遭。"
      ]
    },
    {
      "_id": null,
      "Name": "沁园春",
      "PoetId": "fe152e7a78d544c886a14e4eb0bc31e1",
      "PoetName": "完颜",
      "Form": "词",
      "Tags": [
        "元代",
        "完颜"
      ],
      "Contents": [
        "壮岁耽书，",
        "黄卷青灯，",
        "留连寸阴。",
        "到中年赢得，",
        "清贫更甚，",
        "苍颜明镜，",
        "白发轻簪。",
        "衲被蒙头，",
        "草鞋著脚，",
        "风雨潇潇秋意深。",
        "凄凉否，",
        "瓶中匮粟，",
        "指下忘琴。",
        "一篇梁父高吟。",
        "看谷变陵迁古又今。",
        "便离骚经了，",
        "灵光赋就，",
        "行歌白雪，",
        "愈少知音。",
        "试问先生，",
        "如何即是，",
        "布袖长垂不上襟。",
        "掀髯笑，",
        "一杯有味，",
        "万事无心。"
      ]
    },
    {
      "_id": null,
      "Name": "渔父",
      "PoetId": "fe152e7a78d544c886a14e4eb0bc31e1",
      "PoetName": "完颜",
      "Form": "词",
      "Tags": [
        "元代",
        "完颜"
      ],
      "Contents": [
        "杨柳风前白板扉。",
        "荷花雨里绿蓑衣。",
        "红稻美，",
        "锦鳞肥。",
        "渔笛闲拈月下"
      ]
    },
    {
      "_id": null,
      "Name": "秦楼月",
      "PoetId": "fe152e7a78d544c886a14e4eb0bc31e1",
      "PoetName": "完颜",
      "Form": "未知",
      "Tags": [
        "元代",
        "完颜"
      ],
      "Contents": [
        "寒仍暑。",
        "春来秋去无今古。",
        "无今古。",
        "梁台风月，",
        "汴堤烟雨。",
        "水涵天影秋如许。",
        "夕阳低处征帆举。",
        "征帆举。",
        "一行惊雁，",
        "数声柔橹。"
      ]
    },
    {
      "_id": null,
      "Name": "西江月",
      "PoetId": "fe152e7a78d544c886a14e4eb0bc31e1",
      "PoetName": "完颜",
      "Form": "词",
      "Tags": [
        "元代",
        "完颜"
      ],
      "Contents": [
        "一百八般佛事，",
        "二十四考中书。",
        "山林城市等区区。",
        "著甚由来自苦。",
        "过寺谈些般若，",
        "逢花倒个葫芦。",
        "少时伶俐老来遇。",
        "万事安於所遇。"
      ]
    },
    {
      "_id": null,
      "Name": "青玉案",
      "PoetId": "fe152e7a78d544c886a14e4eb0bc31e1",
      "PoetName": "完颜",
      "Form": "词",
      "Tags": [
        "元代",
        "完颜"
      ],
      "Contents": [
        "冻云封却驼冈路。",
        "有谁访、溪梅去。",
        "梦里疏香风似度。",
        "觉来惟见，",
        "一窗凉月，",
        "瘦影无寻处。",
        "明朝画笔江天暮。",
        "定向渔蓑得奇句。",
        "试问帘前深几许。",
        "儿童笑道，",
        "黄昏时候，",
        "犹是帘纤雨。"
      ]
    }
  ]
}