      "_id": null,
      "Name": "普天乐",
      "PoetId": "b0948cbd546e45b7ba21e3d1b032fc12",
      "PoetName": "滕斌",
      "Form": "词",
      "Tags": [
        "元代",
        "滕斌"
      ],
      "Contents": [
        "叹光阴，",
        "如流水。",
        "区区终日，",
        "枉用心机。",
        "辞是非，",
        "绝名利，",
        "笔砚诗书为活计。",
        "乐齑盐稚子山妻。",
        "茅舍数间，",
        "田园二顷，",
        "归去来兮！"
      ]
    }
  ]
}