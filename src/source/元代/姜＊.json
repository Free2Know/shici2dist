  "Poems": [
    {
      "_id": null,
      "Name": "浣溪沙",
      "PoetId": "769749d330024bcfbf89d6e78edd07c8",
      "PoetName": "姜＊",
      "Form": "词",
      "Tags": [
        "元代",
        "姜＊"
      ],
      "Contents": [
        "山滴岚光水拍堤。",
        "草香沙暖净无泥。",
        "只疑误入武林溪。",
        "两岸桃花烘日出，",
        "四围高柳到天垂。",
        "一尊心事百年期。",
        "视水利，",
        "敬谒祠下，",
        "少道目前之胜慨。",
        "从行者，",
        "前岚州知州平晋尹魏章，",
        "书吏王中千，",
        "中权秉中伯庸，",
        "簿尉史彦英。",
        "金石萃编补正卷三"
      ]
    },
    {
      "_id": null,
      "Name": "鹧鸪天",
      "PoetId": "769749d330024bcfbf89d6e78edd07c8",
      "PoetName": "姜＊",
      "Form": "词",
      "Tags": [
        "元代",
        "姜＊"
      ],
      "Contents": [
        "满谷萧萧落叶黄。",
        "绣衣总马驻平岗。",
        "一川野色迷秋色，",
        "四面山光接水光。",
        "花作阵，",
        "酒为浆。",
        "晋祠风物正重阳。",
        "殷勒留住黄华使，",
        "同放乾坤入醉"
      ]
    }
  ]
}