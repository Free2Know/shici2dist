  "Poems": [
    {
      "_id": null,
      "Name": "六国朝令",
      "PoetId": "eb7f1163a7d147c58a05c42bfa88d037",
      "PoetName": "耶律铸",
      "Form": "未知",
      "Tags": [
        "元代",
        "耶律铸"
      ],
      "Contents": [
        "鸣珂绣毂，",
        "锦带吴钩。",
        "曾雅称、量金结胜游。",
        "信人间无点事、可挂心头。",
        "须知，",
        "不待把闲情酿做闲愁。",
        "只恐落高人第二筹。",
        "歌云容裔，",
        "梦雨迟留。",
        "惯振芳尘，",
        "不楼。",
        "光饰仙春盛迹，",
        "点化温柔。",
        "索教颓纵惜花人，",
        "标傍风流。",
        "快入醉乡来，",
        "刘醉侯。"
      ]
    },
    {
      "_id": null,
      "Name": "南乡子",
      "PoetId": "eb7f1163a7d147c58a05c42bfa88d037",
      "PoetName": "耶律铸",
      "Form": "词",
      "Tags": [
        "元代",
        "耶律铸"
      ],
      "Contents": [
        "匹马赴严宸。",
        "将谓青云上致身。",
        "不是男儿容易事，",
        "风尘。",
        "水远山长愁杀人。",
        "离别若为情。",
        "雪暗西山泪满巾。",
        "还忆夜来分手处，",
        "天津。",
        "桃李无言"
      ]
    },
    {
      "_id": null,
      "Name": "太常引",
      "PoetId": "eb7f1163a7d147c58a05c42bfa88d037",
      "PoetName": "耶律铸",
      "Form": "词",
      "Tags": [
        "元代",
        "耶律铸"
      ],
      "Contents": [
        "扣声寂寞播阳春。",
        "看流水、混行云。",
        "大雅□扶轮。",
        "忍颜继、齐梁后尘。",
        "清风明月，",
        "四时长在，",
        "光景自长新。",
        "不见谪仙人。",
        "更何处、乘槎问津。"
      ]
    },
    {
      "_id": null,
      "Name": "忆秦娥",
      "PoetId": "eb7f1163a7d147c58a05c42bfa88d037",
      "PoetName": "耶律铸",
      "Form": "词",
      "Tags": [
        "元代",
        "耶律铸"
      ],
      "Contents": [
        "恨凝积。",
        "佳人薄命尤堪惜。",
        "尤堪惜。",
        "事如春梦，",
        "了无遗迹。",
        "人生适意无南北。",
        "相逢何必曾相识。",
        "曾相识。",
        "恍疑犹览，",
        "内家园籍。"
      ]
    },
    {
      "_id": null,
      "Name": "木兰花慢",
      "PoetId": "eb7f1163a7d147c58a05c42bfa88d037",
      "PoetName": "耶律铸",
      "Form": "词",
      "Tags": [
        "元代",
        "耶律铸"
      ],
      "Contents": [
        "花枝临太液，",
        "□解语、入温柔。",
        "＊桂窟低迷，",
        "天香飘荡，",
        "倒影迟留。",
        "须知画图难足，",
        "更青山环抱帝王州。",
        "□□□□□□，",
        "□□□□□□。",
        "□□□□□□□。",
        "凤吹绕瀛洲。",
        "记水浅蓬莱，",
        "尘扬沧海，",
        "一醉都休。",
        "华胥梦，",
        "虽无迹，",
        "甚鼎湖、龙去水空流。",
        "青鸟不来难问，",
        "玉妃几度仙游。"
      ]
    },
    {
      "_id": null,
      "Name": "满庭芳",
      "PoetId": "eb7f1163a7d147c58a05c42bfa88d037",
      "PoetName": "耶律铸",
      "Form": "词",
      "Tags": [
        "元代",
        "耶律铸"
      ],
      "Contents": [
        "酒阵诗坛，",
        "征兵命将，",
        "得无倾动华筵。",
        "拟勤春事，",
        "还自要相先。",
        "天地元如逆旅，",
        "应自愧、不驻流年。",
        "凭谁问，",
        "娥心事，",
        "何惜月长圆。",
        "西园张乐地，",
        "献歌呈舞，",
        "燕扰莺喧。",
        "尽未妨颓玉，",
        "锦瑟旁边。",
        "脱落尘凡健笔，",
        "终不负、兴染芳烟。",
        "欢缘在，",
        "判家视草，",
        "仍是玉堂仙。"
      ]
    },
    {
      "_id": null,
      "Name": "眼儿媚",
      "PoetId": "eb7f1163a7d147c58a05c42bfa88d037",
      "PoetName": "耶律铸",
      "Form": "词",
      "Tags": [
        "元代",
        "耶律铸"
      ],
      "Contents": [
        "隔江谁唱后庭花。",
        "烟淡月笼沙。",
        "水云凝恨，",
        "锦帆何事，",
        "也到天涯。",
        "寄声衰柳将烟草，",
        "且莫怨年华。",
        "东君也是，",
        "世间行客，",
        "知遇谁家。"
      ]
    },
    {
      "_id": null,
      "Name": "鹊桥仙",
      "PoetId": "eb7f1163a7d147c58a05c42bfa88d037",
      "PoetName": "耶律铸",
      "Form": "词",
      "Tags": [
        "元代",
        "耶律铸"
      ],
      "Contents": [
        "宜笑宜狂宜醉。",
        "请足成之皇都门外，",
        "玄都观里。",
        "露井树旁歌意。",
        "先生凭恁作生涯，",
        "只嘲柳嘲桃嘲李。",
        "酒龙歌凤，",
        "莫相回避。",
        "就取逢场□戏。",
        "且听人劝要推移，",
        "更宜笑宜，",
        "李树代桃僵。"
      ]
    }
  ]
}