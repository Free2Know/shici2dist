  "Poems": [
    {
      "_id": null,
      "Name": "人月圆",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "词",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "伤心莫问前朝事，",
        "重上越王台。",
        "鹧鸪啼处，",
        "东风草绿，",
        "残照花开。",
        "怅然孤啸，",
        "青山故国，",
        "乔木苍苔。",
        "当时明月，",
        "依依素影，",
        "何处飞来？"
      ]
    },
    {
      "_id": null,
      "Name": "凭栏人",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "未知",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "客有吴郎吹洞箫，",
        "明月沉江春雾晓。",
        "湘灵不可招，",
        "水云中环佩摇。"
      ]
    },
    {
      "_id": null,
      "Name": "南乡子",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "词",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "篷上雨潺潺。",
        "篷底幽人梦故山。",
        "涧户林扉元不闭，",
        "萧闲。",
        "只有飞云可往还。",
        "波冷玉珊珊。",
        "一壑松风引佩环。",
        "咏得池塘春草句，",
        "更阑。",
        "行尽千峰"
      ]
    },
    {
      "_id": null,
      "Name": "双调",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "未知",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "草茫茫秦汉陵阙，",
        "世代兴亡，",
        "却便以月影圆缺。",
        "山人家堆案图书，",
        "当窗松桂，",
        "满地薇蕨，",
        "侯门深何须刺谒，",
        "白云自可悦怡。",
        "到如今世事难说，",
        "天地间不见一个英雄，",
        "不见一个豪杰！",
        "?"
      ]
    },
    {
      "_id": null,
      "Name": "太常引",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "词",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "柳阴濯足水侵矶。",
        "香度野蔷薇。",
        "芳草绿萋萋。",
        "问何事、王孙未归。",
        "一壶浊酒，",
        "一声清唱，",
        "帘幕燕双飞。",
        "风暖试轻衣。",
        "介眉寿、遥瞻翠微。"
      ]
    },
    {
      "_id": null,
      "Name": "如梦令",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "词",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "削迹松陵华寓。",
        "藏密白云深处。",
        "造物已安排，",
        "万事何须先虑。",
        "归去。",
        "归去。",
        "海鹤山猿同住。"
      ]
    },
    {
      "_id": null,
      "Name": "定风波",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "词",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "欹帽垂鞭送客回。",
        "小桥流水一枝梅。",
        "醉后红绡都不记，",
        "□剩，",
        "幽香却解逐人来。",
        "松畔扶间频置酒。",
        "携手。",
        "与君看到十分开。",
        "少壮相从今雪鬓。",
        "因甚。",
        "流年清兴两相催。",
        "云栖子见示管夫人雪梅，",
        "与今日情景适合，",
        "因题一调定风波云。",
        "瓒记。",
        "珊"
      ]
    },
    {
      "_id": null,
      "Name": "忆秦娥",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "词",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "参差玉。",
        "笙砷暮起瑶台曲。",
        "瑶台曲。",
        "轻风香浸，",
        "夜凉肌粟。",
        "黄云巧缀飞霞绿。",
        "清吟未断秋霖续。",
        "秋霖续。",
        "恐辜花意，",
        "倒尊中＊。"
      ]
    },
    {
      "_id": null,
      "Name": "柳梢青",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "词",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "楼上玉笙吹彻。",
        "白露冷、飞琼佩＊。",
        "黛浅含颦，",
        "香残栖梦，",
        "子规啼月。",
        "扬州往事荒凉，",
        "有多少、愁萦思结。",
        "燕语空梁，",
        "鸥盟寒渚，",
        "画阑飘雪。"
      ]
    },
    {
      "_id": null,
      "Name": "江城子",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "词",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "满城风雨近重阳。",
        "湿秋光。",
        "暗横塘。",
        "萧瑟汀蒲，",
        "岸柳送凄凉。",
        "亲旧登高前日梦，",
        "松菊径，",
        "也应荒。",
        "堪将何物比愁长。",
        "绿泱泱。",
        "绕秋江。",
        "流到天涯，",
        "盘屈九回肠。",
        "烟外青萍飞白鸟，",
        "归路阻，",
        "思微茫。"
      ]
    },
    {
      "_id": null,
      "Name": "清平乐",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "词",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "汀烟溪树。",
        "总是伤心处。",
        "望断溪流东北注。",
        "梦逐孤云归去。",
        "山花野鸟初春。",
        "渔郎樵叟南津。",
        "谁识摧颓老子，",
        "醉人推骂从嗔。"
      ]
    },
    {
      "_id": null,
      "Name": "蝶恋花",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "词",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "夜永愁人偏起早。",
        "客鬓萧萧，",
        "镜里看枯槁。",
        "雨叶铺庭风为扫。",
        "闭门寂寞生秋草。",
        "行路难行悲远道。",
        "说着客行，",
        "真个令人恼。",
        "久客还家贫亦好。",
        "子二首折桂令二首水仙子一首小桃红三首以其均为曲调，",
        "故不录。",
        "又珊瑚网名画题跋卷八载倪瓒题画朝中措幽姿不入少年场一首，",
        "乃陆游词，",
        "兹亦"
      ]
    },
    {
      "_id": null,
      "Name": "踏莎行",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "词",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "春渚芹蒲，",
        "秋郊梨枣。",
        "西风沃野收红稻。",
        "檐前炙背媚晴阳，",
        "天涯转瞬萋芳草。",
        "鲁望渔村，",
        "陶朱烟岛。",
        "高风峻节如今扫。",
        "黄鸡啄黍浊醪香，",
        "开门"
      ]
    },
    {
      "_id": null,
      "Name": "鹊桥仙",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "词",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "富豪休恃。",
        "英雄休使。",
        "一旦繁华如洗。",
        "鹊巢何事借鸠居，",
        "看数载、主三易矣。",
        "东家烟起。",
        "西家烟起。",
        "无复碧＊朱＊。",
        "我来重宿半间云，",
        "算旧制"
      ]
    },
    {
      "_id": null,
      "Name": "鹧鸪天",
      "PoetId": "3e8af67e19a3462db3669c2c035d69ef",
      "PoetName": "倪瓒",
      "Form": "词",
      "Tags": [
        "元代",
        "倪瓒"
      ],
      "Contents": [
        "笠泽沿回十五年。",
        "亲知情义日堪怜。",
        "偷见三顾吾何有，",
        "俗士群讥自省愆。",
        "聊复尔，",
        "岂其然。",
        "田翁轻慢牧童颠。",
        "乃知造物深相与，",
        "急使江湖棹去"
      ]
    }
  ]
}