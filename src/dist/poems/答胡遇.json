  "_id": "53850ba27395453db7eaedc1e86a9c29",
  "name": "答胡遇",
  "contents": [
    "一会一分离，",
    "贫游少定期。",
    "酒多为客稳，",
    "米贵入城迟。",
    "晴日偷将睡，",
    "秋山乞与诗。",
    "纵然眉得展，",
    "不似见君时。"
  ],
  "ptype": null,
  "poetName": "姚合",
