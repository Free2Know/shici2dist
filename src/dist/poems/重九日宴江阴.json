  "_id": "cc574f4a5da0478abaf53e39000e9624",
  "name": "重九日宴江阴",
  "contents": [
    "蟋蟀期归晚，",
    "茱萸节候新。",
    "降霜青女月，",
    "送酒白衣人。",
    "高兴要长寿，",
    "卑栖隔近臣。",
    "龙沙即此地，",
    "旧俗坐为邻。"
  ],
  "ptype": null,
  "poetName": "杜审言",
