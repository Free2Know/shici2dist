  "_id": "27cf246ab05a477199c742253773cccb",
  "name": "边愁",
  "contents": [
    "九月蓬根断，",
    "三边草叶腓。",
    "风尘马变色，",
    "霜雪剑生衣。",
    "客思愁阴晚，",
    "边书驿骑归。",
    "殷勤凤楼上，",
    "还袂及春晖。"
  ],
  "ptype": null,
  "poetName": "崔湜",
  "poetId": "9c59aa8b86e944e9a4889dd700d0545e",
