  "_id": "2f7b5e31b06b4179835227f3dcd6fb05",
  "name": "冬夜寄白阁僧",
  "contents": [
    "营营不自息，",
    "暌阔数年情。",
    "林下期难遂，",
    "人间事旋生。",
    "鸟栖寒水迥，",
    "月映积冰清。",
    "石室焚香坐，",
    "悬知不为名。"
  ],
  "ptype": null,
  "poetName": "刘得仁",
  "poetId": "30c08af6ecc541c6a823a0d406855aab",
