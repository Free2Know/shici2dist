  "_id": "5c34fa717b0a46b7b3aed23cb9ea88c1",
  "name": "居延海树闻莺同作",
  "contents": [
    "边池无芳树。",
    "莺声忽听新。",
    "间关如有意。",
    "愁绝若怀人。",
    "明妃失汉宠。",
    "蔡女没胡尘。",
    "坐闻应落泪。",
    "况忆故园春。"
  ],
  "ptype": null,
  "poetName": "陈子昂",
  "poetId": "8b02b0d4021e432395ce71e4783108d3",
