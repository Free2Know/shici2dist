{
  "_id": "5a539f85a66448f0a7a4d01063574457",
  "name": "贻世",
  "contents": [
    "至理不误物，",
    "悠悠自不明。",
    "黄金烧欲尽，",
    "白发火边生。",
    "苦惑神仙谜，",
    "难收日月精。",
    "捕风兼系影，",
    "信矣不须争。"
  ],
  "ptype": null,
  "poetName": "贯休",
