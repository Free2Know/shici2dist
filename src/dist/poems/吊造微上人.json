{
  "_id": "12ba16f625fc4cd1957750034beb00fd",
  "name": "吊造微上人",
  "contents": [
    "至人随化往，",
    "遗路自堪伤。",
    "白塔收真骨，",
    "青山闭影堂。",
    "钟残含细韵，",
    "烟灭有余香。",
    "松上斋乌在，",
    "迟迟立夕阳。"
  ],
  "ptype": null,
  "poetName": "张乔",
  "poetId": "12203b3aad6548b487528cd4bc5d4c4b",
