  "_id": "c241d25abdfd4e6cb42f15aa28e5c74a",
  "name": "重经冯家旧里",
  "contents": [
    "冯家旧宅闭柴关，",
    "修竹犹存潏水湾。",
    "应系星辰天上去，",
    "不留英骨葬人间。"
  ],
  "ptype": null,
  "poetName": "唐彦谦",
