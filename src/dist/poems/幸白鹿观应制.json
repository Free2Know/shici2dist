  "_id": "b2da3a72463d494f8aecba7bc9fdfa6a",
  "name": "幸白鹿观应制",
  "contents": [
    "碧虚清吹下，",
    "蔼蔼入仙宫。",
    "松磴攀云绝，",
    "花源接涧空。",
    "受符邀羽使，",
    "传诀注香童。",
    "讵似闲居日，",
    "徒闻有顺风。"
  ],
  "ptype": null,
  "poetName": "苏颋",
  "poetId": "ae2d87ad6a8845c39c3264947a0f39b0",
