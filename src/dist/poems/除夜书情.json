  "_id": "142bf21a2a87461d94ed875450d980b7",
  "name": "除夜书情",
  "contents": [
    "何处风尘岁，",
    "云阳古驿前。",
    "三冬不再稔，",
    "晓日又明年。",
    "春入江南柳，",
    "寒归塞北天。",
    "还伤知候客，",
    "花景对韦编。"
  ],
  "ptype": null,
  "poetName": "周弘亮",
