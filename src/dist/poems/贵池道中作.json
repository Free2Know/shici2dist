  "_id": "d6807332599a4ff78af7bf077d02a519",
  "name": "贵池道中作",
  "contents": [
    "羸骖驱野岸，",
    "山远路盘盘。",
    "清露月华晓，",
    "碧江星影寒。",
    "离群徒长泣，",
    "去国自加餐。",
    "霄汉宁无旧，",
    "相哀自语端。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "03ce3b6c2c524f708213939828d7fb40",
