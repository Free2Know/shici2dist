{
  "_id": "20bfd908faed4ba28d51f427b79bc3d4",
  "name": "山居晚眺赠王道士",
  "contents": [
    "金坛疏俗宇，",
    "玉洞侣仙群。",
    "花枝栖晚露，",
    "峰叶度晴云。",
    "斜照移山影，",
    "回沙拥籀文。",
    "琴尊方待兴，",
    "竹树已迎曛。"
  ],
  "ptype": null,
  "poetName": "王勃",
  "poetId": "895be36ac87d4db0b8d095b26cae0f05",
