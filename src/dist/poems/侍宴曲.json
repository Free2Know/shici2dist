  "_id": "ac779b16ed7d44b89627c8e9f9543825",
  "name": "侍宴曲",
  "contents": [
    "云陛褰珠扆，",
    "天墀覆绿杨。",
    "隔帘妆隐映，",
    "向席舞低昂。",
    "鸣佩长廊静，",
    "开冰广殿凉。",
    "欢馀剑履散，",
    "同辇入昭阳。"
  ],
  "ptype": null,
  "poetName": "贾至",
  "poetId": "0d8775b64eb64b0d9a7af0ec1f1483ff",
