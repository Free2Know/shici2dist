  "_id": "beff92e4d3444511b6aab300749992bc",
  "name": "读始兴公传",
  "contents": [
    "殁世议方存，",
    "升平道几论。",
    "诗情光日月，",
    "笔力动乾坤。",
    "乱首光雄算，",
    "朝纲在典坟。",
    "明时封禅绩，",
    "山下见丘门。"
  ],
  "ptype": null,
  "poetName": "张祜",
