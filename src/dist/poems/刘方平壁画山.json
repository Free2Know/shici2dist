  "_id": "11fb429b70784456b07ff6de03892773",
  "name": "刘方平壁画山",
  "contents": [
    "墨妙无前，",
    "性生笔先。",
    "回溪已失，",
    "远嶂犹连。",
    "侧径樵客，",
    "长林野烟。",
    "青峰之外，",
    "何处云天。"
  ],
  "ptype": null,
  "poetName": "皇甫冉",
