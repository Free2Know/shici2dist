  "_id": "fa42baa475ff4d89af77658fd7e2be44",
  "name": "陪",
  "contents": [
    "后伏中宵月，",
    "高秋满魄齐。",
    "轮移仙掌外，",
    "影下玉绳西。",
    "蝉雀飞多误，",
    "星萤出自迷。",
    "烦蒸惊顿绝，",
    "吟玩畏闻鸡。"
  ],
  "ptype": null,
  "poetName": "许棠",
  "poetId": "43a44b06f0b241c8a01ce6c465c30163",
