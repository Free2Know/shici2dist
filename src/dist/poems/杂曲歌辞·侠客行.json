{
  "_id": "044330636f5c48c1a0c775795a830430",
  "name": "杂曲歌辞·侠客行",
  "contents": [
    "欲出鸿都门，",
    "阴云蔽城阙。",
    "宝剑黯如水，",
    "微红湿馀血。",
    "白马夜频嘶，",
    "三更霸陵雪。"
  ],
  "ptype": null,
  "poetName": "温庭筠",
