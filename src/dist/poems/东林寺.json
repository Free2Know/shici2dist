  "_id": "7d056c376d8b450f9efeaa4b3262c385",
  "name": "东林寺",
  "contents": [
    "欲去不忍去，",
    "徘徊吟绕廊。",
    "水光秋澹荡，",
    "僧好语寻常。",
    "碑古苔文叠，",
    "山晴钟韵长。",
    "翻思南岳上，",
    "欠此白莲香。"
  ],
  "ptype": null,
  "poetName": "修睦",
  "poetId": "81e8dde9e87e4dfab0864552dda46369",
