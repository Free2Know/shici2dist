  "_id": "a2164dfb70914e79af8d3d4275c5c283",
  "name": "享太庙乐章",
  "contents": [
    "于铄丕嗣，",
    "惟帝之光。",
    "羽籥象德，",
    "金石荐祥。",
    "圣系无极，",
    "景命永昌。",
    "神降上哲，",
    "维天配长。"
  ],
  "ptype": null,
  "poetName": "萧仿",
