  "_id": "93bfeb47bd604deea48dc7ed69cb3776",
  "name": "湘江曲",
  "contents": [
    "湘水无潮秋水阔，",
    "湘中月落行人发。",
    "送人发，",
    "送人归，",
    "白蘋茫茫鹧鸪飞。"
  ],
  "ptype": null,
  "poetName": "张籍",
