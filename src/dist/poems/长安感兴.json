  "_id": "6010674063d142fdba5d0a7f002afed4",
  "name": "长安感兴",
  "contents": [
    "徒劳悲丧乱，",
    "自古戒繁华。",
    "落日狐兔径，",
    "近年公相家。",
    "可悲闻玉笛，",
    "不见走香车。",
    "寂寞墙匡里，",
    "春阴挫杏花。"
  ],
  "ptype": null,
  "poetName": "郑谷",
  "poetId": "8f43db35f49d4c51bd1e9704dd65363c",
