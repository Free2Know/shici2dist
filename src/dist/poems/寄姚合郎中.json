  "_id": "58ef6488a48342ae9cb6fe8185d977a7",
  "name": "寄姚合郎中",
  "contents": [
    "转刺名山郡，",
    "连年别省曹。",
    "分题得客少，",
    "著价买书高。",
    "晚柳蝉和角，",
    "寒城烛照涛。",
    "鄱溪卧疾久，",
    "未获后乘骚。"
  ],
  "ptype": null,
  "poetName": "周贺",
  "poetId": "2cc3ab32df034e3b860066c9490ae869",
