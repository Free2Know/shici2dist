{
  "_id": "f0e038908a1c4ee99621577793e0e444",
  "name": "发潭州",
  "contents": [
    "夜醉长沙酒，",
    "晓行湘水春。",
    "岸花飞送客，",
    "樯燕语留人。",
    "贾傅才未有，",
    "褚公书绝伦。",
    "高名前后事，",
    "回首一伤神。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "526c710fccb149fe933edb19dad0ed50",
