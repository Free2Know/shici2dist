  "_id": "c9fb89626f32479ca11b10da677871c2",
  "name": "题友人屋",
  "contents": [
    "松底诗人宅，",
    "闲门远岫孤。",
    "数家分小径，",
    "一水截平芜。",
    "竹节偶相对，",
    "鸟名多自呼。",
    "爱君真静者，",
    "欲去又踟蹰。"
  ],
  "ptype": null,
  "poetName": "李昌符",
