  "_id": "2c4298fefac54449af50aaa16f699905",
  "name": "夏日题武功姚主簿",
  "contents": [
    "亭午无公事，",
    "垂帘树色间。",
    "僧来茶灶动，",
    "吏去印床闲。",
    "傍竹行寻巷，",
    "当门立看山。",
    "吟诗老不倦，",
    "未省话官班。"
  ],
  "ptype": null,
  "poetName": "朱庆馀",
