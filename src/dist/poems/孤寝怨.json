{
  "_id": "1725af05a2e543c383464f837d6a1abd",
  "name": "孤寝怨",
  "contents": [
    "征戍动经年，",
    "含情拂玳筵。",
    "花飞织锦处，",
    "月落捣衣边。",
    "灯暗愁孤坐，",
    "床空怨独眠。",
    "自君辽海去，",
    "玉匣闭春弦。"
  ],
  "ptype": null,
  "poetName": "崔珪",
  "poetId": "0512713084894e9781d170a97f523a2a",
