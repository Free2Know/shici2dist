  "_id": "51bab48959db4ddbb7176ed05f14cc69",
  "name": "湘灵瑟",
  "contents": [
    "霜风摧兰。",
    "银屏生晓寒。",
    "淡扫眉山。",
    "脸红殷。",
    "潇湘浦，",
    "芙蓉湾。",
    "相思数声哀叹。",
    "画楼尊酒闲。"
  ],
  "ptype": null,
  "poetName": "宋无名氏",
