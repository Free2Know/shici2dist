  "_id": "72be084b65764b2281d4dd1c441e5037",
  "name": "赠山僧",
  "contents": [
    "荣枯虽在目，",
    "名利不关身。",
    "高出城隍寺，",
    "野为云鹤邻。",
    "松声寒后远，",
    "潭色雨余新。",
    "岂住空空里，",
    "空空亦是尘。"
  ],
  "ptype": null,
  "poetName": "李咸用",
