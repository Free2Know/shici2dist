  "_id": "e0d09d26926e4b2385e57d18ef9058f7",
  "name": "夫入相寄姨妹",
  "contents": [
    "相国已随麟阁贵，",
    "家风第一右丞诗。",
    "笄年解笑鸣机妇，",
    "耻见苏秦富贵时。"
  ],
  "ptype": null,
  "poetName": "王韫秀",
