  "_id": "bf111e221d7940c182453216c61f8088",
  "name": "富春赠孙璐",
  "contents": [
    "天柱与天目，",
    "曾栖绝顶房。",
    "青云求禄晚，",
    "白日坐家长。",
    "井气通潮信，",
    "窗风引海凉。",
    "平生诗称在，",
    "老达亦何妨。"
  ],
  "ptype": null,
  "poetName": "李频",
