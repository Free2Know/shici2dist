  "name": "芦花动",
  "contents": [
    "连素穗，",
    "翻秋气，",
    "细节疏茎任长吹。",
    "共作月中声，",
    "孤舟发乡思。"
  ],
  "ptype": null,
  "poetName": "耿湋",
