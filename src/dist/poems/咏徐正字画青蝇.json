  "_id": "e258f938adef4092b2c9b3327ecdb93d",
  "name": "咏徐正字画青蝇",
  "contents": [
    "误点能成物，",
    "迷真许一时。",
    "笔端来已久，",
    "座上去何迟。",
    "顾白曾无变，",
    "听鸡不复疑。",
    "讵劳才子赏，",
    "为入国人诗。"
  ],
  "ptype": null,
  "poetName": "韦应物",
