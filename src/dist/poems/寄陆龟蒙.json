  "_id": "256fe10dfec64553a85264826e56ede2",
  "name": "寄陆龟蒙",
  "contents": [
    "龙楼李丞相，",
    "昔岁仰高文。",
    "黄阁寻无主，",
    "青山竟未焚。",
    "夜船乘海月，",
    "秋寺伴江云。",
    "却恐尘埃里，",
    "浮名点污君。"
  ],
  "ptype": null,
  "poetName": "罗隐",
