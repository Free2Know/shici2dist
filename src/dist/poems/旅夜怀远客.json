  "_id": "40ccb437b0e04bd19c06c4bce7acf040",
  "name": "旅夜怀远客",
  "contents": [
    "异乡多远情，",
    "梦断落江城。",
    "病起惭书癖，",
    "贫家负酒名。",
    "过春花自落，",
    "竟晓月空明。",
    "独此一长啸，",
    "故人天际行。"
  ],
  "ptype": null,
  "poetName": "许浑",
  "poetId": "fec65c0dd6484000837ecf7a1f995f70",
