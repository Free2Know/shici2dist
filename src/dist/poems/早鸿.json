  "_id": "e425df1286e244958157aee913f4bfb1",
  "name": "早鸿",
  "contents": [
    "木落波浪动，",
    "南飞闻夜鸿。",
    "参差天汉雾，",
    "嘹唳月明风。",
    "野水莲茎折，",
    "寒泥稻穗空。",
    "无令一行侣，",
    "相失五湖中。"
  ],
  "ptype": null,
  "poetName": "李群玉",
  "poetId": "a4c6a461feac43a996ed4d80ebb5404f",
