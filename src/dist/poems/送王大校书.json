  "_id": "224ee9e0c6ee43b4922768aac280405e",
  "name": "送王大校书",
  "contents": [
    "导漾自嶓冢，",
    "东流为汉川。",
    "维桑君有意，",
    "解缆我开筵。",
    "云雨从兹别，",
    "林端意渺然。",
    "尺书能不吝，",
    "时望鲤鱼传。"
  ],
  "ptype": null,
  "poetName": "孟浩然",
  "poetId": "0c45914b5aa54e3ab0a658708eed6c00",
