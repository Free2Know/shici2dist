  "_id": "e302c9b6ab694721b5d237fd045c68db",
  "name": "越江吟",
  "contents": [
    "神仙神仙瑶池宴。",
    "片片。",
    "碧桃零落春风晚。",
    "翠云开处，",
    "隐隐金舆挽。",
    "玉麟背冷清风远。"
  ],
  "ptype": null,
  "poetName": "苏易简",
