  "_id": "7b5bdf266f274a74a55ddec2db99ebdc",
  "name": "赠月溪羽士",
  "contents": [
    "月明溪水上，",
    "谁识步虚声。",
    "夜静金波冷，",
    "风微玉练平。",
    "自知尘梦远，",
    "一洗道心清。",
    "更弄瑶笙罢，",
    "秋空鹤又鸣。"
  ],
  "ptype": null,
  "poetName": "戴叔伦",
