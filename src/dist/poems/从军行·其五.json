  "_id": "b6e5f284e065429089a5c5f9e210e587",
  "name": "从军行·其五",
  "contents": [
    "大漠风尘日色昏，",
    "红旗半卷出辕门。",
    "前军夜战洮河北，",
    "已报生擒吐谷浑。"
  ],
  "ptype": null,
  "poetName": "王昌龄",
