  "_id": "712a644acede455bb1417fbdc3256c98",
  "name": "五郊乐章·雍和",
  "contents": [
    "金悬夕肆，",
    "玉俎朝陈。",
    "飨荐黄道，",
    "芬流紫辰。",
    "乃诚乃敬，",
    "载享载諲。",
    "崇荐斯在，",
    "惟皇是宾。"
  ],
  "ptype": null,
  "poetName": "魏征",
