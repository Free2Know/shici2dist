{
  "_id": "06c4fa21160a412f98603f6e29972fed",
  "name": "归田",
  "contents": [
    "陶君三十七，",
    "挂绶出都门。",
    "我亦今年去，",
    "商山淅岸村。",
    "冬修方丈室，",
    "春种桔槔园。",
    "千万人间事，",
    "从兹不复言。"
  ],
  "ptype": null,
  "poetName": "元稹",
  "poetId": "9863285b1f514da9a74432fd80c442aa",
