  "_id": "902e9f86348a460e85ae6f501eff46b1",
  "name": "题岫上人院",
  "contents": [
    "病客与僧闲，",
    "频来不掩关。",
    "高窗云外树，",
    "疏磬雨中山。",
    "离索秋虫响，",
    "登临夕鸟还。",
    "心知落帆处，",
    "明月浙河湾。"
  ],
  "ptype": null,
  "poetName": "许浑",
  "poetId": "fec65c0dd6484000837ecf7a1f995f70",
