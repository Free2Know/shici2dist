  "name": "白云谣",
  "contents": [
    "白云在天，",
    "丘陵自出。",
    "道里悠远，",
    "山川间之。",
    "将子无死，",
    "尚复能来。"
  ],
  "ptype": null,
  "poetName": "先秦无名氏",
