  "_id": "5044e457aac04d5ba2b2c7242a4062a0",
  "name": "濬公院怀旧",
  "contents": [
    "远公传教毕，",
    "身没向他方。",
    "吊客来何见，",
    "门人闭影堂。",
    "纱灯临古砌，",
    "尘札在空床。",
    "寂寞疏钟后，",
    "秋天有夕阳。"
  ],
  "ptype": null,
  "poetName": "耿湋",
