  "_id": "51f9a71bf43e48aead50439a60e3d18d",
  "name": "永同欢",
  "contents": [
    "绣帘卷，",
    "沉烟细。",
    "燕堂深，",
    "玳筵初启。",
    "庭下芝兰，",
    "劝金卮，",
    "有多少雍容和气。"
  ],
  "ptype": null,
  "poetName": "仲殊",
