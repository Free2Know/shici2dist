{
  "_id": "c1bc99354bdc40448b83e865ab1dbd8f",
  "name": "七夕偶题",
  "contents": [
    "宝婺摇珠佩，",
    "常娥照玉轮。",
    "灵归天上匹，",
    "巧遗世间人。",
    "花果香千户，",
    "笙竽滥四邻。",
    "明朝晒犊鼻，",
    "方信阮家贫。"
  ],
  "ptype": null,
  "poetName": "李商隐",
  "poetId": "1c1222ee1bc9466f96b5fa0cb769c5c7",
