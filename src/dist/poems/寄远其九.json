  "_id": "4d103b2056ae4f42b403d90f3cad9762",
  "name": "寄远其九",
  "contents": [
    "长短春草绿。",
    "缘阶如有情。",
    "卷施心独苦。",
    "抽却死还生。",
    "睹物知妾意。",
    "希君种後庭。",
    "闲时当采掇。",
    "念此莫相轻。"
  ],
  "ptype": null,
  "poetName": "李白",
