{
  "_id": "d19760d8f8a3436ba6b13516fb20421c",
  "name": "新春江次",
  "contents": [
    "浦干潮未应，",
    "堤湿冻初销。",
    "粉片妆梅朵，",
    "金丝刷柳条。",
    "鸭头新绿水，",
    "雁齿小红桥。",
    "莫怪珂声碎，",
    "春来五马骄。"
  ],
  "ptype": null,
  "poetName": "白居易",
  "poetId": "4b47c82f564748dcb909526665b648f9",
