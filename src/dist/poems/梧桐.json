{
  "_id": "371ea45a4059490198bfacea628e8221",
  "name": "梧桐",
  "contents": [
    "亭亭南轩外，",
    "贞干修且直。",
    "广叶结青阴，",
    "繁花连素色。",
    "天资韶雅性，",
    "不愧知音识。"
  ],
  "ptype": null,
  "poetName": "戴叔伦",
