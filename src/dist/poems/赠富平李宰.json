  "_id": "efe7bdb6653a446baea61bbe8886c25c",
  "name": "赠富平李宰",
  "contents": [
    "夫君清且贫，",
    "琴鹤最相亲。",
    "简肃诸曹事，",
    "安闲一境人。",
    "陵山云里拜，",
    "渠路雨中巡。",
    "易得连宵醉，",
    "千缸石冻春。"
  ],
  "ptype": null,
  "poetName": "郑谷",
  "poetId": "8f43db35f49d4c51bd1e9704dd65363c",
