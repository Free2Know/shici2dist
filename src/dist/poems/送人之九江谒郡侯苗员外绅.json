{
  "_id": "c592a758e4d54c8086b8122236019905",
  "name": "送人之九江谒郡侯苗员外绅",
  "contents": [
    "泽国寻知己，",
    "南浮不偶游。",
    "湓城分楚塞，",
    "庐岳对江州。",
    "晓饭临孤屿，",
    "春帆入乱流。",
    "双旌相望处，",
    "月白庾公楼。"
  ],
  "ptype": null,
  "poetName": "郑谷",
  "poetId": "8f43db35f49d4c51bd1e9704dd65363c",
