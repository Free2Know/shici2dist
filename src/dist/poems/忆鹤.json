  "_id": "190aa4fcadac4f9bb6702d1f825ee70a",
  "name": "忆鹤",
  "contents": [
    "自尔归仙后，",
    "经秋又过春。",
    "白云寻不得，",
    "紫府去无因。",
    "此地空明月，",
    "何山伴羽人。",
    "终期华表上，",
    "重见令威身。"
  ],
  "ptype": null,
  "poetName": "刘得仁",
