  "_id": "e6956c20b84641caace44476c91381bf",
  "name": "送许员外江外置常平仓",
  "contents": [
    "诏置海陵仓，",
    "朝推画省郎。",
    "还家锦服贵，",
    "出使绣衣香。",
    "水驿风催舫，",
    "江楼月透床。",
    "仍怀陆氏橘，",
    "归献老亲尝。"
  ],
  "ptype": null,
  "poetName": "岑参",
  "poetId": "b7f498137b6b4b36a5fea97e41566cd9",
