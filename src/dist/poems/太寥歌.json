{
  "_id": "113eb7921e334b178d7c3344154a386c",
  "name": "太寥歌",
  "contents": [
    "化元灵哉，",
    "碧虚清哉，",
    "红霞明哉。",
    "冥哉茫哉，",
    "惟化之工无疆哉。"
  ],
  "ptype": null,
  "poetName": "张志和",
