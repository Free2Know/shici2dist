  "_id": "bcb3118739964f6e9920c0213f873ac8",
  "name": "角",
  "contents": [
    "闻说征人说，",
    "呜呜何处边。",
    "孤城沙塞地，",
    "残月雪霜天。",
    "会转胡风急，",
    "吹长碛雁连。",
    "应伤汉车骑，",
    "名未勒燕然。"
  ],
  "ptype": null,
  "poetName": "齐己",
