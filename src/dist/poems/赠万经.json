  "_id": "1e8275d5f0c1415fb88a1c8054b4d2ac",
  "name": "赠万经",
  "contents": [
    "万公长慢世，",
    "昨日又隳官。",
    "纵酒真彭泽，",
    "论诗得建安。",
    "家山伯禹穴，",
    "别墅小长干。",
    "辄有时人至，",
    "窗前白眼看。"
  ],
  "ptype": null,
  "poetName": "严维",
