{
  "_id": "2e424da94b2f4f1e9d948363b0c98661",
  "name": "史",
  "contents": [
    "马记天官设，",
    "班图地里新。",
    "善谈方亹亹，",
    "青简见彬彬。",
    "方朔初闻汉，",
    "荆轲昔向秦。",
    "正辞堪载笔，",
    "终冀作良臣。"
  ],
  "ptype": null,
  "poetName": "李峤",
