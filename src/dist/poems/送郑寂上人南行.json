  "_id": "c36b3b7a427a44799cacf8d83bdeaf5d",
  "name": "送郑寂上人南行",
  "contents": [
    "儒家有释子，",
    "年少学支公。",
    "心出是非外，",
    "迹辞荣辱中。",
    "锡寒秦岭月，",
    "杯急楚江风。",
    "离怨故园里，",
    "小秋梨叶红。"
  ],
  "ptype": null,
  "poetName": "许浑",
  "poetId": "fec65c0dd6484000837ecf7a1f995f70",
