{
  "_id": "350a22227d604a0984f7b11f09c53ab5",
  "name": "送卢评事婺州省觐",
  "contents": [
    "知向东阳去，",
    "晨装见彩衣。",
    "客愁青眼别，",
    "家喜玉人归。",
    "漠漠水烟晚，",
    "萧萧枫叶飞。",
    "双溪泊船处，",
    "候吏拜胡威。"
  ],
  "ptype": null,
  "poetName": "权德舆",
  "poetId": "76a2e65e03af4759ae3fbc09e9b61b8d",
