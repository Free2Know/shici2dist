{
  "_id": "796a491830c5432c88b71dda6f74876c",
  "name": "桥",
  "contents": [
    "乌鹊填应满，",
    "黄公去不归。",
    "势疑虹始见，",
    "形似雁初飞。",
    "妙应七星制，",
    "高分半月辉。",
    "秦王空构石，",
    "仙岛远难依。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "0cbc275c971d47089449819a2beaf2a5",
