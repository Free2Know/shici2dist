  "_id": "41d4164574be4677bb8561d707d33e79",
  "name": "长安送人",
  "contents": [
    "上国多离别，",
    "年年渭水滨。",
    "空将未归意，",
    "说向欲行人。",
    "雁度池塘月，",
    "山连井邑春。",
    "临岐惜分手，",
    "日暮一沾巾。"
  ],
  "ptype": null,
  "poetName": "周贺",
