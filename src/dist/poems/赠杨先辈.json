{
  "_id": "d7aa41d8a0d64a1aaef592810fe587fc",
  "name": "赠杨先辈",
  "contents": [
    "平生闲放久，",
    "野鹿许为群。",
    "居止邻西岳，",
    "轩窗度白云。",
    "斋心饭松子，",
    "话道接茅君。",
    "汉主思清净，",
    "休书谏猎文。"
  ],
  "ptype": null,
  "poetName": "马戴",
  "poetId": "d12652315fc8431eb83ae1f1670f56d5",
