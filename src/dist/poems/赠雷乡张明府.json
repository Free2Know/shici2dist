  "_id": "425cbf84bcab488e837303a286426d9c",
  "name": "赠雷乡张明府",
  "contents": [
    "任官征战后，",
    "度日寄闲身。",
    "封卷还高客，",
    "飞书问野人。",
    "废田教种谷，",
    "生路遣寻薪。",
    "若起柴桑兴，",
    "无先漉酒巾。"
  ],
  "ptype": null,
  "poetName": "曹松",
