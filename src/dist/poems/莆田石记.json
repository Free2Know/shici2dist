  "name": "莆田石记",
  "contents": [
    "石敢当，",
    "镇百鬼，",
    "压灾殃。",
    "官吏福，",
    "百姓康。",
    "风教盛，",
    "礼乐昌。"
  ],
  "ptype": null,
  "poetName": "唐无名氏",
