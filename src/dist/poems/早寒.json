  "_id": "a00c9f077a05459e8c7a7e06c78ccc5e",
  "name": "早寒",
  "contents": [
    "黄叶聚墙角，",
    "青苔围柱根。",
    "被经霜后薄，",
    "镜遇雨来昏。",
    "半卷寒檐幕，",
    "斜开暖阁门。",
    "迎冬兼送老，",
    "只仰酒盈尊。"
  ],
  "ptype": null,
  "poetName": "白居易",
  "poetId": "4b47c82f564748dcb909526665b648f9",
