  "_id": "1aac9b2f0e1d417a86982cef3e2d7e44",
  "name": "刘生",
  "contents": [
    "卿家本六郡，",
    "年长入三秦。",
    "白璧酬知己，",
    "黄金谢主人。",
    "剑锋生赤电，",
    "马足起红尘。",
    "日暮歌钟发，",
    "喧喧动四邻。"
  ],
  "ptype": null,
  "poetName": "杨炯",
  "poetId": "b44ed9425c2d45afb5b55795481db419",
