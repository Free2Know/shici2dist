  "_id": "d888e6e3d3aa40cda529aa859000d0c9",
  "name": "明皇帝挽歌",
  "contents": [
    "律历千年会，",
    "车书万里同。",
    "固期常戴日，",
    "岂意厌观风。",
    "地惨新疆理，",
    "城摧旧战功。",
    "山河万古壮，",
    "今夕尽归空。"
  ],
  "ptype": null,
  "poetName": "郑丹",
