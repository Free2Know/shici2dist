  "_id": "2eecf01bfb384b898e727a5da96db7ad",
  "name": "楼心月",
  "contents": [
    "手把新荷叶一枝。",
    "唤来池上只愁归。",
    "流萤裹在红纱袖，",
    "忽到持杯个个飞。"
  ],
  "ptype": null,
  "poetName": "宋无名氏",
