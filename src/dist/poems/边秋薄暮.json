  "_id": "fa4976b94017497a94c3af097e3cf364",
  "name": "边秋薄暮",
  "contents": [
    "海外秋鹰击，",
    "霜前旅雁归。",
    "边风思鞞鼓，",
    "落日惨旌麾。",
    "浦暗渔舟入，",
    "川长猎骑稀。",
    "客悲逢薄暮，",
    "况乃事戎机。"
  ],
  "ptype": null,
  "poetName": "苏颋",
