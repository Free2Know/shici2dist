  "_id": "1b7f877c1256483ebb3199bb12771476",
  "name": "登杭州城",
  "contents": [
    "漠漠江天外，",
    "登临返照间。",
    "潮来无别浦，",
    "木落见他山。",
    "沙鸟晴飞远，",
    "渔人夜唱闲。",
    "岁穷归未得，",
    "心逐片帆还。"
  ],
  "ptype": null,
  "poetName": "郑谷",
  "poetId": "8f43db35f49d4c51bd1e9704dd65363c",
