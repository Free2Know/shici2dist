{
  "_id": "47f0fa8d1c31412fb2f65a1648a56aab",
  "name": "赠方干",
  "contents": [
    "盛名与高隐，",
    "合近谢敷村。",
    "弟子已得桂，",
    "先生犹灌园。",
    "垂纶侵海介，",
    "拾句历云根。",
    "白日升天路，",
    "如君别有门。"
  ],
  "ptype": null,
  "poetName": "贯休",
