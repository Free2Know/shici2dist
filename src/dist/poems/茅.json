{
  "_id": "6309f5adc41342cfa1ef580a5e70bd34",
  "name": "茅",
  "contents": [
    "楚甸供王日，",
    "衡阳入贡年。",
    "麏包青野外，",
    "鸱啸绮楹前。",
    "尧帝成茨罢，",
    "殷汤祭雨旋。",
    "方期大君锡，",
    "不惧小巫捐。"
  ],
  "ptype": null,
  "poetName": "李峤",
  "poetId": "0cbc275c971d47089449819a2beaf2a5",
