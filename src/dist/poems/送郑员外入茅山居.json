  "_id": "b4e3a1a0479745e9856c5adef3a76227",
  "name": "送郑员外入茅山居",
  "contents": [
    "但见全家去，",
    "宁知几日还。",
    "白云迎谷口，",
    "流水出人间。",
    "冠冕情遗世，",
    "神仙事满山。",
    "其中应有物，",
    "岂贵一身闲。"
  ],
  "ptype": null,
  "poetName": "皇甫冉",
  "poetId": "bc4446a8d5594756be20f1ff356e27ab",
