  "_id": "220095ae5a8243aabba95013d3dc530a",
  "name": "卜居招书侣",
  "contents": [
    "忆昨未知道，",
    "临川每羡鱼。",
    "世途行处见，",
    "人事病来疏。",
    "微雨秋栽竹，",
    "孤灯夜读书。",
    "怜君亦同志，",
    "晚岁傍山居。"
  ],
  "ptype": null,
  "poetName": "杜牧",
  "poetId": "cb365a383b844378951f5ba10473b765",
