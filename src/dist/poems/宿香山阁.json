  "_id": "b93172e457c540aa8d40b1790326fe24",
  "name": "宿香山阁",
  "contents": [
    "暝望香山阁，",
    "梯云宿半空。",
    "轩窗闭潮海，",
    "枕席拂烟虹。",
    "朱网防栖鸽，",
    "纱灯护夕虫。",
    "一闻鸡唱晓，",
    "已见日曈曈。"
  ],
  "ptype": null,
  "poetName": "贾彦璋",
  "poetId": "219879af3a5f479a9f477dcf3e37cd6c",
