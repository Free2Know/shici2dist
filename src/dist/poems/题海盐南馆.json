  "_id": "bf79c2fe2c4a414a8f5eb4d6c1ee3a3b",
  "name": "题海盐南馆",
  "contents": [
    "故人营此地，",
    "台馆尚依依。",
    "黑夜山魈语，",
    "黄昏海燕归。",
    "旧阴杨叶在，",
    "残雨槿花稀。",
    "无复南亭赏，",
    "高檐红烛辉。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "03ce3b6c2c524f708213939828d7fb40",
