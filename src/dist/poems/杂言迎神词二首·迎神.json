  "_id": "90825d97e134404eb099e8868ffa4088",
  "name": "杂言迎神词二首·迎神",
  "contents": [
    "启庭户，",
    "列芳鲜；",
    "目眇眇，",
    "心绵绵，",
    "因风托雨降琼筵。",
    "纷下拜，",
    "屡加笾，",
    "人心望岁祈丰年。"
  ],
  "ptype": null,
  "poetName": "皇甫冉",
