  "_id": "9c4a95b4b57e462183a33a14513620e8",
  "name": "郁林寺",
  "contents": [
    "台殿冠嵯峨，",
    "春来日日过。",
    "水分诸院少，",
    "云近上方多。",
    "众籁凝丝竹，",
    "繁英耀绮罗。",
    "酒酣诗自逸，",
    "乘月棹寒波。"
  ],
  "ptype": null,
  "poetName": "许浑",
  "poetId": "fec65c0dd6484000837ecf7a1f995f70",
