  "_id": "0cb98096e49a46dfba2d43bb2b95c4c9",
  "name": "题广信寺",
  "contents": [
    "亭北敞灵溪，",
    "林梢与槛齐。",
    "野云来影远，",
    "沙鸟去行低。",
    "晚渡明村火，",
    "晴山响郡鼙。",
    "思乡值摇落，",
    "赖不有猿啼。"
  ],
  "ptype": null,
  "poetName": "张乔",
  "poetId": "12203b3aad6548b487528cd4bc5d4c4b",
