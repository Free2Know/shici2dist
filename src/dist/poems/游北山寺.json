  "_id": "6792ffbd3e184196bcadaa4cda169f03",
  "name": "游北山寺",
  "contents": [
    "烟冈影畔寺，",
    "游步此时孤。",
    "庭静众药在，",
    "鹤闲双桧枯。",
    "蓝峰露秋院，",
    "灞水入春厨。",
    "便可栖心迹，",
    "如何返旧途。"
  ],
  "ptype": null,
  "poetName": "喻凫",
