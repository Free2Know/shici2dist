  "_id": "12722a6785a546cba31bb57098e353d8",
  "name": "旅宿",
  "contents": [
    "旅馆无良伴，",
    "凝情自悄然。",
    "寒灯思旧事，",
    "断雁警愁眠。",
    "远梦归侵晓，",
    "家书到隔年。",
    "沧江好烟月，",
    "门系钓鱼船。"
  ],
  "ptype": null,
  "poetName": "杜牧",
