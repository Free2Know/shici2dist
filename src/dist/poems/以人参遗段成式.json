  "_id": "78f8b74f468441aeb0eb1be5a0aad1fb",
  "name": "以人参遗段成式",
  "contents": [
    "人形上品传方志，",
    "我得真英自紫团。",
    "惭非叔子空持药，",
    "更请伯言审细看。"
  ],
  "ptype": null,
  "poetName": "周繇",
