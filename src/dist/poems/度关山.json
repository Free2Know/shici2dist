{
  "_id": "08827bd0dafa4465bb69463fb06e2f84",
  "name": "度关山",
  "contents": [
    "象弭插文犀，",
    "鱼肠莹鸊鹈。",
    "水声分陇咽，",
    "马色度关迷。",
    "晓幕胡沙惨，",
    "危烽汉月低。",
    "仍闻数骑将，",
    "更欲出辽西。"
  ],
  "ptype": null,
  "poetName": "郑锡",
