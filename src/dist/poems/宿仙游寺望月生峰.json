  "_id": "6b2e8d473d5440c6a41ec940a18f399d",
  "name": "宿仙游寺望月生峰",
  "contents": [
    "公门身入洞门行，",
    "出阱离笼似有情。",
    "僧语夜凉云树黑，",
    "月生峰上月初生。"
  ],
  "ptype": null,
  "poetName": "薛能",
