{
  "_id": "dbf23307c5dd4048afbebbb9753deb70",
  "name": "答王无功冬夜载酒乡馆",
  "contents": [
    "颁条忝贵郡，",
    "悬榻久相望。",
    "处士同杨郑，",
    "邦君谢李疆。",
    "讵知方拥彗，",
    "逢子敬惟桑。",
    "明朝蓬户侧，",
    "会自谒任棠。"
  ],
  "ptype": null,
  "poetName": "崔善为",
  "poetId": "b2171fbb5fdf46a695ca4afc28fca995",
