  "_id": "0520a33417f94cebbaf014d115ed522b",
  "name": "题李山人幽居",
  "contents": [
    "襄阳南郭外，",
    "茅屋一书生。",
    "无事焚香坐，",
    "有时寻竹行。",
    "画苔藤杖细，",
    "踏石笋鞋轻。",
    "应笑风尘客，",
    "区区逐世名。"
  ],
  "ptype": null,
  "poetName": "张籍",
  "poetId": "e28934ff12d24f49af3b3cf028d21fec",
