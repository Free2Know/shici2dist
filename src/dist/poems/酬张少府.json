  "_id": "216de6c841f946a99a4794f946888db2",
  "name": "酬张少府",
  "contents": [
    "晚年惟好静，",
    "万事不关心。",
    "自顾无长策，",
    "空知返旧林。",
    "松风吹解带，",
    "山月照弹琴。",
    "君问穷通理，",
    "渔歌入浦深。"
  ],
  "ptype": null,
  "poetName": "王维",
