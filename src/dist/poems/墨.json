  "_id": "7937430964134edbac4f3aa90483db8f",
  "name": "墨",
  "contents": [
    "长安分石炭，",
    "上党结松心。",
    "绕画蝇初落，",
    "含滋绶更深。",
    "悲丝光易染，",
    "叠素彩还沉。",
    "别有张芝学，",
    "书池幸见临。"
  ],
  "ptype": null,
  "poetName": "李峤",
