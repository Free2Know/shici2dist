  "_id": "066dddba50e6407bb43953ae8cdc8bdc",
  "name": "玉清公主挽歌",
  "contents": [
    "学凤年犹小，",
    "乘龙日尚赊。",
    "初封千户邑，",
    "忽驾五云车。",
    "地接金人岸，",
    "山通玉女家。",
    "秋风何太早，",
    "吹落禁园花。"
  ],
  "ptype": null,
  "poetName": "张谓",
