  "_id": "45b8d6865db540ccb2a62c363857e5dc",
  "name": "送智雄上人",
  "contents": [
    "忽起游方念，",
    "飘然不可留。",
    "未知携一锡，",
    "乘兴向何州。",
    "古岸春云散，",
    "遥天晚雨收。",
    "想应重会面，",
    "风月又清秋。"
  ],
  "ptype": null,
  "poetName": "李中",
