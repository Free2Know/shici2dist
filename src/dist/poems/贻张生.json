  "_id": "42542bbcdf9d484798022e31ad801b86",
  "name": "贻张生",
  "contents": [
    "日日见入寺，",
    "未曾含酒容。",
    "闲听老僧语，",
    "坐到夕阳钟。",
    "竹里行多影，",
    "花边偶过踪。",
    "犹言谢生计，",
    "随我去孤峰。"
  ],
  "ptype": null,
  "poetName": "齐己",
