  "_id": "0cb668b311d64a0a8a952cdacdcdca25",
  "name": "牛渚夜泊",
  "contents": [
    "烟老石矶平，",
    "袁郎夜泛情。",
    "数吟人不遇，",
    "千古月空明。",
    "人事年年别，",
    "春潮日日生。",
    "无因逢谢尚，",
    "风物自凄清。"
  ],
  "ptype": null,
  "poetName": "崔涂",
  "poetId": "38436ba6861b4cc2aa2f4b94156aac28",
