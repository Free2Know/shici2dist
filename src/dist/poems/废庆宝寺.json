  "_id": "50ff63c66c954c389967ac687d8468d7",
  "name": "废庆宝寺",
  "contents": [
    "黄叶前朝寺，",
    "无僧寒殿开。",
    "池晴龟出暴，",
    "松暝鹤飞回。",
    "古井碑横草，",
    "阴廊画杂苔。",
    "禅宫亦销歇，",
    "尘世转堪哀。"
  ],
  "ptype": null,
  "poetName": "耿湋",
  "poetId": "788f24bf37734a228bdef82cf65f22c0",
