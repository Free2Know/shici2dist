  "_id": "471d83da52154644bcbf393b94c04d7a",
  "name": "宿韦员外宅",
  "contents": [
    "传经韦相后，",
    "赐笔汉家郎。",
    "幽阁诸生会，",
    "寒宵几刻长。",
    "座中灯泛酒，",
    "檐外月如霜。",
    "人事多飘忽，",
    "邀欢讵可忘。"
  ],
  "ptype": null,
  "poetName": "耿湋",
