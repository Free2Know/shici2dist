  "_id": "6b26f52290f5417aab926203cad6273a",
  "name": "寄白头陀",
  "contents": [
    "近见头陀伴，",
    "云师老更慵。",
    "性灵闲似鹤，",
    "颜壮古于松。",
    "山里犹难觅，",
    "人间岂易逢。",
    "仍闻移住处，",
    "太白最高峰。"
  ],
  "ptype": null,
  "poetName": "白居易",
  "poetId": "4b47c82f564748dcb909526665b648f9",
