{
  "_id": "03b93acaa05b4b1b91c1e9644fd95569",
  "name": "寄诗友",
  "contents": [
    "天地有万物，",
    "尽应输苦心。",
    "他人虽欲解，",
    "此道奈何深。",
    "返朴遗时态，",
    "关门度岁阴。",
    "相思去秋夕，",
    "共对冷灯吟。"
  ],
  "ptype": null,
  "poetName": "齐己",
