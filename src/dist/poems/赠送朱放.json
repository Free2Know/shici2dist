  "_id": "824f9f6711c44b90b6bfd12f465f97c4",
  "name": "赠送朱放",
  "contents": [
    "昔年居汉水，",
    "日醉习家池。",
    "道胜迹常在，",
    "名高身不知。",
    "欲依天目住，",
    "新自始宁移。",
    "生事曾无长，",
    "惟将白接蘺。"
  ],
  "ptype": null,
  "poetName": "严维",
