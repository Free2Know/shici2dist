  "_id": "75d65fdb62064c1585296cfff03fcc83",
  "name": "赠董先生",
  "contents": [
    "寿岁过于百，",
    "时闲到上京。",
    "餐松双鬓嫩，",
    "绝粒四支轻。",
    "雨雪思中岳，",
    "云霞梦赤城。",
    "来年期寿箓，",
    "何处待先生。"
  ],
  "ptype": null,
  "poetName": "徐夤",
