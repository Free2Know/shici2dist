  "_id": "7220fb02e202414da184a92ace06e48f",
  "name": "酬王六寒朝见诒",
  "contents": [
    "贾生流寓日，",
    "扬子寂寥时。",
    "在物多相背，",
    "唯君独见思。",
    "渔为江上曲，",
    "雪作郢中词。",
    "忽枉兼金讯，",
    "长怀伐木诗。"
  ],
  "ptype": null,
  "poetName": "张九龄",
