  "_id": "9daa41e91c7943088b3546fefc03f8ff",
  "name": "寄绩溪陈明府",
  "contents": [
    "古邑猿声里，",
    "空城只半存。",
    "岸移无旧路，",
    "沙涨别成村。",
    "鼓角喧京口，",
    "江山尽汝濆。",
    "六朝兴废地，",
    "行子一销魂。"
  ],
  "ptype": null,
  "poetName": "张乔",
  "poetId": "12203b3aad6548b487528cd4bc5d4c4b",
