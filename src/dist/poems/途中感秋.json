  "_id": "843b7510c70c484b8338cb144d13d418",
  "name": "途中感秋",
  "contents": [
    "节物行摇落，",
    "年颜坐变衰。",
    "树初黄叶日，",
    "人欲白头时。",
    "乡国程程远，",
    "亲朋处处辞。",
    "唯残病与老，",
    "一步不相离。"
  ],
  "ptype": null,
  "poetName": "白居易",
