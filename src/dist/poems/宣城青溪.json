  "_id": "e8ce3e3f115b4c2fb13f9a74f8646567",
  "name": "宣城青溪",
  "contents": [
    "青溪胜桐庐，",
    "水木有佳色。",
    "山貌日高古，",
    "石容天倾侧。",
    "彩鸟昔未名，",
    "白猿初相识。",
    "不见同怀人，",
    "对之空叹息。"
  ],
  "ptype": null,
  "poetName": "李白",
