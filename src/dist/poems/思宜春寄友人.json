  "_id": "340730d383d946bb9e79446e2ef79415",
  "name": "思宜春寄友人",
  "contents": [
    "胜游虽隔年，",
    "魂梦亦依然。",
    "瀑水喧秋思，",
    "孤灯动夜船。",
    "断虹全岭雨，",
    "斜月半溪烟。",
    "旧日吟诗侣，",
    "何人更不眠。"
  ],
  "ptype": null,
  "poetName": "张乔",
  "poetId": "12203b3aad6548b487528cd4bc5d4c4b",
