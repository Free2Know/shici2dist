  "_id": "88673ac3217341c199c8818ce5f988db",
  "name": "孤桐",
  "contents": [
    "天质自森森，",
    "孤高几百寻。",
    "凌霄不屈己，",
    "得地本虚心。",
    "岁老根弥壮，",
    "阳骄叶更阴。",
    "明时思解愠，",
    "愿斫五弦琴。"
  ],
  "ptype": null,
  "poetName": "王安石",
