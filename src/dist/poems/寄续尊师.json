  "_id": "fa549c62e72541c1b665440f50e200ba",
  "name": "寄续尊师",
  "contents": [
    "得道任发白，",
    "亦逢城市游。",
    "新经天上取，",
    "稀药洞中收。",
    "春木带枯叶，",
    "新蒲生漫流。",
    "年年望灵鹤，",
    "常在此山头。"
  ],
  "ptype": null,
  "poetName": "于鹄",
