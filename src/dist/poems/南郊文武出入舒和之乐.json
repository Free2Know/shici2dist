{
  "_id": "61fba56da36c48a7bbea00aa45c040ee",
  "name": "南郊文武出入舒和之乐",
  "contents": [
    "祝史辞正，",
    "人神庆叶。",
    "福以德昭，",
    "享以诚接。",
    "六变云备，",
    "百礼斯浃。",
    "祀事孔明，",
    "祚流万叶。"
  ],
  "ptype": null,
  "poetName": "张九龄",
