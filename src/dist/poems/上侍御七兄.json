  "_id": "4b3aa5b655914b71932aa07ee3ac8e3a",
  "name": "上侍御七兄",
  "contents": [
    "天人俟明路，",
    "益稷分尧心。",
    "利器必先举，",
    "非贤安可任。",
    "吾兄执严宪，",
    "时佐能钓深。"
  ],
  "ptype": null,
  "poetName": "王昌龄",
