  "_id": "90b41d92457d42269da8996442103e85",
  "name": "宴陈十六楼",
  "contents": [
    "西楼对金谷，",
    "此地古人心。",
    "白日落庭内，",
    "黄花生涧阴。",
    "四邻见疏木，",
    "万井度寒砧。",
    "石上题诗处，",
    "千年留至今。"
  ],
  "ptype": null,
  "poetName": "李颀",
