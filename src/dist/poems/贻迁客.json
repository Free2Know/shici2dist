  "_id": "7344e5197f6f4bb2bc64bbca94f5f233",
  "name": "贻迁客",
  "contents": [
    "无机还得罪，",
    "直道不伤情。",
    "微雨昏山色，",
    "疏笼闭鹤声。",
    "闲居多野客，",
    "高枕见江城。",
    "门外长溪水，",
    "怜君又濯缨。"
  ],
  "ptype": null,
  "poetName": "杜牧",
  "poetId": "cb365a383b844378951f5ba10473b765",
