  "_id": "d43fa103c8cc42cdbad14e167b416c14",
  "name": "题樟亭",
  "contents": [
    "晓霁凭虚槛，",
    "云山四望通。",
    "地盘江岸绝，",
    "天映海门空。",
    "树色连秋霭，",
    "潮声入夜风。",
    "年年此光景，",
    "催尽白头翁。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "03ce3b6c2c524f708213939828d7fb40",
