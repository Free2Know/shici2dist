  "_id": "5a6e48b974574fb2b2d297cf3fea2833",
  "name": "洛中晴望",
  "contents": [
    "九陌尽风尘，",
    "嚣嚣昼复昏。",
    "古今人不断，",
    "南北路长存。",
    "叶落上阳树，",
    "草衰金谷园。",
    "乱鸦归未已，",
    "残日半前轩。"
  ],
  "ptype": null,
  "poetName": "于武陵",
  "poetId": "14f8d7fae3da4e8091b0d9719f53521a",
