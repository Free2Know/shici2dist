  "_id": "b6753c2c55664ac9abe7abdac039cebd",
  "name": "祭神州乐章·雍和",
  "contents": [
    "泰圻严享，",
    "阴郊展敬。",
    "礼以导神，",
    "乐以和性。",
    "黝牲在列，",
    "黄琮俯映。",
    "九土既平，",
    "万邦贻庆。"
  ],
  "ptype": null,
  "poetName": "褚亮",
