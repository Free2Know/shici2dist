{
  "_id": "5d1dcff8a4f14a2c8edba785fcdc1ec9",
  "name": "留赠偃师主人",
  "contents": [
    "孤城漏未残，",
    "徒侣拂征鞍。",
    "洛北去游远，",
    "淮南归梦阑。",
    "晓灯回壁暗，",
    "晴雪卷帘寒。",
    "强尽主人酒，",
    "出门行路难。"
  ],
  "ptype": null,
  "poetName": "许浑",
  "poetId": "fec65c0dd6484000837ecf7a1f995f70",
