  "_id": "9db3a9a0b4084a94945c389549793186",
  "name": "览萧必先卷",
  "contents": [
    "满轴编新句，",
    "翛然大雅风。",
    "名因五字得，",
    "命合一言通。",
    "景尽才难尽，",
    "吟终意未终。",
    "似逢曹与谢，",
    "烟雨思何穷。"
  ],
  "ptype": null,
  "poetName": "韦庄",
