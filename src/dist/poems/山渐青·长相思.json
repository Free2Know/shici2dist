  "_id": "18b8dd392f9d46a1ab60e665c881ea48",
  "name": "山渐青·长相思",
  "contents": [
    "山无情。",
    "水无情。",
    "杨柳飞花春雨晴。",
    "征衫长短亭。",
    "拟行行。",
    "重行行。",
    "吟到江南第几程。",
    "江南山渐青。"
  ],
  "ptype": null,
  "poetName": "张辑",
