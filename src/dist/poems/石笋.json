{
  "_id": "85efe4a24b83416cac340e4e247cba7a",
  "name": "石笋",
  "contents": [
    "千古海门石，",
    "移归吟叟居。",
    "窍腥蛟出后，",
    "形瘦浪冲余。",
    "工语宁无玉，",
    "僧知忽有书。",
    "好期仙者叱，",
    "变化向庭隅。"
  ],
  "ptype": null,
  "poetName": "刘昭禹",
  "poetId": "cc511df161e040e99d9ef44421474214",
