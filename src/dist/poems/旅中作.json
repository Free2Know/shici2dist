  "_id": "b8031d2f116d418a848ff9bdbd19276c",
  "name": "旅中作",
  "contents": [
    "妄动远抛山，",
    "其如馁与寒。",
    "投人言去易，",
    "开口说贫难。",
    "泽国云千片，",
    "湘江竹一竿。",
    "时明未忍别，",
    "犹待计穷看。"
  ],
  "ptype": null,
  "poetName": "裴说",
