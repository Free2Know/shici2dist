  "_id": "236558e0505a4dd280005cd6fe8cc19e",
  "name": "南涧耕叟",
  "contents": [
    "年年南涧滨，",
    "力尽志犹存。",
    "雨雪朝耕苦，",
    "桑麻岁计贫。",
    "战添丁壮役，",
    "老忆太平春。",
    "见说经荒后，",
    "田园半属人。"
  ],
  "ptype": null,
  "poetName": "崔涂",
  "poetId": "38436ba6861b4cc2aa2f4b94156aac28",
