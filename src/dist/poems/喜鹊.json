  "_id": "d2361c9b9aef4a5e80484d74b64bcbc5",
  "name": "喜鹊",
  "contents": [
    "弃膻在庭际，",
    "双鹊来摇尾。",
    "欲啄怕人惊，",
    "喜语晴光里。",
    "何况佞幸人，",
    "微禽解如此。"
  ],
  "ptype": null,
  "poetName": "皮日休",
