  "_id": "e75fa3d46cbf4492b55b3de85af8b8cb",
  "name": "袍中诗",
  "contents": [
    "沙场征戍客，",
    "寒苦若为眠。",
    "战袍经手作，",
    "知落阿谁边？",
    "蓄意多添线，",
    "含情更著绵。",
    "今生已过也，",
    "结取后生缘。"
  ],
  "ptype": null,
  "poetName": "开元宫人",
