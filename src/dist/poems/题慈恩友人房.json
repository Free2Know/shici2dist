  "_id": "09e9983ca03a457e80f4c52b9f0e2e9c",
  "name": "题慈恩友人房",
  "contents": [
    "贾生耽此寺，",
    "胜事入诗多。",
    "鹤宿星千树，",
    "僧归烧一坡。",
    "塔棱垂雪水，",
    "江色映茶锅。",
    "长久堪栖息，",
    "休言忆镜波。"
  ],
  "ptype": null,
  "poetName": "李洞",
  "poetId": "764946b24de146e6a613409086ac1c83",
