  "_id": "e4e50d5aeebe43b1b73ae77fb15ee1d8",
  "name": "冬深",
  "contents": [
    "花叶随天意，",
    "江溪共石根。",
    "早霞随类影，",
    "寒水各依痕。",
    "易下杨朱泪，",
    "难招楚客魂。",
    "风涛暮不稳，",
    "舍棹宿谁门。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "526c710fccb149fe933edb19dad0ed50",
