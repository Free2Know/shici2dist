  "_id": "9212cb0178b4474696984a0ba1b03600",
  "name": "故北平公挽歌",
  "contents": [
    "木落园林旷，",
    "庭虚风露寒。",
    "北里清音绝，",
    "南陔芳草残。",
    "远气犹标剑，",
    "浮云尚写冠。",
    "寂寂琴台晚，",
    "秋阴入井干。"
  ],
  "ptype": null,
  "poetName": "上官仪",
  "poetId": "dd06ea8b0a044b09963a84a3370e64b6",
