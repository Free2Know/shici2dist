  "_id": "2251d72b37dd483c90c5cc348ca2e5f0",
  "name": "赠卢司户",
  "contents": [
    "秋色无远近，",
    "出门尽寒山。",
    "白云遥相识，",
    "待我苍梧间。",
    "借问卢耽鹤，",
    "西飞几岁还。"
  ],
  "ptype": null,
  "poetName": "李白",
