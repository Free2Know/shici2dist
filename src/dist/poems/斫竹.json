  "_id": "5786d22091f0424db3a68a359db97af1",
  "name": "斫竹",
  "contents": [
    "寺废竹色死，",
    "宦家宁尔留。",
    "霜根渐随斧，",
    "风玉尚敲秋。",
    "江南苦吟客，",
    "何处送悠悠。"
  ],
  "ptype": null,
  "poetName": "杜牧",
