{
  "_id": "2d508ddecf974e4c996850af0732a38c",
  "name": "警急",
  "contents": [
    "才名旧楚将，",
    "妙略拥兵机。",
    "玉垒虽传檄，",
    "松州会解围。",
    "和亲知拙计，",
    "公主漫无归。",
    "青海今谁得，",
    "西戎实饱飞。"
  ],
  "ptype": null,
  "poetName": "杜甫",
