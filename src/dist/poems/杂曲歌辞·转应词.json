  "_id": "7ddc8d9e7a974b3f8c7465ce3bcc6469",
  "name": "杂曲歌辞·转应词",
  "contents": [
    "边草，",
    "边草，",
    "边草尽来共老。",
    "山南山北雪晴，",
    "千里万里月明。",
    "明月，",
    "明月，",
    "胡笳一声愁绝。"
  ],
  "ptype": null,
  "poetName": "戴叔伦",
