{
  "_id": "d7ee293e593d49538d17ce06fd26fc0e",
  "name": "饮马歌",
  "contents": [
    "边头春未到。",
    "雪满交河道。",
    "暮沙明残照。",
    "塞烽云间小。",
    "断鸿悲。",
    "陇月低。",
    "泪湿征衣悄。",
    "岁华老。"
  ],
  "ptype": null,
  "poetName": "曹勋",
