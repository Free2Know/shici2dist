  "_id": "3d79dff5c8714b98b320ad83a90b3a08",
  "name": "杂曲歌辞·宜城歌",
  "contents": [
    "野水绕空城，",
    "行尘起孤驿。",
    "花台侧生树，",
    "石碣阳镌额。",
    "靡靡度行人，",
    "温风吹宿麦。",
    "知"
  ],
  "ptype": null,
  "poetName": "刘禹锡",
