  "_id": "46c3b6ef17d34d059e73c245eec6a736",
  "name": "送王员外归朝",
  "contents": [
    "往来无尽目，",
    "离别要逢春。",
    "海内罹多事，",
    "天涯见近臣。",
    "芳时万里客，",
    "乡路独归人。",
    "魏阙心常在，",
    "随君亦向秦。"
  ],
  "ptype": null,
  "poetName": "刘长卿",
  "poetId": "1690eff8d5c64ea6b219ded1aecd4075",
