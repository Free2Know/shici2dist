  "_id": "fc2e297446c2453a8b610ba76592ddc3",
  "name": "遇元上人",
  "contents": [
    "七泽过名山，",
    "相逢黄落残。",
    "杉松开寺晚，",
    "泉月话心寒。",
    "祖遍诸方礼，",
    "经曾几处看。",
    "应怀出家院，",
    "紫阁近长安。"
  ],
  "ptype": null,
  "poetName": "齐己",
