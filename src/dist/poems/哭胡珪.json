  "_id": "3c426ed901d948748f6710bb1ef127af",
  "name": "哭胡珪",
  "contents": [
    "才高登上第，",
    "孝极殁庐茔。",
    "一命何无定，",
    "片言徒有声。",
    "故园花自发，",
    "新冢月初明。",
    "寂寞重泉里，",
    "岂知春物荣。"
  ],
  "ptype": null,
  "poetName": "方干",
