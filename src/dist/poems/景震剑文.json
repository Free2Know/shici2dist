{
  "_id": "f6e1c7bc29ea4853840f8f7411e55167",
  "name": "景震剑文",
  "contents": [
    "撝雷电，",
    "运玄星。",
    "摧凶恶，",
    "亨利贞。",
    "乾降精，",
    "坤应灵。",
    "日月象，",
    "岳渎形。"
  ],
  "ptype": null,
  "poetName": "唐无名氏",
