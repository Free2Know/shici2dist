  "_id": "fc052427ae1246a0bad74385cb6dd677",
  "name": "思帝乡",
  "contents": [
    "云髻坠，",
    "凤钗垂。",
    "髻坠钗垂无力，",
    "枕函欹。",
    "翡翠屏深月落，",
    "漏依依。",
    "说尽人间天上，",
    "两心知。"
  ],
  "ptype": null,
  "poetName": "韦庄",
