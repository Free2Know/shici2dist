{
  "_id": "d66917cceb854cbc9f66ec0ced63218a",
  "name": "伤心行",
  "contents": [
    "咽咽学楚吟，",
    "病骨伤幽素。",
    "秋姿白发生，",
    "木叶啼风雨。",
    "灯青兰膏歇，",
    "落照飞蛾舞。",
    "古壁生凝尘，",
    "羁魂梦中语。"
  ],
  "ptype": null,
  "poetName": "李贺",
  "poetId": "cd247d8e8f974620a738b26e2464470c",
