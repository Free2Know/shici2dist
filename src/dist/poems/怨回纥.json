{
  "_id": "6fc9f7c87d8349d8a774c59edb98f7cc",
  "name": "怨回纥",
  "contents": [
    "祖席驻征棹，",
    "开帆候信潮。",
    "隔筵桃叶泣，",
    "吹管杏花飘。",
    "船去鸥飞阁，",
    "人归尘上桥。",
    "别离惆怅泪，",
    "江路湿红蕉。"
  ],
  "ptype": null,
  "poetName": "皇甫松",
  "poetId": "24271099d236401b8ab4205f699af734",
