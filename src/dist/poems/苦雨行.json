  "_id": "894bb2150d7245beab9ba9dd3db89d22",
  "name": "苦雨行",
  "contents": [
    "悠悠飞走情，",
    "同乐在阳和。",
    "岁中三百日，",
    "常恐风雨多。",
    "天人信遐远，",
    "时节易蹉跎。",
    "洞房有明烛，",
    "无乃酣且歌。"
  ],
  "ptype": null,
  "poetName": "刘禹锡",
