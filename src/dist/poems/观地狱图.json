  "_id": "4a75b38eec494b56aa340a336ffb6554",
  "name": "观地狱图",
  "contents": [
    "峨峨非剑阁，",
    "有树不堪攀。",
    "佛手遮不得，",
    "人心似等闲。",
    "周王应未雪，",
    "白起作何颜。",
    "尽日空弹指，",
    "茫茫尘世间。"
  ],
  "ptype": null,
  "poetName": "贯休",
