  "_id": "9cb386e7145c40bdba838890c376523d",
  "name": "送袁偁",
  "contents": [
    "谏猎名空久，",
    "多因病与贫。",
    "买书行几市，",
    "带雨别何人。",
    "客路山连水，",
    "军州日映尘。",
    "凄凉一分手，",
    "俱恨老相亲。"
  ],
  "ptype": null,
  "poetName": "卢纶",
