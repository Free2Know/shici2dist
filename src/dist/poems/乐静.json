{
  "_id": "b175d50c516d428ca626794a50c003c1",
  "name": "乐静",
  "contents": [
    "引手强篸巾，",
    "徐徐起病身。",
    "远心群野鹤，",
    "闲话对村人。",
    "发匣琴徽静，",
    "开瓶酒味真。",
    "纵闻兵赋急，",
    "原宪本家贫。"
  ],
  "ptype": null,
  "poetName": "张祜",
