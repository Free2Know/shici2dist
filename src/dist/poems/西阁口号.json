  "_id": "dbf76983ba4a47b0beef13c5d738b9a0",
  "name": "西阁口号",
  "contents": [
    "山木抱云稠，",
    "寒江绕上头。",
    "雪崖才变石，",
    "风幔不依楼。",
    "社稷堪流涕，",
    "安危在运筹。",
    "看君话王室，",
    "感动几销忧。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "526c710fccb149fe933edb19dad0ed50",
