  "_id": "8ccead1c45cc43669b6999663c611935",
  "name": "楚怨",
  "contents": [
    "秋入楚江水，",
    "独照汨罗魂。",
    "手把绿荷泣，",
    "意愁珠泪翻。",
    "九门不可入，",
    "一犬吠千门。"
  ],
  "ptype": null,
  "poetName": "孟郊",
