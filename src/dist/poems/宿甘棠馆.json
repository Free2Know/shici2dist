  "_id": "d788d91e3d3e4401a4a9e658b7363a65",
  "name": "宿甘棠馆",
  "contents": [
    "尘迹入门尽，",
    "悄然江海心。",
    "水声巫峡远，",
    "山色洞庭深。",
    "风桂落寒子，",
    "岚烟凝夕阴。",
    "前轩鹤归处，",
    "萝月思沈沈。"
  ],
  "ptype": null,
  "poetName": "储嗣宗",
  "poetId": "c8807d0e0225473d977ba4b12021c9ea",
