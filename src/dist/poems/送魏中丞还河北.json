  "_id": "d77c7698ebe04c5989987d7e7312f1bf",
  "name": "送魏中丞还河北",
  "contents": [
    "宁知贵公子，",
    "本是鲁诸生。",
    "上国风尘旧，",
    "中司印绶荣。",
    "辛勤戎旅事，",
    "雪下护羌营。"
  ],
  "ptype": null,
  "poetName": "皇甫冉",
