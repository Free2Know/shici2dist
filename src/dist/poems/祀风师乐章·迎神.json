  "_id": "6675520d86f54da58cc077dd4b588186",
  "name": "祀风师乐章·迎神",
  "contents": [
    "太皞御气，",
    "句芒肇功。",
    "苍龙青旗，",
    "爰候祥风。",
    "律以和应，",
    "神以感通。",
    "鼎俎修蚃，",
    "时惟礼崇。"
  ],
  "ptype": null,
  "poetName": "包佶",
