  "_id": "8b3d6fd4c5094d1d96f339914e007771",
  "name": "越中赠程先生",
  "contents": [
    "纱帽度残春，",
    "虚舟寄一身。",
    "溪边逢越女，",
    "花里问秦人。",
    "古塞连山静，",
    "阴霞落海新。",
    "有时城郭去，",
    "暗与酒家亲。"
  ],
  "ptype": null,
  "poetName": "法振",
  "poetId": "1c5fbb37599644da9a9595932679c978",
