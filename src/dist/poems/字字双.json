  "_id": "6e3d042bb79d45f18beb6435a50e81b6",
  "name": "字字双",
  "contents": [
    "床头锦衾斑复斑，",
    "架上朱衣殷复殷。",
    "空庭明月闲复闲，",
    "夜长路远山复山。"
  ],
  "ptype": null,
  "poetName": "王丽真女",
