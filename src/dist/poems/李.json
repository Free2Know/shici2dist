  "_id": "d5ca5a30dec24004a5b3e3586b434bf4",
  "name": "李",
  "contents": [
    "君子盈天阶，",
    "小人甘自免。",
    "方随炼金客，",
    "林上家绝巘。",
    "背岭花未开，",
    "入云树深浅。",
    "清昼犹自眠，",
    "山鸟时一啭。"
  ],
  "ptype": null,
  "poetName": "王维",
