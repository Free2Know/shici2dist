{
  "_id": "bc5c140bf16f48a19e82e046b9c85846",
  "name": "潼关道中",
  "contents": [
    "白道晓霜迷，",
    "离灯照马嘶。",
    "秋风满关树，",
    "残月隔河鸡。",
    "来往非无倦，",
    "穷通岂易齐。",
    "何年归故社，",
    "披雨剪春畦。"
  ],
  "ptype": null,
  "poetName": "郑谷",
  "poetId": "8f43db35f49d4c51bd1e9704dd65363c",
