  "_id": "e7112c70690740788983bcfdcb505e4f",
  "name": "客中言怀",
  "contents": [
    "白发照乌纱，",
    "逢人只自嗟。",
    "官闲如致仕，",
    "客久似无家。",
    "夜雨孤灯梦，",
    "春风几度花。",
    "故园归有日，",
    "诗酒老生涯。"
  ],
  "ptype": null,
  "poetName": "戴叔伦",
  "poetId": "5d90649e304a4bceb43ad0b2a9c5c274",
