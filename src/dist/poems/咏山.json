  "_id": "e00c92dbaf294741aa4bd409490df047",
  "name": "咏山",
  "contents": [
    "灵山峙千仞，",
    "蔽日且嵯峨。",
    "紫盖云阴远，",
    "香炉烟气多。",
    "石梁高鸟路，",
    "瀑水近天河。",
    "欲知闻道里，",
    "别自有仙歌。"
  ],
  "ptype": null,
  "poetName": "刘斌",
  "poetId": "545a2f223ed94fedaaac4bd9441b9b35",
