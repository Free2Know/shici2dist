  "name": "醉妆词",
  "contents": [
    "者边走，",
    "那边走，",
    "只是寻花柳。",
    "那边走，",
    "者边走，",
    "莫厌金杯酒。"
  ],
  "ptype": null,
  "poetName": "李衍",
