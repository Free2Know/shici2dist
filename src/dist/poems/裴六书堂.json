  "_id": "b55faf0a10f842119265057ae0f5f9b7",
  "name": "裴六书堂",
  "contents": [
    "闲堂闭空阴，",
    "竹林但清响。",
    "窗下长啸客，",
    "区中无遗想。",
    "经纶精微言，",
    "兼济当独往。"
  ],
  "ptype": null,
  "poetName": "王昌龄",
