{
  "_id": "9661d25776f141e29d97469d43ddc27b",
  "name": "赠同席",
  "contents": [
    "四座列吾友，",
    "满园花照衣。",
    "一生知几度，",
    "后到拟先归。",
    "急管侵诸乐，",
    "严城送落晖。",
    "当欢莫离席，",
    "离席却欢稀。"
  ],
  "ptype": null,
  "poetName": "李昌符",
  "poetId": "23cfa21291b447fd90add3ae214e5960",
