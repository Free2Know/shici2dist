  "_id": "8aa0a5c7a0f24275ae10e1b05782261f",
  "name": "敕勒歌",
  "contents": [
    "敕勒川，",
    "阴山下，",
    "天似穹庐，",
    "笼盖四野。",
    "天苍苍，",
    "野茫茫，",
    "风吹草低见牛羊。"
  ],
  "ptype": null,
  "poetName": "南北朝无名氏",
