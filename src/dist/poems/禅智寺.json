{
  "_id": "6f76f9cfa8ca48c9be33e47a4403e6eb",
  "name": "禅智寺",
  "contents": [
    "宝殿依山嶮，",
    "临虚势若吞。",
    "画檐齐木末，",
    "香砌压云根。",
    "远景窗中岫，",
    "孤烟竹里村。",
    "凭高聊一望，",
    "乡思隔吴门。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "03ce3b6c2c524f708213939828d7fb40",
