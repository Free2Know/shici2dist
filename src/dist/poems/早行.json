{
  "_id": "2f9e8c238cca4381b0d597c54ae145bc",
  "name": "早行",
  "contents": [
    "鸡唱催人起，",
    "又生前去愁。",
    "路明残月在，",
    "山露宿云收。",
    "村店烟火动，",
    "渔家灯烛幽。",
    "趋名与趋利，",
    "行役几时休。"
  ],
  "ptype": null,
  "poetName": "王观",
