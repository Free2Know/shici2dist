  "_id": "4412b1c78d884a109b28bb9e58db1bd2",
  "name": "秋日病中",
  "contents": [
    "析析檐前竹，",
    "秋声拂簟凉。",
    "病加阴已久，",
    "愁觉夜初长。",
    "坐拾车前子，",
    "行看肘后方。",
    "无端忧食忌，",
    "开镜倍萎黄。"
  ],
  "ptype": null,
  "poetName": "张祜",
  "poetId": "03ce3b6c2c524f708213939828d7fb40",
