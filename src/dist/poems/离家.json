  "_id": "5b365baf16834c848c6c8d98c3d5a7e6",
  "name": "离家",
  "contents": [
    "八月初长夜，",
    "千山第一程。",
    "款颜唯有梦，",
    "怨泣却无声。",
    "祖席诸宾散，",
    "空郊匹马行。",
    "自怜非达识，",
    "局促为浮名。"
  ],
  "ptype": null,
  "poetName": "韩偓",
  "poetId": "bca72048a93e447f96a7e1dac88dd312",
