{
  "_id": "9928b9c3a1814ba79debb0ae45c245d2",
  "name": "寄景地判官",
  "contents": [
    "渚宫江上别，",
    "倏忽十余年。",
    "举世唯攻说，",
    "多君即不然。",
    "浦珠为履重，",
    "园柳助诗玄。",
    "勉力酬知己，",
    "昌朝正急贤。"
  ],
  "ptype": null,
  "poetName": "贯休",
  "poetId": "2010d2499c414e5c920b044c4657b2cb",
