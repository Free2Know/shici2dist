  "_id": "25b133b02eb04326afb7f63d8395c907",
  "name": "馀干旅舍",
  "contents": [
    "摇落暮天迥，",
    "青枫霜叶稀。",
    "孤城向水闭，",
    "独鸟背人飞。",
    "渡口月初上，",
    "邻家渔未归。",
    "乡心正欲绝，",
    "何处捣寒衣。"
  ],
  "ptype": null,
  "poetName": "刘长卿",
  "poetId": "1690eff8d5c64ea6b219ded1aecd4075",
