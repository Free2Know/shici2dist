{
  "_id": "5b5a96f7a6044bf7943eb2d68dbdd3a2",
  "name": "游春",
  "contents": [
    "海岳三峰古，",
    "春皇二月寒。",
    "绿潭渔子钓，",
    "红树美人攀。",
    "弱蔓环沙屿，",
    "飞花点石关。",
    "溪山游未厌，",
    "琴酌弄晴湾。"
  ],
  "ptype": null,
  "poetName": "邢巨",
  "poetId": "85856748c2a74d43a7a10717b5011218",
