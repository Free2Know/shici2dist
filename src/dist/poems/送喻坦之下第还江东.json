  "_id": "3488d83994c64c17a0544d6586a25ea3",
  "name": "送喻坦之下第还江东",
  "contents": [
    "文战偶未胜，",
    "无令移壮心。",
    "风尘辞帝里，",
    "舟楫到家林。",
    "过楚寒方尽，",
    "浮淮月正沈。",
    "持杯话来日，",
    "不听洞庭砧。"
  ],
  "ptype": null,
  "poetName": "方干",
  "poetId": "06f0291dac634a49ab474dd7cadbf853",
