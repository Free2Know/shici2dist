{
  "_id": "f4f0c3e3894b4937aa5a833705c8b370",
  "name": "绵州巴歌",
  "contents": [
    "子山，",
    "打瓦鼓。",
    "扬平山，",
    "撒白雨。",
    "下白雨，",
    "取龙女。",
    "织得绢，",
    "二丈五。",
    "一半属罗江，",
    "一半属玄武。"
  ],
  "ptype": null,
  "poetName": "隋无名氏",
