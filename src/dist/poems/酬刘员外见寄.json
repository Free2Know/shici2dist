  "_id": "87398773af6c4a338ffa19ba39c32cf1",
  "name": "酬刘员外见寄",
  "contents": [
    "苏耽佐郡时，",
    "近出白云司。",
    "药补清羸疾，",
    "窗吟绝妙词。",
    "柳塘春水慢，",
    "花坞夕阳迟。",
    "欲识怀君意，",
    "明朝访楫师。"
  ],
  "ptype": null,
  "poetName": "严维",
  "poetId": "1f88dc7f7f2a4573a234c7bbb5b0cc63",
