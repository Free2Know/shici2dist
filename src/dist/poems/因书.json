{
  "_id": "8555a5c50f094d45ad09e4372c103cd4",
  "name": "因书",
  "contents": [
    "绝徼南通栈，",
    "孤城北枕江。",
    "猿声连月槛，",
    "鸟影落天窗。",
    "海石分棋子，",
    "郫筒当酒缸。",
    "生归话辛苦，",
    "别夜对凝釭。"
  ],
  "ptype": null,
  "poetName": "李商隐",
