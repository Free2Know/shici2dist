  "_id": "56d7d0d0072b4551b5c5a869522eca9d",
  "name": "蔷薇架",
  "contents": [
    "五色阶前架，",
    "一张笼上被。",
    "殷红稠叠花，",
    "半绿鲜明地。",
    "风蔓罗裙带，",
    "露英莲脸泪。",
    "多逢走马郎，",
    "可惜帘边思。"
  ],
  "ptype": null,
  "poetName": "元稹",
  "poetId": "9863285b1f514da9a74432fd80c442aa",
