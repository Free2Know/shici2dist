  "_id": "5b85407f5c62490e941a584d6579d940",
  "name": "宿山居",
  "contents": [
    "山店灯前客，",
    "酬身未有媒。",
    "乡关贫后别，",
    "风雨夜深来。",
    "上国求丹桂，",
    "衡门长绿苔。",
    "堪惊双鬓雪，",
    "不待岁寒催。"
  ],
  "ptype": null,
  "poetName": "朱庆馀",
