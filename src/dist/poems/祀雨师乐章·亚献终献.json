{
  "_id": "22bfae55796d40d59dd657c5bed9ccbf",
  "name": "祀雨师乐章·亚献终献",
  "contents": [
    "奠既备，",
    "献将终。",
    "神行令，",
    "瑞飞空。",
    "迎乾德，",
    "祈岁功。",
    "乘烟燎，",
    "俨从风。"
  ],
  "ptype": null,
  "poetName": "包佶",
  "poetId": "c2d3c763229c40f283a633c748dabd8a",
