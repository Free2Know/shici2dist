{
  "_id": "e6f52002e3ed4974a7f81d6b306a2bfb",
  "name": "赠王凤二山人",
  "contents": [
    "山兄望鹤信，",
    "山弟听乌占。",
    "养药同开鼎，",
    "休棋各枕奁。",
    "相逢九江底，",
    "共到五峰尖。",
    "愿许为三友，",
    "羞将白发撏。"
  ],
  "ptype": null,
  "poetName": "李洞",
  "poetId": "764946b24de146e6a613409086ac1c83",
