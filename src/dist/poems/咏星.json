{
  "_id": "5f8d005f82884c66a954f7c9f8f8bc8e",
  "name": "咏星",
  "contents": [
    "历历东井舍，",
    "昭昭右掖垣。",
    "云际龙文出，",
    "池中鸟色翻。",
    "流辉下月路，",
    "坠影入河源。",
    "方知颍川集，",
    "别有太丘门。"
  ],
  "ptype": null,
  "poetName": "董思恭",
  "poetId": "4e58c7e42f7843b29401ecbe8f05ef66",
