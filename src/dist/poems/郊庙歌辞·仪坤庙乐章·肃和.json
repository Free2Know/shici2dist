  "_id": "0029079d030844148037f73618352990",
  "name": "郊庙歌辞·仪坤庙乐章·肃和",
  "contents": [
    "祼圭既濯，",
    "郁鬯既陈。",
    "画幕云举，",
    "黄流玉醇。",
    "仪充献酌，",
    "礼盛众禋。",
    "地察惟孝，",
    "愉焉飨亲。"
  ],
  "ptype": null,
  "poetName": "张齐贤",
