  "_id": "7168b08064b8472d97e6077a311d7781",
  "name": "玩华胥",
  "contents": [
    "古岸蟠桃初绽。",
    "新月素光交灿。",
    "尘世几人窥，",
    "我留心高盼。",
    "长叹。",
    "长叹。",
    "回首远游仙梵。"
  ],
  "ptype": null,
  "poetName": "侯善渊",
