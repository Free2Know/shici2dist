  "_id": "e66d930ac7fd462bb3ef6e148d1a87c8",
  "name": "废畦",
  "contents": [
    "秋蔬拥霜露，",
    "岂敢惜凋残。",
    "暮景数枝叶，",
    "天风吹汝寒。",
    "绿沾泥滓尽，",
    "香与岁时阑。",
    "生意春如昨，",
    "悲君白玉盘。"
  ],
  "ptype": null,
  "poetName": "杜甫",
  "poetId": "526c710fccb149fe933edb19dad0ed50",
