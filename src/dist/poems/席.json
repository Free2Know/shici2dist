{
  "_id": "236fe5d9ef6743da9c095934c2e33583",
  "name": "席",
  "contents": [
    "辟席承宣父，",
    "重筵揖戴公。",
    "桂香浮半月，",
    "兰气袭回风。",
    "舞拂丹霞上，",
    "歌清白雪中。",
    "伫将文绮色，",
    "舒卷帝王宫。"
  ],
  "ptype": null,
  "poetName": "李峤",
