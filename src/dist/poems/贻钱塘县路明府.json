  "_id": "db543b3efc524284b144530d4097fcbe",
  "name": "贻钱塘县路明府",
  "contents": [
    "志业不得力，",
    "到今犹苦吟。",
    "吟成五字句，",
    "用破一生心。",
    "世路屈声远，",
    "寒溪怨气深。",
    "前贤多晚达，",
    "莫怕鬓霜侵。"
  ],
  "ptype": null,
  "poetName": "方干",
  "poetId": "06f0291dac634a49ab474dd7cadbf853",
