  "_id": "bd295ce4173b4085b4b916a6d2d83573",
  "name": "西墅新居",
  "contents": [
    "渐渐见苔青，",
    "疏疏遍地生。",
    "闲穿藤屐起，",
    "乱踏石阶行。",
    "野鸟啼幽树，",
    "名僧笑此情。",
    "残阳竹阴里，",
    "老圃打门声。"
  ],
  "ptype": null,
  "poetName": "齐己",
  "poetId": "d9aee30d8768477493378453904a05f3",
