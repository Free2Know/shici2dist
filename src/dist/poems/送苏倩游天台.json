  "_id": "c65ba3640a9142f3bf2cf80688e59ef6",
  "name": "送苏倩游天台",
  "contents": [
    "灵异寻沧海，",
    "笙歌访翠微。",
    "江鸥迎共狎，",
    "云鹤待将飞。",
    "琪树尝仙果，",
    "琼楼试羽衣。",
    "遥知神女问，",
    "独怪阮郎归。"
  ],
  "ptype": null,
  "poetName": "张子容",
  "poetId": "4b156240f87b46f59712d6d70eaeb473",
