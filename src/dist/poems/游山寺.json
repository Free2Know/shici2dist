  "_id": "2258350b27414f89bf0bbe7911f793b6",
  "name": "游山寺",
  "contents": [
    "千峰路盘尽，",
    "林寺昔何名。",
    "步步入山影，",
    "房房闻水声。",
    "多年人迹断，",
    "残照石阴清。",
    "自可求居止，",
    "安闲过此生。"
  ],
  "ptype": null,
  "poetName": "无可",
