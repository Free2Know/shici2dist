  "_id": "e92dbf382e7c448d905c876425c294fb",
  "name": "闲咏",
  "contents": [
    "步月怜清景，",
    "眠松爱绿阴。",
    "早年诗思苦，",
    "晚岁道情深。",
    "夜学禅多坐，",
    "秋牵兴暂吟。",
    "悠然两事外，",
    "无处更留心。"
  ],
  "ptype": null,
  "poetName": "白居易",
