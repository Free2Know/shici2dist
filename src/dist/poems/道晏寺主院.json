  "_id": "be69d16984f943509bdd459e20177251",
  "name": "道晏寺主院",
  "contents": [
    "北邻有幽竹，",
    "潜筠穿我庐。",
    "往来地已密，",
    "心乐道者居。",
    "残花回往节，",
    "轻条荫夏初。",
    "闻钟北窗起，",
    "啸傲永日馀。"
  ],
  "ptype": null,
  "poetName": "韦应物",
