  "_id": "77e56cf3c224435cb6b7344582abb4a5",
  "name": "送胡校书秩满归河中",
  "contents": [
    "古树汾阴道，",
    "悠悠东去长。",
    "位卑仍解印，",
    "身老又还乡。",
    "河水平秋岸，",
    "关门向夕阳。",
    "音书须数附，",
    "莫学晋嵇康。"
  ],
  "ptype": null,
  "poetName": "耿湋",
  "poetId": "788f24bf37734a228bdef82cf65f22c0",
