  "_id": "b52062e1bd2e4696b154f706df677e75",
  "name": "朔管",
  "contents": [
    "寥寥落何处，",
    "一夜过胡天。",
    "送苦秋风外，",
    "吹愁白发边。",
    "望乡皆下泪，",
    "久戍尽休眠。",
    "寂寞空沙晓，",
    "开眸片月悬。"
  ],
  "ptype": null,
  "poetName": "方干",
