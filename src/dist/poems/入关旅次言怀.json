  "_id": "54fb941ba5df43b992fd2f4877dc3aee",
  "name": "入关旅次言怀",
  "contents": [
    "寸心唯自切，",
    "上国与谁期。",
    "月晦时风雨，",
    "秋深日别离。",
    "便休终未肯，",
    "已苦不能疑。",
    "独愧商山路，",
    "千年四皓祠。"
  ],
  "ptype": null,
  "poetName": "黄滔",
