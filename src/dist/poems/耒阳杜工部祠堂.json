  "_id": "14d1e448da9a437ab52cae3bd6440c12",
  "name": "耒阳杜工部祠堂",
  "contents": [
    "手接汨罗水，",
    "天心知所存。",
    "固教工部死，",
    "来伴大夫魂。",
    "流落同千古，",
    "风骚共一源。",
    "消凝伤往事，",
    "斜日隐颓垣。"
  ],
  "ptype": null,
  "poetName": "徐介",
