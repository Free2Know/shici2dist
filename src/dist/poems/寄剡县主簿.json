  "_id": "15cb6d2979bb4030963d94a079d0b6ee",
  "name": "寄剡县主簿",
  "contents": [
    "金庭养真地，",
    "珠篆会稽官。",
    "境胜堪长往，",
    "时危喜暂安。",
    "洞连沧海阔，",
    "山拥赤城寒。",
    "他日抛尘土，",
    "因君拟炼丹。"
  ],
  "ptype": null,
  "poetName": "罗隐",
