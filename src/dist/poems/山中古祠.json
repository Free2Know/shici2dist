{
  "_id": "09f3b36b78464f50b2b44a6cae8aeb5a",
  "name": "山中古祠",
  "contents": [
    "春草空祠墓，",
    "荒林唯鸟飞。",
    "记年碑石在，",
    "经乱祭人稀。",
    "野鼠缘朱帐，",
    "阴尘盖画衣。",
    "近门潭水黑，",
    "时见宿龙归。"
  ],
  "ptype": null,
  "poetName": "张籍",
  "poetId": "e28934ff12d24f49af3b3cf028d21fec",
