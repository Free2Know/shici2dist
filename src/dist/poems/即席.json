{
  "_id": "40b833cd77b047df94ec0372a252fa97",
  "name": "即席",
  "contents": [
    "家住丛台旧，",
    "名参绛圃新。",
    "醉波疑夺烛，",
    "娇态欲沈春。",
    "伴雨聊过楚，",
    "归云定占秦。",
    "桃花正浓暖，",
    "争不浪迷人。"
  ],
  "ptype": null,
  "poetName": "吴融",
  "poetId": "dda938f77a1640fcbcf61eabff53424d",
