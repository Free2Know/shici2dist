  "_id": "e0aec06ce56e48df931b64d7097a5a1d",
  "name": "思乡作",
  "contents": [
    "泸北行人绝，",
    "云南信未还。",
    "庭前花不扫，",
    "门外柳谁攀。",
    "坐久销银烛，",
    "愁多减玉颜。",
    "悬心秋夜月，",
    "万里照关山。"
  ],
  "ptype": null,
  "poetName": "布燮",
  "poetId": "a207b239374c42a6af0650262ae8329a",
