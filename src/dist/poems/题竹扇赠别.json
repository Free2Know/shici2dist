  "_id": "b9c1f4aa9fa8477aaf0637a6fbfde56c",
  "name": "题竹扇赠别",
  "contents": [
    "湘竹殊堪制，",
    "齐纨且未工。",
    "幸亲芳袖日，",
    "犹带旧林风。",
    "掩笑歌筵里，",
    "传书卧阁中。",
    "竟将为别赠，",
    "宁与合欢同。"
  ],
  "ptype": null,
  "poetName": "皇甫冉",
  "poetId": "bc4446a8d5594756be20f1ff356e27ab",
