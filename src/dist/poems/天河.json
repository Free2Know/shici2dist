  "_id": "cfd9c5ceb2d748ee8796a0d75db2e581",
  "name": "天河",
  "contents": [
    "常时任显晦，",
    "秋至辄分明。",
    "纵被微云掩，",
    "终能永夜清。",
    "含星动双阙，",
    "伴月照边城。",
    "牛女年年渡，",
    "何曾风浪生。"
  ],
  "ptype": null,
  "poetName": "杜甫",
