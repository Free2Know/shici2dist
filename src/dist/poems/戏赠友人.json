{
  "_id": "061e9f7b0d5c49b982b58c4bdf091b1e",
  "name": "戏赠友人",
  "contents": [
    "一日不作诗，",
    "心源如废井。",
    "笔砚为辘轳，",
    "吟咏作縻绠。",
    "朝来重汲引，",
    "依旧得清冷。",
    "书赠同怀人，",
    "词中多苦辛。"
  ],
  "ptype": null,
  "poetName": "贾岛",
  "poetId": "38afd5ae38f14cfdaf787ac4dac8217a",
