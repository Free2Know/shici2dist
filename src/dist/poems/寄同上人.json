  "_id": "6a1f1cd1f6fa47c1a46ceda4f3623362",
  "name": "寄同上人",
  "contents": [
    "高高山顶寺，",
    "更有最高人。",
    "定起松鸣屋，",
    "吟圆月上身。",
    "云藏三伏热，",
    "水散百溪津。",
    "曾乞兰花供，",
    "无书又过春。"
  ],
  "ptype": null,
  "poetName": "唐彦谦",
