  "_id": "9a8b530e71ba45f6be0e0d0c3b3c293c",
  "name": "春秋战国门管仲",
  "contents": [
    "美酒浓馨客要沽，",
    "门深谁敢强提壶。",
    "苟非贤主询贤士，",
    "肯信沽人畏子  。"
  ],
  "ptype": null,
  "poetName": "周昙",
